msgstr "I na rekhodo DZOṰHE u bva kha thebuḽu '%1' fhedzi ndi rekhodo dzi bvaho kha thebuḽu '%2' dzine ndeme kha zwipiḓa zwi elanaho zwa elana."

#: querydlg.src
msgctxt ""
"querydlg.src\n"
"STR_QUERY_FULL_JOIN\n"
"string.text"
msgid "Contains ALL records from '%1' and from '%2'."
msgstr "Dzina rekhodo DZOṰHE u bva kha '%1' na u bva kha '%2'."

#: querydlg.src
msgctxt ""
"querydlg.src\n"
"STR_QUERY_CROSS_JOIN\n"
"string.text"
