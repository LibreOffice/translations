msgid "~Single %PRODUCTNAME Calc document"
msgstr "~Еден документ од %PRODUCTNAME Calc"

#: euro.src#STEP_AUTOPILOT___2.string.text
msgid "Complete ~directory"
msgstr "Цела п~апка"

