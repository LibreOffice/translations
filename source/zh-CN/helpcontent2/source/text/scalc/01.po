"par_id1001631901312606\n"
"help.text"
msgid "<emph>Text1</emph> and <emph>Text2</emph> must each take one of the following values: \"ATS\", \"BEF\", \"CYP\", \"DEM\", \"EEK\", \"ESP\", \"EUR\", \"FIM\", \"FRF\", \"GRD\", \"IEP\", \"ITL\", \"LTL\", \"LUF\", \"LVL\", \"MTL\", \"NLG\", \"PTE\", \"SIT\", and \"SKK\"."
msgstr ""

#. RBRUW
#: 04060106.xhp
msgctxt ""
