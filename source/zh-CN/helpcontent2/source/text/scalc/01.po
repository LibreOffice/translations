"func_time.xhp\n"
"par_id3156076\n"
"help.text"
msgid "<item type=\"input\">=TIME(0;0;0)</item> returns 00:00:00"
msgstr "「<item type=\"input\">=TIME(0;0;0)</item>」返回 00:00:00"

#. DKovu
