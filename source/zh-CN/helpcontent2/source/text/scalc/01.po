msgctxt ""
"func_weeknumadd.xhp\n"
"par_id3149914\n"
"help.text"
msgid "<item type=\"input\">=WEEKNUM_EXCEL2003(DATE(2001;12;24);1)</item> returns 52."
msgstr "「<item type=\"input\">=WEEKNUM_EXCEL2003(DATE(2001;12;24);1)</item>」返回 52。"

