msgctxt ""
"04040000.xhp\n"
"hd_id160220162139252941\n"
"help.text"
msgid "Columns After"
msgstr "后方插入列"

#. RB8Ju
#: 04040000.xhp
msgctxt ""
"04040000.xhp\n"
"par_id160220162138041164\n"
"help.text"
msgid "<ahelp hid=\".uno:InsertColumnsAfter\">Inserts a new column after the active cell.</ahelp>"
msgstr "<ahelp hid=\".uno:InsertColumnsAfter\">在活动单元格的后方插入新列。</ahelp>"

