msgid "Tile size"
msgstr "平铺大小"

#. uj7Mx
#: 13050200.xhp
msgctxt ""
"13050200.xhp\n"
"par_id3156448\n"
"help.text"
msgid "<ahelp hid=\"modules/sdraw/ui/vectorize/tiles\">Enter the size of the rectangle for the background fill.</ahelp>"
msgstr "<ahelp hid=\"modules/sdraw/ui/vectorize/tiles\">输入用于背景填充的矩形的大小。</ahelp>"

#. wMeob
#: 13050200.xhp
msgctxt ""
"13050200.xhp\n"
"hd_id3148840\n"
"help.text"
msgid "Source image:"
msgstr "源图像:"

#. C8DaD
