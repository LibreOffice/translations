msgctxt "03102100.xhp#par_id3150518.48.help.text"
msgid "for i = 0 to 20"
msgstr "for i = 0 to 20"

#: 03102100.xhp#par_id3166428.49.help.text
msgctxt "03102100.xhp#par_id3166428.49.help.text"
msgid "for ii = 0 to 2"
msgstr "for ii = 0 to 2"

