"hd_id3152924\n"
"help.text"
msgid "<variable id=\"copytext2application\"><link href=\"text/shared/guide/copytext2application.xhp\">Inserting Data From Text Documents</link></variable>"
msgstr "<variable id=\"copytext2application\"><link href=\"text/shared/guide/copytext2application.xhp\">插入文本文档数据</link></variable>"

#. iFDtC
