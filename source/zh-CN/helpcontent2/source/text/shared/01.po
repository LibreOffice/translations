msgctxt ""
"05020700.xhp\n"
"par_id3148539\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/asiantypography/checkApplySpacing\">Inserts a space between ideographic and alphabetic text.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/asiantypography/checkApplySpacing\">在象形文字与字母文字之间插入空格。</ahelp>"

#. HGAr6
