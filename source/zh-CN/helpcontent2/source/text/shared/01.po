msgctxt ""
"05020200.xhp\n"
"par_id3150254\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/effectspage/underlinecolorlb\">Select the color for the underlining.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/effectspage/underlinecolorlb\">选择下划线颜色。</ahelp>"

