"hd_id3155069\n"
"help.text"
msgid "<link href=\"text/shared/02/12040000.xhp\">Reset Filter/Sorting</link>"
msgstr "<link href=\"text/shared/02/12040000.xhp\">重置筛选/排序</link>"

#. nxp5e
