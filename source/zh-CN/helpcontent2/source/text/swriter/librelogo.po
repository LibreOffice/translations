"LibreLogo.xhp\n"
"par_590\n"
"help.text"
msgid "PRINT “text”[2] ; print “x”<br/> PRINT “text”[1:3] ; print “ex”<br/>"
msgstr "PRINT “text”[2] ; 输出 「x」<br/> PRINT “text”[1:3] ; 输出 「ex」<br/>"

