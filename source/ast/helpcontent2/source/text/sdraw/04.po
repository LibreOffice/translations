msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Comandu</caseinline><defaultinline>Control</defaultinline></switchinline>+F3"

#: 01020000.xhp#par_id3159238.12.help.text
msgid "Close group editor."
msgstr "Zarrar l'editor de grupos."

#: 01020000.xhp#hd_id3150199.13.help.text
msgid "Shift+F3"
msgstr "(Mayús)(F3)"

#: 01020000.xhp#par_id3152994.14.help.text
msgid "Opens the <emph>Duplicate</emph> dialog."
msgstr "Abre'l diálogu <emph>Doblar</emph>."

#: 01020000.xhp#hd_id3154488.15.help.text
msgid "F4"
msgstr "(F4)"

#: 01020000.xhp#par_id3149406.16.help.text
