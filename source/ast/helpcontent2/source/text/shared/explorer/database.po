msgid "Date"
msgstr "Date"

#: 02010100.xhp#par_id3149753.262.help.text
msgid "{d 'YYYY-MM-DD'}"
msgstr "{d 'AAAA-MM-DD'}"

#: 02010100.xhp#par_id3156318.263.help.text
msgctxt "02010100.xhp#par_id3156318.263.help.text"
msgid "Time"
