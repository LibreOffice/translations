msgctxt ""
"13050300.xhp\n"
"par_id3155066\n"
"help.text"
msgid "<ahelp hid=\".uno:ConvertInto3D\">Converts the selected object to a three-dimensional (3D) object.</ahelp>"
msgstr "<ahelp hid=\".uno:ConvertInto3D\">Convierte l'oxetu escoyíu nun oxetu tridimensional (3D).</ahelp>"

