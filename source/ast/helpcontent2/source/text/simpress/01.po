msgstr "<ahelp hid=\"modules/sdraw/ui/copydlg/start\" >Escueya un color pal oxetu escoyíu.</ahelp>"

#: 02120000.xhp
msgctxt ""
"02120000.xhp\n"
"hd_id3156258\n"
"help.text"
msgid "End"
msgstr "Final"

