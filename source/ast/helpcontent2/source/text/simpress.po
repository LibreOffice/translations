"help.text"
msgid "Outline Bar"
msgstr "Barra Esquema"

#: main0211.xhp
msgctxt ""
"main0211.xhp\n"
"hd_id3154013\n"
"help.text"
msgid "<link href=\"text/simpress/main0211.xhp\" name=\"Outline Bar\">Outline Bar</link>"
msgstr "<link href=\"text/simpress/main0211.xhp\" name=\"Barra de esquema\">Barra d'esquema</link>"

#: main0211.xhp
