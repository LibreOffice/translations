#. aopUo
#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"hd_id3145178\n"
"help.text"
msgid "UPPER"
msgstr "MAYÚSC"

#. oaPDP
#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
