msgctxt ""
"04060106.xhp\n"
"par_id3153866\n"
"help.text"
msgid "<item type=\"input\">=LN(3)</item> returns the natural logarithm of 3 (approximately 1.0986)."
msgstr "<item type=\"input\">=LN(3)</item> devuelve'l llogaritmu natural de 3 (aprosimao 1,0986)."

