msgid "=B1=0 => TRUE"
msgstr "=B1=0 > VERDADERU"

#: empty_cells.xhp#par_id4653767.help.text
msgid "=B1=\"\" => TRUE (MS-Excel: FALSE)"
msgstr "=B1=\"\" > VERDADERU (MS-Excel: FALSU)"

#: empty_cells.xhp#par_id8801538.help.text
msgid "C1: =VLOOKUP(...) with empty cell result => displays empty (MS-Excel: displays 0)"
msgstr "C1: =BUSCARV(...) con una caxella balera resulta > amosar baleru (MS-Excel: amuesa 0)"

#: empty_cells.xhp#par_id6746421.help.text
msgid "=ISNUMBER(VLOOKUP(...)) => FALSE"
msgstr "=ESNÚMERO(BUSCARV(...)) > FALSU"

#: empty_cells.xhp#par_id4876247.help.text
msgid "=ISTEXT(VLOOKUP(...)) => FALSE"
msgstr "=ESTEXTO(BUSCARV(...)) > FALSU"

#: empty_cells.xhp#par_id7458723.help.text
