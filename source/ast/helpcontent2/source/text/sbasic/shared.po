msgid "Organizing Libraries"
msgstr "Organización de biblioteques"

#. EgYeV
#: 01030400.xhp
msgctxt ""
"01030400.xhp\n"
"hd_id3125864\n"
"help.text"
msgid "Creating a New Library"
msgstr "Creación d'una biblioteca nueva"

#. Qmf6K
#: 01030400.xhp
msgctxt ""
"01030400.xhp\n"
