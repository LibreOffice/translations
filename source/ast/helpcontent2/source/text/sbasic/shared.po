msgid "Syntax:"
msgstr "Sintaxis:"

#: 03090410.xhp#par_id3155934.4.help.text
msgid "Switch (Expression1, Value1[, Expression2, Value2[..., Expression_n, Value_n]])"
msgstr "Switch (Espresión1, Valor1[, Espresión2, Valor2[..., Espresión_n, Valor_n]])"

