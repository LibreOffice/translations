msgstr "<ahelp hid=\"SW:CHECKBOX:TP_TOX_SELECT:CB_FROMOLE\">Inclou objectes OLE en l'índex.</ahelp>"

#: 04120215.xhp#hd_id3149095.16.help.text
msgid "Use level from source chapter"
msgstr "Utilitzeu el nivell del capítol font"

#: 04120215.xhp#par_id3151250.17.help.text
