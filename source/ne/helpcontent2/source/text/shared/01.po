"hd_id3148566\n"
"help.text"
msgid "Delete"
msgstr "मेट्नुहोस्"

#: 02110000.xhp
msgctxt ""
"02110000.xhp\n"
"par_id3153099\n"
"help.text"
msgid "<ahelp hid=\"HID_GLBLTREE_DEL\">Deletes the selection from the Navigator list.</ahelp>"
msgstr "<ahelp hid=\"HID_GLBLTREE_DEL\">नेभिगेटर सूचीबाट चयन मेट्छ ।</ahelp>"

