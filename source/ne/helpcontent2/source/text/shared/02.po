msgctxt ""
"01140000.xhp\n"
"hd_id3145150\n"
"help.text"
msgid "<variable id=\"textbox_title\">Text Box</variable>"
msgstr ""

#. DXTV6
#: 01140000.xhp
msgctxt ""
"01140000.xhp\n"
"par_id3145790\n"
"help.text"
msgid "<image id=\"img_id3155608\" src=\"cmd/sc_texttoolbox.png\" width=\"0.1665in\" height=\"0.1665in\"><alt id=\"alt_id3155608\">Icon</alt></image>"
