msgid "<link href=\"text/swriter/01/05180000.xhp\" name=\"Wrap\">Wrap</link>"
msgstr "<link href=\"text/swriter/01/05180000.xhp\" name=\"Wrap\">र्याप</link>"

#: 05180000.xhp#par_id3153002.2.help.text
