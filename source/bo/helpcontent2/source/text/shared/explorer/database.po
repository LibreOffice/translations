msgctxt ""
"05030100.xhp\n"
"bm_id3149164\n"
"help.text"
msgid "<bookmark_value>primary keys; defining</bookmark_value>"
msgstr "<bookmark_value>གཙོ་བོའི་འགག་གནད་ཚིག་་; མཚན་འཇོག</bookmark_value>"

