msgctxt ""
"dabawiz02ldap.xhp\n"
"par_idN10579\n"
"help.text"
msgid "Server URL"
msgstr "གཙོ་འཁོར་མིང།"

#: dabawiz02ldap.xhp
msgctxt ""
"dabawiz02ldap.xhp\n"
"par_idN1057D\n"
"help.text"
msgid "<ahelp hid=\"HID_DSADMIN_LDAP_HOSTNAME\">Enter the name of the LDAP server using the format \"ldap.server.com\".</ahelp>"
