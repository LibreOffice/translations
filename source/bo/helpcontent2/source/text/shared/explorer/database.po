"hd_id3154071\n"
"help.text"
msgid "Run"
msgstr "འཁོར་རྒྱུགས།"

#: 11080000.xhp
msgctxt ""
"11080000.xhp\n"
"par_id3151210\n"
"help.text"
msgid "<ahelp hid=\"dbaccess/ui/directsqldialog/execute\">Runs the command that you entered in the <emph>Command to execute </emph>box.</ahelp>"
msgstr "<ahelp hid=\"dbaccess/ui/directsqldialog/execute\">ཁྱེད་ཀྱིས་<emph>ལག་བསྟར་བྱ་རྒྱུའི་བཀའ་ཚིག་</emph>སྒྲོམ་ནང་ནང་འཇུག་བྱས་པའི་བཀའ་ཚིག་ལག་བསྟར་བྱ། </ahelp>"

