msgid "Date min"
msgstr "ཚེས་གྲངས་ཐུང་ཤོས།"

#. WKnRH
#: 01170101.xhp
msgctxt ""
"01170101.xhp\n"
"par_id3155755\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Determines the earliest date that a user can enter.</ahelp> Determines the earliest date that a user can enter."
msgstr ""

#. ao2mE
#: 01170101.xhp
msgctxt ""
