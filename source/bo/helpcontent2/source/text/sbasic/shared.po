msgstr "བརྡ་འཕྲོད་"

#: 03090410.xhp
msgctxt ""
"03090410.xhp\n"
"par_id3155934\n"
"help.text"
msgid "Switch (Expression1, Value1[, Expression2, Value2[..., Expression_n, Value_n]])"
msgstr "Switch (Expression1, Value1[, Expression2, Value2[..., Expression_n, Value_n]])"

#: 03090410.xhp
msgctxt ""
"03090410.xhp\n"
"hd_id3149119\n"
