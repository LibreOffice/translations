msgstr ""

#. FFvts
#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id2949141\n"
"help.text"
msgid "<item type=\"input\">=LEFTB(\"中国\";1)</item> returns \" \" (1 byte is only half a DBCS character and a space character is returned instead)."
msgstr ""

#. mQvUN
#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id2949151\n"
"help.text"
