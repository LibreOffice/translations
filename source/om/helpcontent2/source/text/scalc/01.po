msgctxt ""
"04060118.xhp\n"
"par_id3151052\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_ZINS\">Returns the constant interest rate per period of an annuity.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_ZINS\">Yeroo yerootti waggaa keessa makkii dhalaa dhaabbataa kenna.</ahelp>"

#. wEYqD
#: 04060118.xhp
msgctxt ""
