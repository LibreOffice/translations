msgctxt ""
"04060106.xhp\n"
"par_id3152904\n"
"help.text"
msgid "<item type=\"input\">=COMBINA(3;2)</item> returns 6."
msgstr "<item type=\"input\">=COMBINA(3;2)</item> 6 deebisa."

