msgstr ""

#. FM3FS
#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id2949181\n"
"help.text"
msgid "<item type=\"input\">=LEFTB(\"office\";3)</item> returns \"off\" (3 non-DBCS characters each consisting of 1 byte)."
msgstr ""

