msgctxt ""
"01160500.xhp\n"
"hd_id3154568\n"
"help.text"
msgid "Display area"
msgstr "Iddoo agarsiisi"

#: 01160500.xhp
msgctxt ""
"01160500.xhp\n"
"hd_id3153668\n"
"help.text"
msgid "File name"
msgstr "Maqaa faayilii"

#: 01160500.xhp
msgctxt ""
"01160500.xhp\n"
"hd_id3155892\n"
"help.text"
msgid "separated by"
msgstr ""

