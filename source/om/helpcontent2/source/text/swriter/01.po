"par_id3150689\n"
"help.text"
msgid "<link href=\"text/shared/01/05040200.xhp\" name=\"Text direction\">Text direction</link>."
msgstr "<link href=\"text/shared/01/05040200.xhp\" name=\"Text direction\">Qixa Barruu</link>."

