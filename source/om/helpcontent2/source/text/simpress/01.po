"hd_id3152596\n"
"help.text"
msgid "<link href=\"text/simpress/01/13050000.xhp\" name=\"Convert\">Convert</link>"
msgstr "<link href=\"text/simpress/01/13050000.xhp\" name=\"Convert\">Jijjiiri</link>"

#: 13050000.xhp
msgctxt ""
