msgid "<variable id=\"loeschentext\"><ahelp hid=\".uno:DeleteRows\">Deletes the selected row(s) from the table.</ahelp></variable>"
msgstr "<variable id=\"loeschentext\"><ahelp hid=\".uno:DeleteRows\">Gabaticha irraa tarree(wwan) filataman haqa.</ahelp></variable>"

#: 05110500m.xhp#par_id3149591.100.help.text
msgctxt "05110500m.xhp#par_id3149591.100.help.text"
msgid "On <emph>Table</emph> Bar, click"
msgstr "Kabala <emph>Gabatee</emph> irra cuqaasi"

#: 05110500m.xhp#par_id3147555.help.text
