msgctxt ""
"03060300.xhp\n"
"par_id3154910\n"
"help.text"
msgid "<emph>Result:</emph> Any numeric variable that contains the result of the implication."
msgstr "<emph>Bu'aa:</emph> lakkoofsi jijjirama kamuu bu'aa hordoffii qaba."

#: 03060300.xhp
msgctxt ""
"03060300.xhp\n"
"par_id3156281\n"
