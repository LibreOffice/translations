msgctxt ""
"03090410.xhp\n"
"par_id3153990\n"
"help.text"
msgid "<emph>Expression:</emph> The expression that you want to evaluate."
msgstr "<emph>Himannoo:</emph>Himannoo madaaluuf barbaadde."

