msgstr "<switchinline select=\"appl\"><caseinline select=\"CALC\">Jijjiiramoota gatame mul'isi</caseinline></switchinline>"

#: 02230200.xhp#par_id3159166.13.help.text
msgid "<switchinline select=\"appl\"><caseinline select=\"CALC\"><ahelp hid=\"SC:CHECKBOX:RID_SCDLG_HIGHLIGHT_CHANGES:CB_HIGHLIGHT_REJECT\">Shows or hides the changes that were rejected.</ahelp></caseinline></switchinline>"
msgstr "<switchinline select=\"appl\"><caseinline select=\"CALC\"><ahelp hid=\"SC:CHECKBOX:RID_SCDLG_HIGHLIGHT_CHANGES:CB_HIGHLIGHT_REJECT\">Jijjiramoota kan gatamee ture mul'isa ykn dhoksa.</ahelp></caseinline></switchinline>"

