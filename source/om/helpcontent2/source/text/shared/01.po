msgstr "<switchinline select=\"appl\"><caseinline select=\"WRITER\">Baay'ina Sararawwanii: </caseinline></switchinline>"

#: 01100400.xhp#par_id3149650.33.help.text
msgid "<switchinline select=\"appl\"><caseinline select=\"WRITER\">Number of lines in the file. </caseinline></switchinline>"
msgstr "<switchinline select=\"appl\"><caseinline select=\"WRITER\">Baay'ina sararawwan faayilii keessaa. </caseinline></switchinline>"

