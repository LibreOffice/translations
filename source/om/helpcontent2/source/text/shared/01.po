msgstr "<link href=\"text/shared/01/05110800.xhp\" name=\"Subscript\">Jalarfii</link>"

#: 05110800.xhp#par_id3152790.2.help.text
msgid "<ahelp hid=\".uno:SubScript\">Reduces the font size of the selected text and lowers the text below the baseline.</ahelp>"
msgstr "<ahelp hid=\".uno:SubScript\">Hammamtaa bocquu barruu filatamee hir'isuun barruu sarjala jalatti gadbuusa.</ahelp>"

#: 01010001.xhp#tit.help.text
