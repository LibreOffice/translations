msgstr "<link href=\"text/shared/01/05240100.xhp\" name=\"Vertically\">Olgataan</link>"

#: 05240100.xhp#par_id3149741.2.help.text
msgid "<ahelp hid=\".uno:MirrorVert\">Flips the selected object(s) vertically from top to bottom.</ahelp>"
msgstr "<ahelp hid=\".uno:MirrorVert\">wanta(toota) irraa gara gaditti olgata meelcha.</ahelp>"

