msgstr "<bookmark_value>addaanfageenya sararaa; baafata halqara keewwaata keessaa</bookmark_value><bookmark_value>barruu; addaan fageenya sararaa</bookmark_value>"

#: 05120000.xhp#hd_id3152876.1.help.text
msgid "<link href=\"text/shared/01/05120000.xhp\" name=\"Line Spacing\">Line Spacing</link>"
msgstr "<link href=\"text/shared/01/05120000.xhp\" name=\"Line Spacing\">Addaan Fageenya Sararaa</link>"

#: 05120000.xhp#par_id3153514.2.help.text
msgid "Specify the amount of space to leave between lines of text in a paragraph."
msgstr "Hanga addaan fageenya gidduu sararoota keewwataatti dhiistu ifteessi."

