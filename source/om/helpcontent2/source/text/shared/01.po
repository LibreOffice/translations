msgstr "<switchinline select=\"appl\"><caseinline select=\"CALC\">Jijjiiramoota wardii fudhatame mul'isi </caseinline></switchinline>"

#: 02230200.xhp#par_id3153541.11.help.text
msgid "<switchinline select=\"appl\"><caseinline select=\"CALC\"><ahelp hid=\"SC:CHECKBOX:RID_SCDLG_HIGHLIGHT_CHANGES:CB_HIGHLIGHT_ACCEPT\">Shows or hides the changes that were accepted.</ahelp></caseinline></switchinline>"
msgstr "<switchinline select=\"appl\"><caseinline select=\"CALC\"><ahelp hid=\"SC:CHECKBOX:RID_SCDLG_HIGHLIGHT_CHANGES:CB_HIGHLIGHT_ACCEPT\">Jijjiiramoota fudhataman  mul'isi ykn dhoksa.</ahelp></caseinline></switchinline>"

#: 02230200.xhp#hd_id3149956.12.help.text
