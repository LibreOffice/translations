#. bGAKz
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText1\n"
"LngText.text"
msgid "Welcome to the [FULLPRODUCTNAME] Installation Wizard"
msgstr "[FULLPRODUCTNAME] ਇੰਸਟਾਲੇਸ਼ਨ ਸਹਾਇਕ ਵਲੋਂ ਜੀ ਆਇਆਂ ਨੂੰ"

#. nBYzF
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText2\n"
"LngText.text"
msgid "This installation will update your installed versions of [PRODUCTNAME]"
msgstr "ਇਹ ਇੰਸਟਾਲੇਸ਼ਨ [PRODUCTNAME] ਦੇ ਤੁਹਾਡੇ ਕੋਲ ਇੰਸਟਾਲ ਹੋਏ ਵਰਜ਼ਨ ਨੂੰ ਅੱਪਡੇਟ ਕਰੇਗਾ"

#. gBF5L
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText3\n"
"LngText.text"
msgid "This might take a moment."
msgstr "ਇਸ ਨੂੰ ਕੁਝ ਸਮਾਂ ਲੱਗ ਸਕਦਾ ਹੈ।"

#. FEETb
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ChooseMyOwnText\n"
"LngText.text"
msgid "Not listed (choose location in an extra step)"
msgstr "ਲਿਸਟ ਨਹੀਂ ਕੀਤਾ (ਅਗਲੇ ਪਗ਼ ਵਿੱਚ ਟਿਕਾਣਾ ਚੁਣੋ)।"

#. ey87A
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListPromptText\n"
"LngText.text"
msgid "Choose [PRODUCTNAME] [PRODUCTVERSION] installation for which you want to install the [FULLPRODUCTNAME]"
msgstr "ਚੁਣੋ [PRODUCTNAME] [PRODUCTVERSION] ਅਧਿਸ਼ਠਾਪਨ ਜਿਸਦੇ ਲਈ ਤੁਸੀ [FULLPRODUCTNAME] ਇੰਨਮਟਾਲ ਕਰਣਾ ਚਾਹੁੰਦੇ ਹੋ।"

#. M9B9E
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ChooseManualText\n"
"LngText.text"
msgid "Point the dialog to your [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "ਡਾਈਲਾਗ ਨੂੰ ਆਪਣੀ [PRODUCTNAME] [PRODUCTVERSION] ਇੰਸਟਾਲੇਸ਼ਨ ਦੱਸੋ।"

#. Quddb
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListOKLabelText\n"
"LngText.text"
msgid "Install"
msgstr "ਇੰਸਟਾਲ"

#. CiKia
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListCancelLabel\n"
"LngText.text"
msgid "Abort"
msgstr "ਅਧੂਰਾ ਛੱਡੋ"

#. GjfRp
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AppInvalidText1\n"
"LngText.text"
msgid "This is not a valid [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "ਇਹ ਠੀਕ [PRODUCTNAME] [PRODUCTVERSION] ਇੰਸਟਾਲੇਸ਼ਨ ਨਹੀਂ ਹੈ"

#. AiE42
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AppInvalidText2\n"
"LngText.text"
msgid "Run the installer again and choose a valid [PRODUCTNAME] [PRODUCTVERSION] installation"
msgstr "ਇੰਸਟਾਲਰ ਮੁੜ-ਚਲਾਓ ਅਤੇ ਢੁੱਕਵੀਂ [PRODUCTNAME] [PRODUCTVERSION] ਇੰਸਟਾਲੇਸ਼ਨ ਚੁਣੋ।"

#. iXawi
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"StartInstallText1\n"
"LngText.text"
msgid "Click Install to start the installation"
msgstr "ਇੰਸਟਾਲੇਸ਼ਨ ਸ਼ੁਰੂ ਕਰਨ ਲਈ ਇੰਸਟਾਲ ਕਰੋ ਨੂੰ ਕਲਿੱਕ ਕਰੋ"

#. ikAwX
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"StartInstallText2\n"
"LngText.text"
msgid "Installation might take a minute..."
msgstr "ਇੰਸਟਾਲੇਸ਼ਨ ਨੂੰ ਮਿੰਟ ਲੱਗ ਸਕਦਾ ਹੈ..."

#. jVkZ4
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyQText\n"
"LngText.text"
msgid "Installation failed; most likely your account does not have the necessary privileges."
msgstr "ਇੰਸਟਾਲੇਸ਼ਨ ਫੇਲ੍ਹ ਹੋਈ; ਬਹੁਤਾ ਕਰਕੇ ਤੁਹਾਡੇ ਅਕਾਊਂਟ ਨੂੰ ਲੋੜੀਦੇ ਅਧਿਕਾਰ ਨਾ ਹੋਣ ਕਰਕੇ ਹੋ ਸਕਦਾ ਹੈ।"

#. FG8ap
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyQText2\n"
"LngText.text"
msgid "Do you want to identify as administrator and try again?"
msgstr "ਕੀ ਤੁਸੀਂ ਪਰਸ਼ਾਸ਼ਕ ਵਜੋਂ ਪੁਸ਼ਟੀ ਕਰਕੇ ਮੁੜ ਕੋਸ਼ਿਸ਼ ਕਰੋਗੇ?"

#. ejbhB
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyYES\n"
"LngText.text"
msgid "Yes, identify"
msgstr "ਹਾਂ, ਪਛਾਣ ਦੇਵਾਂਗਾ"

#. QdpWc
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyNO\n"
"LngText.text"
msgid "No, abort installation"
msgstr "ਨਹੀਂ, ਇੰਸਟਾਲੇਸ਼ਨ ਅਧੂਰੀ ਛੱਡੋ"

#. mpz6J
#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallFailedText\n"
"LngText.text"
msgid "Installation failed."
msgstr "ਇੰਸਟਾਲੇਸ਼ਨ ਫੇਲ੍ਹ ਹੋਈ।"

