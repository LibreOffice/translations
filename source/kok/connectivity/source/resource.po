msgctxt ""
"conn_error_message.src\n"
"256 + 2*304 + 0\n"
"string.text"
msgid "No connection to the database exists."
msgstr "डेटाबेसाक खयचेच जोडणी अस्तित्वांतनापुर्ण."

#: conn_error_message.src
msgctxt ""
"conn_error_message.src\n"
"256 + 2*500 + 0\n"
"string.text"
msgid "No $1$ exists."
msgstr "$1$ भायली वाट ना"

