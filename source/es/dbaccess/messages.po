msgctxt "STR_JAVA_SCRIPT"
msgid "JavaScript"
msgstr "JavaScript"

#: dbaccess/inc/strings.hrc:113
msgctxt "STR_BEAN_SHELL"
msgid "BeanShell"
msgstr "BeanShell"

#: dbaccess/inc/strings.hrc:114
msgctxt "STR_JAVA"
msgid "Java"
msgstr "Java"

#: dbaccess/inc/strings.hrc:115
msgctxt "STR_PYTHON"
msgid "Python"
msgstr "Python"

#: dbaccess/inc/strings.hrc:116
msgctxt "STR_DIALOG"
msgid "dialog"
msgstr "diálogo"

#: dbaccess/inc/strings.hrc:117
msgctxt "STR_ERRORS"
msgid "Error(s)"
msgstr "Error(es)"

#: dbaccess/inc/strings.hrc:118
msgctxt "STR_WARNINGS"
msgid "Warnings"
msgstr "Avisos"

#: dbaccess/inc/strings.hrc:119
msgctxt "STR_EXCEPTION"
msgid "caught exception:"
msgstr "capturada excepción:"

#: dbaccess/inc/strings.hrc:120
msgctxt "STR_INVALID_BACKUP_LOCATION"
msgid "You need to choose a backup location other than the document location itself."
msgstr "Para la copia de seguridad debe elegir una ubicación diferente de la ubicación del documento."

#: dbaccess/inc/strings.hrc:121
msgctxt "STR_INVALID_NUMBER_ARGS"
msgid "Invalid number of initialization arguments. Expected 1."
msgstr "La cantidad de argumentos de inicialización es incorrecta. Se esperaba 1."

#: dbaccess/inc/strings.hrc:122
msgctxt "STR_NO_DATABASE"
msgid "No database document found in the initialization arguments."
msgstr "No se encontró un documento de base de datos en los argumentos de inicialización."

#: dbaccess/inc/strings.hrc:123
msgctxt "STR_NOT_READONLY"
msgid "Not applicable to read-only documents."
msgstr "No se aplica a los documentos de solo lectura."

#: dbaccess/inc/strings.hrc:125
msgctxt "STR_QUERY_UNDO_TABWINSHOW"
msgid "Add Table Window"
msgstr "Añadir ventana de tabla"

#: dbaccess/inc/strings.hrc:126
msgctxt "STR_QUERY_UNDO_MOVETABWIN"
msgid "Move table window"
msgstr "Mover ventana de tabla"

#: dbaccess/inc/strings.hrc:127
msgctxt "STR_QUERY_UNDO_INSERTCONNECTION"
msgid "Insert Join"
msgstr "Insertar conexión"

#: dbaccess/inc/strings.hrc:128
msgctxt "STR_QUERY_UNDO_REMOVECONNECTION"
msgid "Delete Join"
msgstr "Eliminar conexión"

#: dbaccess/inc/strings.hrc:129
msgctxt "STR_QUERY_UNDO_SIZETABWIN"
msgid "Resize table window"
msgstr "Redimensionar ventana de tabla"

#: dbaccess/inc/strings.hrc:130
msgctxt "STR_QUERY_UNDO_TABFIELDDELETE"
msgid "Delete Column"
msgstr "Eliminar columna"

#: dbaccess/inc/strings.hrc:131
msgctxt "STR_QUERY_UNDO_TABFIELDMOVED"
msgid "Move column"
msgstr "Mover la columna"

#: dbaccess/inc/strings.hrc:132
msgctxt "STR_QUERY_UNDO_TABFIELDCREATE"
msgid "Add Column"
msgstr "Añadir columna"

#: dbaccess/inc/strings.hrc:133
msgctxt "RID_STR_FIELD_DOESNT_EXIST"
msgid "Invalid expression, field name '$name$' does not exist."
msgstr "La expresión no es válida: el nombre del campo «$name$» no existe."

#: dbaccess/inc/strings.hrc:134
msgctxt "STR_QUERY_UNDO_TABWINDELETE"
msgid "Delete Table Window"
msgstr "Eliminar ventana de tabla"

#: dbaccess/inc/strings.hrc:135
msgctxt "STR_QUERY_UNDO_MODIFY_CELL"
msgid "Edit Column Description"
msgstr "Modificar descripción de columnas"

#: dbaccess/inc/strings.hrc:136
msgctxt "STR_QUERY_UNDO_SIZE_COLUMN"
msgid "Adjust column width"
msgstr "Ajustar anchura de columna"

#: dbaccess/inc/strings.hrc:137
msgctxt "STR_QUERY_SORTTEXT"
msgid "(not sorted);ascending;descending"
msgstr "(sin ordenar);ascendente;descendente"

#: dbaccess/inc/strings.hrc:138
msgctxt "STR_QUERY_FUNCTIONS"
msgid "(no function);Group"
msgstr "(sin función);Agrupar"

#: dbaccess/inc/strings.hrc:139
msgctxt "STR_QUERY_NOTABLE"
msgid "(no table)"
msgstr "(ninguna tabla)"

#: dbaccess/inc/strings.hrc:140
msgctxt "STR_QRY_ORDERBY_UNRELATED"
msgid "The database only supports sorting for visible fields."
msgstr "La base de datos solo admite la clasificación de los campos visibles."

#: dbaccess/inc/strings.hrc:141
msgctxt "STR_QUERY_HANDLETEXT"
msgid "Field;Alias;Table;Sort;Visible;Function;Criterion;Or;Or"
msgstr "Campo;Alias;Tabla;Orden;Visible;Función;Criterio;o;o"

#: dbaccess/inc/strings.hrc:142
msgctxt "STR_QUERY_LIMIT_ALL"
msgid "All"
msgstr "Todo"

#: dbaccess/inc/strings.hrc:143
msgctxt "STR_QRY_TOO_MANY_COLUMNS"
msgid "There are too many columns."
msgstr "Hay demasiadas columnas."

#: dbaccess/inc/strings.hrc:144
msgctxt "STR_QRY_CRITERIA_ON_ASTERISK"
msgid "A condition cannot be applied to field [*]"
msgstr "No se puede aplicar una condición en el campo [*]"

#: dbaccess/inc/strings.hrc:145
msgctxt "STR_QRY_TOO_LONG_STATEMENT"
msgid "The SQL statement created is too long."
msgstr "La expresión SQL creada es demasiado larga."

#: dbaccess/inc/strings.hrc:146
msgctxt "STR_QRY_TOOCOMPLEX"
msgid "Query is too complex"
msgstr "Consulta demasiado compleja"

#: dbaccess/inc/strings.hrc:147
msgctxt "STR_QRY_NOSELECT"
msgid "Nothing has been selected."
msgstr "No se ha seleccionado nada."

#: dbaccess/inc/strings.hrc:148
msgctxt "STR_QRY_SYNTAX"
msgid "SQL syntax error"
msgstr "Error en la sintaxis SQL"

#: dbaccess/inc/strings.hrc:149
msgctxt "STR_QRY_ORDERBY_ON_ASTERISK"
