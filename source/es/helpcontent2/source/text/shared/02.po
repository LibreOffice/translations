msgstr "<ahelp hid=\"cui/ui/fmsearchdialog/rbSearchForNull\">Indica que se van a localizar los campos sin datos.</ahelp>"

#. cfE8f
#: 12100200.xhp
msgctxt ""
"12100200.xhp\n"
"hd_id3153717\n"
"help.text"
msgid "Field content is not NULL"
msgstr "El contenido del campo no es NULL"

#. fMFeY
