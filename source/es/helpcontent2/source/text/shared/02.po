msgctxt ""
"01170802.xhp\n"
"tit\n"
"help.text"
msgid "Table Element Wizard: Field Selection"
msgstr "Asistente para elementos de tabla: Selección de campo"

#. eBuEQ
#: 01170802.xhp
msgctxt ""
"01170802.xhp\n"
"hd_id3155934\n"
"help.text"
msgid "<link href=\"text/shared/02/01170802.xhp\">Table Element Wizard: Field Selection</link>"
