msgid "List Box Wizard: Field Link"
msgstr "Asistente para cuadros de lista: Vínculo de campo"

#. tQdHA
#: 01170903.xhp
msgctxt ""
"01170903.xhp\n"
"hd_id3149119\n"
"help.text"
msgid "<link href=\"text/shared/02/01170903.xhp\">List Box Wizard: Field Link</link>"
