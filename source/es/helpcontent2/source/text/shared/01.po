#: 05210300.xhp
msgctxt ""
"05210300.xhp\n"
"hd_id3149827\n"
"11\n"
"help.text"
msgid "Border"
msgstr "Borde"

#: 05210300.xhp
msgctxt ""
"05210300.xhp\n"
"par_id3155941\n"
"12\n"
