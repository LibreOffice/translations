"05210600.xhp\n"
"par_id3146138\n"
"8\n"
"help.text"
msgid "<ahelp hid=\"HID_TPSHADOW_CTRL\">Click where you want to cast the shadow.</ahelp>"
msgstr "<ahelp hid=\"HID_TPSHADOW_CTRL\" visibility=\"visible\">Pulse con el ratón el punto donde proyectar la sombra.</ahelp>"

#: 05210600.xhp
msgctxt ""
"05210600.xhp\n"
"hd_id3154897\n"
"9\n"
"help.text"
msgid "Distance"
msgstr "Distancia"

#: 05210600.xhp
