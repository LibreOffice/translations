msgctxt ""
"05210100.xhp\n"
"par_id3155412\n"
"48\n"
"help.text"
msgid "Specify the offset for tiling the bitmap in terms of rows and columns."
msgstr "Especifique el desplazamiento del mosaico del bitmap en filas y columnas."

#: 05210100.xhp
msgctxt ""
"05210100.xhp\n"
"hd_id3151115\n"
