msgctxt ""
"05040400.xhp\n"
"par_id3154939\n"
"22\n"
"help.text"
msgid "<ahelp hid=\"SVX:CHECKBOX:RID_SVXPAGE_FOOTER:CB_SHARED_FIRST\">First and even/odd pages share the same content.</ahelp>"
msgstr "<ahelp hid=\"SVX:CHECKBOX:RID_SVXPAGE_FOOTER:CB_SHARED_FIRST\">La primera y las páginas pares / impares comparten el mismo contenido.</ahelp>"

