msgid "<variable id=\"ref_epub_export\"><link href=\"text/shared/01/ref_epub_export.xhp\">Export as EPUB</link></variable>"
msgstr "<variable id=\"ref_epub_export\"><link href=\"text/shared/01/ref_epub_export.xhp\">Exportar a EPUB</link></variable>"

#. bBK9a
#: ref_epub_export.xhp
msgctxt ""
"ref_epub_export.xhp\n"
"par_id3154044\n"
"help.text"
msgid "Export the current file to <link href=\"text/shared/00/00000002.xhp#epub\">EPUB</link>."
msgstr "Exporta el archivo actual como <link href=\"text/shared/00/00000002.xhp#epub\">EPUB</link>."

#. Rhx4d
#: ref_epub_export.xhp
msgctxt ""
"ref_epub_export.xhp\n"
