msgctxt ""
"05020301.xhp\n"
"par_id3153727\n"
"92\n"
"help.text"
msgid "\"plus\" 0;\"minus\" 0;\"null\" 0"
msgstr "0 \"positivo\";0 \"negativo\";0 \"cero\""

