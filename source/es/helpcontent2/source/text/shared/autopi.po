msgctxt ""
"webwizard04.xhp\n"
"par_idN1057E\n"
"help.text"
msgid "<ahelp hid=\"34240\">Displays the format of the files on the index page.</ahelp>"
msgstr "<ahelp hid=\"34240\">Muestra el formato de los archivos en la página de índice.</ahelp>"

