msgstr "<ahelp hid=\"HID_DLGCONVERT_CBCANCEL\">Cierra el Convertidor de euros.</ahelp>"

#: 01150000.xhp
msgctxt ""
"01150000.xhp\n"
"hd_id3154986\n"
"29\n"
"help.text"
msgid "Help"
msgstr "Ayuda"

#: 01150000.xhp
msgctxt ""
"01150000.xhp\n"
"par_id3155413\n"
"30\n"
"help.text"
