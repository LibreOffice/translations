"help.text"
msgid "<item type=\"input\">=ODD(0)</item> returns 1."
msgstr "<item type=\"input\">=REDONDEA.IMPAR(0)</item> devuelve 1."

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id5675527\n"
"help.text"
msgid "<item type=\"input\">=ODD(-3.1)</item> returns -5."
msgstr "<item type=\"input\">=REDONDEA.IMPAR(-3.1)</item> devuelve -5."

#: 04060106.xhp
msgctxt ""
