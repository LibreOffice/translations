msgctxt ""
"04060104.xhp\n"
"par_id3158417\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_FORMEL\">Displays the formula of a formula cell as a text string.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_FORMEL\">Muestra la fórmula de una celda de fórmula como cadena de texto.</ahelp>"

