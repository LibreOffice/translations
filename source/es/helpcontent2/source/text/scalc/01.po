"func_weeknum_ooo.xhp\n"
"par_id3154280\n"
"61\n"
"help.text"
msgid "2 = Monday (ISO 8601)"
msgstr "2 = lunes (ISO 8601)"

#: func_weeknum_ooo.xhp
msgctxt ""
"func_weeknum_ooo.xhp\n"
"par_id3154281\n"
"66\n"
"help.text"
msgid "any other value = Monday (ISO 8601)"
msgstr "cualquier otro valor = lunes (ISO 8601)"

#: func_weeknum_ooo.xhp
