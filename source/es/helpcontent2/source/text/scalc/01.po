#: 04060181.xhp
msgctxt ""
"04060181.xhp\n"
"hd_id2950603\n"
"help.text"
msgid "EXPON.DIST"
msgstr "DISTR.EXP.N"

#: 04060181.xhp
msgctxt ""
"04060181.xhp\n"
"par_id2949563\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_EXP_DIST_MS\">Returns the exponential distribution.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_EXP_DIST_MS\">Devuelve la distribución exponencial.</ahelp>"

#: 04060181.xhp
msgctxt ""
