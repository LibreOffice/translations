msgctxt ""
"04060106.xhp\n"
"par_id3152787\n"
"help.text"
msgid "<item type=\"input\">=ABS(-56)</item> returns 56."
msgstr "<item type=\"input\">=ABS(-56)</item> devuelve 56."

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3148752\n"
"help.text"
msgid "<item type=\"input\">=ABS(12)</item> returns 12."
msgstr "<item type=\"input\">=ABS(12)</item> devuelve 12."

