#: 05050000.xhp
msgctxt ""
"05050000.xhp\n"
"par_id3154758\n"
"2\n"
"help.text"
msgid "<ahelp hid=\".\">Sets the sheet name and hides or shows selected sheets.</ahelp>"
msgstr "<ahelp hid=\".\">Establece el nombre de la hoja y oculta o muestra las hojas seleccionadas.</ahelp>"

