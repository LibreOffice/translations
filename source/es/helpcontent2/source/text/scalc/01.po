msgid "INT"
msgstr "ENTERO"

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3158441\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_GANZZAHL\">Rounds a number down to the nearest integer.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_GANZZAHL\">Redondea un número a la baja al entero más próximo.</ahelp>"

