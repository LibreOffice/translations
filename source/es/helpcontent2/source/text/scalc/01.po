msgctxt ""
"04060106.xhp\n"
"par_id3154546\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_ABS\">Returns the absolute value of a number.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_ABS\">Devuelve el valor absoluto de un número.</ahelp>"

