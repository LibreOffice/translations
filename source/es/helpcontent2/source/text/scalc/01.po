"func_year.xhp\n"
"par_id3151168\n"
"44\n"
"help.text"
msgid "<item type=\"input\">=YEAR(2)</item> returns 1900"
msgstr "<item type=\"input\">=AÑO(2)</item> devuelve 1900"

#: func_year.xhp
