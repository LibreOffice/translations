#: autoformat.xhp
msgctxt ""
"autoformat.xhp\n"
"tit\n"
"help.text"
msgid "Using AutoFormat for Tables"
msgstr "Uso de formato automático en tablas"

#. V4grY
#: autoformat.xhp
msgctxt ""
"autoformat.xhp\n"
