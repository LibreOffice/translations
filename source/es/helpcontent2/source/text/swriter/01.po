msgstr "Columna"

#: 05120000.xhp#hd_id3154762.1.help.text
msgid "<link href=\"text/swriter/01/05120000.xhp\" name=\"Column\">Column</link>"
msgstr "<link href=\"text/swriter/01/05120000.xhp\" name=\"Columna\">Columna</link>"

#: 05120000.xhp#par_id3149052.2.help.text
msgid "Set the width of columns, or select, insert, and delete columns."
msgstr "Ajusta el ancho de las columnas o selecciona, inserta y borra columnas."

#: 05120000.xhp#hd_id3146322.4.help.text
msgid "<link href=\"text/swriter/01/05120100.xhp\" name=\"Width...\">Width...</link>"
msgstr "<link href=\"text/swriter/01/05120100.xhp\" name=\"Ancho...\">Ancho...</link>"

