msgid "<SDFIELD TYPE=DOCINFO SUBTYPE=COMMENT SDFIXED>Description</SDFIELD>"
msgstr "<SDFIELD TYPE=DOCINFO SUBTYPE=COMMENT SDFIXED>Descripción</SDFIELD>"

#: 04090007.xhp#par_id3151083.31.help.text
msgid "Creation date"
msgstr "Creación de fecha"

#: 04090007.xhp#par_id3153298.32.help.text
msgid "<SDFIELD TYPE=DOCINFO SUBTYPE=CREATE FORMAT=DATE SDNUM=\"1031;1031;QQ YY\">1. Quarter 98</SDFIELD>"
msgstr "<SDFIELD TYPE=DOCINFO SUBTYPE=CREATE FORMAT=DATE SDNUM=\"1031;1031;TT AA\">1er trimestre 98</SDFIELD>"

#: 04090007.xhp#par_id3150175.33.help.text
msgid "Creation time (fixed content)"
msgstr "Creación de hora (contenido fijo)"

#: 04090007.xhp#par_id3156134.34.help.text
