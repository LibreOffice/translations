msgid "The dictionary is already full."
msgstr "El diccionario está lleno."

#: lingu.src
msgctxt ""
"lingu.src\n"
"RID_SVXSTR_DIC_ERR_READONLY\n"
"string.text"
msgid "The dictionary is read-only."
msgstr "El diccionario es de solo lectura."
