"04060182.xhp\n"
"par_id3153068\n"
"5\n"
"help.text"
msgid "FINV(Number; DegreesFreedom1; DegreesFreedom2)"
msgstr "INVF(número; graus_liberdade_1; graus_liberdade_2)"

