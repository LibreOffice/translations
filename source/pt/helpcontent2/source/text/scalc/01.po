"05110000.xhp\n"
"hd_id3154021\n"
"11\n"
"help.text"
msgid "Number format"
msgstr "Formato numérico"

#: 05110000.xhp
msgctxt ""
"05110000.xhp\n"
"par_id3159239\n"
