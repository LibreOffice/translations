"bm_id3157762\n"
"help.text"
msgid "<bookmark_value>PI function</bookmark_value>"
msgstr "<bookmark_value>Função PI</bookmark_value>"

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"hd_id3157762\n"
"help.text"
msgid "PI"
msgstr "PI"

#: 04060106.xhp
