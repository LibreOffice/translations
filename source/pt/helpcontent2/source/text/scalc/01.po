msgctxt ""
"04060185.xhp\n"
"par_id3150119\n"
"116\n"
"help.text"
msgid "<item type=\"input\">=TTEST(A1:A50;B1:B50;2;2)</item>"
msgstr "<item type=\"input\">=TESTET(A1:A50;B1:B50;2;2)</item>"

