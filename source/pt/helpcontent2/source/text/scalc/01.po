msgstr "<item type=\"input\">=ANO(1)</item> devolve 1899"

#: func_year.xhp
msgctxt ""
"func_year.xhp\n"
"par_id3151168\n"
"44\n"
"help.text"
msgid "<item type=\"input\">=YEAR(2)</item> returns 1900"
msgstr "<item type=\"input\">=ANO(2)</item> devolve 1900"

#: func_year.xhp
msgctxt ""
"func_year.xhp\n"
"par_id3150115\n"
"45\n"
"help.text"
msgid "<item type=\"input\">=YEAR(33333.33)</item> returns 1991"
