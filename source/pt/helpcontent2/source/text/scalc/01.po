"par_id3153891\n"
"380\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_DISC\">Calculates the allowance (discount) of a security as a percentage.</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_DISC\">Devolve a taxa de desconto de um título.</ahelp>"

#: 04060103.xhp
msgctxt ""
"04060103.xhp\n"
"hd_id3153982\n"
"381\n"
"help.text"
msgid "Syntax"
msgstr "Sintaxe"

