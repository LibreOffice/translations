msgctxt ""
"04060107.xhp\n"
"par_id3145569\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_MINV\">Returns the inverse array.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_MINV\">Devolve a matriz inversa. </ahelp>"

#: 04060107.xhp
