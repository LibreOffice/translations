msgid "Close"
msgstr "Fechar"

#. twQjc
#: 04120300.xhp
msgctxt ""
"04120300.xhp\n"
"par_id3149036\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/bibliographyentry/close\">Closes the dialog.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/bibliographyentry/close\">Fecha a caixa de diálogo.</ahelp>"

#. hEjoD
