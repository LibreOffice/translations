"par_id3155743\n"
"44\n"
"help.text"
msgid "<ahelp hid=\"HID_HANGULDLG_BUTTON_CLOSE\">Closes the dialog.</ahelp>"
msgstr "<ahelp hid=\"HID_HANGULDLG_BUTTON_CLOSE\">Fecha a caixa de diálogo.</ahelp>"

