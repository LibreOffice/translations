msgctxt ""
"05250500.xhp\n"
"par_id3151387\n"
"2\n"
"help.text"
msgid "<ahelp hid=\".uno:SetObjectToForeground\">Moves the selected object in front of text.</ahelp>"
msgstr "<ahelp hid=\".uno:SetObjectToForeground\">Move o objeto selecionado para a frente do texto.</ahelp>"

