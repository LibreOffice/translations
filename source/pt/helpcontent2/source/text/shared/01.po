"par_id3156422\n"
"15\n"
"help.text"
msgid "<ahelp hid=\"HID_SC_SORT_DATE\">Lists the date and time that the change was made.</ahelp>"
msgstr "<ahelp hid=\"HID_SC_SORT_DATE\">Lista a data e a hora em que a alteração foi feita.</ahelp>"

#: 02230401.xhp
