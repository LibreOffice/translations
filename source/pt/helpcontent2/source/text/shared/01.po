msgctxt ""
"05220000.xhp\n"
"par_id3155179\n"
"24\n"
"help.text"
msgid "Set the anchor type and the anchor position."
msgstr "Defina o tipo e a posição da âncora."

#: 05220000.xhp
msgctxt ""
"05220000.xhp\n"
"hd_id3154381\n"
"25\n"
"help.text"
msgid "Graphic field"
msgstr "Campo gráfico"

