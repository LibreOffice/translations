msgctxt ""
"02010100.xhp\n"
"par_id3150661\n"
"help.text"
msgid "Example: select * from Item where ItemName like 'The *%' {escape '*'}"
msgstr "Exemplo: select * from Item where ItemName like 'The *%' {escape '*'}"

#. newGb
