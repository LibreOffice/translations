msgctxt ""
"02010100.xhp\n"
"par_id3156052\n"
"help.text"
msgid "Example: select Article.* from {oj item LEFT OUTER JOIN orders ON item.no=orders.ANR}"
msgstr "Exemplo: selecione Article.* from {oj item LEFT OUTER JOIN orders ON item.no=orders.ANR}"

