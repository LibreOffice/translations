msgctxt "SC_OPCODE_REPLACE"
msgid "Length"
msgstr "tamanho"

#: sc/inc/scfuncs.hrc:3738
msgctxt "SC_OPCODE_REPLACE"
msgid "The number of characters to be replaced."
msgstr "O número de caracteres que pretende substituir."

#: sc/inc/scfuncs.hrc:3739
