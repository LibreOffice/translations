msgstr "isihluzi esingachazwanga"

#: xmlfilterdialogstrings.src#STR_FILTER_HAS_BEEN_SAVED.string.text
msgid "The XML filter '%s' has been saved as package '%s'. "
msgstr "Isihluzi se-XML '%s' sigcinwe njengobekelelo lwe- '%s'. "

#: xmlfilterdialogstrings.src#STR_FILTERS_HAVE_BEEN_SAVED.string.text
msgid "%s XML filters have been saved in the package '%s'."
msgstr "Izihluzi ze-%s XML zigcinwe kubekelelo lwe-'%s'."

#: xmlfilterdialogstrings.src#STR_FILTER_PACKAGE.string.text
msgid "XSLT filter package"
msgstr "Ubekelelo lwesihluzi se-XSLT"

#: xmlfilterdialogstrings.src#STR_FILTER_INSTALLED.string.text
msgid "The XML filter '%s' has been installed successfully."
msgstr "Isihluzi se-XML '%s' sihlonyelwe ngempumelelo."

#: xmlfilterdialogstrings.src#STR_FILTERS_INSTALLED.string.text
msgid "%s XML filters have been installed successfully."
msgstr "Izihluzi ze-%s XML zihlonyelwe ngempumelelo."

#: xmlfilterdialogstrings.src#STR_NO_FILTERS_FOUND.string.text
msgid "No XML filter could be installed because the package '%s' does not contain any XML filters."
msgstr "Akukho sihluzi se-XML asikwazanga kuhlonyelwa kuba ubekelelo lwe-'%s' aluqulethanga naziphi izihluzi ze-XML."

