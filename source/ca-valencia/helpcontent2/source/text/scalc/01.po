msgctxt ""
"func_eastersunday.xhp\n"
"par_id3146072\n"
"help.text"
msgid "Pentecost Sunday = EASTERSUNDAY(Year) + 49"
msgstr "Diumenge de Pentecosta = DIUMENGEPASQUA(Any) + 49"

#. RwEdh
#: func_eastersunday.xhp
msgctxt ""
"func_eastersunday.xhp\n"
"par_id3149553\n"
"help.text"
