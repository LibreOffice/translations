"par_idN10A7B\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/scenariomenu/delete\">Deletes the selected scenario.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/scenariomenu/delete\">Suprimeix l'escenari seleccionat.</ahelp>"

