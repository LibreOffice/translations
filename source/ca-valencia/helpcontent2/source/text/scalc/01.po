#. mABBt
#: func_eastersunday.xhp
msgctxt ""
"func_eastersunday.xhp\n"
"par_id3156156\n"
"help.text"
msgid "Easter Monday = EASTERSUNDAY(Year) + 1"
msgstr "Dilluns de Pasqua = DIUMENGEPASQUA(Any) + 1"

#. zGhAa
#: func_eastersunday.xhp
msgctxt ""
"func_eastersunday.xhp\n"
"par_id3147521\n"
