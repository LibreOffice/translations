msgctxt ""
"04090007.xhp\n"
"par_id3156275\n"
"help.text"
msgid "<SDFIELD TYPE=DATETIME SDNUM=\"1031;1031;DD/MM/YY\">17/02/98</SDFIELD>"
msgstr "<SDFIELD TYPE=DATETIME SDNUM=\"1031;1031;DD/MM/YY\">17/02/98</SDFIELD>"

#. gSgE6
#: 04090007.xhp
msgctxt ""
"04090007.xhp\n"
"par_id3145774\n"
"help.text"
msgid "Time is fixed"
msgstr "S'ha fixat l'hora"

