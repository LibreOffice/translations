msgctxt ""
"05090200.xhp\n"
"par_id3149099\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/tablecolumnpage/next\">Displays the table columns found to the right of the current column.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/tablecolumnpage/next\">Mostra les columnes de la taula que es troben a la dreta de la columna actual.</ahelp>"

#. eBCsN
