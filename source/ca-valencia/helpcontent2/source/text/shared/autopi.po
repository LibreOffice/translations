msgstr "<ahelp hid=\"HID_DLGCONVERT_CBCANCEL\">Tanca el convertidor d'euros.</ahelp>"

#: 01150000.xhp
msgctxt ""
"01150000.xhp\n"
"hd_id3154986\n"
"29\n"
"help.text"
msgid "Help"
msgstr "Ajuda"

#: 01150000.xhp
msgctxt ""
"01150000.xhp\n"
"par_id3155413\n"
