msgctxt ""
"01170101.xhp\n"
"par_id3150928\n"
"help.text"
msgid "<ahelp hid=\".\">Select the selection state of the current control.</ahelp>"
msgstr "<ahelp hid=\".\">Seleccioneu l'estat de selecció del control actual.</ahelp>"

