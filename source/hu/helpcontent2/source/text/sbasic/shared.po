"par_id3148455\n"
"help.text"
msgid "Print #iNumber, \"This is a new line of text\""
msgstr "Print #iNumber, \"Ez a szöveg egy új sora\""

#: 03020104.xhp
msgctxt ""
"03020104.xhp\n"
"par_id3163805\n"
"help.text"
msgid "MsgBox \"All files will be closed\",0,\"Error\""
msgstr "MsgBox \"Minden fájl lezárásra kerül\",0,\"Hiba\""

