msgctxt ""
"03060600.xhp\n"
"bm_id3156024\n"
"help.text"
msgid "<bookmark_value>XOR operator (logical)</bookmark_value>"
msgstr "<bookmark_value>XOR operátor (logikai)</bookmark_value>"

#: 03060600.xhp
msgctxt ""
"03060600.xhp\n"
"hd_id3156024\n"
"help.text"
