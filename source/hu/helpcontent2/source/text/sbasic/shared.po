msgctxt ""
"03120308.xhp\n"
"par_id3150669\n"
"help.text"
msgid "RSet Text As String = Text or RSet Variable1 = Variable2"
msgstr "RSet Szöveg As String = Szöveg vagy RSet Változó1 = Változó2"

