msgstr "<link href=\"text/sbasic/shared/03120302.xhp\" name=\"LCase függvény\">LCase függvény</link>"

#: 03120302.xhp
msgctxt ""
"03120302.xhp\n"
"par_id3145609\n"
"help.text"
msgid "Converts all uppercase letters in a string to lowercase."
msgstr "A karakterláncban előforduló nagybetűket kisbetűvé alakítja."

