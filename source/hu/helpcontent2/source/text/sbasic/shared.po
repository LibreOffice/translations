"tit\n"
"help.text"
msgid "LCase Function"
msgstr "LCase függvény"

#: 03120302.xhp
msgctxt ""
"03120302.xhp\n"
"bm_id3152363\n"
"help.text"
msgid "<bookmark_value>LCase function</bookmark_value>"
msgstr "<bookmark_value>LCase függvény</bookmark_value>"

#: 03120302.xhp
msgctxt ""
"03120302.xhp\n"
"hd_id3152363\n"
"help.text"
