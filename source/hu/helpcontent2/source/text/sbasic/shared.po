msgstr "<link href=\"text/sbasic/shared/03030301.xhp\" name=\"Date utasítás\">Date utasítás</link>"

#: 03030301.xhp
msgctxt ""
"03030301.xhp\n"
"par_id3147291\n"
"help.text"
msgid "Returns the current system date as a string, or resets the date. The date format depends on your local system settings."
msgstr "Karakterláncként adja vissza az aktuális dátumot, vagy visszaállítja a dátumot. A dátumformátum a rendszer területi beállításaitól függ."

#: 03030301.xhp
msgctxt ""
"03030301.xhp\n"
"hd_id3148686\n"
"help.text"
msgid "Syntax:"
msgstr "Szintaxis:"

#: 03030301.xhp
msgctxt ""
"03030301.xhp\n"
"par_id3146794\n"
"help.text"
msgid "Date ; Date = Text As String"
msgstr "Date ; Date = Szöveg As String"

#: 03030301.xhp
