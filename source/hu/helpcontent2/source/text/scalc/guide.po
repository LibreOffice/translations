msgctxt "specialfilter.xhp#par_id3150086.31.help.text"
msgid "Standard"
msgstr "Standard"

#: specialfilter.xhp#par_id3150202.32.help.text
msgctxt "specialfilter.xhp#par_id3150202.32.help.text"
msgid "Business"
msgstr "Üzleti"

