msgctxt ""
"04060116.xhp\n"
"par_id2107303\n"
"help.text"
msgid "1*3*5*7* ... *n"
msgstr "1*3*5*7* ... *n"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id4071779\n"
"help.text"
msgid "FACTDOUBLE(0) returns 1 by definition."
msgstr "A FACTDOUBLE(0) definíció szerint 1-et ad vissza."

