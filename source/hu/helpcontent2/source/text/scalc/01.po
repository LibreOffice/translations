msgid "Delete"
msgstr "Törlés"

#. juRBz
#: 02110000.xhp
msgctxt ""
"02110000.xhp\n"
"par_idN10A7B\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/scenariomenu/delete\">Deletes the selected scenario.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/scenariomenu/delete\">Törli a kijelölt esetet.</ahelp>"

