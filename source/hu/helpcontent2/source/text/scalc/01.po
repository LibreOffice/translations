msgctxt ""
"func_networkdays.xhp\n"
"par_id241020160012187036\n"
"help.text"
msgid "<link href=\"text/scalc/01/func_networkdays.intl.xhp\">NETWORKDAYS.INTL</link>"
msgstr ""

