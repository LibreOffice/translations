msgctxt ""
"04060110.xhp\n"
"par_id3145355\n"
"help.text"
msgid "<item type=\"input\">=DECIMAL(\"17\";10)</item> returns 17."
msgstr "Az <item type=\"input\">=TIZEDES(\"17\";10)</item> eredménye 17."

#. CFhft
