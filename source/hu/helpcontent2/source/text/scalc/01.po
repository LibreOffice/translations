#: 04060104.xhp
msgctxt ""
"04060104.xhp\n"
"par_id3154117\n"
"help.text"
msgid "<item type=\"input\">=N(123)</item> returns 123"
msgstr "Az <item type=\"input\">=S(123)</item> a 123 értéket adja vissza."

