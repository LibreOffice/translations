"hd_id04032016185123\n"
"help.text"
msgid "=FORECAST.ETS.PI.ADD(DATE(2014;1;1);Values;Timeline;0.9;1;TRUE();1)"
msgstr "=ELŐREJELZÉS.ESIM.PI.ADD(DÁTUM(2014;1;1);Értékek;Idővonal;0,9;1;TRUE();1)"

