msgctxt ""
"04060116.xhp\n"
"par_id3151309\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_FACTDOUBLE\">Returns the double factorial of a number.</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_FACTDOUBLE\">Kiszámítja a szám dupla faktoriálisát.</ahelp>"

