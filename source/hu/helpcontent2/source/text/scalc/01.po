msgctxt ""
"04060107.xhp\n"
"hd_id3154970\n"
"help.text"
msgid "TRANSPOSE"
msgstr "TRANSZPONÁLÁS"

#: 04060107.xhp
msgctxt ""
"04060107.xhp\n"
"par_id3155276\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_MTRANS\">Transposes the rows and columns of an array.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_MTRANS\">Egy tömb sorait és oszlopait cseréli fel.</ahelp>"

#: 04060107.xhp
msgctxt ""
"04060107.xhp\n"
"par_id3153843\n"
"help.text"
msgid "TRANSPOSE(Array)"
msgstr "TRANSZPONÁLÁS(mátrix)"

#: 04060107.xhp
msgctxt ""
"04060107.xhp\n"
"par_id3153857\n"
"help.text"
msgid "<emph>Array</emph> represents the array in the spreadsheet that is to be transposed."
msgstr "A <emph>mátrix</emph> a táblázatban található transzponálni kívánt mátrix."

#: 04060107.xhp
msgctxt ""
