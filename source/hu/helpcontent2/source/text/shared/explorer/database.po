"help.text"
msgid "<variable id=\"toolbars\"><link href=\"text/shared/explorer/database/toolbars.xhp\">Toolbars</link></variable>"
msgstr "<variable id=\"toolbars\"><link href=\"text/shared/explorer/database/toolbars.xhp\">Eszköztárak</link></variable>"

#: toolbars.xhp
msgctxt ""
"toolbars.xhp\n"
"par_idN10545\n"
"help.text"
msgid "In a database file window, you can see the following toolbars."
msgstr "Az adatbázisfájl ablakban a következő eszköztárak jelennek meg."

#: toolbars.xhp
msgctxt ""
"toolbars.xhp\n"
"par_idN10548\n"
"help.text"
