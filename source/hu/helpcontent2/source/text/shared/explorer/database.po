msgctxt ""
"querywizard06.xhp\n"
"par_idN10579\n"
"help.text"
msgid "<ahelp hid=\".\">Enter the value for the grouping condition.</ahelp>"
msgstr "<ahelp hid=\".\">Írja be az értéket a csoportosítási feltételhez.</ahelp>"

