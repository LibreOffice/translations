msgctxt ""
"serverauthentication.xhp\n"
"par_idN1062C\n"
"help.text"
msgid "<ahelp hid=\".\">Enter the server name of your POP 3 or IMAP mail server.</ahelp>"
msgstr "<ahelp hid=\".\">Adja meg a POP3- vagy IMAP-kiszolgáló nevét.</ahelp>"

#: serverauthentication.xhp
msgctxt ""
"serverauthentication.xhp\n"
"par_idN1063B\n"
"help.text"
msgid "Port"
msgstr "Port"

