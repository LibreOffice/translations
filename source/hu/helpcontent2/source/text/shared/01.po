#: packagemanager.xhp
msgctxt ""
"packagemanager.xhp\n"
"par_id9581591\n"
"help.text"
msgid "<item type=\"literal\">unopkg add --shared path_filename.oxt</item>"
msgstr "<item type=\"literal\">unopkg add --shared útvonal_fájlnév.oxt</item>"

