#. YDBgo
#: password_dlg.xhp
msgctxt ""
"password_dlg.xhp\n"
"par_id313339\n"
"help.text"
msgid "The permission password must be entered to edit the document."
msgstr "A jogosultsági jelszót meg kell adni a fájl szerkesztéséhez."

#. Whd98
#: password_dlg.xhp
msgctxt ""
"password_dlg.xhp\n"
"hd_id3146857\n"
"help.text"
msgid "Password"
msgstr "Jelszó"

#. JAcAa
#: password_dlg.xhp
msgctxt ""
"password_dlg.xhp\n"
"par_id3150502\n"
"help.text"
msgid "<ahelp hid=\".\">Type a password. A password is case sensitive.</ahelp>"
