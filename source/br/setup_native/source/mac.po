#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"OKLabel\n"
"LngText.text"
msgid "Ok"
msgstr "Mat eo"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallLabel\n"
"LngText.text"
msgid "Install"
msgstr "Staliañ"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AbortLabel\n"
"LngText.text"
msgid "Abort"
msgstr "Dilezel"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText1\n"
"LngText.text"
msgid "Welcome to the [FULLPRODUCTNAME] Installation Wizard"
msgstr "Degemer mar er meziant staliañ [FULLPRODUCTNAME]"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText2\n"
"LngText.text"
msgid "This installation will update your installed versions of [PRODUCTNAME]"
msgstr "Gant ar staliañ-mañ e vo hizivaet hoc'h handelvoù eus [PRODUCTNAME]"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText3\n"
"LngText.text"
msgid "This might take a moment."
msgstr "Moarvat e pado un tammig amzer."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ChooseMyOwnText\n"
"LngText.text"
msgid "Not listed (choose location in an extra step)"
msgstr "N'emañ ket war ar roll (dibabit ul lec'hiadur gant ur bazenn ouzhpenn)"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListPromptText\n"
"LngText.text"
msgid "Choose [PRODUCTNAME] [PRODUCTVERSION] installation for which you want to install the [FULLPRODUCTNAME]"
msgstr "Dibabit staliañ [PRODUCTNAME] [PRODUCTVERSION] ma fell deoc'h staliañ [FULLPRODUCTNAME] evitañ"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ChooseManualText\n"
"LngText.text"
msgid "Point the dialog to your [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "Henchit ar voestad emziviz davit staliadur ho [PRODUCTNAME] [PRODUCTVERSION]."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListOKLabelText\n"
"LngText.text"
msgid "Install"
msgstr "Staliañ"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListCancelLabel\n"
"LngText.text"
msgid "Abort"
msgstr "Dilezel"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AppInvalidText1\n"
"LngText.text"
msgid "This is not a valid [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "N'eo ket ur staliadur talvoudek eus [PRODUCTNAME] [PRODUCTVERSION]."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AppInvalidText2\n"
"LngText.text"
msgid "Run the installer again and choose a valid [PRODUCTNAME] [PRODUCTVERSION] installation"
msgstr "Lañsit ar meziant staliañ en-dro ha dibabit ur staliadur talvoudek eus [PRODUCTNAME] [PRODUCTVERSION]."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"StartInstallText1\n"
"LngText.text"
msgid "Click Install to start the installation"
msgstr "Klikañ war Staliañ a-benn kregiñ gant ar staliañ"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"StartInstallText2\n"
"LngText.text"
msgid "Installation might take a minute..."
msgstr "Ur vunutennvezh e pado ar staliañ..."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyQText\n"
"LngText.text"
msgid "Installation failed, most likely your account does not have the necessary privileges."
msgstr "C'hwitadenn war ar staliañ, moarvat rak n'emañ ket ar brientoù ret gant ho kont."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyQText2\n"
"LngText.text"
msgid "Do you want to identify as administrator and try again?"
msgstr "Fellout a ra deoc'h ma viot anavet evel ardead ha klask en-dro ?"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyYES\n"
"LngText.text"
msgid "Yes, identify"
msgstr "Ya, ra vin anavet"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyNO\n"
"LngText.text"
msgid "No, abort installation"
msgstr "Dilezel a fell din"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallFailedText\n"
"LngText.text"
msgid "Installation failed."
msgstr "C'hwitadenn war ar staliañ."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallCompleteText\n"
"LngText.text"
msgid "Installation of [PRODUCTNAME] language pack completed."
msgstr "Echu eo staliadur pakad yezh [PRODUCTNAME]."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallCompleteText2\n"
"LngText.text"
msgid "Call '[PRODUCTNAME]-Preferences-Language Settings-Languages' to change the user interface language."
msgstr "Galvit '[PRODUCTNAME]-Gwellvezioù-Arventennoù yezh-Yezhoù' a-benn kemmañ yezh ketal an arveriaded."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallCompleteTextPatch\n"
"LngText.text"
msgid "Installation of [FULLPRODUCTNAME] completed"
msgstr "Echu eo staliadur [FULLPRODUCTNAME]"
