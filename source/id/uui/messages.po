msgstr "~Simpan"

#: uui/inc/strings.hrc:41
msgctxt "RID_KEEP_PASSWORD"
msgid "~Remember password until end of session"
msgstr "Ingat sandi hingga akhi~r sesi"

#: uui/inc/strings.hrc:42
