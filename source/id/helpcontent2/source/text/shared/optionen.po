msgctxt ""
"01060500.xhp\n"
"par_id3156139\n"
"35\n"
"help.text"
msgid "Search result:"
msgstr ""

#: 01060500.xhp
msgctxt ""
"01060500.xhp\n"
"par_id3150979\n"
"36\n"
"help.text"
msgid "win"
msgstr "win"

#: 01060500.xhp
msgctxt ""
"01060500.xhp\n"
"par_id3159239\n"
"37\n"
"help.text"
msgid "Finds win, but not win95, os2win, or upwind"
msgstr ""

