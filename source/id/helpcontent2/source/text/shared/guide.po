#. tarEf
#: import_ms.xhp
msgctxt ""
"import_ms.xhp\n"
"hd_id3154898\n"
"help.text"
msgid "Converting all documents of a folder"
msgstr "Mengkonversi semua dokumen dalam sebuah folder"

#. BFe46
#: import_ms.xhp
msgctxt ""
