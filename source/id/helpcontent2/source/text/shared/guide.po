"hd_id3158421\n"
"help.text"
msgid "<variable id=\"keyboard\"><link href=\"text/shared/guide/keyboard.xhp\" name=\"Shortcuts (%PRODUCTNAME Accessibility)\">Shortcuts (<item type=\"productname\">%PRODUCTNAME</item> Accessibility)</link></variable>"
