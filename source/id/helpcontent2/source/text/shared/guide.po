msgctxt ""
"digitalsign_send.xhp\n"
"par_idN106EA\n"
"help.text"
msgid "Choose <emph>Tools - Macros - Digital Signature</emph>."
msgstr "Pilih <emph>Perkakas - Makro - Tanda Tangan Digital</emph>."

