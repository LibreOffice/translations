"help.text"
msgid "select Article.* from {oj item LEFT OUTER JOIN orders ON item.no=orders.ANR}"
msgstr "Contoh: select Article.* from {oj item LEFT OUTER JOIN orders ON item.no=orders.ANR}"

#. yARgJ
