"09070000.xhp\n"
"par_id0122200902231630\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Removes the hyperlink, leaving plain text.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Menghapus pranala, meninggalkan teks biasa.</ahelp>"

