msgstr "<ahelp hid=\"cui/ui/hyperlinkdocpage/fileopen\">Buka <emph>Buka</emph> dialog, di mana Anda dapat memilih file.</ahelp>"

#: 09070300.xhp
msgctxt ""
"09070300.xhp\n"
"hd_id3149828\n"
"help.text"
msgid "Target in document"
msgstr "target dalam dokumen"

