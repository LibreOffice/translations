"09070000.xhp\n"
"hd_id3156192\n"
"help.text"
msgid "Reset"
msgstr "Reset"

#: 09070000.xhp
msgctxt ""
"09070000.xhp\n"
"par_id3149234\n"
"help.text"
msgid "<ahelp hid=\".\">Resets the entries in the dialog to their original state.</ahelp>"
msgstr "<ahelp hid=\".\">Mereset entri dalam dialog ke keadaan semula.</ahelp>"

#: 09070100.xhp
