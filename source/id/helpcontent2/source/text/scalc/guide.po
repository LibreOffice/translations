#. w4Hcx
#: dbase_files.xhp
msgctxt ""
"dbase_files.xhp\n"
"par_idN107AE\n"
"help.text"
msgid "Click <emph>Browse</emph>."
msgstr "Klik <emph>Oke</emph>."

#. EyJdW
#: dbase_files.xhp
msgctxt ""
"dbase_files.xhp\n"
"par_idN107B6\n"
"help.text"
msgid "Locate the directory that contains the dBASE file, and click <emph>OK</emph>."
msgstr "Temukan direktori yang berisi file dBASE, dan klik <emph>OK</emph>."

#. 5kG55
#: dbase_files.xhp
msgctxt ""
"dbase_files.xhp\n"
"par_idN107BE\n"
"help.text"
msgid "Click <emph>Create</emph>."
msgstr "Klik <emph>Oke</emph>."

#. rJEHy
#: dbase_files.xhp
msgctxt ""
"dbase_files.xhp\n"
"par_idN107F1\n"
"help.text"
msgid "To Save a Spreadsheet as a dBASE File"
msgstr "Untuk Menyimpan Lembar Sebar sebagai Berkas dBASE"

#. i2EnS
#: dbase_files.xhp
msgctxt ""
"dbase_files.xhp\n"
"par_idN107F8\n"
"help.text"
msgid "Choose <emph>File - Save As</emph>."
msgstr "Pilih <emph>Berkas - Buka</emph>."

#. N4YuB
#: dbase_files.xhp
msgctxt ""
"dbase_files.xhp\n"
"par_idN10800\n"
"help.text"
msgid "In the <emph>File format</emph> box, select \"dBASE file\"."
msgstr "Dalam kotak <emph>Format File</emph>, pilih \"file dBASE\"."

#. rrFCz
#: dbase_files.xhp
msgctxt ""
"dbase_files.xhp\n"
"par_idN107F9\n"
"help.text"
msgid "In the <emph>File name</emph> box, type a name for the dBASE file."
