#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id3153927\n"
"help.text"
msgid "IMREAL(\"ComplexNumber\")"
msgstr "IMREAL(\"ComplexNumber\")"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id3155986\n"
"help.text"
msgid "<item type=\"input\">=IMREAL(\"1+3j\")</item> returns 1."
msgstr "<item type=\"input\">=IMREAL(\"1+3j\")</item> mengembalikan 1."

