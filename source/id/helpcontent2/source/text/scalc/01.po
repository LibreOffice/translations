msgctxt ""
"04060116.xhp\n"
"par_id3152807\n"
"help.text"
msgid "<item type=\"input\">=IMSQRT(\"3+4i\")</item> returns 2+1i."
msgstr "<item type=\"input\">=IMSQRT(\"3+4i\")</item> mengembalikan 2+1i."

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
