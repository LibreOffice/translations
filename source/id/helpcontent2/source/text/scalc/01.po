msgctxt ""
"04060116.xhp\n"
"hd_id3154959\n"
"help.text"
msgid "IMABS"
msgstr "IMABS"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"par_id3149895\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_IMABS\">The result is the absolute value of a complex number.</ahelp>"
