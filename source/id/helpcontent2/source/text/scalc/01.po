msgctxt ""
"func_year.xhp\n"
"par_id3150115\n"
"help.text"
msgid "<item type=\"input\">=YEAR(33333.33)</item> returns 1991"
msgstr "<item type=\"input\">=YEAR(33333.33)</item> Kembali 1991"

