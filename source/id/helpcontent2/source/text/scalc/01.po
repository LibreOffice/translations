msgctxt ""
"04060116.xhp\n"
"par_id3156131\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_IMSQRT\">The result is the square root of a complex number.</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMSQRT\">Hasilnya adalah akar kuadrat dari sebuah bilangan kompleks.</ahelp>"

