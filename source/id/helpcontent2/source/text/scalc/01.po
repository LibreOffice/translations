msgctxt ""
"02110000.xhp\n"
"par_id3152994\n"
"help.text"
msgid "<image id=\"img_id3150515\" src=\"sw/res/sc20186.png\" width=\"0.2228in\" height=\"0.2228in\"><alt id=\"alt_id3150515\">Icon</alt></image>"
