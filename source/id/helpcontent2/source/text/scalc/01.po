msgctxt ""
"04060119.xhp\n"
"par_id3154636\n"
"help.text"
msgid "<item type=\"input\">=CUMPRINC_ADD(0.0075;360;125000;1;1;0)</item> returns -68.27827"
msgstr "<item type=\"input\">=CUMPRINC_ADD(0.0075;360;125000;1;1;0)</item> Kembali -68.27827"

