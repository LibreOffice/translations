msgctxt ""
"12090102.xhp\n"
"par_idN108B2\n"
"help.text"
msgid "<ahelp hid=\".\">Opens the Filter dialog.</ahelp>"
msgstr "<ahelp hid=\".\">Buka dialog Filter.</ahelp>"

