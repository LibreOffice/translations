msgid "<bookmark_value>spreadsheets; shortcut keys in</bookmark_value>        <bookmark_value>shortcut keys; spreadsheets</bookmark_value>        <bookmark_value>sheet ranges; filling</bookmark_value>"
msgstr "<bookmark_value>lembar sebar; tombol pintas di</bookmark_value>        <bookmark_value>tombol pintas; lembar sebar</bookmark_value>        <bookmark_value>rentang lembar; isian</bookmark_value>"

#. qPZBx
