"hd_id3151285\n"
"15\n"
"help.text"
msgid "<link href=\"text/shared/02/02170000.xhp\" name=\"Background Color\">Background Color</link>"
msgstr "<link href=\"text/shared/02/02170000.xhp\" name=\"Background Color\">Warna Latar Belakang</link>"

#: main0215.xhp
msgctxt ""
