msgid "Footnote"
msgstr "Catatan Kaki"

#. tQaKG
#: 02150000.xhp
msgctxt ""
"02150000.xhp\n"
"par_id3156098\n"
"help.text"
msgid "Converts an endnote to a footnote."
msgstr "Ubah catatan akhir menjadi catatan kaki."

#. sDCDg
