msgstr "Viền trường"

#: 01090500.xhp#par_idN10561.help.text
msgid "<ahelp hid=\".\">Specifies the field border style.</ahelp>"
msgstr "<ahelp hid=\".\">Ghi rõ kiểu dáng cho viền của trường.</ahelp>"

#: 01090500.xhp#par_idN10564.help.text
