msgid "<<"
msgstr "<<"

#. vfnjK
#: 12070100.xhp
msgctxt ""
"12070100.xhp\n"
"par_id3154897\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/insertdbcolumnsdialog/allleft\">Removes all database fields from the <emph>Table column(s)</emph> list box.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/insertdbcolumnsdialog/allleft\">Gỡ bỏ mọi trường cơ sở dữ liệu khỏi hộp liệt kê <emph>Cột bảng</emph>.</ahelp>"

#. 9aFnJ
#: 12070100.xhp
msgctxt ""
