msgstr "Trong hộp <emph>Tên tập tin</emph>, gõ tên cho tập tin dBASE."

#: dbase_files.xhp#par_idN10801.help.text
msgctxt "dbase_files.xhp#par_idN10801.help.text"
msgid "Click <emph>Save</emph>."
msgstr "Nhấn <emph>Lưu</emph>."

