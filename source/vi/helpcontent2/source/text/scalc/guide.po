#. ounFQ
#: csv_formula.xhp
msgctxt ""
"csv_formula.xhp\n"
"tit\n"
"help.text"
msgid "Importing and Exporting Text Files"
msgstr "Nhập và xuất các tập tin văn bản"

#. AzAKW
#: csv_formula.xhp
msgctxt ""
"csv_formula.xhp\n"
