msgctxt ""
"01020000.xhp\n"
"par_id9901425\n"
"help.text"
msgid "Insert cells (as in menu Insert - Cells)"
msgstr "Chèn các ô (giống như mục trình đơn <emph>Chèn > Ô</emph>"

#. LSCN9
#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"par_id3389080\n"
