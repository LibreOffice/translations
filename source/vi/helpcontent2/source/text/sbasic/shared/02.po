msgstr "Trường thuộc số"

#: 20000000.xhp#par_id3146107.help.text
msgid "<image id=\"img_id3147499\" src=\"cmd/sc_insertnumericfield.png\" width=\"0.0874inch\" height=\"0.0874inch\"><alt id=\"alt_id3147499\">Icon</alt></image>"
