"par_id3153361\n"
"help.text"
msgid "GetGenderIndex = Switch(sName = \"Jane\", \"female\", sName = \"John\", \"male\")"
msgstr "GetGenderIndex = Switch(sName = \"Nguyệt\", \"nữ\", sName = \"Tuấn\", \"nam\")"

