"03090410.xhp\n"
"hd_id3159157\n"
"help.text"
msgid "Example:"
msgstr "Thí dụ :"

#: 03090410.xhp
msgctxt ""
"03090410.xhp\n"
"par_id3149579\n"
"help.text"
msgid "sGender = GetGenderIndex( \"John\" )"
msgstr "sGender = GetGenderIndex( \"Tuấn\" )"

#: 03090410.xhp
msgctxt ""
