msgctxt ""
"03080103.xhp\n"
"par_id3148798\n"
"help.text"
msgid "Sin(Alpha) = side opposite the angle/hypotenuse"
msgstr "Sin(Alpha) = cạnh đối với góc/cạnh huyền"

