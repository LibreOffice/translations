msgstr "Xem thêm: câu lệnh <link href=\"text/sbasic/shared/03020201.xhp\" name=\"Get\"><item type=\"literal\">Get</item></link>"

#: 03020204.xhp
msgctxt ""
"03020204.xhp\n"
"hd_id3125863\n"
"help.text"
msgid "Syntax:"
msgstr "Cú pháp:"

#: 03020204.xhp
msgctxt ""
