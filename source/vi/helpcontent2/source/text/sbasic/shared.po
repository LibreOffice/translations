msgstr ""

#: 03060400.xhp
msgctxt ""
"03060400.xhp\n"
"par_id3159414\n"
"help.text"
msgid "Negates an expression by inverting the bit values."
msgstr "Phủ định một biểu thức bằng cách đảo ngược các giá trị bit."

#: 03060400.xhp
msgctxt ""
"03060400.xhp\n"
"hd_id3149457\n"
"help.text"
msgid "Syntax:"
msgstr "Cú pháp:"

#: 03060400.xhp
msgctxt ""
"03060400.xhp\n"
"par_id3150360\n"
"help.text"
msgid "Result = Not Expression"
msgstr "Kết quả = không phải biểu thức"

