"par_idN126D7\n"
"help.text"
msgid "Opens the Arrows toolbar to insert lines and arrows."
msgstr ""

#: main0210.xhp
msgctxt ""
"main0210.xhp\n"
"par_idN10770\n"
"help.text"
msgid "<link href=\"text/shared/01/05270000.xhp\" name=\"Points\">Points</link>"
msgstr "<link href=\"text/shared/01/05270000.xhp\" name=\"ĐIểm\">Điểm</link>"

#: main0210.xhp
msgctxt ""
