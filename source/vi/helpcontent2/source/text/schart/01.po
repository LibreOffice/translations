msgctxt ""
"05040201.xhp\n"
"hd_id3155336\n"
"help.text"
msgid "Major interval"
msgstr "Khoảng lớn"

#. h82YW
#: 05040201.xhp
msgctxt ""
"05040201.xhp\n"
"par_id3143218\n"
"help.text"
msgid "<ahelp hid=\"modules/schart/ui/tp_Scale/EDT_STEP_MAIN\">Defines the interval for the main division of the axes.</ahelp> The main interval cannot be larger than the value area."
msgstr "<ahelp hid=\"modules/schart/ui/tp_Scale/EDT_STEP_MAIN\">Xác định khoảng cho sự phân chia chính của trục.</ahelp> Không cho phép khoảng chính lớn hơn phạm vi giá trị."

