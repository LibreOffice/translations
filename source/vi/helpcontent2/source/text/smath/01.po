msgid "Update"
msgstr "Cập nhật"

#: 03070000.xhp#bm_id3153768.help.text
#, fuzzy
msgid "<bookmark_value>updating formula view</bookmark_value><bookmark_value>formula view; updating</bookmark_value>"
