"help.text"
msgid "<SDFIELD TYPE=DOCINFO SUBTYPE=CREATE FORMAT=DATE SDNUM=\"1031;1031;QQ YY\">1. Quarter 98</SDFIELD>"
msgstr "<SDFIELD TYPE=DOCINFO SUBTYPE=CREATE FORMAT=DATE SDNUM=\"1031;1031;QQ YY\">1° trimestre 98</SDFIELD>"

