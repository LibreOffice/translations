msgstr "Le prime ... righe"

#. t9di6
#: 06090000.xhp
msgctxt ""
"06090000.xhp\n"
"par_id4136478\n"
"help.text"
msgid "<ahelp hid=\".\">Repeats the first n rows as a header.</ahelp>"
msgstr "<ahelp hid=\".\">Ripete le prime \"n\" righe come intestazione.</ahelp>"

#. pHxwj
#: 06090000.xhp
msgctxt ""
"06090000.xhp\n"
"hd_id3151315\n"
