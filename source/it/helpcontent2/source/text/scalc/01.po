#: 04060185.xhp
msgctxt ""
"04060185.xhp\n"
"par_id2950521\n"
"help.text"
msgid "T.DIST(Number; DegreesFreedom; Cumulative)"
msgstr "DISTRIB.T(Numero; Gradi libertà; Cumulativo)"

