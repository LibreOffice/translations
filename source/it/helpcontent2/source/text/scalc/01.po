msgctxt ""
"04060115.xhp\n"
"par_id3156002\n"
"help.text"
msgid "<item type=\"input\">=HEX2BIN(\"6a\";8)</item> returns 01101010."
msgstr "<item type=\"input\">=HEX.BINARIO(6a;8)</item> restituisce 01101010."

