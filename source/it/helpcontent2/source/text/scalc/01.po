msgctxt ""
"04060106.xhp\n"
"par_id3158025\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_RAD\">Converts degrees to radians.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_RAD\">Converte gradi in radianti.</ahelp>"

#. ZGmDj
