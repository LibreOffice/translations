"hd_id4150026\n"
"help.text"
msgid "BITAND"
msgstr "BIT.E"

#: 04060120.xhp
msgctxt ""
"04060120.xhp\n"
"par_id4146942\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_BITAND\">Returns a bitwise logical \"and\" of the parameters.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_BITAND\">Restituisce un \"e\" logico binario dei parametri.</ahelp>"

#: 04060120.xhp
msgctxt ""
"04060120.xhp\n"
"par_id4146878\n"
"help.text"
msgid "BITAND(number1; number2)"
msgstr "BIT.E(Numero1; Numero2)"

#: 04060120.xhp
msgctxt ""
"04060120.xhp\n"
"par_id4151228\n"
"help.text"
msgid "<emph>Number1</emph> and <emph>number2</emph> are positive integers less than 2 ^ 48 (281 474 976 710 656)."
msgstr "<emph>Numero1</emph> e <emph>Numero2</emph> sono interi positivi minori di 2 ^ 48 (281 474 976 710 656)."

#: 04060120.xhp
msgctxt ""
"04060120.xhp\n"
"par_id4149246\n"
"help.text"
msgid "<item type=\"input\">=BITAND(6;10)</item> returns 2 (0110 & 1010 = 0010)."
msgstr "<item type=\"input\">=BIT.E(6;10)</item> restituisce 2 (0110 & 1010 = 0010)."

#: 04060120.xhp
