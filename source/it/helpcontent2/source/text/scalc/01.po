#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id2946958\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_QUARTILE_INC\">Returns the quartile of a data set.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_QUARTILE_INC\">Restituisce il quartile di un campione.</ahelp>"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"par_id2253976\n"
"help.text"
