msgctxt ""
"04060106.xhp\n"
"par_id3147323\n"
"help.text"
msgid "<item type=\"input\">=INT(-1.3)</item> returns -2."
msgstr "<item type=\"input\">=INT(-1,3)</item> restituisce -2."

#. LhjQ2
