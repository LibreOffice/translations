"bm_id3154930\n"
"help.text"
msgid "<bookmark_value>TDIST function</bookmark_value>         <bookmark_value>t-distribution</bookmark_value>"
msgstr "<bookmark_value>DISTRIBT, funzione</bookmark_value><bookmark_value>Distribuzione t</bookmark_value>"

