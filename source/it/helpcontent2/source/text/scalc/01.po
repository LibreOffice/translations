"hd_id4146139\n"
"help.text"
msgid "BITOR"
msgstr "BIT.O"

#: 04060120.xhp
msgctxt ""
"04060120.xhp\n"
"par_id4150140\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_BITOR\">Returns a bitwise logical \"or\" of the parameters.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_BITOR\">Restituisce un \"o\" logico binario dei parametri.</ahelp>"

#: 04060120.xhp
msgctxt ""
"04060120.xhp\n"
"par_id4148733\n"
"help.text"
msgid "BITOR(number1; number2)"
