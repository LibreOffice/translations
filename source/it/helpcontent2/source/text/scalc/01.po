"12120300.xhp\n"
"bas_id971625928577365\n"
"help.text"
msgid "msg = msg & \" in cell: \" & \"'\" & CellAddress & \"'\""
msgstr "msg = msg & \" nella cella: \" & \"'\" & CellAddress & \"'\""

