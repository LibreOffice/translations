#. 6Pmbf
#: func_searchb.xhp
msgctxt ""
"func_searchb.xhp\n"
"par_id241573517292388\n"
"help.text"
msgid "<emph>Find Text</emph>: The text or text expression to be found."
msgstr "<emph>Trova testo</emph>: il testo o l'espressione di testo da trovare."

#. ovdFE
#: func_searchb.xhp
msgctxt ""
"func_searchb.xhp\n"
"par_id991573517299918\n"
"help.text"
