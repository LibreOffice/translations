msgctxt ""
"04060185.xhp\n"
"par_id2949773\n"
"help.text"
msgid "<item type=\"input\">=T.DIST(1; 10; TRUE)</item> returns 0.8295534338"
msgstr "<item type=\"input\">=DISTRIB.T(1; 10; VERO)</item> restituisce 0,8295534338."

