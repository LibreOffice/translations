msgctxt ""
"04060115.xhp\n"
"par_id3146877\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_BESSELY\">Calculates the Bessel function of the second kind Yn(x).</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_BESSELY\">Calcola la funzione di Bessel del secondo tipo Yn(x).</ahelp>"

#: 04060115.xhp
msgctxt ""
"04060115.xhp\n"
"par_id3148884\n"
"help.text"
msgid "BESSELY(X; N)"
msgstr "BESSEL.Y(X; N)"

