"02230401.xhp\n"
"hd_id3157962\n"
"16\n"
"help.text"
msgid "Comment"
msgstr "Commento"

#: 02230401.xhp
msgctxt ""
"02230401.xhp\n"
"par_id3150868\n"
"17\n"
"help.text"
msgid "<ahelp hid=\"HID_SC_SORT_COMMENT\">Lists the comments that are attached to the change.</ahelp>"
msgstr "<ahelp hid=\"HID_SC_SORT_COMMENT\">Mostra i commenti associati alla modifica.</ahelp>"

#: 02230401.xhp
msgctxt ""
"02230401.xhp\n"
"hd_id3154218\n"
"18\n"
"help.text"
msgid "Accept"
msgstr "Accetta"

#: 02230401.xhp
msgctxt ""
"02230401.xhp\n"
"par_id3152935\n"
"19\n"
"help.text"
