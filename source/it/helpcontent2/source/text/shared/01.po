"help.text"
msgid "<bookmark_value>text; italics</bookmark_value><bookmark_value>italic text</bookmark_value><bookmark_value>characters; italics</bookmark_value>"
msgstr "<bookmark_value>Testo;corsivo</bookmark_value><bookmark_value>Corsivo;testo</bookmark_value><bookmark_value>Carattere;corsivo</bookmark_value>"

