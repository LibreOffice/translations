msgid "<link href=\"text/shared/01/01100500.xhp\" name=\"Priority Table\">Priority Table</link>"
msgstr "<link href=\"text/shared/01/01100500.xhp\" name=\"Tabella delle priorità\">Tabella delle priorità</link>"

#: 05210200.xhp#tit.help.text
msgctxt "05210200.xhp#tit.help.text"
msgid "Colors"
msgstr "Colori"

#: 05210200.xhp#hd_id3152895.1.help.text
msgid "<link href=\"text/shared/01/05210200.xhp\" name=\"Colors\">Colors</link>"
msgstr "<link href=\"text/shared/01/05210200.xhp\" name=\"Colori\">Colori</link>"

#: 05210200.xhp#par_id3149119.2.help.text
msgid "Select a color to apply, save the current color list, or load a different color list."
msgstr "Potete scegliere un colore da applicare, salvare la tavola di colori attiva o caricare una tavola di colori differente."

