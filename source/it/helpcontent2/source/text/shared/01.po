"par_id3153331\n"
"65\n"
"help.text"
msgid "Arrow Up"
msgstr "Freccia in alto"

#: 06140100.xhp
msgctxt ""
"06140100.xhp\n"
"hd_id3155388\n"
"66\n"
"help.text"
msgid "Arrow Down"
msgstr "Freccia in basso"

#: 06140100.xhp
msgctxt ""
"06140100.xhp\n"
"par_id3147335\n"
"67\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/menuassignpage/down\">Moves the selected item down one position.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/menuassignpage/down\">Sposta in basso di una posizione la voce selezionata.</ahelp>"

#: 06140100.xhp
msgctxt ""
"06140100.xhp\n"
"par_id3148943\n"
"help.text"
msgid "<image id=\"img_id3145609\" src=\"dbaccess/res/sortdown.png\" width=\"0.1563inch\" height=\"0.1665inch\"><alt id=\"alt_id3145609\">Icon</alt></image>"
msgstr "<image id=\"img_id3145609\" src=\"dbaccess/res/sortdown.png\" width=\"0.1563inch\" height=\"0.1665inch\"><alt id=\"alt_id3145609\">Icona</alt></image>"

