msgstr "<ahelp hid=\".\">Aggiunge una nuova riga di campi di controllo.</ahelp>"

#: querywizard04.xhp#par_idN105F1.help.text
msgctxt "querywizard04.xhp#par_idN105F1.help.text"
msgid "-"
msgstr "-"

#: querywizard04.xhp#par_idN1060B.help.text
