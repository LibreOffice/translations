msgstr "Dim dAlfa as Double"

#: 03080104.xhp#par_id3145252.22.help.text
msgid "d1 = InputBox$ (\"Enter the length of the side opposite the angle: \",\"opposite\")"
msgstr "d1 = InputBox$ (\"Inserite la lunghezza del lato opposto all'angolo: \",\"Opposto\")"

#: 03080104.xhp#par_id3149582.23.help.text
msgid "dAlpha = InputBox$ (\"Enter the Alpha angle (in degrees): \",\"Alpha\")"
msgstr "dAlfa = InputBox$ (\"Inserite l'angolo Alfa (in gradi): \",\"Alfa\")"

