msgctxt ""
"03060300.xhp\n"
"par_id3145750\n"
"help.text"
msgid "vOut = A > B Imp B > C ' returns -1"
msgstr "vOut = A > B Imp B > C ' restituisce -1"

#: 03060300.xhp
msgctxt ""
"03060300.xhp\n"
"par_id3156441\n"
"help.text"
