"03080102.xhp\n"
"par_id3154141\n"
"help.text"
msgid "<literal>Cos</literal>(Alpha) = Adjacent/Hypotenuse"
msgstr "<literal>Cos</literal>(Alfa) = Adiacente/Ipotenusa"

