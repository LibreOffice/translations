msgid "Sub ExampleDateValue"
msgstr "Sub EsempioDateValue"

#: 03030102.xhp#par_id3153363.26.help.text
msgid "msgbox DateValue(\"12/02/1997\")"
msgstr "msgbox DateValue(\"12/02/1997\")"

