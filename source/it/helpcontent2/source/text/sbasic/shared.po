msgid "sDate = Right$(sDate, 4) + Mid$(sDate, 4, 2) + Left$(sDate, 2)"
msgstr "sData = Right$(sData, 4) + Mid$(sData, 4, 2) + Left$(sData, 2)"

#: 03090101.xhp#par_id3154098.18.help.text
msgid "sToday = Date$"
msgstr "sOggi = Date$"

#: 03090101.xhp#par_id3144765.19.help.text
