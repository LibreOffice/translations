msgstr ";\t59\t,\t44\t:\t58\t{Tab}\t9\t{Space}\t32"

#. DFGo9
#: dbaccess/inc/strings.hrc:287
msgctxt "STR_AUTODELIMITER_MISSING"
msgid "#1 must be set."
msgstr "#1 պետք է տրվի."

#. ZDRBf
#: dbaccess/inc/strings.hrc:288
msgctxt "STR_AUTODELIMITER_MUST_DIFFER"
msgid "#1 and #2 must be different."
msgstr "#1 -ը և #2 -ը պետք է տարբեր լինեն:"

#. 9oCZr
#: dbaccess/inc/strings.hrc:289
msgctxt "STR_AUTONO_WILDCARDS"
msgid "Wildcards such as ?,* are not allowed in #1."
msgstr "Տեղարման նշաններ, ինչպիսիք են ?,* -ը, չեն թույլատրվում #1 -ում:"

#. BdzcB
#: dbaccess/inc/strings.hrc:291
msgctxt "STR_CONNECTION_TEST"
msgid "Connection Test"
msgstr "Միացման փորձարկում"

#. oAAKs
#: dbaccess/inc/strings.hrc:292
msgctxt "STR_CONNECTION_SUCCESS"
msgid "The connection was established successfully."
msgstr "Կապը հաջողությամբ հաստատվեց:"

#. 5V7Ay
#: dbaccess/inc/strings.hrc:293
msgctxt "STR_CONNECTION_NO_SUCCESS"
msgid "The connection could not be established."
msgstr "Կապը չհաջողվեց հաստատել:"

#. wvNFP
#: dbaccess/inc/strings.hrc:294
msgctxt "STR_JDBCDRIVER_SUCCESS"
msgid "The JDBC driver was loaded successfully."
msgstr "JDBC վարորդը հաջողությամբ բեռնվեց:"

#. RdMCN
#: dbaccess/inc/strings.hrc:295
msgctxt "STR_JDBCDRIVER_NO_SUCCESS"
msgid "The JDBC driver could not be loaded."
msgstr "JDBC -ի վարորդը չհաջողվեց բեռնել:"

#. dyCvN
#: dbaccess/inc/strings.hrc:296
msgctxt "STR_MSACCESS_FILTERNAME"
msgid "MS Access file"
msgstr "MS Access նիշք"

#. jFwxU
#: dbaccess/inc/strings.hrc:297
msgctxt "STR_FIREBIRD_FILTERNAME"
msgid "Firebird Database"
msgstr "Firebird շտեմարան"

#. 8Uiv2
#: dbaccess/inc/strings.hrc:299
msgctxt "STR_RSC_CHARSETS"
msgid "System"
msgstr "Համակարգ"

#. pnwDB
#: dbaccess/inc/strings.hrc:300
msgctxt "STR_ERROR_DURING_CREATION"
msgid "Error during creation"
msgstr "Սխալ ստեղծման ժամանակ"

#. hnyJF
#: dbaccess/inc/strings.hrc:301
msgctxt "STR_UNEXPECTED_ERROR"
msgid "An error occurred. The operation could not be performed."
msgstr "Սխալ է տեղի ունեցել. Գործողությունը հնարավոր չի եղել կատարել։"

#. kXCG9
#: dbaccess/inc/strings.hrc:302
msgctxt "STR_COULDNOTOPEN_LINKEDDOC"
msgid "The document \"$file$\" could not be opened."
msgstr "\"$file$\" փաստաթուղթը չհաջողվեց բացել:"

#. bFHHW
#: dbaccess/inc/strings.hrc:303
