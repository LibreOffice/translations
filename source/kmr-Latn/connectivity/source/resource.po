msgstr "Lêpirsîn nayê xebitandin. Ev jixwe hatiye jê birîn."

#: conn_shared_res.src
msgctxt ""
"conn_shared_res.src\n"
"STR_QUERY_MORE_TABLES\n"
"string.text"
