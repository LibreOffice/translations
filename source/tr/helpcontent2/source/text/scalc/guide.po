"database_sort.xhp\n"
"par_idN10645\n"
"help.text"
msgid "Select the sort options that you want."
msgstr "İstediğiniz sıralama seçeneklerini seçin."

#. dxfCa
#: database_sort.xhp
msgctxt ""
"database_sort.xhp\n"
"par_idN1063D\n"
"help.text"
msgid "Click <emph>OK</emph>."
msgstr "<emph>Tamam</emph> düğmesine tıklayın."

#. biDGg
#: database_sort.xhp
msgctxt ""
"database_sort.xhp\n"
"par_id1846980\n"
"help.text"
