msgstr "Sözdizimi"

#: 04060106.xhp#par_id3146786.201.help.text
msgid "EXP(Number)"
msgstr "ÜS(Sayı)"

#: 04060106.xhp#par_id3155608.202.help.text
msgid "<emph>Number</emph> is the power to which e is to be raised."
msgstr "<emph>Sayı</emph> e tabanına uygulanmış üstür."

#: 04060106.xhp#hd_id3154418.203.help.text
msgctxt "04060106.xhp#hd_id3154418.203.help.text"
msgid "Example"
msgstr "Örnek"

#: 04060106.xhp#par_id3156340.204.help.text
msgid "<item type=\"input\">=EXP(1)</item> returns 2.71828182845904, the mathematical constant e to Calc's accuracy."
msgstr "<item type=\"input\">=ÜS(1)</item> Calc doğruluğunda e matematik sabitinin değeri 2,71828182845904 sonucunu verir."

