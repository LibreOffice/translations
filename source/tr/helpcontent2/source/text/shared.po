#: 3dsettings_toolbar.xhp#tit.help.text
msgid "3D-Settings"
msgstr "3B Ayarları"

#: 3dsettings_toolbar.xhp#par_idN1055A.help.text
msgid "<variable id=\"3dtoolbar\"><link href=\"text/shared/3dsettings_toolbar.xhp\">3D-Settings</link></variable>"
msgstr "<variable id=\"3dtoolbar\"><link href=\"text/shared/3dsettings_toolbar.xhp\">3B Ayarları</link></variable>"

#: 3dsettings_toolbar.xhp#par_idN1056A.help.text
msgid "<ahelp hid=\".\">The 3D-Settings toolbar controls properties of selected 3D objects.</ahelp>"
msgstr "<ahelp hid=\".\">3B Ayar araç çubuğu ile seçilmiş 3B nesnelerin özelliklerini kontrol edebilirsiniz.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN10575.help.text
msgid "Extrusion on/off"
msgstr "Çekme açık/kapalı"

#: 3dsettings_toolbar.xhp#par_idN10579.help.text
msgid "<ahelp hid=\".\">Switches the 3D effects on and off for the selected objects.</ahelp>"
msgstr "<ahelp hid=\".\">Seçilmiş nesne için 3B efektlerini açıp, kapatır.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN10590.help.text
msgid "Tilt Down"
msgstr "Eğim Aşağı"

#: 3dsettings_toolbar.xhp#par_idN10594.help.text
msgid "<ahelp hid=\".\">Tilts the selected object downwards by five degrees.</ahelp>"
msgstr "<ahelp hid=\".\">Seçilmiş nesneyi 5 derece kadar aşağı doğru eğer.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN105AB.help.text
msgid "Tilt Up"
msgstr "Eğim Yukarı"

#: 3dsettings_toolbar.xhp#par_idN105AF.help.text
msgid "<ahelp hid=\".\">Tilts the selected object upwards by five degrees.</ahelp>"
msgstr "<ahelp hid=\".\">Seçilmiş nesneyi 5 derece kadar yukarı eğer.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN105C6.help.text
msgid "Tilt Left"
msgstr "Eğim Sola"

#: 3dsettings_toolbar.xhp#par_idN105CA.help.text
msgid "<ahelp hid=\".\">Tilts the selected object left by five degrees.</ahelp>"
msgstr "<ahelp hid=\".\">Seçilmiş nesneyi 5 derece kadar sola eğer.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN105E1.help.text
msgid "Tilt Right"
msgstr "Eğim Sağa"

#: 3dsettings_toolbar.xhp#par_idN105E5.help.text
msgid "<ahelp hid=\".\">Tilts the selected object right by five degrees.</ahelp>"
msgstr "<ahelp hid=\".\">Seçilmiş nesneyi 5 derece kadar sağa eğer.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN105FC.help.text
msgid "Depth"
msgstr "Derinlik"

#: 3dsettings_toolbar.xhp#par_idN10600.help.text
msgid "<ahelp hid=\".\">Opens the Extrusion Depth window.</ahelp>"
msgstr "<ahelp hid=\".\">Çekme Derinliği Penceresini Açar.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN10617.help.text
msgid "<ahelp hid=\".\">Select an extrusion depth.</ahelp>"
msgstr "<ahelp hid=\".\">Bir çekme derinliği seç.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN10632.help.text
msgid "<ahelp hid=\".\">Enter an extrusion depth.</ahelp>"
msgstr "<ahelp hid=\".\">Bir çekme derinliği gir.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN1064C.help.text
msgid "Direction"
msgstr "Yön"

#: 3dsettings_toolbar.xhp#par_idN10650.help.text
msgid "<ahelp hid=\".\">Opens the Extrusion Direction window.</ahelp>"
msgstr "<ahelp hid=\".\">Çekme Derinliği Penceresi Açar.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN10667.help.text
msgid "<ahelp hid=\".\">Select a direction.</ahelp>"
msgstr "<ahelp hid=\".\">Bir yön seç.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN10698.help.text
msgid "<ahelp hid=\".\">Select a perspective or parallel extrusion method.</ahelp>"
msgstr "<ahelp hid=\".\">Bir perspektif ya da paralel çekme yöntemi seçin.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN106B3.help.text
msgid "Lighting"
msgstr "Aydınlatma"

#: 3dsettings_toolbar.xhp#par_idN106B7.help.text
msgid "<ahelp hid=\".\">Opens the Extrusion Lighting window.</ahelp>"
msgstr "<ahelp hid=\".\">Çekme Aydıntlatma Penceresini Açar.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN106C6.help.text
msgid "<ahelp hid=\".\">Select a lighting direction.</ahelp>"
msgstr "<ahelp hid=\".\">Bir aydınlatma yönü seçin.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN106E1.help.text
msgid "<ahelp hid=\".\">Select a lighting intensity.</ahelp>"
msgstr "<ahelp hid=\".\">Bir aydınlatma şiddeti seçin.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN106FC.help.text
msgid "Surface"
msgstr "Yüzey"

#: 3dsettings_toolbar.xhp#par_idN10700.help.text
msgid "<ahelp hid=\".\">Opens the Extrusion Surface window.</ahelp>"
msgstr "<ahelp hid=\".\">Çekme Yüzey Penceresini Açar.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN10717.help.text
msgid "<ahelp hid=\".\">Select a surface material or a wireframe display.</ahelp>"
msgstr "<ahelp hid=\".\">Bir yüzey malzemesi ya da tel kafes görüntüsü seçin.</ahelp>"

#: 3dsettings_toolbar.xhp#par_idN10732.help.text
msgid "3D Color"
msgstr "3B Rengi"

#: 3dsettings_toolbar.xhp#par_idN10736.help.text
msgid "<ahelp hid=\".\">Opens the Extrusion Color toolbar.</ahelp>"
msgstr "<ahelp hid=\".\">Çekme Rengi araç çubuğunu açar.</ahelp>"

