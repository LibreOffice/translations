"help.text"
msgid "<variable id=\"registertrue\"><link href=\"text/swriter/guide/registertrue.xhp\">Printing with Page line-spacing</link></variable>"
msgstr ""

#. dagQY
#: registertrue.xhp
msgctxt ""
"registertrue.xhp\n"
"par_id391604248923423\n"
