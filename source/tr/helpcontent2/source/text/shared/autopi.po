msgstr "<ahelp hid=\".\">Yeni alt formunuzda bulunacak bütün alanlar görüntülenir.</ahelp>"

#: 01090210.xhp#par_idN10597.help.text
msgid "<link href=\"text/shared/autopi/01090220.xhp\" name=\"Form Wizard - Get joined fields\">Form Wizard - Get joined fields</link>"
msgstr "<link href=\"text/shared/autopi/01090220.xhp\" name=\"Form Wizard - Get joined fields\">Form Sihirbazı - Birleştirilmiş alanları al</link>"

