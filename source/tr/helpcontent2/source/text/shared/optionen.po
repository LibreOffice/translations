msgid "../images/img.jpg"
msgstr "../images/img.jpg"

#: 01010200.xhp#par_id3145652.58.help.text
msgctxt "01010200.xhp#par_id3145652.58.help.text"
msgid "../images/img.jpg"
msgstr "../images/img.jpg"

#: 01010200.xhp#par_id3155064.59.help.text
msgid "absolute"
