"par_id3157808\n"
"help.text"
msgid "<image id=\"img_id3148685\" src=\"formula/res/refinp1.png\" width=\"0.1665in\" height=\"0.1665in\"><alt id=\"alt_id3148685\">Icon</alt></image>"
msgstr "<image id=\"img_id3148685\" src=\"formula/res/refinp1.png\" width=\"0.1665in\" height=\"0.1665in\"><alt id=\"alt_id3148685\">Simge</alt></image>"

