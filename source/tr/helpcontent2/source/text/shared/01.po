msgctxt ""
"06040300.xhp\n"
"par_id3147573\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/acorexceptpage/newdouble\">Adds the current entry to the list of exceptions.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/acorexceptpage/newdouble\">Mevcut girdiyi istisna listesine ekler.</ahelp>"

