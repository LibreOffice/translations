msgid "<variable id=\"textbox_title\">Text Box</variable>"
msgstr "<variable id=\"textbox_title\">Metin Kutusu</variable>"

#. DXTV6
#: 01140000.xhp
msgctxt ""
"01140000.xhp\n"
"par_id3145790\n"
