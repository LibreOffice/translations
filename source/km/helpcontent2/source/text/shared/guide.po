"par_id3150449\n"
"help.text"
msgid "<link href=\"text/shared/01/01010300.xhp\">Business Cards</link>"
msgstr "<link href=\"text/shared/01/01010300.xhp\">នាមប័ណ្ណ</link>"

#. goNpN
