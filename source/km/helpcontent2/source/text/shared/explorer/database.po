msgstr "<ahelp hid=\".\">លុប​តារាង​ដែល​បាន​ជ្រើស ។</ahelp>"

#: toolbars.xhp#par_idN105A5.help.text
msgctxt "toolbars.xhp#par_idN105A5.help.text"
msgid "Rename"
msgstr "ប្តូរ​​ឈ្មោះ"

#: toolbars.xhp#par_idN105A9.help.text
msgid "<ahelp hid=\".\">Renames the selected table.</ahelp>"
msgstr "<ahelp hid=\".\">ប្តូរ​ឈ្មោះ​របស់​តារាង​ដែល​បាន​ជ្រើស ។</ahelp>"

