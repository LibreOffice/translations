msgstr "<ahelp hid=\"SW:PUSHBUTTON:TP_CHAR_URL:PB_URL\">កំណត់​ទីតាំង​ឯកសារ​ដែល​អ្នក​ចង់​​ត​ភ្ជាប់ និង​បន្ទាប់​មក​ចុច​ <emph>បើក</emph> ។</ahelp>"

#: 05020400.xhp#hd_id3156152.18.help.text
msgid "Reference"
msgstr "សេចក្តី​​​យោង"

#: 05020400.xhp#par_id3155450.19.help.text
msgid "<ahelp hid=\"SW:EDIT:TP_CHAR_URL:ED_TEXT\">Enter the text that you want to display for the hyperlink.</ahelp>"
msgstr "<ahelp hid=\"SW:EDIT:TP_CHAR_URL:ED_TEXT\">បញ្ចូល​អត្ថបទ​ដែល​អ្នក​ចង់​បង្ហាញ​សម្រាប់​តំណខ្ពស់​ ។​</ahelp>"

#: 05020400.xhp#hd_id3158430.28.help.text
