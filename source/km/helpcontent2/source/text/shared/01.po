msgid "Insert Version Comment"
msgstr "បញ្ចូល​មតិយោបល់​កំណែ"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3150466\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versioncommentdialog/VersionCommentDialog\">Enter a comment here when you are saving a new version. If you clicked <emph>Show</emph> to open this dialog, you cannot edit the comment.</ahelp>"
msgstr ""

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"hd_id3149514\n"
"help.text"
msgid "Always save version when closing"
msgstr "តែង​តែ​រក្សា​ទុក​កំណែ​មួ​យ នៅ​ពេល​បិទ​"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3153823\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versionsofdialog/always\">If you have made changes to your document, $[officename] automatically saves a new version when you close the document.</ahelp>"
msgstr "<ahelp hid=\"sfx/ui/versionsofdialog/always\">ប្រសិន​បើ​អ្នក​បាន​ធ្វើ​ការ​ផ្លាស់​ប្តូរ​​​ឯកសារ​របស់​អ្នក​​ $[officename] រក្សា​ទុក​កំណែ​ថ្មី​ដោយ​​ស្វ័យ​ប្រវត្តិ​​ ពេល​អ្នក​បិទ​​ឯកសារ​ ។​</ahelp>"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id6663823\n"
"help.text"
msgid "If you save the document manually, do not change the document after saving, and then close, no new version will be created."
msgstr "ប្រសិន​បើ​អ្នក​រក្សាទុក​ឯកសារ​ដោយ​ដៃ កុំ​ផ្លាស់ប្ដូរ​ឯកសារ​ ក្រោយ​​រក្សាទុក បន្ទាប់​មក​បិទ គ្មាន​កំណែ​ថ្មី​នឹង​ត្រូវ​បាន​បង្កើត​ឡើយ ។"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"hd_id3159167\n"
"help.text"
msgid "Existing versions"
msgstr "កំណែ​ដែល​មាន​ស្រាប់"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3156327\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versionsofdialog/versions\">Lists the existing versions of the current document, the date and the time they were created, the author and the associated comments.</ahelp>"
msgstr "<ahelp hid=\"sfx/ui/versionsofdialog/versions\">រាយ​កំណែ​ដែល​មាន​ស្រាប់​នៃ​ឯកសារ​បច្ចុប្បន្ន​ កាលបរិច្ឆេទ​ និង ពេល​វេលា​ ដែល​ពួក​វា​ត្រូវ​បាន​បង្កើត​ អ្នក​និពន្ធ​ និង​​មតិយោបល់​ដែល​ទាក់​ទង​​ ។​</ahelp>"

#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"hd_id3149578\n"
"help.text"
msgid "Open"
msgstr "បើក"

