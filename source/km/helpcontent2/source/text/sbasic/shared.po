msgctxt "03104400.xhp#hd_id3159157.8.help.text"
msgid "Parameters:"
msgstr "ប៉ារ៉ាម៉ែត្រ ៖"

#: 03104400.xhp#par_id3155419.9.help.text
msgid "<emph>oTest:</emph> the Basic Uno object that you want to test."
msgstr "<emph>oTest ៖</emph> វត្ថុ Basic Uno ដែល​អ្នក​ចង់​សាកល្បង ។"

#: 03104400.xhp#par_id3149236.10.help.text
msgid "<emph>Uno-Interface-Name:</emph> list of Uno interface names."
msgstr "<emph>Uno-Interface-Name ៖</emph> បញ្ជី​នៃ​ឈ្មោះ​ចំណុច​ប្រទាក់ Uno ។"

