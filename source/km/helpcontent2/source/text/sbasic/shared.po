msgid "DefDbL d"
msgstr "DefDbL d"

#: 03101140.xhp#par_idN105C1.help.text
msgctxt "03101140.xhp#par_idN105C1.help.text"
msgid "DefInt i"
msgstr "DefInt i"

#: 03101140.xhp#par_idN105C4.help.text
