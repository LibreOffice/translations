"04030000.xhp\n"
"hd_id3150014\n"
"help.text"
msgid "Vertical"
msgstr "បញ្ឈរ"

#: 04030000.xhp
msgctxt ""
"04030000.xhp\n"
"par_id3145241\n"
"help.text"
msgid "<ahelp hid=\"modules/sdraw/ui/dlgsnap/vert\">Inserts a vertical snap line.</ahelp>"
msgstr "<ahelp hid=\"modules/sdraw/ui/dlgsnap/vert\">បញ្ចូល​​​បន្ទាត់ខ្ទាស់​​បញ្ឈរ​មួយ​​</ahelp>"

