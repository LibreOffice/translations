msgctxt ""
"04030000.xhp\n"
"par_id3145348\n"
"help.text"
msgid "<ahelp hid=\"modules/sdraw/ui/dlgsnap/horz\">Inserts a horizontal snap line.</ahelp>"
msgstr "<ahelp hid=\"modules/sdraw/ui/dlgsnap/horz\">​​បញ្ចូល​បន្ទាត់​ខ្ទាស់​ផ្ដេក​មួយ​ ។​</ahelp>"

