msgctxt ""
"combine_etc.xhp\n"
"par_id3145829\n"
"help.text"
msgid "<emph>Merge</emph>"
msgstr "<emph>បញ្ចូល​គ្នា</emph>"

#. sZzhM
#: combine_etc.xhp
msgctxt ""
"combine_etc.xhp\n"
"par_id3154680\n"
"help.text"
msgid "<emph>Subtract</emph>"
msgstr "<emph>ដក</emph>"

#. 9APE9
#: combine_etc.xhp
msgctxt ""
"combine_etc.xhp\n"
"par_id3153034\n"
"help.text"
msgid "<emph>Intersect</emph>."
msgstr "<emph>ប្រសព្វ</emph>."

#. bMdUM
#: combine_etc.xhp
