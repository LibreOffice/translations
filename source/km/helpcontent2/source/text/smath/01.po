#: 03090600.xhp#par_id3149774.40.help.text
msgid "Transparent"
msgstr "ថ្លា"

#: 03090600.xhp#par_id3155074.41.help.text
msgid "<ahelp hid=\"HID_SMA_PHANTOMX\">Inserts a placeholder for a transparent character. This character takes up the space of \"a\" but does not display it.</ahelp> You can also type <emph>phantom <?></emph> in the <emph>Commands</emph> window."
msgstr "<ahelp hid=\"HID_SMA_PHANTOMX\">បញ្ចូល​កន្លែង​ដាក់​សម្រាប់​តួ​អក្សរ​ថ្លា ។ តួ​អក្សរ​នេះ​យក​កន្លែង​របស់ \"a\" ប៉ុន្តែ​វា​មិន​បង្ហាញ​ទេ ។</ahelp> អ្នក​អាច​វាយ<emph>phantom <?></emph> ក្នុង​បង្អួច <emph>ពាក្យ​បញ្ជា</emph> ផង​ដែរ ។"

