#: 03090600.xhp#par_id3148764.58.help.text
msgid "Wide circumflex"
msgstr "សញ្ញា ^ ធំ"

#: 03090600.xhp#par_id3147311.57.help.text
msgid "<ahelp hid=\"HID_SMA_WIDEHATX\">Inserts a wide circumflex (\"hat\") with a placeholder. </ahelp> You can also type <emph>widehat</emph> in the <emph>Commands</emph> window."
msgstr "<ahelp hid=\"HID_SMA_WIDEHATX\">បញ្ចូល​សញ្ញា ^ ធំ (\"មួក\") ជាមួយ​កន្លែង​ដាក់​មួយ ។</ahelp> អ្នក​ក៏​អាច​វាយ <emph>widehat</emph> ក្នុង​បង្អួច <emph>ពាក្យ​បញ្ជា</emph> ។"

