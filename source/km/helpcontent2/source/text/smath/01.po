#: 03090600.xhp#par_id3147621.32.help.text
msgid "Line over"
msgstr "បន្ទាត់​លើ"

#: 03090600.xhp#par_id3147492.33.help.text
msgid "<ahelp hid=\"HID_SMA_OVERLINEX\">Inserts a line over a placeholder.</ahelp> You can also type <emph>overline <?></emph> in the <emph>Commands</emph> window. The line adjusts itself to correct length."
msgstr "<ahelp hid=\"HID_SMA_OVERLINEX\">បញ្ចូល​បន្ទាត់​លើ​កន្លែង​ជំនួស ។</ahelp> អ្នក​ក៏​អាច​វាយ <emph>overline <?></emph> នៅ​ក្នុង​បង្អួច <emph>ពាក្យ​បញ្ជា</emph>  ។ បន្ទាត់​កែតម្រូវ​ដោយ​ខ្លួន​វា​ផ្ទាល់ ដើម្បី​ឲ្យ​ត្រូវ​នឹង​ប្រវែង ។"

