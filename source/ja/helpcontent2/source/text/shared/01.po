"bm_id3152823\n"
"help.text"
msgid "<bookmark_value>status bar on/off</bookmark_value>"
msgstr "<bookmark_value>ステータスバーのオン/オフ</bookmark_value>"

#: 03060000.xhp
msgctxt ""
