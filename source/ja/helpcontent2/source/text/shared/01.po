#: 04140000.xhp
msgctxt ""
"04140000.xhp\n"
"tit\n"
"help.text"
msgid "Inserting Pictures"
msgstr "画像の挿入"

#: 04140000.xhp
msgctxt ""
"04140000.xhp\n"
"hd_id3154350\n"
"help.text"
msgid "<link href=\"text/shared/01/04140000.xhp\" name=\"Inserting Images\">Inserting Images</link>"
msgstr "<link href=\"text/shared/01/04140000.xhp\" name=\"画像の挿入\">画像の挿入</link>"

