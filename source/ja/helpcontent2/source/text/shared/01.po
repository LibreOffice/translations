"help.text"
msgid "Delete Rows"
msgstr "行の削除"

#: 05340404.xhp
msgctxt ""
"05340404.xhp\n"
"hd_id3147617\n"
"1\n"
"help.text"
msgid "<link href=\"text/shared/01/05340404.xhp\" name=\"Delete Rows\">Delete Rows</link>"
msgstr "<link href=\"text/shared/01/05340404.xhp\" name=\"行の削除\">行の削除</link>"

