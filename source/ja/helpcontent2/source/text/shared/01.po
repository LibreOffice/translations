"hd_id3151054\n"
"47\n"
"help.text"
msgid "<switchinline select=\"appl\"><caseinline select=\"CALC\">Date & Time </caseinline></switchinline>"
msgstr "<switchinline select=\"appl\"><caseinline select=\"CALC\">日付と時刻 </caseinline></switchinline>"

#: 02070000.xhp
msgctxt ""
