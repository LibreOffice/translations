msgstr "<link href=\"text/shared/01/01010202.xhp\" name=\"書式\">書式</link>"

#: 01010202.xhp
msgctxt ""
"01010202.xhp\n"
"par_id3153255\n"
"2\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/labelformatpage/LabelFormatPage\">Set paper formatting options.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/labelformatpage/LabelFormatPage\">用紙の書式設定オプションを設定します。</ahelp>"

#: 01010202.xhp
msgctxt ""
"01010202.xhp\n"
"hd_id3159194\n"
"3\n"
"help.text"
msgid "Horizontal pitch"
msgstr "横の間隔"

