msgctxt ""
"05200100.xhp\n"
"par_id3146794\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/linetabpage/LB_END_STYLE\">Select the arrowhead that you want to apply to the selected line.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/linetabpage/LB_END_STYLE\">選択した線に適用する矢印を選択します。</ahelp>"

