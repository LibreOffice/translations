"help.text"
msgid "<link href=\"text/shared/01/04060000.xhp\" name=\"Scan\">Scan</link>"
msgstr "<link href=\"text/shared/01/04060000.xhp\" name=\"スキャン\">スキャン</link>"

#: 04060000.xhp
msgctxt ""
