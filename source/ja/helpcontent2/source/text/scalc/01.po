msgctxt ""
"04060110.xhp\n"
"par_id3149141\n"
"help.text"
msgid "<item type=\"input\">=LEFT(\"output\";3)</item> returns “out”."
msgstr "<item type=\"input\">=LEFT(\"output\";3)</item> は、「out」を返します。"

