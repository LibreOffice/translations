msgstr "Dim oStruct as new com.sun.star.beans.Property"

#: 03131500.xhp#hd_id3156153.5.help.text
msgctxt "03131500.xhp#hd_id3156153.5.help.text"
msgid "Syntax:"
msgstr "構文:"

#: 03131500.xhp#par_id3155341.6.help.text
