"par_590\n"
"help.text"
msgid "PRINT “text”[2] ; print “x”<br/> PRINT “text”[1:3] ; print “ex”<br/>"
msgstr "PRINT “text”[2] ; “x”を表示<br/> PRINT “text”[1:3] ; “ex”を表示<br/>"

