msgctxt ""
"ErrVBA.xhp\n"
"N0045\n"
"help.text"
msgid "errTitle = \"Error \"& Err &\" at line \"& Erl &\" in \"& Err.Source"
msgstr "errTitle = \"Error \"& Err &\" at line \"& Erl &\" in \"& Err.Source"

