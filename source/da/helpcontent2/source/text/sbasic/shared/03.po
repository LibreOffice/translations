msgctxt ""
"sf_session.xhp\n"
"par_id191601031056673\n"
"help.text"
msgid "<emph>body</emph>: The contents of the message as an unformatted text."
msgstr "<emph>body</emph> (krop): Indholdet af teksten som en uformateret tekst."

