msgctxt ""
"func_ceiling.xhp\n"
"par_id811586214136666\n"
"help.text"
msgid "<input>=CEILING.XCL(-45.67; -2)</input> returns -46."
msgstr "<input>=LOFT.XCL(-45,67; -2)</input> returnerer -46."

#. GMzhD
#: func_ceiling.xhp
msgctxt ""
"func_ceiling.xhp\n"
"bm_id8952518\n"
