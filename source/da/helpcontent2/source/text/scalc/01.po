msgctxt "04060118.xhp#hd_id3153317.165.help.text"
msgid "Syntax"
msgstr "Syntaks"

#: 04060118.xhp#par_id3150423.166.help.text
msgid "COUPNCD(Settlement; Maturity; Frequency; Basis)"
msgstr "KUPONDAG.NÆSTE (Afregningsdato; Udløbsdato; Frekvens; Basis)"

