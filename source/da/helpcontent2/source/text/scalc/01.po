msgid "Sheet from file"
msgstr "Ark fra fil"

#: 04050100.xhp#par_idN105C1.help.text
msgid "<link href=\"text/scalc/01/04050100.xhp\">Sheet from file</link>"
msgstr "<link href=\"text/scalc/01/04050100.xhp\">Ark fra fil</link>"

#: 04050100.xhp#par_idN105D1.help.text
msgid "<ahelp hid=\"26275\">Inserts a sheet from a different spreadsheet file.</ahelp>"
msgstr "<ahelp hid=\"26275\">Indsætter et ark fra en anden regnearksfil.</ahelp>"

#: 04050100.xhp#par_idN105F7.help.text
msgid "Use the <link href=\"text/shared/01/01020000.xhp\">File - Open</link> dialog to locate the spreadsheet."
msgstr "Brug dialogen <link href=\"text/shared/01/01020000.xhp\">Filer - Åbn</link> til at finde regnearket."

#: 04050100.xhp#par_idN10609.help.text
msgid "In the <link href=\"text/scalc/01/04050000.xhp\">Insert Sheet</link> dialog, select the sheet that you want to insert."
msgstr "I dialogen <link href=\"text/scalc/01/04050000.xhp\">Indsæt ark</link> skal du vælge det ark, du vil indsætte."

