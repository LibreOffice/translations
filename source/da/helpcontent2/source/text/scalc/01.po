msgctxt ""
"func_ceiling.xhp\n"
"par_id281586208138400\n"
"help.text"
msgid "<input>=CEILING(3.45)</input> returns 4."
msgstr "<input>=AFRUND.LOFT(3.45)</input> returnerer 4."

