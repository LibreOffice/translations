msgstr "<bookmark_value>skjule;datafelter, fra beregninger i pivottabel</bookmark_value><bookmark_value>vise valgmuligheder i pivottabel</bookmark_value><bookmark_value>sortere;valgmuligheder i pivottabel</bookmark_value><bookmark_value>datafelt valgmuligheder for pivottabel</bookmark_value>"

#. rZueD
#: 12090106.xhp
msgctxt ""
"12090106.xhp\n"
"par_idN10542\n"
"help.text"
msgid "Data Field Options"
msgstr "Datafeltindstillinger"

#. MeeX5
#: 12090106.xhp
msgctxt ""
"12090106.xhp\n"
"par_idN10546\n"
"help.text"
msgid "You can specify additional options for column, row, and page data fields in the <link href=\"text/scalc/01/12090105.xhp\">pivot table</link>."
msgstr "Du kan specificere yderligere indstillinger for kolonner, rækker og side datafelter i <link href=\"text/scalc/01/12090105.xhp\">pivottabellen</link>."

#. ATBoM
