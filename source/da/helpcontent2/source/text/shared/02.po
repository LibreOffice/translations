msgctxt ""
"14020100.xhp\n"
"par_id0305200912080616\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Shows only queries.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Viser kun forespørgsler.</ahelp>"

#. GDWEv
