msgstr "<image src=\"cmd/lc_printdefault.svg\" id=\"img_id561649333208955\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id161649333208955\">Ikonet Udskriv fil direkte</alt></image>"

