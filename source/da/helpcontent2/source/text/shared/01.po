"par_id3154347\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/paratabspage/radiobuttonBTN_TABTYPE_CENTER\">Aligns the center of the text to the tab stop.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/paratabspage/radiobuttonBTN_TABTYPE_CENTER\">justerer midten af teksten til tabulatorstoppet.</ahelp>"

