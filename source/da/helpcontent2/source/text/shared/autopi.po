msgid ">>"
msgstr ">>"

#: 01090100.xhp#par_id3145121.13.help.text
msgid "<ahelp hid=\"HID_DLGFORM_CMDMOVEALL\">Click to move all fields to the box that the arrow is pointing to.</ahelp>"
msgstr "<ahelp hid=\"HID_DLGFORM_CMDMOVEALL\">Klik for at flytte alle felter til boksen som pilen peger på.</ahelp>"

#: 01090100.xhp#hd_id3155419.14.help.text
msgctxt "01090100.xhp#hd_id3155419.14.help.text"
msgid "<"
msgstr "<"

#: 01090100.xhp#par_id3149763.15.help.text
msgid "<ahelp hid=\"HID_DLGFORM_CMDREMOVESELECTED\">Click to move the selected field(s) to the box that the arrow is pointing to.</ahelp>"
msgstr "<ahelp hid=\"HID_DLGFORM_CMDREMOVESELECTED\">Klik for at flytte de valgte felter til boksen som pilen peger på.</ahelp>"

#: 01090100.xhp#hd_id3159399.16.help.text
msgctxt "01090100.xhp#hd_id3159399.16.help.text"
msgid "<<"
msgstr "<<"

#: 01090100.xhp#par_id3156329.17.help.text
msgid "<ahelp hid=\"HID_DLGFORM_CMDREMOVEALL\">Click to move all fields to the box that the arrow is pointing to.</ahelp>"
msgstr "<ahelp hid=\"HID_DLGFORM_CMDREMOVEALL\">Klik for at flytte alle felter til boksen som pilen peger på.</ahelp>"

#: 01090100.xhp#par_idN1074A.help.text
msgid "^"
msgstr "^"

#: 01090100.xhp#par_idN1074E.help.text
msgid "<ahelp hid=\".\">Click to move the selected field up one entry in the list.</ahelp>"
msgstr "<ahelp hid=\".\">Klik for at flytte det valgte felt en plads op på listen.</ahelp>"

#: 01090100.xhp#par_idN10751.help.text
msgid "v"
msgstr "v"

