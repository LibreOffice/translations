msgctxt ""
"data_forms.xhp\n"
"tit\n"
"help.text"
msgid "Working with Forms"
msgstr "Arbejde med formularer"

#. 5AgdK
#: data_forms.xhp
msgctxt ""
"data_forms.xhp\n"
"bm_id5762199\n"
"help.text"
