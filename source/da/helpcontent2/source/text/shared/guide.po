msgctxt ""
"breaking_lines.xhp\n"
"par_idN106F1\n"
"help.text"
msgid "Choose <menuitem>Format - Cells - Alignment</menuitem>."
msgstr "Vælg <menuitem>Formater ▸ Celler ▸ Justering</menuitem>."

#. jjEEf
#: breaking_lines.xhp
msgctxt ""
"breaking_lines.xhp\n"
"par_idN106F9\n"
"help.text"
msgid "Select <emph>Wrap text automatically</emph>."
msgstr "Marker <emph>Automatisk linjeskift</emph>."

#. kdY87
#: breaking_lines.xhp
msgctxt ""
"breaking_lines.xhp\n"
"par_idN10700\n"
"help.text"
