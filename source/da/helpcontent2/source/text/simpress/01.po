msgstr "<image id=\"img_id3153607\" src=\"cmd/sc_deletecolumns.png\" width=\"0.566cm\" height=\"0.566cm\"><alt id=\"alt_id3153607\">Ikon</alt></image>"

#: 05120500m.xhp#par_id3154423.116.help.text
msgid "Delete Column"
msgstr "Slet kolonne"

