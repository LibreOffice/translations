"par_id3154089\n"
"help.text"
msgid "<variable id=\"durchlauftext\"><ahelp hid=\".\">Places the object in front of the text.</ahelp></variable>"
msgstr "<variable id=\"durchlauftext\"><ahelp hid=\".\">Placerer objektet foran teksten.</ahelp></variable>"

#: 05060200.xhp
