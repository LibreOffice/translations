msgctxt ""
"mailmerge03.xhp\n"
"par_id3109225\n"
"help.text"
msgid "<ahelp hid=\".\">Enable to leave empty lines out of the address.</ahelp>"
msgstr "<ahelp hid=\".\">Aktivere at udelade tomme linjer i adressen.</ahelp>"

