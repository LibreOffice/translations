"par_id5591296\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Closes the dialog.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Lukker dialogen.</ahelp>"

#: 04090001.xhp
msgctxt ""
