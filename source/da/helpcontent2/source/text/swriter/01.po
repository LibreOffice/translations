msgid "<SDFIELD TYPE=DATETIME SDVAL=\"35843,4240335648\" SDNUM=\"1031;1031;HH:MM:SS\">10:10:36</SDFIELD>"
msgstr "<SDFIELD TYPE=DATETIME SDVAL=\"35843,4240335648\" SDNUM=\"1031;1031;TT:MM:SS\">10:10:36</SDFIELD>"

