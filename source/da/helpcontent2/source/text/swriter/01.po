"par_idN10885\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Select this icon to browse through wrong table formulas.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Vælg dette ikon for at gennemse forkerte tabelformler.</ahelp>"

#: 02110100.xhp
