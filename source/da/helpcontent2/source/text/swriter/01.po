msgctxt ""
"mailmerge03.xhp\n"
"par_idN10581\n"
"help.text"
msgid "<ahelp hid=\".\">Adds an address block to the mail merge document.</ahelp>"
msgstr "<ahelp hid=\".\">Føjer en adresseblok til brevfletningsdokumentet.</ahelp>"

