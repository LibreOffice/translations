msgstr "Formát súboru"

#: webwizard04.xhp#par_idN1057E.help.text
msgid "<ahelp hid=\"34240\">Displays the format of the files on the index page.</ahelp>"
msgstr "<ahelp hid=\"34240\">Zobrazí v obsahu formát súborov.</ahelp>"

