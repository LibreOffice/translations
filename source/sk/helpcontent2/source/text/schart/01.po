msgctxt ""
"05120000.xhp\n"
"par_id3147001\n"
"help.text"
msgid "<ahelp hid=\".uno:Backward\">Sends the selected data series backward (to the left).</ahelp>"
msgstr "<ahelp hid=\".uno:Backward\">Prenesie vybraný dátový rad dozadu (doľava).</ahelp>"

