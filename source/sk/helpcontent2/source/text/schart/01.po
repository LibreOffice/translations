msgstr "<ahelp hid=\"modules/schart/ui/insertaxisdlg/primaryX\">Zobrazí os X s pododdielovým rozdelením.</ahelp>"

#: 04040000.xhp
msgctxt ""
"04040000.xhp\n"
"hd_id3147003\n"
"help.text"
msgid "Y axis"
msgstr "Os y"

#: 04040000.xhp
msgctxt ""
"04040000.xhp\n"
"par_id3154020\n"
"help.text"
msgid "<ahelp hid=\"modules/schart/ui/insertaxisdlg/primaryY\">Displays the Y axis as a line with subdivisions.</ahelp>"
msgstr "<ahelp hid=\"modules/schart/ui/insertaxisdlg/primaryY\">Zobrazí os Y s pododdielovým rozdelením.</ahelp>"

#: 04040000.xhp
msgctxt ""
"04040000.xhp\n"
"hd_id3150345\n"
"help.text"
msgid "Z axis"
msgstr "Os z"

