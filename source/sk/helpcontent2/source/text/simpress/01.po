msgctxt ""
"06050000.xhp\n"
"tit\n"
"help.text"
msgid "Animation"
msgstr "Animácia"

#: 06050000.xhp
msgctxt ""
"06050000.xhp\n"
"hd_id3153726\n"
"1\n"
"help.text"
