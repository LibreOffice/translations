#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"OKLabel\n"
"LngText.text"
msgid "Ok"
msgstr "ٹھیک ہے"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallLabel\n"
"LngText.text"
msgid "Install"
msgstr "نصب"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AbortLabel\n"
"LngText.text"
msgid "Abort"
msgstr "روکیے"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText1\n"
"LngText.text"
msgid "Welcome to the [FULLPRODUCTNAME] Installation Wizard"
msgstr "[FULLPRODUCTNAME] کی تنصیب میں خوش آمدید"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText2\n"
"LngText.text"
msgid "This installation will update your installed versions of [PRODUCTNAME]"
msgstr "اس تنصیب کے نتیجے میں آپ کے موجودہ [PRODUCTNAME] کا نسخہ جدید ہو جائے گا"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IntroText3\n"
"LngText.text"
msgid "This might take a moment."
msgstr "کچھ دیر انتظار کیجئے۔"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ChooseMyOwnText\n"
"LngText.text"
msgid "Not listed (choose location in an extra step)"
msgstr "فہرست میں موجود نہیں (ایک اضافی مرحلے میں مقام منتخب کیجئے)"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListPromptText\n"
"LngText.text"
msgid "Choose [PRODUCTNAME] [PRODUCTVERSION] installation for which you want to install the [FULLPRODUCTNAME]"
msgstr "مطلوبہ [PRODUCTNAME] [PRODUCTVERSION] کا انتخاب کیجئے جس کے لئے آپ [FULLPRODUCTNAME] کی تنصیب کرنا چاہتے ہیں"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ChooseManualText\n"
"LngText.text"
msgid "Point the dialog to your [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "اپنی موجودہ [PRODUCTNAME] [PRODUCTVERSION] کی تنصیب کی جانب راہنمائی کیجئے۔"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListOKLabelText\n"
"LngText.text"
msgid "Install"
msgstr "نصب"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"ListCancelLabel\n"
"LngText.text"
msgid "Abort"
msgstr "روکیے"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AppInvalidText1\n"
"LngText.text"
msgid "This is not a valid [PRODUCTNAME] [PRODUCTVERSION] installation."
msgstr "معاف کیجئے، یہ [PRODUCTNAME] [PRODUCTVERSION] کی درست تنصیب نہیں ہے۔"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"AppInvalidText2\n"
"LngText.text"
msgid "Run the installer again and choose a valid [PRODUCTNAME] [PRODUCTVERSION] installation"
msgstr "تنصیب کنندہ کو پھر سے چلائیے اور [PRODUCTNAME] [PRODUCTVERSION] کی ایک درست تنصیب منتخب کیجئے"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"StartInstallText1\n"
"LngText.text"
msgid "Click Install to start the installation"
msgstr "تنصیب کے عمل کا آغاز کرنے کے لئے نصب پر کلک کیجئے"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"StartInstallText2\n"
"LngText.text"
msgid "Installation might take a minute..."
msgstr "تنصیب میں کچھ وقت لگے گا..."

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyQText\n"
"LngText.text"
msgid "Installation failed, most likely your account does not have the necessary privileges."
msgstr "تنصیب ناکام ہو گئی، زیادہ امکان اس بات کا ہے کہ آپ کے موجودہ اکاؤنٹ کے پاس ضروری اختیارات موجود نہیں۔"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyQText2\n"
"LngText.text"
msgid "Do you want to identify as administrator and try again?"
msgstr "کیا آپ بطور ایڈمنسٹریٹر اپنی شناخت کروا کر دوبارہ کوشش کرنا چاہیں گے؟"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyYES\n"
"LngText.text"
msgid "Yes, identify"
msgstr "جی، شناخت کیجئے"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"IdentifyNO\n"
"LngText.text"
msgid "No, abort installation"
msgstr "نہیں، تنصیب روک دیجئے"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallFailedText\n"
"LngText.text"
msgid "Installation failed."
msgstr "تنصیب ناکام ہو گئی۔"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallCompleteText\n"
"LngText.text"
msgid "Installation of [PRODUCTNAME] language pack completed."
msgstr "شکریہ، [PRODUCTNAME] کے لئے زبان کی تنصیب مکمل ہو گئی۔"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallCompleteText2\n"
"LngText.text"
msgid "Call '[PRODUCTNAME]-Preferences-Language Settings-Languages' to change the user interface language."
msgstr "یوزر انٹرفیس کی زبان تبدیل کرنے کے لئے '[PRODUCTNAME]-ترجیحات-ترتیباتِ زبان-زبان' پر جائیے۔"

#: macinstall.ulf
msgctxt ""
"macinstall.ulf\n"
"InstallCompleteTextPatch\n"
"LngText.text"
msgid "Installation of [FULLPRODUCTNAME] completed"
msgstr "شکریہ، [FULLPRODUCTNAME] کی تنصیب مکمل ہو گئی"
