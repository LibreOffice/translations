#: autocorr_url.xhp
msgctxt ""
"autocorr_url.xhp\n"
"par_id131606170828813\n"
"help.text"
msgid "Autocorrected hyperlink"
msgstr "Enllaç corregit automàticament"

#. FLUHT
#: autocorr_url.xhp
msgctxt ""
"autocorr_url.xhp\n"
"par_id21605798391315\n"
"help.text"
