msgstr "Suprimeix el salt manual"

#: 02190000.xhp#hd_id3150541.1.help.text
msgid "<link href=\"text/scalc/01/02190000.xhp\" name=\"Delete Manual Breaks\">Delete Manual Break</link>"
msgstr "<link href=\"text/scalc/01/02190000.xhp\" name=\"Suprimeix el salt manual\">Suprimeix el salt manual</link>"

#: 02190000.xhp#par_id3154365.2.help.text
msgid "<ahelp hid=\".\">Choose the type of manual break that you want to delete.</ahelp>"
msgstr "<ahelp hid=\".\">Trieu el tipus de salt manual que voleu suprimir.</ahelp>"

