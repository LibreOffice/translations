msgid "Return value:"
msgstr "Valor de retorn:"

#: 03100060.xhp#par_idN105F1.help.text
msgid "Decimal number."
msgstr "Nombre decimal."

#: 03100060.xhp#par_idN105F4.help.text
msgctxt "03100060.xhp#par_idN105F4.help.text"
