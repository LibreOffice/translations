msgstr "oLib = oLibContainer.GetByName(Libname)"

#: sample_code.xhp#par_id3145271.82.help.text
msgid "oLibDialog = oLib.GetByName(DialogName)"
msgstr "oLibDialog = oLib.GetByName(DialogName)"

