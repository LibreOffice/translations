msgid "~Finish"
msgstr "~Hetisa"

#: euro.src#MESSAGES___1.string.text
msgid "Select directory"
msgstr "Hlawula xikongomisi"

#: euro.src#MESSAGES___2.string.text
msgid "Select file"
