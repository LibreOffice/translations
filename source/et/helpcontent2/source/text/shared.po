msgid "<link href=\"text/shared/02/14020100.xhp\" name=\"Add Tables\">Add Tables</link>"
msgstr "<link href=\"text/shared/02/14020100.xhp\" name=\"Lisa tabelid\">Lisa tabelid</link>"

