"help.text"
msgid "<link href=\"text/shared/explorer/database/11030000.xhp\" name=\"dBase\">dBASE</link>"
msgstr "<link href=\"text/shared/explorer/database/11030000.xhp\" name=\"dBase\">dBASE</link>"

#: 11030000.xhp
msgctxt ""
