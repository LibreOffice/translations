msgctxt ""
"02010100.xhp\n"
"par_id3145223\n"
"help.text"
msgid "not equal to"
msgstr "mittevõrdne"

#: 02010100.xhp
msgctxt ""
"02010100.xhp\n"
"par_id3145635\n"
"help.text"
msgid "... the content of the field does not correspond to the specified expression."
msgstr "... välja sisu ei võrdu näidatud avaldisega."

#: 02010100.xhp
msgctxt ""
"02010100.xhp\n"
"par_id3146815\n"
"help.text"
msgid "greater than"
msgstr "suurem kui"

#: 02010100.xhp
msgctxt ""
"02010100.xhp\n"
"par_id3149150\n"
