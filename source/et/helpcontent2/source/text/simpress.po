msgid "<ahelp hid=\".\">Increases the font size of the selected text.</ahelp>"
msgstr "<ahelp hid=\".\">Suurendab valitud teksti fondi suurust.</ahelp>"

#: main0203.xhp
msgctxt ""
"main0203.xhp\n"
"hd_id0122200903104228\n"
