msgid "d1 = InputBox$ (\"Enter the length of the opposite side: \",\"Opposite Side\")"
msgstr "d2 = InputBox$ (\"Anna alfa-kulman vastaisen sivun pituus: \",\"Vastainen kateetti\")"

