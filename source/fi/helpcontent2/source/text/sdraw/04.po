msgstr "<emph>Toiminto</emph>"

#: 01020000.xhp#hd_id3149946.44.help.text
msgid "Plus(+) Key"
msgstr "Plus(+)"

#: 01020000.xhp#par_id3159119.45.help.text
msgid "Zooms in."
msgstr "Lähennä."

#: 01020000.xhp#hd_id3150655.46.help.text
msgid "Minus(-) Key"
msgstr "Miinus(-)"

#: 01020000.xhp#par_id3145827.47.help.text
msgid "Zooms out."
msgstr "Loitonna."

#: 01020000.xhp#hd_id3149886.99.help.text
msgid "Multiple(×) Key (number pad)"
msgstr "Kerto(×) (numeronäppäimistö)"

#: 01020000.xhp#par_id3150746.102.help.text
msgid "Zooms to fit entire page in screen."
msgstr "Zoomaa koko sivun näyttöön sopivaksi."

#: 01020000.xhp#hd_id3154841.101.help.text
msgid "Divide (÷) Key (number pad)"
msgstr "Jako(÷) (numeronäppäimistö)"

#: 01020000.xhp#par_id3153039.100.help.text
msgid "Zooms in on the current selection."
msgstr "Lähennytään valittua objektia."

#: 01020000.xhp#hd_id3150867.52.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Shift+G"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Vaihto+G"

#: 01020000.xhp#par_id3149250.53.help.text
msgid "Groups selected objects."
msgstr "Ryhmittele valitut objektit."

#: 01020000.xhp#hd_id3149955.54.help.text
msgid "Shift+<switchinline select=\"sys\"><caseinline select=\"MAC\">Command+Option</caseinline><defaultinline>Ctrl+Alt</defaultinline></switchinline>+A"
msgstr "Vaihto+<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento+Optio</caseinline><defaultinline>Ctrl+Alt</defaultinline></switchinline>+A"

#: 01020000.xhp#par_id3148582.55.help.text
msgid "Ungroups selected group."
msgstr "Puretaan valittu ryhmitys."

#: 01020000.xhp#hd_id3146852.56.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Shift+K"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Vaihto+K"

#: 01020000.xhp#par_id3153110.57.help.text
msgid "Combines selected objects."
msgstr "Nivo valitut objektit."

#: 01020000.xhp#hd_id3153567.58.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command+Option</caseinline><defaultinline>Ctrl+Alt</defaultinline></switchinline>+Shift+K"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento+Optio</caseinline><defaultinline>Ctrl+Alt</defaultinline></switchinline>+Vaihto+K"

#: 01020000.xhp#par_id3147366.59.help.text
msgid "Uncombines selected objects."
msgstr "Puretaan valittujen objektien nivonta."

#: 01020000.xhp#hd_id3153730.60.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Shift+ +"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Vaihto+Plus(+)"

#: 01020000.xhp#par_id3155928.61.help.text
msgid "Bring to front."
msgstr "Tuo eteen."

#: 01020000.xhp#hd_id3145245.62.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+ +"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Plus(+)"

#: 01020000.xhp#par_id3148393.63.help.text
msgid "Bring forward."
msgstr "Siirrä eteenpäin."

#: 01020000.xhp#hd_id3150928.64.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+ -"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Miinus(-)"

#: 01020000.xhp#par_id3156062.65.help.text
msgid "Send backward."
msgstr "Siirrä taaksepäin."

#: 01020000.xhp#hd_id3145298.66.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Shift+ -"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Vaihto+Miinus(-)"

#: 01020000.xhp#par_id3149028.67.help.text
msgid "Send to back."
msgstr "Vie taakse."

#: 01020000.xhp#hd_id3147533.68.help.text
msgid "Shortcut Keys Specific to Drawings"
msgstr "Piirrosten erikoispikanäppäimet"

#: 01020000.xhp#hd_id3154865.69.help.text
msgctxt "01020000.xhp#hd_id3154865.69.help.text"
msgid "Shortcut Keys"
msgstr "Pikanäppäin"

#: 01020000.xhp#par_id3155370.70.help.text
msgctxt "01020000.xhp#par_id3155370.70.help.text"
msgid "<emph>Effect</emph>"
msgstr "<emph>Toiminto</emph>"

#: 01020000.xhp#par_idN10AD7.help.text
msgid "Page Up"
msgstr "Page Up"

#: 01020000.xhp#par_idN10ADC.help.text
msgid "Switch to previous page"
msgstr "Vaihdetaan edelliselle dialle"

#: 01020000.xhp#par_idN10AE2.help.text
msgid "Page Down"
msgstr "Page Down"

#: 01020000.xhp#par_idN10AE7.help.text
msgid "Switch to next page"
msgstr "Vaihdetaan seuraavalle dialle"

#: 01020000.xhp#par_idN10AED.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Page Up"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Page Up"

#: 01020000.xhp#par_idN10AFD.help.text
msgid "Switch to previous layer"
msgstr "Siirrytään alempaan kerrokseen"

#: 01020000.xhp#par_idN10AF8.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Page Down"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Page Down"

#: 01020000.xhp#par_idN10AF2.help.text
msgid "Switch to next layer"
msgstr "Siirrytään ylempään kerrokseen"

#: 01020000.xhp#hd_id3153927.71.help.text
msgid "Arrow Key"
msgstr "Nuolinäppäin"

#: 01020000.xhp#par_id3155986.72.help.text
msgid "Moves the selected object in the direction of the arrow key."
msgstr "Siirretään valittua objektia näppäinnuolen suuntaan."

#: 01020000.xhp#hd_id3156259.73.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Arrow Key"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+nuolinäppäin"

#: 01020000.xhp#par_id3147171.74.help.text
msgid "Moves the page view in the direction of the arrow key."
msgstr "Siirrytään sivulla näppäinnuolen suuntaan."

#: 01020000.xhp#hd_id3152484.79.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>-click while dragging an object. Note: you must first enable the <link href=\"text/shared/optionen/01070500.xhp\" name=\"Copy when moving\">Copy when moving</link> option in <switchinline select=\"sys\"><caseinline select=\"MAC\">%PRODUCTNAME - Preferences</caseinline><defaultinline>Tools - Options</defaultinline></switchinline> - %PRODUCTNAME Draw - General to use this shortcut key."
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>-napsautus objektia vedettäessä. <link href=\"text/shared/optionen/01070500.xhp\" name=\"Copy when moving\">Kopioi siirrettäessä</link> -valintaruutu on merkittävä <switchinline select=\"sys\"><caseinline select=\"MAC\">%PRODUCTNAME - Asetukset</caseinline><defaultinline>Työkalut - Asetukset</defaultinline></switchinline> - %PRODUCTNAME Draw - Yleistä-lehdellä, jotta tämä pikanäppäin toimisi!"

#: 01020000.xhp#par_id3149450.80.help.text
msgid "Creates a copy of the dragged object when mouse button is released."
msgstr "Luo vedettävän objektin kopion, kun hiiren painike vapautetaan."

#: 01020000.xhp#hd_id3154643.104.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Enter with keyboard focus (F6) on a drawing object icon on Tools bar"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Enter näppäinkohdistus (F6) Piirros-palkin kuvakkeessa"

#: 01020000.xhp#par_id3150756.105.help.text
msgid "Inserts a drawing object of default size into the center of the current view."
msgstr "Lisää oletuskokoisen piirrosobjektin työstettävän näkymän keskelle."

#: 01020000.xhp#hd_id3151189.106.help.text
msgid "Shift+F10"
msgstr "Vaihto+F10"

#: 01020000.xhp#par_id3151266.107.help.text
msgid "Opens the context menu for the selected object."
msgstr "Valitun objektin kohdevalikko avataan."

#: 01020000.xhp#hd_id3156100.108.help.text
msgctxt "01020000.xhp#hd_id3156100.108.help.text"
msgid "F2"
msgstr "F2"

#: 01020000.xhp#par_id3156323.109.help.text
msgid "Enters text mode."
msgstr "Siirrytään tekstitilaan."

#: 01020000.xhp#hd_id3147563.110.help.text
msgid "Enter"
msgstr "Enter"

#: 01020000.xhp#par_id3150258.111.help.text
msgid "Enters text mode if a text object is selected."
msgstr "Siirrytään tekstitilaan, jos valittuna on tekstiobjekti."

#: 01020000.xhp#hd_id3155851.112.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Enter"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+Enter"

#: 01020000.xhp#par_id3154046.113.help.text
msgid "Enters text mode if a text object is selected. If there are no text objects or if you have cycled through all of the text objects on the page, a new page is inserted."
msgstr "Siirrytään tekstitilaan, jos tekstiobjekti on valittu. Jos ei ole tekstiobjekteja tai sivun kaikki sivun tekstit on käyty läpi, lisätään uusi sivu."

#: 01020000.xhp#hd_id3149977.81.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Option</caseinline><defaultinline>Alt</defaultinline></switchinline>"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Optio</caseinline><defaultinline>Alt</defaultinline></switchinline>"

#: 01020000.xhp#par_id3152812.82.help.text
msgid "Press the <switchinline select=\"sys\"><caseinline select=\"MAC\">Option</caseinline><defaultinline>Alt</defaultinline></switchinline> key and drag with the mouse to draw or resize an object from the center of the object outward."
msgstr "Painetaan <switchinline select=\"sys\"><caseinline select=\"MAC\">Optio</caseinline><defaultinline>Alt</defaultinline></switchinline>-näppäintä ja vedetään hiirellä, jotta objekti pysyisi keskitettynä, kun sitä piirretään tai sen kokoa muutetaan."

#: 01020000.xhp#hd_id3143232.83.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Option</caseinline><defaultinline>Alt</defaultinline></switchinline>+ click on an object"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Optio</caseinline><defaultinline>Alt</defaultinline></switchinline>+ napsautus objektissa"

#: 01020000.xhp#par_id3156007.84.help.text
msgid "Selects the object behind the currently selected object."
msgstr "Valitaan objekti, joka on valittuna olevan takana."

#: 01020000.xhp#hd_id3147252.85.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Option</caseinline><defaultinline>Alt</defaultinline></switchinline>+Shift+click an object"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Optio</caseinline><defaultinline>Alt</defaultinline></switchinline>+Vaihto+objektin napsautus"

#: 01020000.xhp#par_id3145666.86.help.text
msgid "Selects the object in front of the currently selected object."
msgstr "Valitaan objekti, joka on nyt valittuna olevan takana."

#: 01020000.xhp#hd_id3155325.87.help.text
msgid "Shift key while selecting an object"
msgstr "Vaihto-näppäin painettuna objektia valittaessa"

#: 01020000.xhp#par_id3159343.88.help.text
msgid "Adds or removes object to or from the selection."
msgstr "Lisää objektin valintaan tai poistaa siitä."

#: 01020000.xhp#hd_id3083282.75.help.text
msgid "Shift+ drag while moving an object"
msgstr "Vaihto+vetäminen hiirellä objektia muutettaessa"

#: 01020000.xhp#par_id3145620.76.help.text
msgid "The movement of the selected object is constrained by multiples of 45 degrees."
msgstr "Valitun objektin siirtosuunta on rajattu 45 asteen kerrannaisiin."

#: 01020000.xhp#hd_id3154933.89.help.text
msgid "Shift+drag while creating or resizing an object"
msgstr "Vaihto+veto hiirellä, kun luodaan objekti tai muutetaan sen kokoa"

#: 01020000.xhp#par_id3148831.90.help.text
msgid "Constrains the size to keep the object's aspect ratio."
msgstr "Objektin suhteiden säilyttämiseksi kokoa rajoitetaan."

#: 01020000.xhp#hd_id3154205.91.help.text
msgid "Tab"
msgstr "Sarkain"

#: 01020000.xhp#par_id3148804.92.help.text
msgid "Cycles through the objects on the page in the order in which they were created."
msgstr "Kohdistus kiertää sivun objekteissa niiden luomisjärjestyksessä."

#: 01020000.xhp#hd_id3145410.93.help.text
msgid "Shift+Tab"
msgstr "Vaihto+Sarkain"

#: 01020000.xhp#par_id3149764.94.help.text
msgid "Cycles through the objects on the page in the reverse-order in which they were created."
msgstr "Kohdistus kiertää sivun objekteissa käänteisesti luomisjärjestykseen nähden."

#: 01020000.xhp#hd_id3158399.97.help.text
msgid "Esc"
msgstr "Esc"

#: 01020000.xhp#par_id3109840.98.help.text
msgid "Exits current mode."
msgstr "Poistutaan toimintatilasta."
