msgctxt ""
"05340200.xhp\n"
"par_id3147008\n"
"6\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/colwidthdialog/default\" visibility=\"visible\">Automatically adjusts the column width based on the current font.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/colwidthdialog/default\" visibility=\"visible\">Sarakeleveys tulee vallitsevan fontin mukaan säätyväksi.</ahelp>"

