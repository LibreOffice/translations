"12\n"
"help.text"
msgid "<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_SHADOW:LB_SHADOW_COLOR\">Select a color for the shadow.</ahelp>"
msgstr "<ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_SHADOW:LB_SHADOW_COLOR\">Valitaan varjon väri.</ahelp>"

#: 05210600.xhp
