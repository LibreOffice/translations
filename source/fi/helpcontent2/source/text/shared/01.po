"01010100.xhp\n"
"par_id3153822\n"
"103\n"
"help.text"
msgid "<ahelp hid=\"HID_TEMPLATEDLG_TB_BACK\">Moves back to the previous window in the dialog.</ahelp>"
msgstr "<ahelp hid=\"HID_TEMPLATEDLG_TB_BACK\">Siirrytään valintaikkunan aiempaan näkymään.</ahelp>"

