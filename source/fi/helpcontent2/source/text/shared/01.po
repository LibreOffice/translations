"help.text"
msgid "<bookmark_value>saving as command; precautions</bookmark_value>"
msgstr "<bookmark_value>tallennus nimellä -komento; varotoimet</bookmark_value>"

#: 01070000.xhp
msgctxt ""
"01070000.xhp\n"
"hd_id3151260\n"
