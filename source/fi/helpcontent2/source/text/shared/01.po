"hd_id3154188\n"
"135\n"
"help.text"
msgid "<switchinline select=\"appl\"><caseinline select=\"WRITER\">No Format</caseinline></switchinline>"
msgstr "<switchinline select=\"appl\"><caseinline select=\"WRITER\">Ei muotoilua</caseinline></switchinline>"

#: 02100000.xhp
msgctxt ""
