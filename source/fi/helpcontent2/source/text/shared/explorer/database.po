#: 02010100.xhp
msgctxt ""
"02010100.xhp\n"
"par_id3148478\n"
"help.text"
msgid "SELECT \"PtNo\" AS \"PartNum\" FROM \"Parts\""
msgstr "SELECT \"PtNo\" AS \"Osanumero\" FROM \"Parts\""

