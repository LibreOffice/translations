#: 01020000.xhp#par_ii3151019.161.help.text
msgid "Go and select to end of line"
msgstr "Mene valiten rivin loppuun"

#: 01020000.xhp#hd_ii3149371.162.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command+Arrow Up</caseinline><defaultinline>Ctrl+Home</defaultinline></switchinline>"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento+Ylänuolinäppäin</caseinline><defaultinline>Ctrl+Home</defaultinline></switchinline>"

#: 01020000.xhp#par_ii3149396.164.help.text
