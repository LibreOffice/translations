#: 01020000.xhp#par_ii3151055.167.help.text
msgid "Go and select text to start of text block in slide"
msgstr "Mene valiten dian tekstilohkon alkuun"

#: 01020000.xhp#hd_ii3151075.168.help.text
msgid "<switchinline select=\"sys\"><caseinline select=\"MAC\">Command+Arrow Down</caseinline><defaultinline>Ctrl+End</defaultinline></switchinline>"
msgstr "<switchinline select=\"sys\"><caseinline select=\"MAC\">Komento+Alanuolinäppäin</caseinline><defaultinline>Ctrl+End</defaultinline></switchinline>"

#: 01020000.xhp#par_ii3149732.170.help.text
