msgstr ""

#: 01120000.xhp
msgctxt ""
"01120000.xhp\n"
"hd_id2013916\n"
"help.text"
msgid "<link href=\"text/swriter/01/01120000.xhp\">Print Preview</link>"
msgstr "<link href=\"text/swriter/01/01120000.xhp\">Pregled ispisa</link>"

#: 01120000.xhp
msgctxt ""
"01120000.xhp\n"
"par_id1471907\n"
"help.text"
msgid "<ahelp hid=\".uno:PrintPreview\">Displays a preview of the printed page or closes the preview.</ahelp>"
msgstr "<ahelp hid=\".uno:PrintPreview\">Nudi pregled stranice za ispis ili zatvara pregled.</ahelp>"

#: 01120000.xhp
msgctxt ""
"01120000.xhp\n"
"par_id8697470\n"
"help.text"
msgid "Use the icons on the <emph>Print Preview Bar</emph> to scroll through the pages of the document or to print the document."
msgstr "Koristite ikonu <emph>Pregled stranice</emph> kako biste pregledali sve stranice dokumenta ili kako biste ispisali dokument."

#: 01120000.xhp
msgctxt ""
"01120000.xhp\n"
"par_id4314706\n"
"help.text"
msgid "You can also press Page Up and Page Down keys to scroll through the pages."
msgstr "Možete koristiti tipke Page Up i Page Down za prelistavanje stranica."

#: 01120000.xhp
msgctxt ""
"01120000.xhp\n"
"par_id4771874\n"
"help.text"
msgid "You cannot edit your document while you are in the print preview."
msgstr "Dok pregledavate dokument, ne možete ga uređivati."

#: 01120000.xhp
msgctxt ""
"01120000.xhp\n"
"par_id5027008\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">To exit the print preview, click the <emph>Close Preview</emph> button.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Za izlaz odaberite <emph>Zatvori pregled</emph>.</ahelp>"

#: 01120000.xhp
msgctxt ""
"01120000.xhp\n"
"par_id2649311\n"
"help.text"
msgid "<link href=\"text/swriter/main0210.xhp\" name=\"Page View Object Bar\">Page View Object Bar</link>"
msgstr ""

