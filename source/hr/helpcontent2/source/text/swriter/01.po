msgid "<link href=\"text/swriter/01/04090000.xhp\" name=\"Insert - Field\">Insert - Field</link>."
msgstr "<link href=\"text/swriter/01/04090000.xhp\" name=\"Umetni – Polje\">Umetni – Polje</link>."

