msgid "Output"
msgstr "Izlaz"

#: 01150000.xhp
msgctxt ""
"01150000.xhp\n"
"par_id3155138\n"
"help.text"
msgid "Determines whether to send your form letters to a printer or save them to a file."
msgstr "Određuje hoće li se obrazac pisma ispisati ili spremuti kao dokument."

#: 01150000.xhp
msgctxt ""
"01150000.xhp\n"
"hd_id3150485\n"
"help.text"
msgid "Printer"
msgstr "Pisač"

#: 01150000.xhp
msgctxt ""
"01150000.xhp\n"
"par_id3149167\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/mailmerge/printer\">Prints the form letters.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/mailmerge/printer\">Ispisuje obrasce pisma.</ahelp>"

