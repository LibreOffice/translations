msgid "Insert graphics"
msgstr "Faka iintjengiso"

#: strings.src#RID_STR_CONTROL_SUBSTITUTED_NAME.string.text
msgid "substituted"
msgstr "janyelwe"

#: strings.src#RID_STR_CONTROL_SUBSTITUTED_EPXPLAIN.string.text
msgid "An error occurred while this control was being loaded. It was therefore replaced with a placeholder."
msgstr "Iphoso yenzeke ngesikhathi ilawulo leli lizaliswa. Ngakho-ke lajanyiselelwa sibambelindawo."

#: strings.src#RID_STR_READERROR.string.text
msgid "Error reading data from database"
msgstr "Iphoso ekufundeni idatha ebuya ebulungelweni"

#: strings.src#RID_STR_CONNECTERROR.string.text
msgid "Connection failed"
msgstr "Ithintwano libhalelwe"

#: strings.src#RID_ERR_LOADING_FORM.string.text
msgid "The data content could not be loaded."
msgstr "Okungaphakathi kwedatha akhenge kuzaliswe."

#: strings.src#RID_ERR_REFRESHING_FORM.string.text
msgid "The data content could not be updated"
msgstr "Okungaphakathi kwedatha akhenge kulethwe ebujameni banjesi"

#: strings.src#RID_STR_ERR_INSERTRECORD.string.text
msgid "Error inserting the new record"
msgstr "Iphoso ekufakeni irekhodi elitjha"

#: strings.src#RID_STR_ERR_UPDATERECORD.string.text
msgid "Error updating the current record"
msgstr "Iphoso ekuletheni irekhodi elitjha ebujameni banjesi"

#: strings.src#RID_STR_ERR_DELETERECORD.string.text
msgid "Error deleting the current record"
msgstr "Iphoso ekucimeni irekhodi lanjesi"

#: strings.src#RID_STR_ERR_DELETERECORDS.string.text
msgid "Error deleting the specified records"
msgstr "Iphoso ekucimeni amarekhodi atjhiwoko"

#: strings.src#RID_STR_NEED_NON_NULL_OBJECT.string.text
msgid "The object cannot be NULL."
msgstr "Ilwazi ledatha angeze laba NGELINGASISEMTHETHWENI."

#: strings.src#RID_STR_OPEN_GRAPHICS.string.text
msgid "Insert graphics from..."
msgstr "Faka iintjengiso ezibuya..."

#: strings.src#RID_STR_CLEAR_GRAPHICS.string.text
msgid "Remove graphics"
msgstr "Susa iintjengiso"

#: strings.src#RID_STR_INVALIDSTREAM.string.text
msgid "The given stream is invalid."
msgstr "Ukutlola idatha phezu kwemagnethiki theyibhu akusebenzi."

#: strings.src#RID_STR_SYNTAXERROR.string.text
msgid "Syntax error in query expression"
msgstr "Iphoso yehlelo lelimi ekuzwakalisweni kombuzo"

#: strings.src#RID_STR_INCOMPATIBLE_TYPES.string.text
msgid "The value types supported by the binding cannot be used for exchanging data with this control."
msgstr "Imihlobo yamanani esekelwa kubotjhwa angeze yasetjenziselwa ukurweyila idatha ngelawulweli."

#: strings.src#RID_STR_LABEL_RECORD.string.text
msgid "Record"
msgstr "Rekhodi"

#: strings.src#RID_STR_INVALID_VALIDATOR.string.text
msgid "The control is connected to an external value binding, which at the same time acts as validator. You need to revoke the value binding, before you can set a new validator."
msgstr "Ilawulo lithintene nenani langaphandle elibophako, okuthi ngasikhathi sinye libe lisebenza njengesiqinisekisi. Utlhoga ukucima inani elibophako ngaphambi kokubeka isiqinisekisi esitjha."

#: strings.src#RID_STR_LABEL_OF.string.text
msgid "of"
msgstr "ye"

