msgctxt ""
"04050100.xhp\n"
"par_id6637165\n"
"help.text"
msgid "b = INTERCEPT(Data_Y ;Data_X)"
msgstr "b = INTERCEPÇÃO(Dados_Y ;Dados_X)"

#. MJUyE
#: 04050100.xhp
msgctxt ""
"04050100.xhp\n"
"par_id7879268\n"
"help.text"
