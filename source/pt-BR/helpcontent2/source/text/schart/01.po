"par_id3424481\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Apply a line curve model.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Aplique um modelo de linha curva.</ahelp>"

#. u3vBC
