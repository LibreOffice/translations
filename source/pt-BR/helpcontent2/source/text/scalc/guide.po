msgctxt ""
"autoformat.xhp\n"
"par_id3159203\n"
"help.text"
msgid "<link href=\"text/scalc/01/05110000.xhp\">Format - AutoFormat Styles</link>"
msgstr "<link href=\"text/scalc/01/05110000.xhp\">Formatar - Estilos de autoformatação</link>"

