"hd_id3152935\n"
"help.text"
msgid "Formats"
msgstr "Formatos"

#. sSEUh
#: 02070000.xhp
msgctxt ""
"02070000.xhp\n"
"par_id3125863\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/pastespecial/formats\">Inserts cell format attributes.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/pastespecial/formats\">Insere os atributos de formatação das célula.</ahelp>"

#. vt4Hc
