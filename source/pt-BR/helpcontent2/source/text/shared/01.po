msgctxt ""
"06040400.xhp\n"
"par_id3166460\n"
"help.text"
msgid "<ahelp hid=\".\">Resets the quotation marks to the default symbols.</ahelp>"
msgstr "<ahelp hid=\".\">Restabelece as aspas para os símbolos padrão.</ahelp>"

