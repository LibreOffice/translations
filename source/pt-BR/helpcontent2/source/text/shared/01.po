"help.text"
msgid "<variable id=\"minimalrowh1\"><link href=\"text/shared/01/minimal_row_height.xhp\">Minimal Row Height</link></variable>"
msgstr "<variable id=\"minimalrowh1\"><link href=\"text/shared/01/minimal_row_height.xhp\">Altura mínima da linha</link></variable>"

#. c8ffb
