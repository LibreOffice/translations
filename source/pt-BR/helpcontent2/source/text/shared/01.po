msgstr "<link href=\"text/shared/01/02230000.xhp\" name=\"Alterações\">Alterações</link>"

#: 02230000.xhp#par_id3145759.2.help.text
msgid "<ahelp hid=\".\">Lists the commands that are available for tracking changes in your file.</ahelp>"
msgstr "<ahelp hid=\".\">Lista os comandos que estão disponíveis para rastrear as alterações em seu arquivo.</ahelp>"

