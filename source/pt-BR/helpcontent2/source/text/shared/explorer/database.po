msgctxt ""
"tablewizard03.xhp\n"
"par_idN10572\n"
"help.text"
msgid "<ahelp hid=\".\">Select the field name.</ahelp>"
msgstr "<ahelp hid=\".\">Selecione o nome do campo.</ahelp>"

