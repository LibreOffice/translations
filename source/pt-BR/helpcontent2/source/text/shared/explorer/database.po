"par_idN1054D\n"
"help.text"
msgid "<link href=\"text/shared/explorer/database/menuedit.xhp\">Edit</link>"
msgstr "<link href=\"text/shared/explorer/database/menuedit.xhp\">Editar</link>"

#: menuedit.xhp
msgctxt ""
"menuedit.xhp\n"
"par_idN1055D\n"
"help.text"
