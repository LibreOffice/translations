msgctxt ""
"dabawiz03auth.xhp\n"
"par_idN10545\n"
"help.text"
msgid "<ahelp hid=\".\">Enter the user name to access the database.</ahelp>"
msgstr "<ahelp hid=\".\">Digite o nome de usuário para acessar o banco de dados.</ahelp>"

#: dabawiz03auth.xhp
