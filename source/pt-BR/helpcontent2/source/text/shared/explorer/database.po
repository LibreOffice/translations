msgctxt ""
"tablewizard03.xhp\n"
"par_idN10587\n"
"help.text"
msgid "<ahelp hid=\".\">Select a field and click > to add it to the list of primary key fields.</ahelp>"
msgstr "<ahelp hid=\".\">Selecione um campo ou clique em > para adicioná-lo à lista de campos de chave primária.</ahelp>"

