#. HEucf
#: text_frame.xhp
msgctxt ""
"text_frame.xhp\n"
"hd_id3156104\n"
"help.text"
msgid "To Insert a Text Frame"
msgstr "Para inserir um quadro de texto"

#. yqfuF
#: text_frame.xhp
msgctxt ""
"text_frame.xhp\n"
"par_id3149961\n"
"help.text"
msgid "Select the text that you want to include in the frame."
msgstr "Selecione o texto que deseja incluir no quadro."

