msgid "Anchor <-> Text"
msgstr "Ancorar <-> Texto"

#: 02110000.xhp#par_id3150314.24.help.text
msgid "<ahelp hid=\"HID_NAVI_TBX12\">Jumps between the footnote text and the footnote anchor.</ahelp>"
msgstr "<ahelp hid=\"HID_NAVI_TBX12\">Vai para a área entre o texto e a âncora da nota de rodapé.</ahelp>"

