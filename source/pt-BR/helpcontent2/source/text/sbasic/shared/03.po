#: sf_menu.xhp
msgctxt ""
"sf_menu.xhp\n"
"bas_id261643134374056\n"
"help.text"
msgid "Set oMenu = oDoc.CreateMenu(\"My Menu\")"
msgstr "Set oMenu = oDoc.CreateMenu(\"Meu Menu\")"

