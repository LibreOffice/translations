msgctxt ""
"04060181.xhp\n"
"par_id3149393\n"
"help.text"
msgid "<item type=\"input\">=B(10;1/6;2)</item> returns a probability of 29%."
msgstr "=B(10; 1/6; 2) эҳтимолият 29%."

