"tit\n"
"help.text"
msgid "NOW"
msgstr "JETZT"

#: func_now.xhp
msgctxt ""
"func_now.xhp\n"
"bm_id3150521\n"
"help.text"
msgid "<bookmark_value>NOW function</bookmark_value>"
msgstr "<bookmark_value>JETZT (Funktion)</bookmark_value>"

#: func_now.xhp
msgctxt ""
