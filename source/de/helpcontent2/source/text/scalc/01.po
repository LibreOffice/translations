#: 04060115.xhp#bm_id3150014.help.text
msgid "<bookmark_value>DELTA function</bookmark_value><bookmark_value>recognizing;equal numbers</bookmark_value>"
msgstr "<bookmark_value>DELTA (Funktion)</bookmark_value><bookmark_value>Erkennen;gleiche Zahlen</bookmark_value>"

#: 04060115.xhp#hd_id3150014.129.help.text
msgid "DELTA"
msgstr "DELTA"

#: 04060115.xhp#par_id3148760.130.help.text
msgid "<ahelp hid=\"HID_AAI_FUNC_DELTA\">The result is TRUE (1) if both numbers, which are delivered as an argument, are equal, otherwise it is FALSE (0).</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_DELTA\">Das Ergebnis ist WAHR (1), wenn beide Zahlen, die als Argument übergeben werden, gleich sind, sonst FALSCH (0).</ahelp>"

#: 04060115.xhp#hd_id3155435.131.help.text
msgctxt "04060115.xhp#hd_id3155435.131.help.text"
msgid "Syntax"
msgstr "Syntax"

#: 04060115.xhp#par_id3145247.132.help.text
msgid "DELTA(Number1; Number2)"
msgstr "DELTA(Zahl 1; Zahl 2)"

