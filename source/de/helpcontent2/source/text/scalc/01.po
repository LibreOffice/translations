msgctxt ""
"04060116.xhp\n"
"par_id3151393\n"
"help.text"
msgid "<item type=\"input\">=IMPOWER(\"2+3i\";2)</item> returns -5+12i."
msgstr "<item type=\"input\">=IMPOTENZ(\"2+3i\";2)</item> ergibt -5+12i."

