msgctxt ""
"04060184.xhp\n"
"par_id2853733\n"
"help.text"
msgid "<item type=\"input\">=MODE.MULT(A1:A50)</item>"
msgstr "<item type=\"input\">=MODUS.VIELF(A1:A50)</item>"

