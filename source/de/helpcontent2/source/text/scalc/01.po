"hd_id3151344\n"
"help.text"
msgid "Syntax"
msgstr "Syntax"

#: 04060185.xhp
msgctxt ""
"04060185.xhp\n"
"par_id3147405\n"
"help.text"
msgid "FORECAST.LINEAR(Value; DataY; DataX)"
msgstr "PROGNOSE.LINEAR(Wert; DatenY; DatenX)"

