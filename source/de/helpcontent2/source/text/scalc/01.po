"04060107.xhp\n"
"par_idN11B41\n"
"help.text"
msgid "D"
msgstr "D"

#: 04060107.xhp
msgctxt ""
"04060107.xhp\n"
"par_idN11B48\n"
"help.text"
msgid "1"
msgstr "1"

