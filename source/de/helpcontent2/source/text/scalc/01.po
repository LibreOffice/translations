msgctxt ""
"04060105.xhp\n"
"par_id3154314\n"
"help.text"
msgid "<item type=\"input\">=NOT(AND(A;B))</item> returns TRUE"
msgstr "<item type=\"input\">=NICHT(UND(A;B))</item> ergibt WAHR"

