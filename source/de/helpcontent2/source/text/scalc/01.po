msgctxt ""
"04060105.xhp\n"
"par_id3083444\n"
"help.text"
msgid "<item type=\"input\">=OR(A;B)</item> returns TRUE"
msgstr "<item type=\"input\">=ODER(A;B)</item> ergibt WAHR"

#. BcGUg
