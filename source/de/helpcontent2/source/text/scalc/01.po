msgctxt "04060115.xhp#hd_id3149002.133.help.text"
msgid "Example"
msgstr "Beispiel"

#: 04060115.xhp#par_id3151020.134.help.text
msgid "<item type=\"input\">=DELTA(1;2)</item> returns 0."
msgstr "<item type=\"input\">=DELTA(1;2)</item> ergibt 0."

