msgctxt ""
"numbers_text.xhp\n"
"par_id0908200901265544\n"
"help.text"
msgid "<link href=\"text/shared/01/05020300.xhp\">Format - Cells - Numbers</link>"
msgstr "<link href=\"text/shared/01/05020300.xhp\">Format – Zellen – Register: Zahlen</link>"

