"04120201.xhp\n"
"hd_id3154474\n"
"38\n"
"help.text"
msgid "Edit"
msgstr "Bearbeiten"

#: 04120201.xhp
msgctxt ""
"04120201.xhp\n"
"par_id3153675\n"
"39\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/tocstylespage/edit\">Opens the <emph>Paragraph Style</emph> dialog, where you can modify the selected paragraph style.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/tocstylespage/edit\">Öffnet den Dialog <emph>Absatzvorlage</emph>, in dem Sie die ausgewählte Absatzvorlage bearbeiten können.</ahelp>"

