msgstr "Datenbank.Tabelle.Firma"

#. Qocp5
#: 04090200.xhp
msgctxt ""
"04090200.xhp\n"
"par_id3148673\n"
"help.text"
msgid "Database.Table.Company NEQ \"\""
msgstr "Datenbank.Tabelle.Firma NEQ \"\""

#. Ps8wW
