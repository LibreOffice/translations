"04090003.xhp\n"
"par_id3149558\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/fldfuncpage/remove\">Removes the selected item from the list.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/remove\">Entfernt den ausgewählten Eintrag aus der Liste.</ahelp>"

#. LUZ9B
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"hd_id3145126\n"
"help.text"
msgid "Move Up"
msgstr "Nach oben"

#. AyxCU
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"par_id3155970\n"
"help.text"
