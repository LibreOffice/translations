msgctxt ""
"stylist_update.xhp\n"
"tit\n"
"help.text"
msgid "Updating Styles From Selections"
msgstr "Formatvorlage aus Auswahl aktualisieren"

#. BjxtY
#: stylist_update.xhp
msgctxt ""
"stylist_update.xhp\n"
"bm_id3155915\n"
"help.text"
msgid "<bookmark_value>Stylist, see Styles window</bookmark_value>      <bookmark_value>styles; updating from selections</bookmark_value>      <bookmark_value>templates; updating from selections</bookmark_value>      <bookmark_value>Styles window; updating from selections</bookmark_value>      <bookmark_value>updating; styles, from selections</bookmark_value>"
msgstr "<bookmark_value>Formatvorlagen; Fenster</bookmark_value><bookmark_value>Vorlagen; aktualisieren aus Auswahl</bookmark_value><bookmark_value>Dokumentvorlagen; aktualisieren aus Auswahl</bookmark_value><bookmark_value>Formatvorlagen; aktualisieren aus Auswahl</bookmark_value><bookmark_value>Aktualisieren; Formatvorlagen aus Auswahl</bookmark_value>"

