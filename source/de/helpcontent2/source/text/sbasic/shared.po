"03120306.xhp\n"
"par_id3153189\n"
"help.text"
msgid "sInput = InputBox(\"Please input a date in the international format 'YYYY-MM-DD'\")"
msgstr "sInput = InputBox(\"Geben Sie bitte ein Datum im englischen Format MM-DD-YYYY ein\")"

