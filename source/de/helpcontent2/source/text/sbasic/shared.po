msgstr "8 – APPEND (Datei zum Erweitern geöffnet)"

#. 8huJZ
#: 03020405.xhp
msgctxt ""
"03020405.xhp\n"
"par_id3154757\n"
"help.text"
msgid "32 - BINARY (file open in binary mode)."
msgstr "32 – BINARY (Datei im Binärzugriff geöffnet)."

#. xBfKE
