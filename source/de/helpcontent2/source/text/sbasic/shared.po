msgctxt ""
"03120112.xhp\n"
"par_id3151380\n"
"help.text"
msgid "MsgBox \"From \" + ChrW(913) + \" to \" + ChrW(937)"
msgstr "MsgBox \"Von \" + ChrW(913) + \" bis \" + ChrW(937)"

#. zPJDa
