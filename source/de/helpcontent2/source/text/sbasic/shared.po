"par_id3154491\n"
"help.text"
msgid "\"red= \" & red(lVar) & Chr(13)&_"
msgstr "\"rot= \" & red(lVar) & Chr(13)&_"

#: 03010305.xhp
msgctxt ""
"03010305.xhp\n"
"par_id3149401\n"
"help.text"
msgid "\"green= \" & green(lVar) & Chr(13)&_"
msgstr "\"grün= \" & green(lVar) & Chr(13)&_"

#: 03010305.xhp
msgctxt ""
"03010305.xhp\n"
"par_id3150716\n"
