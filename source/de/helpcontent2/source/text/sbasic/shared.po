msgid "<variable id=\"ChrW_h1\"><link href=\"text/sbasic/shared/03120112.xhp\">ChrW Function [VBA]</link></variable>"
msgstr "<variable id=\"ChrW_h1\"><link href=\"text/sbasic/shared/03120112.xhp\">VBA-Funktion ChrW</link></variable>"

