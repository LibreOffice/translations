msgstr "<variable id=\"signexisitingpdf\"><link href=\"text/shared/01/signexistingpdf.xhp\" name=\"Sign Existing PDF Files\">Bestehende PDF-Dateien signieren</link></variable>"

#: signexistingpdf.xhp
