#: 05350500.xhp
msgctxt ""
"05350500.xhp\n"
"par_id3145119\n"
"help.text"
msgid "<ahelp hid=\"svx/ui/docking3deffects/texcolor\">Converts the texture to color.</ahelp>"
msgstr "<ahelp hid=\"svx/ui/docking3deffects/texcolor\">Die Textur wird in Farbe konvertiert.</ahelp>"

