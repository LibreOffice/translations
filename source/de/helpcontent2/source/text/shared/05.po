"hd_id3153116\n"
"help.text"
msgid "<link href=\"text/shared/05/00000002.xhp\" name=\"Icons in the Documentation\">Icons in the Documentation</link>"
msgstr "<link href=\"text/shared/05/00000002.xhp\" name=\"Verwendete Symbole\">Verwendete Symbole</link>"

#. BF6HG
