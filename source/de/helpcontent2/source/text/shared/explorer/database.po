msgstr "Generierte Werte"

#. PVw9o
#: dabaadvpropgen.xhp
msgctxt ""
"dabaadvpropgen.xhp\n"
"par_idN10553\n"
"help.text"
msgid "<link href=\"text/shared/explorer/database/dabaadvpropgen.xhp\">Generated Values</link>"
msgstr "<link href=\"text/shared/explorer/database/dabaadvpropgen.xhp\">Generierte Werte</link>"

#. x7uc3
