msgctxt ""
"04050100.xhp\n"
"par_id7184057\n"
"help.text"
msgid "m = EXP(SLOPE(LN(Data_Y);Data_X))"
msgstr "m = EXP(STEIGUNG(LN(Daten_Y);Daten_X))"

#. SPjRP
