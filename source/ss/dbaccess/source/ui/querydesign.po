msgid "Please note that some databases may not support this join type."
msgstr "Uyacelwa caphela kutsi lamanye emadathabhesi angahle angayisekeli lenhlobo yelijoyina."

#: querydlg.src#STR_QUERY_INNER_JOIN.string.text
msgid "Includes only records for which the contents of the related fields of both tables are identical."
msgstr "Kufaka ekhatsi emarekhodi kuphela lawo lacuketse emasimi lahlobene lakuwo ombabili lamatafula kunalokufanana tjika."

#: querydlg.src#STR_QUERY_LEFTRIGHT_JOIN.string.text
msgid "Contains ALL records from table '%1' but only records from table '%2' where the values in the related fields are matching."
msgstr "Kucuketse ONKHE emarekhodi etafuleni '%1' kodvwa ngemarekhodi odvwa kulelitafula '%2' lapho kuhambelana khona tinsimi letihlobanako."

#: querydlg.src#STR_QUERY_FULL_JOIN.string.text
msgid "Contains ALL records from '%1' and from '%2'."
msgstr "Kucuketse ONKHE emarekhodi lasuka ku '%1' naku '%2'."

#: querydlg.src#STR_QUERY_CROSS_JOIN.string.text
msgid "Contains the cartesian product of ALL records from '%1' and from '%2'."
msgstr ""

#: querydlg.src#STR_QUERY_NATURAL_JOIN.string.text
msgid "Contains only one column for each pair of equally-named columns from '%1' and from '%2'."
msgstr ""
