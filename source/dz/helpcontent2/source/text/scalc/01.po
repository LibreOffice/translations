#: 04060112.xhp#par_id3157876.34.help.text
msgid "void CALLTYPE fn(out, in1, in2, ...)"
msgstr "void CALLTYPE fn(ཕྱི་ཁར། ནང་ན་༡། ནང་ན་ ༢ ...)།"

