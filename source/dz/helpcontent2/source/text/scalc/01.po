msgid "Example"
msgstr "དཔེར་བརྗོད།"

#: 04060116.xhp#par_id3149253.93.help.text
msgid "            <item type=\"input\">=IMEXP(\"1+j\") </item>returns 1.47+2.29j (rounded)."
msgstr ""

