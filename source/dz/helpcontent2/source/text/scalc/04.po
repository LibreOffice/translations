#: 01020000.xhp#par_id3154798.142.help.text
msgid "Displays the options for the current field."
msgstr "ད་ལྟོའི་ས་སྒོ་གི་དོན་ལུ་ གདམ་ཁ་ཚུ་ བཀྲམ་སྟོན་འབདཝ་ཨིན།"

#: 01020000.xhp#hd_id3148418.141.help.text
msgid "Delete"
msgstr "བཏོན་གཏང་།"

