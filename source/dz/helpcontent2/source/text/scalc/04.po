msgctxt ""
"01020000.xhp\n"
"par_id3145236\n"
"help.text"
msgid "Moves one sheet to the left."
msgstr "ཤོག་ཁྲམ་གཅིག་གཡོན་ལུ་སྤོཝ་ཨིན།"

