msgstr "<bookmark_value>ཕྱིར་འདྲེན་འབད་དོ་;ནང་ཐིག་ཚུ་</bookmark_value><bookmark_value>དཔར་བསྐྲུན་འབད་དོ་; ནང་ཐིག་ཚུ་</bookmark_value><bookmark_value>ཁྱབ་ཚད་ཚུ་;ཁྱབ་ཚད་ཚུ་དཔར་བསྐྲུན་འབད་</bookmark_value><bookmark_value>དཔར་བསྐྲུན་ཁྱབ་ཚད་ཀྱི་པི་ཌི་ཨེཕ་ཕྱིར་འདྲེན་</bookmark_value><bookmark_value>ནང་ཐིག་ཁྱབ་ཚད་; དཔར་བསྐྲུན་འབད་དོ་</bookmark_value><bookmark_value>ནང་ཐིག་ཚུ་; ཁྱབ་ཚད་ཚུ་དཔར་བསྐྲུན་འབད་</bookmark_value><bookmark_value>ཁྱབ་ཚད་ཚུ་དཔར་བསྐྲུན་འབད་</bookmark_value><bookmark_value>བསལ་དོ་, བཏོན་གཏང་ནི་ལྟ་</bookmark_value><bookmark_value>ངེས་འཛིན་འབད་དོ་;དཔར་བསྐྲུན་བད་ཡོད་པའི་ཁྱབ་ཚད་ཚུ་</bookmark_value><bookmark_value>དཔར་བསྐྲུན་འབད་དེ་ཡོད་པའི་ཁྱབ་ཚད་ཚུ་རྒྱ་བསྐྱེད་གཏང་དོ་</bookmark_value><bookmark_value>ཁ་སྐོང་བརྐྱབ་དོ་;དཔར་བསྐྲུན་ཁྱབ་ཚད་ཚུ་ནང་ནང་ཐིག་ཚུ་</bookmark_value><bookmark_value>བཏོན་གཏང་དོ་;ཁྱབ་ཚད་ཚུ་དཔར་བསྐྲུན་འབད་</bookmark_value>"

#: printranges.xhp#par_idN108D7.help.text
msgid "<variable id=\"printranges\"><link href=\"text/scalc/guide/printranges.xhp\">Defining Print Ranges on a Sheet</link> </variable>"
msgstr "<variable id=\"printranges\"><link href=\"text/scalc/guide/printranges.xhp\">ལེབ་གྲངས་ཅིག་གུ་ དཔར་བསྐྲུན་ཁྱབ་ཚད་ ངེས་འཛིན་འབད་དོ་</link></variable>"

#: printranges.xhp#par_idN108F5.help.text
msgid "You can define which range of cells on a spreadsheet to print."
msgstr "ཁྱོད་ཀྱིས་ ཤོག་ཁྲམ་གུ་གི་ནང་ཐིག་ཁྱབ་ཚད་ག་འདི་དཔར་བསྐྲུན་འབད་ནི་ཨིན་ན་ ངེས་འཛིན་འབད་བཏུབ།"

#: printranges.xhp#par_idN108FB.help.text
msgid "The cells on the sheet that are not part of the defined print range are not printed or exported. Sheets without a defined print range are not printed and not exported to a PDF file, unless the document uses the Excel file format."
msgstr "ངེས་འཛིན་འབད་ཡོད་པའི་དཔར་བསྐྲུན་ཁྱབ་ཚད་འདི་གི་ཡན་ལག་མེན་མི་ ལེབ་གྲངས་གུའི་ནང་ཐིག་འདི་ཚུ་ དཔར་བསྐྲུན་དང་ཕྱིར་འདྲེན་མ་འབད་བས། ངེས་འཛིན་འབད་ཡོད་པའི་དཔར་བསྐྲུན་ཁྱབ་ཚད་མེད་མི་ལེབ་གྲངས་ཚུ་ ཡིག་ཆ་འདི་གི་ཨེགསི་སེལ་ཡིག་སྣོད་རྩ་སྒྲིག་ལག་ལེན་མ་འཐབ་ཚུན་ པི་ཌི་ཨེཕ་ཡིག་སྣོད་ལུ་དཔར་བསྐྲུན་དང་ཕྱིར་འདྲེན་མི་འབད།"

#: printranges.xhp#par_idN1077A.help.text
msgid "For files opened in Excel format, all sheets that do not contain a defined print range are printed. The same behavior occurs when you export the Excel formatted spreadsheet to a PDF file."
msgstr "ཨེགསི་སེལ་རྩ་སྒྲིག་ནང་ཁ་ཕྱེ་ཡོད་པའི་ཡིག་སྣོད་ཚུ་གི་དོན་ལུ་ ངེས་འཛིན་འབད་ཡོད་པའི་དཔར་བསྐྲུན་ཁྱབ་ཚད་མེད་མི་ལེབ་གྲངས་ཚུ་ཆ་མཉམ་ དཔར་བསྐྲུན་འབད་ཡོད། སྤྱོད་ལམ་གཅིག་པ་འདི་ ཁྱོད་ཀྱིས་ཨེགསི་སེལ་རྩ་སྒྲིག་འབད་འབད་བའི་ཤོག་ཁྲམ་འདི་ པི་ཌི་ཨེཕ་ཡིག་སྣོད་ལུ་ཕྱིར་འདྲེད་འབད་བའི་སྐབས་འབྱུངམ་ཨིན།"

#: printranges.xhp#par_idN108FE.help.text
msgid "To Define a Print Range"
msgstr "དཔར་བསྐྲུན་ཁྱབ་ཚད་ཅིག་ངེས་འཛིན་འབད་ནི་ལུ་ "

#: printranges.xhp#par_idN10905.help.text
msgid "Select the cells that you want to print."
msgstr "ཁྱོད་ར་གིས་ དཔར་བསྐྲུན་འབད་དགོ་མནོ་མི་ ནང་ཐིག་ཚུ་སེལ་འཐུ་འབད།"

#: printranges.xhp#par_idN10909.help.text
msgid "Choose <emph>Format - Print Ranges - Define</emph>."
msgstr "<emph>རྩ་སྒྲིག་ - དཔར་བསྐྲུན་ཁྱབ་ཚད་ - ངེས་འཛིན་</emph> གདམ་ཁ་རྐྱབས།"

#: printranges.xhp#par_idN10910.help.text
msgid "To Add Cells to a Print Range"
msgstr "དཔར་བསྐྲུན་ཁྱབ་ཚད་ཅིག་ལུ་ ནང་ཐིག་ཁ་སྐོང་རྐྱབ་ནི་ལུ་"

#: printranges.xhp#par_idN10917.help.text
msgid "Select the cells that you want to add to the existing print range."
msgstr "ཡོད་བཞིན་པའི་དཔར་བསྐྲུན་ཁྱབ་ཚད་ལུ་ཁ་སྐོང་འབད་ནི་ཨིན་པའི་ནང་ཐིག་ཚུ་ སེལ་འཐུ་འབད།"

#: printranges.xhp#par_idN1091B.help.text
msgid "Choose <emph>Format - Print Ranges - Add</emph>."
msgstr "<emph>རྩ་སྒྲིག་ - དཔར་བསྐྲུན་ཁྱབ་ཚད་ - ཁ་སྐོང་</emph> གདམ་ཁ་རྐྱབས།"

#: printranges.xhp#par_idN10922.help.text
msgid "To Clear a Print Range"
msgstr "དཔར་བསྐྲུན་ཁྱབ་ཚད་ཅིག་ བསལ་ནི་ལུ་"

#: printranges.xhp#par_idN10929.help.text
msgid "Choose <emph>Format - Print Ranges - Remove</emph>."
msgstr "<emph>རྩ་སྒྲིག་ - དཔར་བསྐྲུན་ཁྱབ་ཚད་ - རྩ་བསྐྲད་</emph> གདམ་ཁ་རྐྱབས།"

#: printranges.xhp#par_idN10953.help.text
msgid "Using the Page Break Preview to Edit Print Ranges"
msgstr "དཔར་བསྐྲུན་ཁྱབ་ཚད་ཞུན་དག་འབད་ནི་གི་དོན་ལུ་ ཤོག་མཚམས་སྔོན་ལྟ་ལག་ལེན་འཐབ་དོ།"

#: printranges.xhp#par_idN1093E.help.text
msgid "In the <emph>Page Break Preview</emph>, print ranges as well as page break regions are outlined by a blue border and contain a centered page number in gray. Nonprinting areas have a gray background."
msgstr "<emph>ཤོག་མཚམས་སྔོན་ལྟ་</emph> ནང་ དཔར་བསྐྲུན་ཁྱབ་ཚད་ དེ་གིས་མ་ཚད་པར་ ཤོག་མཚམས་མངའ་ཁོངས་ཚུ་ མཐའ་མཚམས་ཧོནམ་གྱིས་ མཐའ་ཐིག་བཟོ་ཡོདཔ་དང་ སྦུག་ནང་བཙུགས་ཡོད་པའི་ཤོག་ཨང་སྐྱ་ཐལ་ཡོདཔ་ཨིན། དཔར་བསྐྲུན་མ་ཡིན་པའི་མངའ་ཁོངས་ཚུ་ལུ་ སྐྱ་ཐལ་རྒྱབ་གཞི་ཅིག་ཡོདཔ་ཨིན། "

#: printranges.xhp#par_id3153143.8.help.text
msgid "To define a new page break region, drag the border to a new location. When you define a new page break region, an automatic page break is replaced by a manual page break."
msgstr "ཤོག་མཚམས་མངའ་ཁོངས་གསརཔ་ཅིག་ ངེས་འཛིན་འབད་ནིའི་དོན་ལུ་ མཐའ་མཚམསའདི་གནས་ཁོངས་གསརཔ་ཅིག་ནང་འདྲུད། དེ་ངེས་འཛིན་འབད་བའི་སྐབས་ རང་བཞིན་ཤོག་མཚམས་འདི་ ལག་དེབ་ཤོག་མཚམས་ཀྱིས་ ཚབ་བཙུགས་འབདཝ་ཨིན།"

#: printranges.xhp#par_idN10930.help.text
msgid "To View and Edit Print Ranges"
msgstr "དཔར་བསྐྲུན་ཁྱབ་ཚད་འདི་ སྟོན་ནི་དང་ཞུན་དག་འབད་ནི་ལུ་"

#: printranges.xhp#par_idN10937.help.text
msgctxt "printranges.xhp#par_idN10937.help.text"
msgid "Choose <emph>View - Page Break Preview</emph>."
msgstr "<emph>མཐོང་སྣང་ - ཤོག་མཚམས་སྔོན་ལྟ་</emph> གདམ་ཁ་རྐྱབས།"

#: printranges.xhp#par_idN1082A.help.text
msgid "To change the default zoom factor of the <emph>Page Break Preview</emph>, double click the percentage value on the <emph>Status</emph> bar, and select a new zoom factor."
msgstr "<emph>ཤོག་མཚམས་སྔོན་ལྟ་</emph> གི་སྔོན་སྒྲིག་རྒྱས་ཟུམ་ཆ་རྐྱེན་ བསྒྱུར་བཅོས་འབད་ནི་ལུ་ <emph>གནས་ཚད་</emph> ཕྲ་རིང་གུ་གུའི་བརྒྱ་ཆ་བེ་ལུ་གུ་ ཨེབ་གཏང་འབད་ དེ་ལས་ རྒྱས་ཟུམ་ཆ་རྐྱེན་གསརཔ་ཅིག་སེལ་འཐུ་འབད། "

#: printranges.xhp#par_idN10836.help.text
msgid "Edit the print range."
msgstr "དཔར་བསྐྲུན་ཁྱབ་ཚད་ ཞུན་དག་འབད།"

#: printranges.xhp#par_idN10944.help.text
msgid "To change the size of a print range, drag a border of the range to a new location."
msgstr "དཔར་བསྐྲུན་ཁྱབ་ཚད་ཀྱི་ཚད་ བསྒྱུར་བཅོས་འབད་ནི་ལུ་ ཁྱབ་ཚད་ཀྱི་མཐའ་མཚམས་ཅིག་ གནས་ཁོངས་གསརཔ་ཅིག་ནང་འདྲུད།"

#: printranges.xhp#par_id3151075.12.help.text
msgid "To delete a manual page break that is contained in a print range, drag the border of the page break outside of the print range."
msgstr "དཔར་བསྐྲུན་ཁྱབ་ཚད་ནང་ཡོད་པའི་ ལག་དེབ་ཤོག་མཚམས་ཅིག་བཏོན་གཏང་དགོ་པ་ཅིན་ ཤོག་མཚམས་ཀྱི་མཐའ་མཚམས་འདི་ དཔར་བསྐྲུན་ཁྱབ་ཚད་ཀྱི་ཕྱི་ཁར་འདྲུད།"

#: printranges.xhp#par_idN10948.help.text
msgid "To clear a print range, drag a border of the range onto the opposite border of the range."
msgstr "དཔར་བསྐྲུན་ཁྱབ་ཚད་འདི་བསལ་ནི་གི་དོན་ལུ་ ཁྱབ་ཚད་ཀྱི་མཐའ་མཚམས་ཅིག་ ཁྱབ་ཚད་ཀྱི་རྒྱབ་འགལ་མཐའ་མཚམས་གུ་འདྲུད། "

#: printranges.xhp#par_idN10862.help.text
msgid "To exit the <emph>Page Break Preview</emph>, choose <emph>View - Normal</emph>."
msgstr " <emph>ཤོག་ལེབ་རྒྱུན་ཆད་སྔོན་ལྟ་</emph>འདི་ཕྱིར་ཐོན་འབད་ནི་དོན་ལུ་  <emph>སྟོན་ - སྤྱིར་གཏང་</emph>གདམ་ཁ་བརྐྱབ།"

#: printranges.xhp#par_idN109CF.help.text
msgid "<link href=\"text/scalc/01/05080300.xhp\">Editing Print Ranges</link>"
msgstr "<link href=\"text/scalc/01/05080300.xhp\">དཔར་བསྐྲུན་ཁྱབ་ཚད་ ཞུན་དག་འབད་ནི་</link>"

#: keyboard.xhp#tit.help.text
msgid "Shortcut Keys (%PRODUCTNAME Calc Accessibility)"
