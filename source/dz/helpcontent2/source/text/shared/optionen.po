msgid "Remove"
msgstr "རྩ་བསྐྲད་གཏང་།"

#: javaparameters.xhp#par_idN105C6.help.text
msgid "<ahelp hid=\"svx:PushButton:RID_SVXDLG_JAVA_PARAMETER:PB_REMOVE\">Deletes the selected JRE start parameter.</ahelp>"
msgstr "<ahelp hid=\"svx:PushButton:RID_SVXDLG_JAVA_PARAMETER:PB_REMOVE\">སེལ་འཐུ་འབད་ཡོད་པའི་ཇེ་ཨར་ཨི་འགོ་བཙུགས་ནིའི་ཚད་བཟུང་བཏོན་གཏངམ་ཨིན།</ahelp>"

