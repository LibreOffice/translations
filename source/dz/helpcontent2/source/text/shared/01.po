msgid "Left margin"
msgstr "གཡོན་གྱི་ས་སྟོང་།"

#: 01010202.xhp#par_id3154823.12.help.text
msgid "<ahelp hid=\"SW:METRICFIELD:TP_LAB_FMT:FLD_LEFT\">Displays the distance from the left edge of the page to the left edge of the first label or business card. If you are defining a custom format, enter a value here.</ahelp>"
msgstr "<ahelp hid=\"SW:METRICFIELD:TP_LAB_FMT:FLD_LEFT\">གིས་  ཤོག་ལེབ་ཀྱི་གཡོན་མཐའམ་ལས་  ཁ་ཡིག་དང་པའི་གཡོན་གྱི་མཐའམ་  ཡང་ན་  ཚོང་འབྲེལ་ཤོག་བྱང་ཚུན་གྱི་  གྱང་ཚད་འདི་བཀྲམ་སྟོན་འབདཝ་ཨིན།   ཁྱོད་ཀྱིས་  སྲོལ་སྒྲིག་རྩ་སྒྲིག་འདི་ངེས་འཛིན་འབད་དོ་ཡོད་པ་ཅིན་  གནས་གོང་ཅིག་ནཱ་ལུ་ཐོ་བཀོད་འབད།  </ahelp>"

#: 01010202.xhp#hd_id3156346.13.help.text
msgid "Upper margin"
msgstr "ཡར་གྱི་ས་སྟོང་།"

#: 01010202.xhp#par_id3150355.14.help.text
msgid "<ahelp hid=\"SW:METRICFIELD:TP_LAB_FMT:FLD_UPPER\">Displays distance from the top edge of the page to the top of the first label or business card. If you are defining a custom format, enter a value here.</ahelp>"
msgstr "<ahelp hid=\"SW:METRICFIELD:TP_LAB_FMT:FLD_UPPER\"> ཤོག་ལེབ་ཀྱི་མགོའི་མཐའམ་ལས་  ཁ་ཡིག་དང་པའི་མགོ་་ཡང་ན་ཚོང་འབྲེལ་གྱི་ཤོག་བྱང་ཚུན་ གྱང་ཚད་འདི་བཀྲམ་སྟོན་འབདཝ་ཨིན།  ཁྱོད་ཀྱིས་  སྲོལ་སྒྲིག་རྩ་སྒྲིག་འདི་ངེས་འཛིན་འབད་དོ་ཡོད་པ་ཅིན་  གནས་གོང་ཅིག་ནཱ་ལུ་ཐོ་བཀོད་འབད། </ahelp>"

