"tit\n"
"help.text"
msgid "Align Left"
msgstr "གཡོན་ལུ་ཕྲང་།"

#: 05070100.xhp
msgctxt ""
"05070100.xhp\n"
"hd_id3147069\n"
"1\n"
"help.text"
