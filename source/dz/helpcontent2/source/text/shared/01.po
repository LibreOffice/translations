msgstr "དེ་སྦེ།"

#: 05030600.xhp
msgctxt ""
"05030600.xhp\n"
"par_id3154380\n"
"5\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/backgroundpage/selectlb\">Select the type of background that you want to apply.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/backgroundpage/selectlb\">ཁྱོད་ཀྱིས་འཇུག་སྤྱོད་འབད་དགོ་པའི་  རྒྱབ་གཞི་གི་དབྱེ་བ་འདི་སེལ་འཐུ་འབད།</ahelp>"

