msgctxt ""
"05210500.xhp\n"
"par_id3155535\n"
"18\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/bitmaptabpage/LB_COLOR\">Select a foreground color, and then click in the grid to add a pixel to the pattern.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/bitmaptabpage/LB_COLOR\">གདོང་གཞི་ཚོས་གཞི་འདི་སེལ་འཐུ་འབད་བཞིནམ་ལས་ པིཀ་སེལ་འདི་དཔེ་རིས་ལུ་ཁ་སྐོང་བརྐྱབ་ནི་དོན་ལུ་  གིརིཌི་ལུ་ཨེབ་གཏང་།</ahelp>"

