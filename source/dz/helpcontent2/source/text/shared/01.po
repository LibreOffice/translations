msgid "Colors"
msgstr "ཚོས་གཞི་ཚུ།"

#: 05200100.xhp#par_id3147226.8.help.text
msgid "<variable id=\"farbetext\"><ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_LINE:LB_COLOR\">Select a color for the line.</ahelp></variable>"
msgstr "<variable id=\"farbetext\"><ahelp hid=\"SVX:LISTBOX:RID_SVXPAGE_LINE:LB_COLOR\">གྲལ་ཐིག་དོན་ལུ་ཚོས་གཞི་འདི་སེལ་འཐུ་འབད།</ahelp></variable>"

