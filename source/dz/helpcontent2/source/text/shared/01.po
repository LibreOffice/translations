msgid "after ... seconds"
msgstr "སྐར་ཆག་གི་་་་ཤུལ་མ།"

#: 01100500.xhp#par_id3145315.31.help.text
msgid "<ahelp hid=\"SFX2_RADIOBUTTON_TP_DOCINFORELOAD_RB_FORWARDUPDATE\">Enter the number of seconds to wait before redirecting the browser to a different file.</ahelp>"
msgstr "<ahelp hid=\"SFX2_RADIOBUTTON_TP_DOCINFORELOAD_RB_FORWARDUPDATE\">ཡིག་སྣོད་སོ་སོ་ལུ་བརྡ་འཚོལ་འདི་  སླར་ལོག་མ་གཏང་པའི་ཤུལ་མ་བསྒུག་ནི་དོན་ལུ་  སྐར་ཅག་གི་ཨང་གྲངས་འདི་ཐོ་བཀོད་འབད།</ahelp>"

