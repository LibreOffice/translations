msgctxt ""
"05230300.xhp\n"
"par_id3150669\n"
"8\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/rotationtabpage/MTR_FLD_POS_Y\">Enter the vertical distance from the top edge of the page to the pivot point.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/rotationtabpage/MTR_FLD_POS_Y\">ཤོག་ལེབ་ཀྱི་མགོའི་མཐའམ་ལས་  ཀེར་ཕྲང་གྱང་ཚད་འདི་  ལྟེ་ཤིང་དཔག་ནི་ལུ་ ཐོ་བཀོད་འབད།</ahelp>"

