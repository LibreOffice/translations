msgctxt ""
"05210100.xhp\n"
"hd_id3153364\n"
"61\n"
"help.text"
msgid "Background color"
msgstr "ཚོས་གཞི་རྒྱབ་གཞི།"

#: 05210100.xhp
msgctxt ""
"05210100.xhp\n"
"par_id3152940\n"
"62\n"
"help.text"
