msgid "Individual words"
msgstr "ངོ་རྐྱང་གི་མིང་ཚིག་ཚུ།"

#: 05020200.xhp#par_id3152935.46.help.text
msgid "<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_CHAR_EFFECTS_CB_INDIVIDUALWORDS\">Applies the selected effect only to words and ignores spaces.</ahelp>"
msgstr "<ahelp hid=\"SVX_CHECKBOX_RID_SVXPAGE_CHAR_EFFECTS_CB_INDIVIDUALWORDS\">སེལ་འཐུ་འབད་ཡོད་པའི་ནུས་པ་འདི་  མིང་ཚིག་ཚུ་ལུ་རྐྱངམ་ཅིག་འཇུག་སྤྱོད་འབད་ནི་དང་  བར་སྟོང་འདི་སྣང་མེད་བཞགཔ་ཨིན།</ahelp>"

