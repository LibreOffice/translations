msgid "Server URL"
msgstr "སར་བར་ཡུ་ཨར་ཨེལ།"

#: dabawiz02ldap.xhp#par_idN1057D.help.text
msgid "<ahelp hid=\"HID_DSADMIN_LDAP_HOSTNAME\">Enter the name of the LDAP server using the format \"ldap.server.com\".</ahelp>"
msgstr "<ahelp hid=\"HID_DSADMIN_LDAP_HOSTNAME\"> རྩ་སྒྲིག་འདི་ལག་ལེན་འཐབ་སྟེ་ ཨེལ་ཌི་ཨེ་པི་སར་བར་གྱི་མིང་འདི་བཙུགས།\"ldap.server.com\".</ahelp>"

#: dabawiz02ldap.xhp#par_idN10594.help.text
