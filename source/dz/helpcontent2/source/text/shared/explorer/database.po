msgstr "<ahelp hid=\".\">གནད་སྡུད་ས་སྒོའི་དོན་ལུ་  ཡིག་འབྲུ་གི་ཨང་གྲངས་འདི་  གསལ་བཀོད་འབདཝ་ཨིན། </ahelp>"

#: tablewizard02.xhp#par_idN1059B.help.text
msgctxt "tablewizard02.xhp#par_idN1059B.help.text"
msgid "Decimal places"
msgstr "བཅུ་ཚགས་གནས་ས་ཚུ།"

#: tablewizard02.xhp#par_idN1059F.help.text
msgid "<ahelp hid=\".\">Specifies the number of decimal places for the data field. This option is only available for numerical or decimal data fields.</ahelp>"
msgstr "<ahelp hid=\".\">གིས་  གནད་སྡུད་ས་སྒོའི་དོན་ལུ་  བཅུ་ཚག་གནས་ས་གི་ཨང་གྲངས་འདི་གསལ་བཀོད་འབདཝ་ཨིན།  གདམ་ཁ་འདི་  ཨང་གྲངས་དོན་ལུ་ ཡང་ན་  བཅུ་ཚག་གནད་སྡུད་ས་སྒོའི་དོན་ལུ་རྐྱངམ་ཅིག་  འཐོབ་ཚུགསཔ་ཨིན།</ahelp>"

#: tablewizard02.xhp#par_idN105A2.help.text
msgctxt "tablewizard02.xhp#par_idN105A2.help.text"
msgid "Default value"
msgstr "སྔོན་སྒྲིག་གནས་གོང་།"

#: tablewizard02.xhp#par_idN105A6.help.text
msgid "<ahelp hid=\".\">Specifies the default value for a Yes/No field.</ahelp>"
msgstr "<ahelp hid=\".\">ཨིན་དང་/མེན་གྱི་ས་སྒོའི་དོན་ལུ་  སྔོན་སྒྲིག་གནས་གོང་འདི་གསལ་བཀོད་འབདཝ་ཨིན།</ahelp>"

#: tablewizard02.xhp#par_idN10730.help.text
msgctxt "tablewizard02.xhp#par_idN10730.help.text"
msgid "Auto-increment statement"
msgstr "རང་བཞིན་ཡར་འཕར་གསལ་བཤད།"

#: tablewizard02.xhp#par_id6706747.help.text
msgctxt "tablewizard02.xhp#par_id6706747.help.text"
msgid "<ahelp hid=\".\">Enter the SQL command specifier that instructs the data source to auto-increment a specified Integer data field.</ahelp> For example, the following MySQL statement used the AUTO_INCREMENT statement to increase the \"id\" field each time the statement creates a data field:"
msgstr "<ahelp hid=\".\"> གནད་སྡུད་འབྱུང་ཁུངས་འདི་  ཧྲིལ་ཨང་གནད་སྡུད་ས་སྒོ་ལུ་  རང་བཞིན་གྱི་ཡར་འཕར་གསལ་བཀོད་འབད་ནི་ལུ་  གནད་སྡུད་འབྱུང་ཁུངས་སླབ་སྟོན་འབད་མི་  ཨེསི་ཀིའུ་ཨེལ་བརྡ་བཀོད་གསལ་བཀོད་འདི་བཙུགས།   </ahelp> དཔེར་ན་  འོག་གི་མའིཨེསི་ཀིའུ་ཨེལ་གསལ་བཤད་དེ་གིས་   གསལ་བཤད་ཀྱིས་ཚར་རེ་གནད་སྡུད་ས་སྒོ་ གསར་བསྐྲུན་འབདཝ་ད་  \"id\"ས་སྒོ་ཡར་འཕར་གཏང་ནི་ལུ་  རང་བཞིན་གྱི་ཡར་འཕར་གསལ་བཤད་འདི་ལག་ལེན་འཐབ་མི་འདི་ཨིན།"

#: tablewizard02.xhp#par_id8946501.help.text
msgctxt "tablewizard02.xhp#par_id8946501.help.text"
msgid "CREATE TABLE \"table1\" (\"id\" INTEGER AUTO_INCREMENT)"
msgstr " \"table1\" (\"id\" INTEGER AUTO_INCREMENT)ཐིག་ཁྲམ་གསར་བསྐྲུན་འབད།"

#: tablewizard02.xhp#par_id4846949.help.text
msgid "For this example, you must enter AUTO_INCREMENT into the Auto-increment statement box."
msgstr "འ་ནི་དཔེ་གི་དོན་ལུ་  ཁྱོད་ཀྱིས་  རང་བཞིན་གྱི་ཡར་འཕར་འདི་   <emph>རང་བཞིན་གྱི་ཡར་འཕར་གསལ་བཤད་</emph>སྒྲོམ་ནང་བཙུགས་དགོ།"

