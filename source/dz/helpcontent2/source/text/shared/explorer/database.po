msgid "Base DN"
msgstr "གཞི་རྟེན་ཌི་ཨེན།"

#: dabawiz02ldap.xhp#par_idN10598.help.text
msgid "<ahelp hid=\"HID_DSADMIN_LDAP_BASEDN\">Enter the starting point to search the LDAP database, for example, \"dc=com\".</ahelp>"
msgstr "<ahelp hid=\"HID_DSADMIN_LDAP_BASEDN\">ཨེལ་ཌི་ཨེ་པི་གནད་སྡུད་གཞི་རྟེན་འཚོལ་ནི་ལུ་  འགོ་བཙུགས་ས་ཚིག་འདི་བཙུགས་   དཔེར་ན་ \"dc=com\".</ahelp>"

#: dabawiz02ldap.xhp#par_idN105B7.help.text
msgctxt "dabawiz02ldap.xhp#par_idN105B7.help.text"
msgid "Port number"
msgstr "འདྲེན་བྱེད་ཨང་གྲངས།"

#: dabawiz02ldap.xhp#par_idN105BB.help.text
msgid "<ahelp hid=\"HID_DSADMIN_LDAP_PORTNUMBER\">Enter the port of the LDAP server, normally 389.</ahelp>"
msgstr "<ahelp hid=\"HID_DSADMIN_LDAP_PORTNUMBER\">༣༨༩ སྤྱིར་བཏང་སྦེ་ ཨེལ་ཌི་ཨེ་པི་གི་འདྲེན་ལམ་བཙུགས།</ahelp>"

#: dabawiz02ldap.xhp#par_idN105CA.help.text
msgid "Use secure connection (SSL)"
msgstr "མཐའ་བཙན་མཐུད་ལམ་ལག་ལེན་འཐབ་ (ཨེསི་ཨེསི་ཨེལ་)"

#: dabawiz02ldap.xhp#par_idN105CE.help.text
msgid "<ahelp hid=\"HID_DSADMIN_USESSL_LDAP\">Creates a secure connection to the LDAP server through the Secure Sockets Layer (SSL).</ahelp> By default, an SSL connection uses port 636. A regular connection uses port 389."
msgstr "<ahelp hid=\"HID_DSADMIN_USESSL_LDAP\">མཐའ་བཙན་སོ་ཀེཊིསི་བང་རིམ་(ཨེསི་ཨེསི་ཨེལ་)བརྒྱུད་དེ་ཨེལ་ཌི་ཨེ་པི་སར་བར་ལུ་མཐའ་བཙན་མཐུད་ལམ་གསར་བསྐྲུན་འབདཝ་ཨིན།</ahelp> སྔོན་སྒྲིག་གི་ཐོག་ལས་ ཨེསི་ཨེསི་ཨེལ་མཐུལ་ལམ་གྱིས་འདྲེན་ལམ་༦༣༦ལག་ལེན་འཐབ་ཨིན། དུས་རྒྱུན་མཐུད་ལམ་གྱིས་འདྲེན་ལམ་༣༨༩ལག་ལེན་འཐབ་ཨིན།"

