msgstr "<image id=\"img_id3155892\" src=\"sfx2/res/indexon_small.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3155892\">ངོས་དཔར།</alt></image>"

#: 00000110.xhp#par_id3147089.3.help.text
msgid "<ahelp hid=\"HID_HELP_TOOLBOXITEM_INDEX\">Hides and shows the <emph>navigation pane</emph></ahelp>"
msgstr "<ahelp hid=\"HID_HELP_TOOLBOXITEM_INDEX\">གིས་ <emph>nའགྲུལ་བསྐྱོད་ པེན་ </emph></ahelp>སྦ་ནི་དང་སྟོན་ནི་ཡོདཔ་ཨིན།"

#: 00000110.xhp#par_id3152781.help.text
