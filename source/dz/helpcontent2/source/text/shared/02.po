msgstr "འབྲེལ་མཐུད་ཡུད་པའི་ནང་ཐིག་དེ་ བསལ་གཏང་ཡོདཔ་ཨིན།"

#: 01170102.xhp#par_idN10FE0.help.text
msgid "Enter text or a number in the linked cell"
msgstr "འབྲེལ་མཐུད་ཡོད་པའི་ནང་ཐིག་ནང་་ཚིག་ཡིག་ཡང་ན་ཨང་ཅིག་ཐོ་བཀོད་འབད།"

#: 01170102.xhp#par_idN10FE6.help.text
msgid "Text or number is copied into the text box"
msgstr "ཚིག་ཡིག་གི་སྒྲོམ་འདི་ནང་ ཚིག་ཡིག་ཡང་ན་ཨང་འདི་འདྲ་བཤུས་རྐྱབ་ཡོདཔ་ཨིན།"

#: 01170102.xhp#par_idN10FED.help.text
#, fuzzy
