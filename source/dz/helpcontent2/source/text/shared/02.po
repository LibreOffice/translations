msgid "Automatic Sort"
msgstr "རང་བཞིན་གྱི་ དབྱེ་སེལ།"

#. ZLrhK
#: 01170300.xhp
msgctxt ""
"01170300.xhp\n"
"par_id3153748\n"
"help.text"
msgid "<ahelp hid=\"modules/spropctrlr/ui/taborder/autoB\">Click the<emph> Automatic Sort</emph> button to automatically sort the controls according to their position in the document.</ahelp>"
msgstr "<ahelp hid=\"modules/spropctrlr/ui/taborder/autoB\">ཚད་འཛིན་ཚུ་ཡིག་ཆ་ནང་ཁོང་རའི་གནས་ས་དང་འཁྲིལ་<emph> རང་བཞིན་གྱིས་དབྱེ་སེལ་འབད་ནི་ལུ་</emph> རང་བཞིན་གྱི་དབྱེ་སེལ་ཨེབ་རྟ་ཨེབ་གཏང་།</ahelp>"

