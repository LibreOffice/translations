#: 01170102.xhp#par_idN10FF3.help.text
msgid "Formula result is copied into the text box"
msgstr "མན་ངག་གི་གྲུབ་འབྲས་འདི་ ཚིག་ཡིག་གི་སྒྲོམ་ནང་འདྲ་བཤུས་རྐྱབ་ཡོདཔ་ཨིན།"

#: 01170102.xhp#par_idN10FFA.help.text
#, fuzzy
msgctxt "01170102.xhp#par_idN10FFA.help.text"
msgid "Clear the linked cell"
msgstr "འབྲེལ་མཐུད་ཡོད་པའི་ནང་ཐིག་འདི་བསལ་གཏང་།"

#: 01170102.xhp#par_idN11000.help.text
msgid "Text box is cleared"
msgstr "ཚིག་ཡིག་གི་སྒྲོམ་འདི་བསལ་བཏང་ཡོད།"

#: 01170102.xhp#par_idN1100B.help.text
msgid "Numerical field and formatted field with linked cell"
msgstr "འབྲེལ་མཐུད་ཡོད་པའི་ནང་ཐིག་དང་བཅས་ ཨང་གྲངས་ཀྱི་ས་སྒོ་དང་ རྩ་སྒྲིག་འབད་ཡོད་པའི་ས་སྒོ་།"

#: 01170102.xhp#par_idN11018.help.text
