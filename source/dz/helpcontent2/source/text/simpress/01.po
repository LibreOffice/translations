"bm_id3154013\n"
"help.text"
msgid "<bookmark_value>summary slide</bookmark_value>"
msgstr "<bookmark_value>བཅུད་དོན་བཤུད།</bookmark_value>"

#: 04140000.xhp
msgctxt ""
"04140000.xhp\n"
