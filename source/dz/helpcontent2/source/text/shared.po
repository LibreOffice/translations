#: fontwork_toolbar.xhp#tit.help.text
msgid "Fontwork"
msgstr "ཡིག་གཟུགས་ལཱ།"

#: fontwork_toolbar.xhp#par_idN1055A.help.text
msgid "<link href=\"text/shared/fontwork_toolbar.xhp\">Fontwork</link>"
msgstr "<link href=\"text/shared/fontwork_toolbar.xhp\">Fontwork</link>"

#: fontwork_toolbar.xhp#par_idN1056A.help.text
msgid "The Fontwork toolbar opens when you select a Fontwork object."
msgstr "ཁྱོད་ཀྱིས་ཡིག་གཟུགས་ལཱ་གི་དངོས་པོ་ཅིག་སེལ་འཐུ་འབད་བའི་སྐབས་ལུ་ ཡིག་གཟུགས་ལཱ་གི་ལག་ཆས་ཕྲ་རིང་གིས་ཁ་ཕྱེཝ་ཨིན།"

#: fontwork_toolbar.xhp#par_idN1056D.help.text
msgid "Fontwork Gallery"
msgstr "ཡིག་གཟུགས་ལཱ་གི་སྟོན་ཁང་།"

#: fontwork_toolbar.xhp#par_idN10571.help.text
msgid "<ahelp hid=\".\">Opens the Fontwork Gallery where you can select another preview. Click OK to apply the new set of properties to your Fontwork object.</ahelp>"
msgstr "<ahelp hid=\".\">ཁྱོད་ཀྱིས་སྔོན་ལྟ་གཞན་སེལ་འཐུ་འབད་ཚུགས་པའི་ཡིག་གཟུགས་ལཱ་གི་སྟོན་ཁང་ ཁ་ཕྱེཝ་ཨིན། ཁྱོད་རའི་ཡིག་གཟུགས་ལཱ་གི་དངོས་པོ་ལུ་ རྒྱུ་དངོས་ཀྱི་ཆ་ཚན་གསརཔ་ འཇུག་སྤྱོད་འབད་ནིའི་དོན་ལུ་ བཏུབ་ལུ་ཨེབ་གཏང་འབད།</ahelp>"

#: fontwork_toolbar.xhp#par_idN10588.help.text
msgid "Fontwork Shape"
msgstr "ཡིག་གཟུགས་ལཱ་གི་དབྱིབས།"

#: fontwork_toolbar.xhp#par_idN1058C.help.text
msgid "<ahelp hid=\".\">Opens the Fontwork Shape toolbar. Click a shape to apply the shape to all selected Fontwork objects.</ahelp>"
msgstr "<ahelp hid=\".\">ཡིག་གཟུགས་ལཱ་གི་དབྱིབས་ཀྱི་ལག་ཆས་ཕྲ་རིང་ཁ་ཕྱེཝ་ཨིན། སེལ་འཐུ་འབད་ཡོད་པའི་ཡིག་གཟུགས་ལཱ་གི་དངོས་པོ་ཆ་མཉམ་ལུ་ དབྱིབས་འཇུག་སྤྱོད་འབད་ནིའི་དོན་ལས་ དབྱིབས་ཅིག་ལུ་ཨེབ་གཏང་འབད། </ahelp>"

#: fontwork_toolbar.xhp#par_idN105A6.help.text
msgid "Fontwork Same Letter Heights"
msgstr "ཡིག་གཟུགས་ལཱ་གི་ཡི་གུའི་མཐོ་ཚད་ཆ་འདྲ་བ།"

#: fontwork_toolbar.xhp#par_idN105AA.help.text
msgid "<ahelp hid=\".\">Switches the letter height of the selected Fontwork objects from normal to the same height for all objects.</ahelp>"
msgstr "<ahelp hid=\".\">དངོས་པོ་ཆ་མཉམ་ལུ་ སྤྱིར་བཏང་ལས་ཆ་འདྲ་བའི་མཐོ་ཚད་ཀྱི་བར་སེལ་འཐུ་འབད་ཡོད་པའི་ཡིག་གཟུགས་ལཱ་གི་དངོས་པོའི་ཡི་གུའི་མཐོ་ཚད་འདི་ སོ་ར་བསྒྱུར་འབདཝ་ཨིན། </ahelp>"

#: fontwork_toolbar.xhp#par_idN105C1.help.text
msgid "Fontwork Alignment"
msgstr "ཡིག་གཟུགས་ལཱ་གི་ཕྲང་སྒྲིག"

#: fontwork_toolbar.xhp#par_idN105C5.help.text
msgid "<ahelp hid=\".\">Opens the Fontwork Alignment window.</ahelp>"
msgstr "<ahelp hid=\".\">ཡིག་གཟུགས་ལཱ་གི་ཕྲང་སྒྲིག་སྒོ་སྒྲིག་ཁ་ཕྱེཝ་ཨིན། </ahelp>"

#: fontwork_toolbar.xhp#par_idN105DC.help.text
msgid "<ahelp hid=\".\">Click to apply the alignment to the selected Fontwork objects.</ahelp>"
msgstr "<ahelp hid=\".\">སེལ་འཐུ་འབད་ཡོད་པའི་ཡིག་གཟུགས་ལཱ་གི་དངོས་པོ་ལུ་ཕྲང་སྒྲིག་འཇུག་སྤྱོད་འབད་ནིའི་དོན་ལས་ ཨེབ་གཏང་འབད།</ahelp>"

#: fontwork_toolbar.xhp#par_idN105F3.help.text
msgid "Fontwork Character Spacing"
msgstr "ཡིག་གཟུགས་ལཱ་གི་ཡིག་འབྲུའི་བར་སྟོང་བཞག་ཐངས།"

#: fontwork_toolbar.xhp#par_idN105F7.help.text
msgid "<ahelp hid=\".\">Opens the Fontwork Character Spacing window.</ahelp>"
msgstr "<ahelp hid=\".\">ཡིག་གཟུགས་ལཱ་གི་ཡིག་འབྲུའི་བར་སྟོང་བཞག་ཐངས་སྒོ་སྒྲིག་ཁ་ཕྱེཝ་ཨིན།</ahelp>"

#: fontwork_toolbar.xhp#par_idN1060E.help.text
msgid "<ahelp hid=\".\">Click to apply the character spacing to the selected Fontwork objects.</ahelp>"
msgstr "<ahelp hid=\".\">སེལ་འཐུ་འབད་ཡོད་པའི་ཡིག་གཟུགས་ལཱ གི་དངོས་པོ་ཚུ་ལུ་ ཡིག་འབྲུའི་བར་སྟོང་བཞག་ཐངས་འཇུག་སྤྱོད་འབད་ནིའི་དོན་ལས་ ཨེབ་གཏང་འབད།</ahelp>"

#: fontwork_toolbar.xhp#par_idN1061D.help.text
msgid "Custom"
msgstr "སྲོལ་སྒྲིག"

#: fontwork_toolbar.xhp#par_idN10621.help.text
msgid "<ahelp hid=\".\">Opens the Fontwork Character Spacing dialog where you can enter a new character spacing value.</ahelp>"
msgstr "<ahelp hid=\".\">ཁྱོད་ཀྱིས་ ཡིག་གཟུགས་བར་སྟོང་བཞག་ཐངས་ཀྱི་བེ་ལུ་གསརཔ་ཅིག་བཙུགས་ཚུགས་པའི་ཡིག་གཟུགས་ལཱ་གི་ཡིག་འབྲུ་བར་སྟོང་བཞག་ཐངས་ཀྱི་ཌའི་ལོག་ ཁ་ཕྱེཝ་ཨིན།</ahelp>"

#: fontwork_toolbar.xhp#par_idN10638.help.text
msgid "Value"
msgstr "བེ་ལུ།"

#: fontwork_toolbar.xhp#par_idN1063C.help.text
msgid "<ahelp hid=\".\">Enter the Fontwork character spacing value.</ahelp>"
msgstr "<ahelp hid=\".\">ཡིག་གཟུགས་ལཱ་གི་ཡིག་འབྲུ་བར་སྟོང་བཞག་ཐངས་ཀྱི་བེ་ལུ་བཙུགས།</ahelp>"

#: fontwork_toolbar.xhp#par_idN1064B.help.text
msgid "Kern Character Pairs"
msgstr "ཀརནི་ཡིག་འབྲུའི་ཟུང་།"

#: fontwork_toolbar.xhp#par_idN1064F.help.text
msgid "<ahelp hid=\".\">Switches the <link href=\"text/shared/00/00000005.xhp#kerning\"> kerning</link> of character pairs on and off.</ahelp>"
msgstr "<ahelp hid=\".\">ཡིག་འབྲུའི་ཟུང་ <link href=\"text/shared/00/00000005.xhp#kerning\">ཀར་ནིང་</link>ཚུ་ཨཱོན་དང་ཨོཕ་སྦེ་སོར་བསྒྱུར་འབདཝ་ཨིན།</ahelp>"

#: main0214.xhp#tit.help.text
msgid "Query Design Bar"
msgstr "བཀོད་སྒྲིག་ཕྲ་རིང་ འདྲི་དཔྱད་འབད།"

#: main0214.xhp#hd_id3159176.1.help.text
msgid "<link href=\"text/shared/main0214.xhp\" name=\"Query Design Bar\">Query Design Bar</link>"
msgstr "<link href=\"text/shared/main0214.xhp\" name=\"Query Design Bar\">འདྲི་དཔྱད་བཀོད་སྒྲིག་ཕྲ་རིང་</link>"

#: main0214.xhp#par_id3150085.2.help.text
msgid "<ahelp hid=\".\">When creating or editing an SQL query, use the icons in the <emph>Query Design</emph> Bar to control the display of data.</ahelp>"
msgstr "<ahelp hid=\".\"> ཨེསི་ཀིའུ་ཨེལ་འདྲི་དཔྱད་ཅིག་ཞུན་དག་འབདཝ་ད་  ཡང་ན་  གསར་བསྐྲུན་འབདཝ་ད་  གནས་སྡུད་ཀྱི་བཀྲམ་སྟོན་ཚད་འཛིན་འབད་ནི་ལུ་<emph>འདྲི་དཔྱད་བཀོད་སྒྲིག་</emph>ཕྲ་རིང་ནང་གི་  ངོས་དཔར་ཚུ་ལག་ལེན་འཐབ།</ahelp>"

#: main0214.xhp#par_id3150276.5.help.text
msgid "Depending on whether you have created the query or view in the <emph>Design</emph> or <emph>SQL</emph> tab page, the following icons appear:"
msgstr "འོག་གི་ངོས་པར་ཚུ་འབྱུང་ཚུལ་ཡང་ ཁྱོད་ཀྱིས་ <emph>Design</emph> ཡང་ན་ <emph>SQL</emph> མཆོང་ལྡེ་ཤོག་ལེབ་ནང་ འདྲི་དཔྱད་ ཡངན་ མཐོང་སྣང་གསར་བསྐྲུན་འབད་ཡོདཔ་དང་མེདཔ་ལུ་རག་ལསཔ་ཨིན།"

#: main0214.xhp#hd_id3151384.3.help.text
