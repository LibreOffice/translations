msgctxt ""
"03010301.xhp\n"
"par_id3154012\n"
"help.text"
msgid "MsgBox \"The color \" & lVar & \" consists of:\" & Chr(13) &_"
msgstr "MsgBox \"The color \" & lVar & \" consists of:\"& Chr(13) &_"

