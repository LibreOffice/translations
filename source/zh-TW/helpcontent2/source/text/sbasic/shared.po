msgstr "MsgBox cDate(1000.25) REM 09.26.1902 06:00:00"

#: 03100300.xhp#par_id3155133.13.help.text
msgid "MsgBox cDate(1001.26) REM 09.27.1902 06:14:24"
msgstr "MsgBox cDate(1001.26) REM 09.27.1902 06:14:24"

