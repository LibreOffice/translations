msgid "If Dir(sSubDir1,16)=\"\" then ' Does the directory exist ?"
msgstr "If Dir(sSubDir1,16) =\"\" then ' 目錄是否存在？"

#: 03020411.xhp#par_id3156423.20.help.text
msgid "MkDir sSubDir1"
msgstr "MkDir sSubDir1"

