msgctxt ""
"01170101.xhp\n"
"par_id3166426\n"
"138\n"
"help.text"
msgid "<ahelp hid=\".\">Specify the number of decimal places displayed for a numerical or currency control.</ahelp>"
msgstr "<ahelp hid=\".\">指定數字或貨幣控制項所要顯示的小數位數。</ahelp>"

#: 01170101.xhp
msgctxt ""
"01170101.xhp\n"
"hd_id3159091\n"
"144\n"
"help.text"
msgid "Default button"
msgstr "標準按鈕"

