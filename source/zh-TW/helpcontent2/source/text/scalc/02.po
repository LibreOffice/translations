msgid "Number format: Default"
msgstr "數字格式：預設值"

#: 02150000.xhp#hd_id3149182.1.help.text
msgid "<link href=\"text/scalc/02/02150000.xhp\" name=\"Number format: Default\">Number format: Default</link>"
msgstr "<link href=\"text/scalc/02/02150000.xhp\" name=\"數字格式： 標準\">數字格式： 標準</link>"

#: 02150000.xhp#par_id3163802.2.help.text
msgid "<ahelp hid=\".uno:NumberFormatStandard\" visibility=\"visible\">Applies the default number format to the selected cells.</ahelp>"
msgstr "<ahelp hid=\".uno:NumberFormatStandard\" visibility=\"visible\">將預設數字格式套用至選取的儲存格。</ahelp>"

