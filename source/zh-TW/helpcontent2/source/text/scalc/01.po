msgctxt ""
"04060110.xhp\n"
"par_id3154671\n"
"help.text"
msgid "SEARCH(\"FindText\"; \"Text\" [; Position])"
msgstr ""

#. yP57Q
#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id3146080\n"
