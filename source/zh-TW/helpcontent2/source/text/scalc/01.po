msgstr "<bookmark_value>SQRTPI 函式</bookmark_value><bookmark_value>平方根; Pi 乘積</bookmark_value>"

#. 5w2mG
#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"hd_id3164560\n"
"help.text"
msgid "SQRTPI"
msgstr "SQRTPI"

#. yYVKB
#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3164596\n"
"help.text"
