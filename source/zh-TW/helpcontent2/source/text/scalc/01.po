msgstr "<bookmark_value>LOWER 函式</bookmark_value>"

#. DtaeX
#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"hd_id3153983\n"
"help.text"
msgid "LOWER"
msgstr "LOWER"

#. 4fGYG
