msgctxt ""
"webwizard07fc.xhp\n"
"par_idN1055C\n"
"help.text"
msgid "<ahelp hid=\"41041\">Enter the name or IP address of the FTP server.</ahelp>"
msgstr "<ahelp hid=\"41041\">輸入 FTP 伺服器的名稱或 IP 位址。</ahelp>"

