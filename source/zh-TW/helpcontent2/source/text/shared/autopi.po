msgstr "正式私人信函"

#: 01010100.xhp
msgctxt ""
"01010100.xhp\n"
"par_idN1061D\n"
"help.text"
msgid "<ahelp hid=\"HID_LTRWIZ_OPTPRIVOFFICIALLETTER\">Specifies that you want to create a formal personal letter.</ahelp>"
msgstr "<ahelp hid=\"HID_LTRWIZ_OPTPRIVOFFICIALLETTER\">指定要建立正式私人信函。</ahelp>"

#: 01010100.xhp
msgctxt ""
"01010100.xhp\n"
"hd_id3147275\n"
