#: webwizard07fc.xhp
msgctxt ""
"webwizard07fc.xhp\n"
"par_idN10586\n"
"help.text"
msgid "Choose a remote directory"
msgstr "選擇遠端目錄"

#: webwizard07fc.xhp
msgctxt ""
"webwizard07fc.xhp\n"
"par_idN1058A\n"
"help.text"
