msgctxt ""
"webwizard03.xhp\n"
"par_idN10557\n"
"help.text"
msgid "Layouts"
msgstr "版面配置"

#: webwizard03.xhp
msgctxt ""
"webwizard03.xhp\n"
"par_idN1055B\n"
"help.text"
msgid "<ahelp hid=\"34220\">Select the layout for the index page.</ahelp>"
msgstr "<ahelp hid=\"34220\">選取索引頁面的版面配置。</ahelp>"

#: webwizard03.xhp
msgctxt ""
