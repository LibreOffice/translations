msgid "<link href=\"text/shared/explorer/database/dabawiz03auth.xhp\">Authentication</link>"
msgstr "<link href=\"text/shared/explorer/database/dabawiz03auth.xhp\">認證</link>"

#: dabawiz02adabas.xhp#par_idN10560.help.text
