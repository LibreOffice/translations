"05040300.xhp\n"
"hd_id3150290\n"
"13\n"
"help.text"
msgid "Height"
msgstr "高度"

#: 05040300.xhp
msgctxt ""
"05040300.xhp\n"
"par_id3155429\n"
"14\n"
"help.text"
msgid "<ahelp hid=\"svx/ui/headfootformatpage/spinHeight\">Enter the height that you want for the header.</ahelp>"
msgstr "<ahelp hid=\"svx/ui/headfootformatpage/spinHeight\">輸入您需要的頁首高度。</ahelp>"

#: 05040300.xhp
