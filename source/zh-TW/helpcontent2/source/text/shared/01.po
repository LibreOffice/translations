"02110000.xhp\n"
"par_id3156435\n"
"31\n"
"help.text"
msgid "<ahelp hid=\"HID_GLBLTREE_UPD_IDX\">Updates all indexes.</ahelp>"
msgstr "<ahelp hid=\"HID_GLBLTREE_UPD_IDX\">更新所有索引。</ahelp>"

