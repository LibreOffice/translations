"mm_seladdblo.xhp\n"
"par_idN10651\n"
"help.text"
msgid "<ahelp hid=\".\">Enter the country/region string that shall not be printed.</ahelp>"
msgstr "<ahelp hid=\".\">Enigu la landan/regionan ĉenon kiu ne presiĝu.</ahelp>"

