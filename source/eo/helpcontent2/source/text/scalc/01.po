msgstr ""

#: func_networkdays.xhp#par_id3147328.249.help.text
msgid "=NETWORKDAYS(C3;D3;F3:J3) returns 17 workdays."
msgstr "=NETLABORTAG(C3;D3;F3:J3) liveras 17 labortagojn."

