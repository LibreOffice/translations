msgctxt "04060104.xhp#hd_id3147175.39.help.text"
msgid "Syntax"
msgstr "Sintakso"

#: 04060104.xhp#par_id3149821.40.help.text
msgid "ISREF(Value)"
msgstr ""

#: 04060104.xhp#par_id3146152.41.help.text
msgid "<emph>Value</emph> is the value to be tested, to determine whether it is a reference."
msgstr "<emph>Valoro</emph> estas la valoro testota, por eltrovi ĉu ĝi estas referenco."

