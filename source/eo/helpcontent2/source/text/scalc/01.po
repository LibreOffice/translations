msgctxt "04060182.xhp#hd_id3154763.17.help.text"
msgid "Example"
msgstr "Ekzemplo"

#: 04060182.xhp#par_id3149383.18.help.text
msgid "            <item type=\"input\">=FISHER(0.5)</item> yields 0.55."
