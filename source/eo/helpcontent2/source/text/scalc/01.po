msgid "            <item type=\"input\">=SYD($A$2;$B$2;$C$2;D2)</item>         "
msgstr "=DEPRECO.JARA($A$2;$B$2;$C$2;D2)"

#: 04060103.xhp#par_id3156124.83.help.text
