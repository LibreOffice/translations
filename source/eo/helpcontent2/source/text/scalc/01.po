#: 04060106.xhp#hd_id3145991.24.help.text
msgid "ROUNDDOWN"
msgstr "RONDIGI.MALSUPREN"

#: 04060106.xhp#par_id3146020.25.help.text
msgid "<ahelp hid=\"HID_FUNC_ABRUNDEN\">Rounds a number down, toward zero, to a certain precision.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_ABRUNDEN\">Subrondigas numeron al la antaŭa numero, direkte al  nul, laŭ specifita precizo.</ahelp>"

