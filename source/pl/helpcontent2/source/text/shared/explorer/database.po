#: menuview.xhp#par_idN10567.help.text
msgctxt "menuview.xhp#par_idN10567.help.text"
msgid "Forms"
msgstr "Formularze"

#: menuview.xhp#par_idN1056B.help.text
msgid "<ahelp hid=\".\">Selects the forms container and shows all forms in the detail view.</ahelp>"
msgstr "<ahelp hid=\".\">Wybiera kontener formularzy i wyświetla wszystkie formularze w widoku szczegółowym.</ahelp>"

#: menuview.xhp#par_idN1056E.help.text
msgid "Reports"
msgstr "Raporty"

#: menuview.xhp#par_idN10572.help.text
msgid "<ahelp hid=\".\">Selects the reports container and shows all reports in the detail view.</ahelp>"
msgstr "<ahelp hid=\".\">Wybiera kontener raportów i wyświetla wszystkie raporty w widoku szczegółowym.</ahelp>"

#: menuview.xhp#par_idN10575.help.text
