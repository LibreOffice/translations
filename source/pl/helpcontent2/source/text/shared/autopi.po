msgctxt ""
"01090100.xhp\n"
"par_id3156194\n"
"19\n"
"help.text"
msgid "<ahelp hid=\"HID_DLGFORM_FIELDSSELECTED\">Displays the fields that are in the new form.</ahelp>"
msgstr "<ahelp hid=\"HID_DLGFORM_FIELDSSELECTED\">Wyświetla pola, które znajdą się w nowym formularzu.</ahelp>"

