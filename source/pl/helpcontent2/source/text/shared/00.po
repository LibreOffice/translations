"par_id3151370\n"
"help.text"
msgid "<variable id=\"objektleiste\">Icon on the Formatting Bar: </variable>"
msgstr "<variable id=\"objektleiste\">Ikona na pasku narzędzi: </variable>"

#: 00000007.xhp
msgctxt ""
