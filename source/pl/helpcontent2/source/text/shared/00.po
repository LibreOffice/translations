"par_id3152352\n"
"help.text"
msgid "<variable id=\"textobjektleiste\">Icon on the Formatting Bar: </variable>"
msgstr "<variable id=\"textobjektleiste\">Ikona na pasku formatowania: </variable>"

#: 00000007.xhp
msgctxt ""
