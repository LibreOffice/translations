msgid "Pop Art"
msgstr "Styl pop"

#: 24010000.xhp
msgctxt ""
"24010000.xhp\n"
"par_id3153512\n"
"help.text"
msgid "<ahelp hid=\".uno:GraphicFilterPopart\">Converts an image to a pop-art format.</ahelp>"
msgstr "<ahelp hid=\".uno:GraphicFilterPopart\">Konwertuje obraz na format w stylu pop art.</ahelp>"

