msgid "Bottom"
msgstr "Od dołu"

#: 05100700.xhp#hd_id3150249.1.help.text
msgid "<link href=\"text/shared/01/05100700.xhp\" name=\"Bottom\">Bottom</link>"
msgstr "<link href=\"text/shared/01/05100700.xhp\" name=\"Do dołu\">Do dołu</link>"

