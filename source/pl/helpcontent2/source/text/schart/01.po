msgctxt "05030000.xhp#hd_id3145800.1.help.text"
msgid "Legend"
msgstr "Legenda"

#: 05030000.xhp#par_id3146972.2.help.text
msgid "<variable id=\"legende\"><ahelp hid=\".uno:Legend\">Defines the border, area and character attributes for a legend.</ahelp></variable>"
msgstr "<variable id=\"legende\"><ahelp hid=\".uno:Legend\">Określa obramowanie, obszar i właściwości znaków legendy.</ahelp></variable>"

