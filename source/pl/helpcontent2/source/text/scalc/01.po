#: 04060104.xhp
msgctxt ""
"04060104.xhp\n"
"par_id3147583\n"
"help.text"
msgid "Returns the number of the referenced row."
msgstr "Zwraca numer wiersza odwołania."

#. puDDA
#: 04060104.xhp
msgctxt ""
"04060104.xhp\n"
"par_id3151222\n"
"help.text"
msgid "<item type=\"input\">=CELL(\"ROW\";D2)</item> returns 2."
msgstr "<item type=\"input\">=KOMÓRKA(\"ROW\";D2)</item> zwraca wartość 2."

