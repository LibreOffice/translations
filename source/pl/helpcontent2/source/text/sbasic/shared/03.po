msgid "Readonly"
msgstr "Tylko do odczytu"

#. TkMLa
#: sf_exception.xhp
msgctxt ""
"sf_exception.xhp\n"
"par_id621584978211403\n"
"help.text"
msgid "Description"
msgstr "Opis"

#. tJZkJ
#: sf_exception.xhp
