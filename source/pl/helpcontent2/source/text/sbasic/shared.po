msgctxt ""
"replace.xhp\n"
"par_id71552552474769\n"
"help.text"
msgid "REM * only first (respecting case) occurrence (count=1)"
msgstr "REM * tylko pierwsze wystąpienie (odnoszące się do wielkości liter) (count=1)"

#. QEPtG
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"par_id861587778446685\n"
"help.text"
msgid "REM returns D*FGHI because the search starts at position 4, which is also the start of the returned string."
msgstr "REM zwraca D*FGHI, ponieważ wyszukiwanie rozpoczyna się od pozycji 4, która jest również początkiem zwróconego łańcucha."

#. nDtDv
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"bas_id491622734884707\n"
"help.text"
msgid "MsgBox Replace(\"aBbcnnbnn\", \"b\", \"$£\", compare:=False)  'returns \"aB$£cnn$£nn\""
msgstr "MsgBox Replace(\"aBbcnnbnn\", \"b\", \"$£\", compare:=False)  'zwraca \"aB$£cnn$£nn\""

#. KMcAt
#: replace.xhp
msgctxt ""
"replace.xhp\n"
"bas_id341622734993202\n"
"help.text"
msgid "REM Replace all (count = -1) \"b\" with \"$£\" respecting casing (compare=False) starting from first letter (start=1)"
msgstr "REM Zamień wszystkie (count = -1) \"b\" na \"$£\" z uwzględnieniem wielkości liter (compare=False) i od pierwszej litery (start=1)"

