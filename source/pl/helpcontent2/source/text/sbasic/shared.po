msgctxt "03100300.xhp#hd_id3148552.5.help.text"
msgid "Return value:"
msgstr "Zwracana wartość:"

#: 03100300.xhp#par_id3159414.6.help.text
msgctxt "03100300.xhp#par_id3159414.6.help.text"
msgid "Date"
msgstr "Data"

