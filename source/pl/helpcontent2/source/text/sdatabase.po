#: dabawiz02pgsql.xhp
msgctxt ""
"dabawiz02pgsql.xhp\n"
"par_id471643044279530\n"
"help.text"
msgid "<literal>dbname=MyDatabase host=myHost port=5432</literal>"
msgstr "<literal>dbname=MojaBazaDanych host=Serwer port=5432</literal>"

