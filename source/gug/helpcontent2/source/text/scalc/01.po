msgctxt ""
"04060103.xhp\n"
"par_id3148840\n"
"help.text"
msgid "<item type=\"input\">=ACCRINT(\"2001-02-28\";\"2001-08-31\";\"2001-05-01\";0.1;1000;2;0)</item> returns 16.94444."
msgstr "<item type=\"input\">=INT.ACUM(\"2001-02-28\";\"2001-08-31\";\"2001-05-01\";0.1;1000;2;0)</item> devuelve 16,94444."

