msgctxt ""
"func_floor.xhp\n"
"par_id311586287600048\n"
"help.text"
msgid "<input>=FLOOR(3.45, 3)</input> returns 3."
msgstr "<input>=MULTIPLO.INFERIOR(3,45; 3)</input> devuelve 3."

