msgctxt ""
"func_ceiling.xhp\n"
"par_id181586214438808\n"
"help.text"
msgid "<input>=ISO.CEILING(-45.67; 2)</input> returns -44."
msgstr "<input>=MULTIPLO.SUPERIOR.ISO(-45.67; 2)</input> devuelve -44."

