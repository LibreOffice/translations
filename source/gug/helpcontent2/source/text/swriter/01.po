msgid "<ahelp hid=\"modules/swriter/ui/inserttable/nameedit\">Enter a name for the table.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/inserttable/nameedit\">Escriba un nombre para la tabla.</ahelp>"

