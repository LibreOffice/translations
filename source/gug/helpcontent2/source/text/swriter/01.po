msgid "<variable id=\"outlinelisth1\"><link href=\"text/swriter/01/05030800.xhp\">Outline & List</link></variable>"
msgstr "<variable id=\"outlinelisth1\"><link href=\"text/swriter/01/05030800.xhp\">Esquema y lista</link></variable>"

#. x3BNB
