"hd_id3154652\n"
"help.text"
msgid "<link href=\"text/swriter/01/05200000.xhp\" name=\"Merge Table\">Merge Table</link>"
msgstr "<link href=\"text/swriter/01/05200000.xhp\" name=\"Merge Table\">Combinar tablas</link>"

#. 7qBjV
