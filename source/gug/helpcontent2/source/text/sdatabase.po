"par_id3154860\n"
"help.text"
msgid "<ahelp hid=\"dbaccess/ui/indexdesigndialog/ID_INDEX_DROP\">Deletes the current index.</ahelp>"
msgstr "<ahelp hid=\"dbaccess/ui/indexdesigndialog/ID_INDEX_DROP\">Elimina el índice actual.</ahelp>"

