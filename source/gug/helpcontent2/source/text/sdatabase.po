msgctxt ""
"migrate_macros.xhp\n"
"par_id0112200902353554\n"
"help.text"
msgid "<link href=\"https://wiki.documentfoundation.org/Macros_in_Database_Documents\">An in depth explanation by the developers (Wiki).</link>"
msgstr ""

