#: euro.src#STEP_ZERO.string.text
msgid "~Cancel"
msgstr "Ghairi"

#: euro.src#STEP_ZERO___1.string.text
msgid "~Help"
msgstr "Msaada"

#: euro.src#STEP_ZERO___2.string.text
msgid "<<~Back"
msgstr "<<Nyuma"

#: euro.src#STEP_ZERO___3.string.text
msgid "~Convert"
msgstr "Geuza"

#: euro.src#STEP_ZERO___4.string.text
msgid "Note: Currency amounts from external links and currency conversion factors in formulas cannot be converted."
msgstr "Zingatia: Kiasi cha hela kutoka viungo vya nje na kigawe cha ubadilishaji wa hela kwenye fomula hakiwezi kugeuzwa."

#: euro.src#STEP_ZERO___5.string.text
msgid "First, unprotect all sheets."
msgstr "Kwanza, ondoa ukingaji katika laha zote."

#: euro.src#STEP_ZERO___6.string.text
msgid "Currencies:"
msgstr "Hela:"

#: euro.src#STEP_ZERO___7.string.text
msgid "C~ontinue>>"
msgstr "Endelea>>"

#: euro.src#STEP_ZERO___8.string.text
msgid "C~lose"
msgstr "Funga"

#: euro.src#STEP_CONVERTER.string.text
msgid "~Entire document"
msgstr "Andiko zima"

#: euro.src#STEP_CONVERTER___1.string.text
msgid "Selection"
msgstr "Uteuzi"

#: euro.src#STEP_CONVERTER___2.string.text
msgid "Cell S~tyles"
msgstr "Mitindo ya Seli"

#: euro.src#STEP_CONVERTER___3.string.text
msgid "Currency cells in the current ~sheet"
msgstr "Seli za hela kwenye laha ya sasa"

#: euro.src#STEP_CONVERTER___4.string.text
msgid "Currency cells in the entire ~document"
msgstr "Seli za hela kwenye andiko zima"

#: euro.src#STEP_CONVERTER___5.string.text
msgid "~Selected range"
msgstr "Masafa yaliyoteuliwa"

#: euro.src#STEP_CONVERTER___6.string.text
msgid "Select Cell Styles"
msgstr "Teua Mitindo ya Seli"

#: euro.src#STEP_CONVERTER___7.string.text
msgid "Select currency cells"
msgstr "Teua seli za hela"

#: euro.src#STEP_CONVERTER___8.string.text
msgid "Currency ranges:"
msgstr "Masafa ya hela:"

#: euro.src#STEP_CONVERTER___9.string.text
msgid "Templates:"
msgstr "Templeti:"

#: euro.src#STEP_AUTOPILOT.string.text
msgid "Extent"
msgstr "Kadiri"

#: euro.src#STEP_AUTOPILOT___1.string.text
msgid "~Single %PRODUCTNAME Calc document"
msgstr "Andiko moja %PRODUCTNAME la Kikokotoa"

#: euro.src#STEP_AUTOPILOT___2.string.text
msgid "Complete ~directory"
msgstr "Saraka nzima"

#: euro.src#STEP_AUTOPILOT___3.string.text
msgid "Source Document:"
msgstr "Andiko Chanzo:"

#: euro.src#STEP_AUTOPILOT___4.string.text
msgid "Source directory:"
msgstr "Saraka chanzo:"

#: euro.src#STEP_AUTOPILOT___5.string.text
msgid "~Including subfolders"
msgstr "Inajumuisha mafolda ya ndani"

#: euro.src#STEP_AUTOPILOT___6.string.text
msgid "Target directory:"
msgstr "Saraka lengwa:"

#: euro.src#STEP_AUTOPILOT___7.string.text
msgid "Temporarily unprotect sheet without query"
msgstr "Ondoa kinga ya laha kwa muda bila kuuliza"

#: euro.src#STEP_AUTOPILOT___10.string.text
msgid "Also convert fields and tables in text documents"
msgstr "Geuza pia nyuga na majedwali katika maandiko matini"

#: euro.src#STATUSLINE.string.text
msgid "Conversion status: "
msgstr "Hali ya ugeuzaji: "

#: euro.src#STATUSLINE___1.string.text
msgid "Conversion status of the cell templates:"
msgstr "Ugeuzaji wa hali ya seli za templeti:"

#: euro.src#STATUSLINE___2.string.text
msgid "Registration of the relevant ranges: Sheet %1Number%1 of %2TotPageCount%2"
msgstr "Usajili wa masafa husika: Laha %1Number%1 za %2TotPageCount%2"

#: euro.src#STATUSLINE___3.string.text
msgid "Entry of the ranges to be converted..."
msgstr "Ingizo la masafa ya kugeuzwa..."

#: euro.src#STATUSLINE___4.string.text
msgid "Sheet protection for each sheet will be restored..."
msgstr "Ukingaji wa laha utarejeshwa kwa kila laha..."

#: euro.src#STATUSLINE___5.string.text
msgid "Conversion of the currency units in the cell templates..."
msgstr "Ubadilishaji wa viwango vya hela katika seli za templeti..."

#: euro.src#MESSAGES.string.text
msgid "~Finish"
msgstr "~Finish"

#: euro.src#MESSAGES___1.string.text
msgid "Select directory"
msgstr "Teua saraka"

#: euro.src#MESSAGES___2.string.text
msgid "Select file"
msgstr "Teua faili"

#: euro.src#MESSAGES___3.string.text
msgid "Select target directory"
msgstr "Teua saraka lengwa"

#: euro.src#MESSAGES___4.string.text
msgid "non-existant"
msgstr "isiokuwepo"

#: euro.src#MESSAGES___5.string.text
msgid "Euro Converter"
msgstr "Euro Converter"

#: euro.src#MESSAGES___6.string.text
msgid "Should protected spreadsheets be temporarily unprotected?"
msgstr "Je lahajedwali zilizokingwa zisikingwe kwa muda?"

#: euro.src#MESSAGES___7.string.text
msgid "Enter the password to unprotect the table %1TableName%1"
msgstr "Ingiza nywila kuondoa kinga ya jedwali %1TableName%1"

#: euro.src#MESSAGES___8.string.text
msgid "Wrong Password!"
msgstr "Nywila Isiyosahihi!"

#: euro.src#MESSAGES___9.string.text
msgid "Protected Sheet"
msgstr "Laha Iliyokingwa"

#: euro.src#MESSAGES___10.string.text
msgid "Warning!"
msgstr "Tahadhari!"

#: euro.src#MESSAGES___11.string.text
msgid "Protection for the sheets will not be removed."
msgstr "Ukingaji wa laha hauwezi kuondolewa."

#: euro.src#MESSAGES___12.string.text
msgid "Sheet cannot be unprotected"
msgstr "Laha haiwezi kukingwa"

#: euro.src#MESSAGES___13.string.text
msgid "The Wizard cannot edit this document as cell formats cannot be modified in documents containing protected spreadsheets."
msgstr "Pailoti Otomati haiwezi kuhariri andiko hili kwavile fomati za seli haziwezi kurekebishwa kwenye maandiko yaliyo na lahajedwali zilizokingwa."

#: euro.src#MESSAGES___14.string.text
msgid "Please note that the Euro Converter will, otherwise, not be able to edit this document!"
msgstr "Tafadhali zingatia kwamba Kigeuzi Euro kinaweza, vinginevyo, kisiweze kuhariri andiko hili!"

#: euro.src#MESSAGES___15.string.text
msgid "Please choose a currency to be converted first!"
msgstr "Tafadhali chagua hela ya kubadilisha kwanza!"

#: euro.src#MESSAGES___16.string.text
msgid "Password:"
msgstr "Nywila:"

#: euro.src#MESSAGES___17.string.text
msgid "OK"
msgstr "SAWA"

#: euro.src#MESSAGES___18.string.text
msgid "Cancel"
msgstr "Ghairi"

#: euro.src#MESSAGES___19.string.text
msgid "Please select a %PRODUCTNAME Calc document for editing!"
msgstr "Tafadhali teua %PRODUCTNAME kikokotoa andiko kwa ajili ya kuhariri!"

#: euro.src#MESSAGES___20.string.text
msgid "'<1>' is not a directory!"
msgstr "'<1>' saraka haipo!"

#: euro.src#MESSAGES___21.string.text
msgid "Document is read-only!"
msgstr "Andiko linasomeka-tu!"

#: euro.src#MESSAGES___22.string.text
msgid "The '<1>' file already exists.<CR>Do you want to overwrite it?"
msgstr "Faili '<1>' tayari lipo.<CR>Je unataka kuliandikiza?"

#: euro.src#MESSAGES___23.string.text
msgid "Do you really want to terminate conversion at this point?"
msgstr "Kweli unataka kukatisha ugeuzaji katika hatua hii?"

#: euro.src#MESSAGES___24.string.text
msgid "Cancel Wizard"
msgstr "Ghairi Pailoti Otomati"

