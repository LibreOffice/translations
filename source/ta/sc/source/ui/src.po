#: scfuncs.src
msgctxt ""
"scfuncs.src\n"
"RID_SC_FUNCTION_DESCRIPTIONS1.SC_OPCODE_SUM_SQ\n"
"2\n"
"string.text"
msgid "number "
msgstr "எண் "

