msgctxt ""
"wikisettings.xhp\n"
"par_id7285073\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Removes the selected entry from the list.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">წაშლის მონიშნულ ჩანაწერს სიიდან.</ahelp>"

