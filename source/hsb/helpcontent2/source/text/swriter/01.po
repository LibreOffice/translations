#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"par_id3153351\n"
"help.text"
msgid "Displays the name of the selected macro."
msgstr "Pokazuje mjeno wubraneho makra."

#. D7DBC
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"hd_id3156269\n"
"help.text"
msgid "Placeholder"
msgstr "Zastupowacy tekst"

#. G2o86
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"par_id3156282\n"
"help.text"
msgid "Type the text that you want to appear in the placeholder field."
msgstr "Zapodajće tekst, kotryž so ma w zastupowacym polu zjewić."

#. FCAcm
