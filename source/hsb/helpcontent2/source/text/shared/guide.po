#. uNUCA
#: language_select.xhp
msgctxt ""
"language_select.xhp\n"
"par_id3154510\n"
"help.text"
msgid "Select the <emph>Language</emph> and click <emph>OK</emph>."
msgstr "Wubjerće <emph>Rěč</emph> a klikńće na <emph>W porjadku</emph>."

#. be3Dj
#: language_select.xhp
msgctxt ""
"language_select.xhp\n"
"par_id3154164\n"
"help.text"
msgid "In <item type=\"productname\">%PRODUCTNAME</item> Calc, choose <emph>Format - Cells</emph> and proceed accordingly."
msgstr "Wubjerće w <item type=\"productname\">%PRODUCTNAME</item> Calc <emph>Format – Cele…</emph> a postupujće wotpowědnje."

#. fKJ3A
#: language_select.xhp
msgctxt ""
"language_select.xhp\n"
"hd_id3154272\n"
"help.text"
msgid "Selecting a language for a Character Style"
msgstr "Rěč za znamješkowu předłohu wubrać"

#. mMKRc
#: language_select.xhp
msgctxt ""
"language_select.xhp\n"
"par_id3145649\n"
"help.text"
