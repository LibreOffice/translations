#: sc/inc/scfuncs.hrc:3562
msgctxt "SC_OPCODE_CLEAN"
msgid "Text"
msgstr "Besedilo"

#: sc/inc/scfuncs.hrc:3563
msgctxt "SC_OPCODE_CLEAN"
