msgstr "Tvori hiperpovezavo."

#: sc/inc/scfuncs.hrc:3490
msgctxt "SC_OPCODE_HYPERLINK"
msgid "URL"
msgstr "URL"

#: sc/inc/scfuncs.hrc:3491
msgctxt "SC_OPCODE_HYPERLINK"
msgid "The clickable URL."
msgstr "URL, katerega je mogoče klikniti."

