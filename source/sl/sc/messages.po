msgstr "Besedilo"

#: sc/inc/scfuncs.hrc:3529
msgctxt "SC_OPCODE_ASC"
msgid "The text to convert."
msgstr "Besedilo za pretvorbo."

#: sc/inc/scfuncs.hrc:3535
msgctxt "SC_OPCODE_CODE"
