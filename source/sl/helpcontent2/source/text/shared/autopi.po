msgctxt ""
"01090100.xhp\n"
"par_idN1074E\n"
"help.text"
msgid "<ahelp hid=\".\">Click to move the selected field up one entry in the list.</ahelp>"
msgstr "<ahelp hid=\".\">Kliknite, če želite pomakniti izbrano polje na seznamu navzgor.</ahelp>"

#: 01090100.xhp
msgctxt ""
"01090100.xhp\n"
"par_idN10751\n"
"help.text"
msgid "v"
msgstr "v"

