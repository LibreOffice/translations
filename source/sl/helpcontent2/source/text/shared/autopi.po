msgctxt ""
"01010300.xhp\n"
"par_idN10630\n"
"help.text"
msgid "Letter signs"
msgstr "Pisemske oznake"

#: 01010300.xhp
msgctxt ""
"01010300.xhp\n"
"par_idN10634\n"
"help.text"
msgid "<ahelp hid=\".\">Includes a line with references to a business letter on the letter template.</ahelp>"
msgstr "<ahelp hid=\".\">V predlogo za poslovno pismo vključi vrstico s sklici.</ahelp>"

#: 01010300.xhp
msgctxt ""
"01010300.xhp\n"
"par_idN1064B\n"
"help.text"
msgid "Subject line"
msgstr "Predmetna vrstica"

#: 01010300.xhp
msgctxt ""
"01010300.xhp\n"
"par_idN1064F\n"
