msgid "No"
msgstr "Ne"

#: 02010100.xhp
msgctxt ""
"02010100.xhp\n"
"par_id3150997\n"
"help.text"
msgid "."
msgstr "."

#: 02010100.xhp
msgctxt ""
"02010100.xhp\n"
"par_id3155331\n"
"help.text"
msgid "=0 returns all records for which the Yes/No field has the status \"No\" or \"Off\" (no selection)."
msgstr "=0 vrne vse zapise, za katere ima polje Da/Ne stanje »Da« ali »Ne« (ni izbora)."

#: 02010100.xhp
msgctxt ""
"02010100.xhp\n"
"par_id3154179\n"
"help.text"
msgid "Null"
msgstr "Null"

#: 02010100.xhp
msgctxt ""
"02010100.xhp\n"
"par_id3147035\n"
"help.text"
msgid "IS NULL"
