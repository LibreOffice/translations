"05100700.xhp\n"
"par_id3154764\n"
"help.text"
msgid "<ahelp hid=\".uno:CellVertBottom\">Aligns the contents of the cell to the bottom edge of the cell.</ahelp>"
msgstr "<ahelp hid=\".uno:CellVertBottom\">Poravna vsebino celice na spodnji rob celice.</ahelp>"

#: 05100700.xhp
