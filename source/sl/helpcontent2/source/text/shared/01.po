msgctxt ""
"05110400.xhp\n"
"bm_id3152942\n"
"help.text"
msgid "<bookmark_value>strikethrough;characters</bookmark_value>"
msgstr "<bookmark_value>prečrtano;znaki</bookmark_value>"

#: 05110400.xhp
