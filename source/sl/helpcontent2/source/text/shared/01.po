"par_id3150976\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/linetabpage/MF_SYMBOL_WIDTH\">Enter a width for the symbol.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/linetabpage/MF_SYMBOL_WIDTH\">Vnesite širino simbola. </ahelp>"

#: 05200100.xhp
