"par_id3148520\n"
"help.text"
msgid "<link href=\"text/shared/01/05290300.xhp\">Enter Group</link>"
msgstr "<link href=\"text/shared/01/05290300.xhp\">Vstopi v skupino</link>"

#: 05320000.xhp
msgctxt ""
