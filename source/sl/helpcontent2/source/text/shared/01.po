msgctxt ""
"02230401.xhp\n"
"par_id3155421\n"
"help.text"
msgid "green"
msgstr "zelena"

#: 02230401.xhp
msgctxt ""
"02230401.xhp\n"
"par_id3149237\n"
"help.text"
