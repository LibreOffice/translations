"par_id3154908\n"
"help.text"
msgid "Keywords"
msgstr "Ključne besede"

#: about_meta_tags.xhp
msgctxt ""
"about_meta_tags.xhp\n"
"par_id3156422\n"
"help.text"
msgid "<META NAME=\"KEYWORDS\" CONTENT=\"Field Content\">"
msgstr "<META NAME=\"KEYWORDS\" CONTENT=\"Vsebina polja\">"

