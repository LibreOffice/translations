"hd_id3148756\n"
"help.text"
msgid "Indexes"
msgstr "Kazala"

#: 02110000.xhp
msgctxt ""
"02110000.xhp\n"
"par_id3156435\n"
"help.text"
msgid "<ahelp hid=\"HID_GLBLTREE_UPD_IDX\">Updates all indexes.</ahelp>"
msgstr "<ahelp hid=\"HID_GLBLTREE_UPD_IDX\">Posodobi vsa kazala.</ahelp>"

