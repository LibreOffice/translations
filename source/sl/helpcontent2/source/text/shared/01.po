msgstr "<ahelp hid=\"cui/ui/textanimtabpage/TSB_PIXEL\">Vrednost prirastka se meri v pikah.</ahelp>"

#: 05320000.xhp
msgctxt ""
"05320000.xhp\n"
"hd_id3149766\n"
"help.text"
msgid "Increment box"
msgstr "Prirastek"

