msgctxt ""
"02110000.xhp\n"
"hd_id3153524\n"
"help.text"
msgid "Links"
msgstr "Povezave"

#: 02110000.xhp
msgctxt ""
"02110000.xhp\n"
"par_id3154224\n"
"help.text"
msgid "<ahelp hid=\"HID_GLBLTREE_UPD_LINK\">Updates all links.</ahelp>"
msgstr "<ahelp hid=\"HID_GLBLTREE_UPD_LINK\">Posodobi vse povezave.</ahelp>"

#: 02110000.xhp
msgctxt ""
"02110000.xhp\n"
"hd_id3154938\n"
"help.text"
msgid "All"
msgstr "Vse"

#: 02110000.xhp
msgctxt ""
"02110000.xhp\n"
"par_id3154154\n"
"help.text"
msgid "<ahelp hid=\"HID_GLBLTREEUPD_ALL\">Updates all contents.</ahelp>"
msgstr "<ahelp hid=\"HID_GLBLTREEUPD_ALL\">Posodobi vso vsebino.</ahelp>"

