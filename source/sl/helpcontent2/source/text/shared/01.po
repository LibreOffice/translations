msgctxt ""
"02220000.xhp\n"
"par_id3154073\n"
"help.text"
msgid "<ahelp hid=\"svx/ui/imapdialog/TBI_SELECT\">Selects a hotspot in the image map for editing.</ahelp>"
msgstr "<ahelp hid=\"svx/ui/imapdialog/TBI_SELECT\">Na sliki s povezavami izbere točko povezave za urejanje.</ahelp>"

