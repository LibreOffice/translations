msgctxt ""
"02110000.xhp\n"
"par_id3149164\n"
"help.text"
msgid "<ahelp hid=\"HID_GLBLTREE_UPDATE\">Click and choose the contents that you want to update.</ahelp>"
msgstr "<ahelp hid=\"HID_GLBLTREE_UPDATE\">Kliknite in izberite vsebino, ki jo želite posodobiti.</ahelp>"

