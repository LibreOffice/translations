#: dabawiz00.xhp
msgctxt ""
"dabawiz00.xhp\n"
"par_idN1063D\n"
"help.text"
msgid "<link href=\"text/sdatabase/dabawiz02jdbc.xhp\">Set up JDBC connection</link>"
msgstr "<link href=\"text/sdatabase/dabawiz02jdbc.xhp\">Ustvari povezavo JDBC</link>"

