msgctxt ""
"main0800.xhp\n"
"tit\n"
"help.text"
msgid "$[officename] and the Internet"
msgstr "$[officename] in internet"

#: main0800.xhp
msgctxt ""
"main0800.xhp\n"
"hd_id3153089\n"
