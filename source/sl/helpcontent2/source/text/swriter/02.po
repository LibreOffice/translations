msgstr "<ahelp hid=\"modules/swriter/ui/inputwinmenu/and\">Preveri izraz z vrednostmi, ki se ujemajo z logičnim IN.</ahelp>"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3153814\n"
"help.text"
msgid "Example: 1 AND 2 displays 1 (true)"
msgstr "Primer: 1 AND 2 prikaže 1 (velja oz. true)"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3153938\n"
"help.text"
msgid "Boolean Not"
msgstr "Logični ne"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3148633\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/inputwinmenu/not\">Tests for values matching the Boolean NOT.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/inputwinmenu/not\">Preveri izraz z vrednostmi, ki se ujemajo z logičnim NE.</ahelp>"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3148655\n"
"help.text"
msgid "Example: NOT 1 (true) displays 0 (false)"
msgstr "Primer: NOT 1 (velja oz. true) prikaže 0 (ne velja oz. false)"

