msgstr "<image id=\"img_id3156399\" src=\"media/helpimg/starmath/un21201.svg\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id3156399\">Ikona za plus</alt></image>"

#: 03090100.xhp
msgctxt ""
