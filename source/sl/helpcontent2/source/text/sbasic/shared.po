msgstr "Tiskanje polj obrazcev pričeto"

#: 01040000.xhp
msgctxt ""
"01040000.xhp\n"
"par_id331600160656617\n"
"help.text"
msgid "...before printing form fields."
msgstr "... pred tiskanjem polj obrazcev."

