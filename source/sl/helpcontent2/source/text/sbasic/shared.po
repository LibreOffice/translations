"par_id561548420579525\n"
"help.text"
msgid "print  \"100:  the number 120 occurs in the range: \" & retStr"
msgstr "print \"100:  Število 120 se pojavi v obsegu: \" & retStr"

#: partition.xhp
msgctxt ""
"partition.xhp\n"
"par_id921548420596118\n"
"help.text"
msgid "print \"   : -1 the number -5 occurs in the range: \" & retStr"
msgstr "print \"   : -1 Število -5 se pojavi v obsegu: \" & retStr"

#: partition.xhp
