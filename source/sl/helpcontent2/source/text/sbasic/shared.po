msgctxt ""
"03120308.xhp\n"
"par_id3150669\n"
"help.text"
msgid "RSet Text As String = Text or RSet Variable1 = Variable2"
msgstr "RSet Besedilo As String = Besedilo or RSet Spremenljivka1 = Spremenljivka2"

