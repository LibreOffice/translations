"help.text"
msgid "<bookmark_value>End statement</bookmark_value>"
msgstr "<bookmark_value>ukaz End</bookmark_value>"

#: 03090404.xhp
msgctxt ""
"03090404.xhp\n"
"hd_id3150771\n"
"help.text"
msgid "<link href=\"text/sbasic/shared/03090404.xhp\" name=\"End Statement\">End Statement</link>"
msgstr "<link href=\"text/sbasic/shared/03090404.xhp\" name=\"Ukaz End\">Ukaz End</link>"

