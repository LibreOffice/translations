msgctxt ""
"03090202.xhp\n"
"par_id3149482\n"
"help.text"
msgid "statements"
msgstr "ukazi"

#: 03090202.xhp
msgctxt ""
"03090202.xhp\n"
"par_id3147124\n"
"help.text"
msgid "If condition = True Then Exit For"
msgstr "If pogoj = True Then Exit For"

#: 03090202.xhp
msgctxt ""
"03090202.xhp\n"
"par_id3153159\n"
"help.text"
msgid "statements"
msgstr "ukazi"

