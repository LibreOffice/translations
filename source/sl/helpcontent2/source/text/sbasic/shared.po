"help.text"
msgid "msgbox(\"Long date format : \" & FormatDateTime(d,vbLongDate))"
msgstr "msgbox(\"Dolga oblika datuma: \" & FormatDateTime(d,vbLongDate))"

#: 03150000.xhp
msgctxt ""
