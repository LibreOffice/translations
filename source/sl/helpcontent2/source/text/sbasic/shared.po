msgctxt ""
"03090101.xhp\n"
"par_id3154490\n"
"help.text"
msgid "sDate = InputBox(\"Enter the expiration date (MM.DD.YYYY)\")"
msgstr "sDatum = InputBox(\"Vnesite rok veljavnosti (DD.MM.LLLL)\")"

