msgctxt ""
"01040000.xhp\n"
"par_id951599840240048\n"
"help.text"
msgid "Document could not be saved."
msgstr "Dokumenta ni bilo mogoče shraniti."

#: 01040000.xhp
msgctxt ""
"01040000.xhp\n"
"par_id851599839190548\n"
"help.text"
msgid "-no UI-"
msgstr "- brez uporabniškega vmesnika -"

