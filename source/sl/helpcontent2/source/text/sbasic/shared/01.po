"06130000.xhp\n"
"par_id3153705\n"
"help.text"
msgid "<ahelp hid=\"modules/BasicIDE/ui/modulepage/newdialog\">Lets you manage the macro libraries.</ahelp>"
msgstr "<ahelp hid=\"modules/BasicIDE/ui/modulepage/newdialog\">Omogoča upravljanje knjižnice makrov.</ahelp>"

