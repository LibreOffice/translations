"par_id3151191\n"
"help.text"
msgid "<ahelp hid=\"SID_INSERT_TIMEFIELD\">Adds a time field.</ahelp>"
msgstr "<ahelp hid=\"SID_INSERT_TIMEFIELD\">Doda časovno polje.</ahelp>"

#: 20000000.xhp
msgctxt ""
"20000000.xhp\n"
"hd_id3154733\n"
