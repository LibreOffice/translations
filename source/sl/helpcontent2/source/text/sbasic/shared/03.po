"sf_string.xhp\n"
"par_id691584978211774\n"
"help.text"
msgid "Carriage return + Linefeed: Chr(13) & Chr(10)"
msgstr "Prehod na začetek vrstice (CF) + pomik v novo vrstico (LF): Chr(13) & Chr(10)"

