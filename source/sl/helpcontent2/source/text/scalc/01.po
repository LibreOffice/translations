msgctxt ""
"func_ifs.xhp\n"
"par_id1001556235718948\n"
"help.text"
msgid "THEN result1"
msgstr "THEN rezultat1 (potem rezultat1)"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id571556235725969\n"
"help.text"
msgid "ELSE IF expression2 is TRUE"
