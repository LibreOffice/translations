msgctxt ""
"04060115.xhp\n"
"par_id3153960\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_BESSELI\">Calculates the modified Bessel function of the first kind In(x).</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_BESSELI\">Izračuna prilagojeno Besselovo funkcijo.</ahelp>"

#: 04060115.xhp
msgctxt ""
"04060115.xhp\n"
"par_id3147295\n"
"help.text"
msgid "BESSELI(X; N)"
msgstr "BESSELI(X; N)"

