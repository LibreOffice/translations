msgctxt ""
"04060116.xhp\n"
"par_id3143222\n"
"help.text"
msgid "<item type=\"input\">=IMABS(\"5+12j\")</item> returns 13."
msgstr "<item type=\"input\">=IMABS(\"5+12j\")</item> vrne 13."

