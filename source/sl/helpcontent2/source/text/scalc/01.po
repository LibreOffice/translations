msgid "<item type=\"input\">=IMDIV(\"-238+240i\";\"10+24i\")</item> returns 5+12i."
msgstr "<item type=\"input\">=IMDIV(\"-238+240i\";\"10+24i\")</item> vrne 5+12i."

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"bm_id3153039\n"
"help.text"
