"par_id3150521\n"
"help.text"
msgid "TDIST(Number; DegreesFreedom; Mode)"
msgstr "TDIST(število; stopnje_svobode; način)"

#: 04060185.xhp
msgctxt ""
"04060185.xhp\n"
"par_id3146991\n"
"help.text"
