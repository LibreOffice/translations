#: sf_database.xhp
msgctxt ""
"sf_database.xhp\n"
"par_id631701441607725\n"
"help.text"
msgid "Constant"
msgstr "Constante"

#. 7n7YU
#: sf_database.xhp
msgctxt ""
"sf_database.xhp\n"
