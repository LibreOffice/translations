#: 04090007.xhp
msgctxt ""
"04090007.xhp\n"
"par_id3151257\n"
"help.text"
msgid "Fields"
msgstr "Champs"

#. ZxiaR
#: 04090007.xhp
msgctxt ""
"04090007.xhp\n"
"par_id3148970\n"
"help.text"
msgid "$[officename] Tag"
msgstr "Balise $[officename]"

#. UWG97
#: 04090007.xhp
msgctxt ""
"04090007.xhp\n"
"par_id3147102\n"
"help.text"
msgid "Date is fixed"
msgstr "Date fixe"

