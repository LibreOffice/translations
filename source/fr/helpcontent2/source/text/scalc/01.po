msgstr "<bookmark_value>NB.VIDE, fonction</bookmark_value><bookmark_value>Comptage;cellules vides</bookmark_value><bookmark_value>Cellules vides;comptage</bookmark_value>"

#. 34Lr6
#: 04060181.xhp
msgctxt ""
"04060181.xhp\n"
"hd_id3150896\n"
"help.text"
msgid "<variable id=\"countblank_head\"><link href=\"text/scalc/01/04060181.xhp#countblank\">COUNTBLANK</link></variable>"
msgstr "<variable id=\"countblank_head\"><link href=\"text/scalc/01/04060181.xhp#countblank\">NB.VIDE</link></variable>"

#. HUWNd
#: 04060181.xhp
msgctxt ""
"04060181.xhp\n"
"par_id3155260\n"
"help.text"
