msgctxt ""
"04060106.xhp\n"
"hd_id3144877\n"
"help.text"
msgid "SIN"
msgstr "SIN"

#. PzCz2
#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3144906\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_SIN\">Returns the sine of the given angle (in radians).</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_SIN\">Renvoie le sinus du nombre donné (en radians).</ahelp>"

#. HhouY
