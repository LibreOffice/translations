msgctxt ""
"04030000.xhp\n"
"par_id3150767\n"
"help.text"
msgid "<ahelp hid=\".uno:InsertRowsBefore\">Inserts a new row above the active cell.</ahelp>"
msgstr "<ahelp hid=\".uno:InsertRowsBefore\">Insère une nouvelle ligne au-dessus de la cellule active.</ahelp>"

#. NGsGA
