msgstr "<ahelp hid=\"cui/ui/possizetabpage/TSB_AUTOGROW_WIDTH\">Étend la largeur de l'objet à la largeur du texte, si l'objet est plus petit que le texte.</ahelp>"

#: 05230100.xhp
msgctxt ""
"05230100.xhp\n"
"hd_id3145746\n"
"help.text"
msgid "Fit height to text"
msgstr "Adapter la hauteur au texte"

#: 05230100.xhp
msgctxt ""
"05230100.xhp\n"
"par_id31540680\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/possizetabpage/TSB_AUTOGROW_HEIGHT\">Expands the height of the object to the height of the text, if the object is smaller than the text.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/possizetabpage/TSB_AUTOGROW_HEIGHT\">Étend la hauteur de l'objet à la hauteur du texte, si l'objet est plus petit que le texte.</ahelp>"

