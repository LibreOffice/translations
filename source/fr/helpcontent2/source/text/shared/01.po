msgctxt ""
"05230100.xhp\n"
"par_id3152349\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/possizetabpage/TSB_SIZEPROTECT\">Prevents you from resizing the object.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/possizetabpage/TSB_SIZEPROTECT\">Empêche de modifier la taille de l'objet.</ahelp>"

#: 05230100.xhp
msgctxt ""
"05230100.xhp\n"
"hd_id3153525\n"
"help.text"
