"hd_id3152920\n"
"help.text"
msgid "<switchinline select=\"appl\"><caseinline select=\"CALC\">Position.</caseinline></switchinline>"
msgstr "<switchinline select=\"appl\"><caseinline select=\"CALC\">Position</caseinline></switchinline>"

#: 02230401.xhp
