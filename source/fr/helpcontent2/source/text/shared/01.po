#: 05340400.xhp
msgctxt ""
"05340400.xhp\n"
"par_id3147175\n"
"help.text"
msgid "<ahelp hid=\".\">Go to the last record in the table.</ahelp>"
msgstr "<ahelp hid=\".\">Aller au dernier enregistrement dans la table.</ahelp>"

