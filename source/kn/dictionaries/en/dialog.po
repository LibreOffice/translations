msgctxt ""
"en_en_US.properties\n"
"metric\n"
"property.text"
msgid "Convert to metric (°C, km/h, m, kg, l)"
msgstr "ಮೆಟ್ರಿಕ್‌ಗೆ ಪರಿವರ್ತಿಸಿ (°C, km/h, m, kg, l)"

#: en_en_US.properties
msgctxt ""
"en_en_US.properties\n"
"hlp_numsep\n"
"property.text"
msgid "Common (1000000 → 1,000,000) or ISO (1000000 → 1 000 000)."
