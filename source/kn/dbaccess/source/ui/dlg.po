msgctxt "dbadminsetup.src#PAGE_DBWIZARD_MYSQL_JDBC.STR_MYSQL_DEFAULT.string.text"
msgid "Default: 3306"
msgstr "ಪೂರ್ವನಿಯೋಜಿತ: 3306"

