"help.text"
msgid "GetGenderIndex = Switch(sName = \"Jane\", \"female\", sName = \"John\", \"male\")"
msgstr "GetGenderIndex = Switch(sName = \"Jane\", \"female\", sName = \"John\", \"male\")"

