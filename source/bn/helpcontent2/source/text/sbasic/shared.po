"03020406.xhp\n"
"tit\n"
"help.text"
msgid "FileCopy Statement"
msgstr ""

#: 03020406.xhp
msgctxt ""
"03020406.xhp\n"
"bm_id3154840\n"
"help.text"
msgid "<bookmark_value>FileCopy statement</bookmark_value>"
msgstr "<bookmark_value>FileCopy statement</bookmark_value>"

#: 03020406.xhp
