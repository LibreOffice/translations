msgstr ""

#: 03060600.xhp
msgctxt ""
"03060600.xhp\n"
"par_id3153191\n"
"help.text"
msgid "vOut = vB > vA XOR vB > vC ' returns -1"
msgstr ""

#: 03060600.xhp
msgctxt ""
"03060600.xhp\n"
"par_id3153144\n"
"help.text"
msgid "vOut = vA > vB XOR vB > vD ' returns -1"
msgstr ""

#: 03060600.xhp
msgctxt ""
"03060600.xhp\n"
"par_id3154944\n"
"help.text"
msgid "vOut = (vB > vD XOR vB > vA) ' returns 0"
msgstr ""

#: 03060600.xhp
msgctxt ""
"03060600.xhp\n"
"par_id3148455\n"
"help.text"
msgid "vOut = vB XOR vA ' returns 2"
msgstr ""

