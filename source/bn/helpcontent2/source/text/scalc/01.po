msgstr "<bookmark_value>ISODD_ADD function</bookmark_value>"

#: 04060104.xhp
msgctxt ""
"04060104.xhp\n"
"hd_id3153939\n"
"help.text"
msgid "ISODD_ADD"
msgstr "ISODD_ADD"

#: 04060104.xhp
msgctxt ""
"04060104.xhp\n"
"par_id3153538\n"
"help.text"
