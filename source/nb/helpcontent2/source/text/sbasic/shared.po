"03090410.xhp\n"
"par_id3153361\n"
"help.text"
msgid "GetGenderIndex = Switch(sName = \"Jane\", \"female\", sName = \"John\", \"male\")"
msgstr "GetGenderIndex = Switch(sName = \"Jane\", \"kvinne\", sName = \"John\", \"mann\")"

