msgstr "<bookmark_value>lag; slette</bookmark_value><bookmark_value>slette; lag</bookmark_value>"

#: 02140000.xhp#hd_id3153541.1.help.text
msgctxt "02140000.xhp#hd_id3153541.1.help.text"
msgid "Delete Layer"
msgstr "Slett lag"

