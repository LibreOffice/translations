msgid "<link href=\"text/smath/01/06020000.xhp#importfile\" name=\"Import Formula\">Import Formula</link>"
msgstr "<link href=\"text/smath/01/06020000.xhp#importfile\" name=\"Import Formula\">Importer formel</link>"

