"par_id3151177\n"
"help.text"
msgid "<image id=\"img_id3156379\" src=\"cmd/sc_graphic.png\"><alt id=\"alt_id3156379\">Icon</alt></image>"
msgstr "<image id=\"img_id3156379\" src=\"cmd/sc_graphic.png\"><alt id=\"alt_id3156379\">Ikon</alt></image>"

