"par_id8186895\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Select a database and table.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Velg en database og en tabell.</ahelp>"

