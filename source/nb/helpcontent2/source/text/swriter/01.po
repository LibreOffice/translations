msgctxt ""
"05090200.xhp\n"
"hd_id3150756\n"
"help.text"
msgid "<link href=\"text/swriter/01/05090200.xhp\" name=\"Columns\">Columns</link>"
msgstr "<link href=\"text/swriter/01/05090200.xhp\" name=\"Columns\">Kolonner</link>"

#. zhj6t
#: 05090200.xhp
msgctxt ""
