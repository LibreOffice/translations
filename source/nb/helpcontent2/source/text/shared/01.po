msgid "<link href=\"text/shared/01/05210200.xhp\" name=\"Colors\">Colors</link>"
msgstr "<link href=\"text/shared/01/05210200.xhp\" name=\"Colors\">Farger</link>"

#: 05210200.xhp#par_id3149119.2.help.text
