msgstr "Vis"

#. YkB3Y
#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3153061\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versionsofdialog/show\">Displays the entire comment for the selected version.</ahelp>"
msgstr "<ahelp hid=\"sfx/ui/versionsofdialog/show\">Vis hele merknaden til den valgte versjonen.</ahelp>"

#. WsCjQ
#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"hd_id3154923\n"
"help.text"
msgid "Delete"
msgstr "Slett"

#. ZCgpA
#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3149669\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versionsofdialog/delete\">Deletes the selected version.</ahelp>"
msgstr "<ahelp hid=\"sfx/ui/versionsofdialog/delete\">Slett den valgte versjonen.</ahelp>"

#. nMEA6
#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"hd_id3148739\n"
"help.text"
msgid "Compare"
msgstr "Sammenligne"

#. rndPk
#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3152811\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versionsofdialog/compare\">Compare the changes that were made in each version.</ahelp> If you want, you can <link href=\"text/shared/01/02230400.xhp\"><emph>Manage Changes</emph></link>."
msgstr "<ahelp hid=\"sfx/ui/versionsofdialog/compare\">Sammenlign endringene som ble gjort i hver versjon.</ahelp> Hvis du vil, kan du <link href=\"text/shared/01/02230400.xhp\"><emph>Administrer endringer</emph></link>."

#. 7yFCP
