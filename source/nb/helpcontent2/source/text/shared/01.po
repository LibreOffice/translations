msgctxt ""
"05030500.xhp\n"
"par_id3153364\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/borderpage/shadows\">Click a shadow style for the selected borders.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/borderpage/shadows\">Klikk på en skyggestil for de valgte kantene.</ahelp>"

