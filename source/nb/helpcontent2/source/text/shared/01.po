"par_id3155829\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/shadowtabpage/LB_SHADOW_COLOR\">Select a color for the shadow.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/shadowtabpage/LB_SHADOW_COLOR\">Velg en farge for skyggen.</ahelp>"

#. 7aXiH
