"par_id3156060\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/borderpage/distancemf\">Enter the width of the shadow.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/borderpage/distancemf\">Angi hvor bred skyggen skal være.</ahelp>"

