"hd_id3162942\n"
"help.text"
msgid "<variable id=\"NumbersLink\"><link href=\"text/shared/01/05020300.xhp\">Numbers</link></variable>"
msgstr "<variable id=\"NumbersLink\"><link href=\"text/shared/01/05020300.xhp\">Tall</link></variable>"

#. z8GFZ
