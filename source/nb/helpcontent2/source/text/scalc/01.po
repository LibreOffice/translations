msgid "<item type=\"input\">=AND(12<13;14>12;7<6)</item> returns FALSE."
msgstr "<item type=\"input\">=OG(12<13; 14>12; 7<6)</item> gir USANN."

#. 3MAQE
#: 04060105.xhp
msgctxt ""
"04060105.xhp\n"
"par_id3149946\n"
"help.text"
msgid "<item type=\"input\">=AND(FALSE();TRUE())</item> returns FALSE."
msgstr "<item type=\"input\">=AND(FALSE();TRUE())</item> returnerer FALSE."

#. XXJC8
#: 04060105.xhp
msgctxt ""
"04060105.xhp\n"
"bm_id3149015\n"
"help.text"
