"par_id3148745\n"
"help.text"
msgid "<image id=\"img_id3159256\" src=\"cmd/sc_thesaurus.svg\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id3159256\">Icon Scenarios</alt></image>"
msgstr "<image id=\"img_id3159256\" src=\"cmd/sc_thesaurus.svg\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id3159256\">Ikon Scenarier</alt></image>"

