msgid "<item type=\"input\">=SECOND(C4)</item> returns 17 if contents of C4 = <item type=\"input\">12:20:17</item>."
msgstr "<item type=\"input\">=SECOND(C4)</item> returnerer 17 hvis innholdet i C4 = <item type=\"input\">12:20:17</item>."

