"help.text"
msgid "<variable id=\"ctrls_h1\"><link href=\"text/sbasic/shared/03/sf_base.xhp\" name=\"SFDocuments.Base service\"><literal>SFDocuments</literal>.<literal>Base</literal> service</link></variable>"
