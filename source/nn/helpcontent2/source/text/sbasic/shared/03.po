msgctxt ""
"sf_exception.xhp\n"
"par_id481587218636884\n"
"help.text"
msgid "<emph>arg0[, arg1, ...]</emph>: Any number of arguments of any type."
msgstr "<emph>arg0[, arg1, ...]</emph>: Eit vilkårleg tal på argument av kva type som helst."

