msgctxt ""
"01170100.xhp\n"
"par_id3146914\n"
"help.text"
msgid "(UpArrow)"
msgstr "(Pil opp)"

#: 01170100.xhp
msgctxt ""
"01170100.xhp\n"
"par_id3153714\n"
"help.text"
msgid "Goes to the previous line."
msgstr "Går til førre linje."

#: 01170100.xhp
msgctxt ""
"01170100.xhp\n"
"par_id3159266\n"
"help.text"
msgid "(DownArrow)"
msgstr "(Pil ned)"

