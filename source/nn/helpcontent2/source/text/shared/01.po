"par_id3145744\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/pastespecial/formulas\">Inserts cells containing formulae.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/pastespecial/formulas\">Set inn celler som inneheld formlar.</ahelp>"

#. 8hAk2
