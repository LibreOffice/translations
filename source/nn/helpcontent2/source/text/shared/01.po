msgctxt ""
"02250000.xhp\n"
"par_id3151118\n"
"help.text"
msgid "<ahelp hid=\".uno:Bib/sdbsource\">Select the data source for the bibliography database.</ahelp>"
msgstr "<ahelp hid=\".uno:Bib/sdbsource\">Vel datakjelda for litteraturdatabasen.</ahelp>"

#. h2RiF
