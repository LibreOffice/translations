msgctxt ""
"collab.xhp\n"
"par_id4263740\n"
"help.text"
msgid "<link href=\"text/shared/01/01070000.xhp\">Save As</link>"
msgstr "<link href=\"text/shared/01/01070000.xhp\">Lagra som</link>"

