msgctxt ""
"stylist_fromselect.xhp\n"
"par_id3156097\n"
"help.text"
msgid "<variable id=\"open_sidebar\">Choose <menuitem>View - Styles</menuitem> to open the <emph>Styles</emph> deck in the Sidebar.</variable>"
msgstr "<variable id=\"open_sidebar\">Vel <menuitem>Vis → Stilar</menuitem> for å opna panelet <emph>Stilar</emph> i sidepanelet.</variable>"

#. rKRNy
#: stylist_fromselect.xhp
msgctxt ""
"stylist_fromselect.xhp\n"
"par_id3153402\n"
"help.text"
msgid "<variable id=\"select_category\">Click the icon at the top of the <emph>Styles</emph> deck for the style category of the new style.</variable>"
msgstr "<variable id=\"select_category\">Trykk på ikonet øvst på panelet <emph>Stilar</emph> for kategorien til den nye stilen.</variable>"

#. tQGu2
#: stylist_fromselect.xhp
msgctxt ""
"stylist_fromselect.xhp\n"
"par_id3153119\n"
