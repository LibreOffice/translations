"04090003.xhp\n"
"par_id3155970\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/fldfuncpage/up\">Moves the selected item up in the list.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/up\">Flyttar det merkte elementet opp i lista.</ahelp>"

#. P4c6b
