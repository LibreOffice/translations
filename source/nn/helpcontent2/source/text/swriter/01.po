msgid "Remove"
msgstr "Fjern"

#. ntJZC
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"par_id3149558\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/fldfuncpage/remove\">Removes the selected item from the list.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/remove\">Fjernar det merkte elementet frå lista.</ahelp>"

#. LUZ9B
#: 04090003.xhp
