"par_id241030160012187036\n"
"help.text"
msgid "<link href=\"text/scalc/01/func_workday.intl.xhp\">WORKDAY.INTL</link>"
msgstr "<link href=\"text/scalc/01/func_workday.intl.xhp\">ARBEIDSDAG.INTL</link>"

