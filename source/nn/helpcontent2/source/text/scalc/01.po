msgstr "<ahelp hid=\"HID_AAI_FUNC_SQRTPI\">Returnerer kvadratrota av (pi gongar eit tal).</ahelp>"

#. bTULe
#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3164627\n"
"help.text"
msgid "SQRTPI(Number)"
msgstr "ROTPI(Tal)"

#. pvRg7
