msgctxt ""
"12090104.xhp\n"
"par_id3150768\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"visible\">Displays the name of the filtered data range in the table.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"visible\">Viser namnet til det filtrerte dataområdet i tabellen.</ahelp>"

