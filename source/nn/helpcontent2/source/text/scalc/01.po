msgctxt ""
"04060106.xhp\n"
"hd_id3165434\n"
"help.text"
msgid "TANH"
msgstr "TAN"

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3165462\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_TANHYP\">Returns the hyperbolic tangent of a number.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_TANHYP\">Returnerer hyperbolsk tangens til eit tal.</ahelp>"

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3165494\n"
"help.text"
msgid "TANH(Number)"
msgstr "TANH(tal)"

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3165508\n"
"help.text"
msgid "Returns the hyperbolic tangent of <emph>Number</emph>."
msgstr "Returnerer den hyperbolske tangens av <emph>Tal</emph>."

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3165541\n"
"help.text"
msgid "<item type=\"input\">=TANH(0)</item> returns 0, the hyperbolic tangent of 0."
msgstr "<item type=\"input\">=TANH(0)</item> returnerer 0, den hyperbolske tangens av 0."

