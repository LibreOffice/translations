msgctxt ""
"02140700.xhp\n"
"par_id2308201415431820502\n"
"help.text"
msgid "<ahelp hid=\".\">Number of decimal places of the numbers generated.</ahelp>"
msgstr "<ahelp hid=\".\">Talet på desimalplassar for dei genererte tala.</ahelp>"

