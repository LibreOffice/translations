msgstr "<variable id=\"variable name\"><ahelp hid=\".\">VISS.SANN er ein fleirfunksjons VISS-funksjon.</ahelp></variable>"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id271556235333493\n"
"help.text"
msgid "IFS( expression1, result1[, expression2, result2][, …] )"
msgstr "VISS.SANN(uttrykk1, resultat1[, uttrykk2, resultat2][, …])"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id31556235655212\n"
"help.text"
msgid "<emph>expression1, expression2, ...</emph> are any boolean values or expressions that can be TRUE or FALSE"
msgstr "<emph>uttrykk1, uttrykk2, …</emph> er logiske verdiar som kan vera SANN eller USANN"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id441556235649549\n"
"help.text"
msgid "<emph>result1, result2, ... </emph> are the values that are returned if the logical test is TRUE"
msgstr "<emph>resultat1, resultat2, …</emph> er verdiar som vert returnerte viss den logiske testen er SANN"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id641556235704257\n"
"help.text"
msgid "IFS( expression1, result1, expression2, result2, expression3, result3 ) is executed as"
msgstr "VISS.SANN(uttrykk1, resultat1,uttrykk2, resultat2, uttrykk3, resultat3) vert utført som"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id551556235712759\n"
"help.text"
msgid "IF expression1 is TRUE"
msgstr "VISS uttrykk1 er SANN"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id1001556235718948\n"
"help.text"
msgid "THEN result1"
msgstr "SÅ resultat1"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id571556235725969\n"
"help.text"
msgid "ELSE IF expression2 is TRUE"
msgstr "ELLES VISS uttrykk2 er SANN"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id581556235731982\n"
"help.text"
msgid "THEN result2"
msgstr "SÅ resultat2"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id961556235738258\n"
"help.text"
msgid "ELSE IF expression3 is TRUE"
msgstr "ELLES VISS uttrykk3 er SANN"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id951556235743954\n"
"help.text"
msgid "THEN result3"
msgstr "SÅ resultat3"

#: func_ifs.xhp
msgctxt ""
"func_ifs.xhp\n"
"par_id671556235758504\n"
"help.text"
msgid "To get a default result should no expression be TRUE, add a last expression that is always TRUE, like TRUE or 1=1 followed by the default result."
