msgctxt ""
"12090103.xhp\n"
"par_id3153093\n"
"help.text"
msgid "<ahelp hid=\".\">Select a logical operator for the filter.</ahelp>"
msgstr "<ahelp hid=\".\">Vel ein logisk operator for filteret.</ahelp>"

