#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id591535122756686\n"
"help.text"
msgid "Monday"
msgstr "Måndag"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id881535122835126\n"
"help.text"
msgid "Monday"
msgstr "Måndag"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id241535122903569\n"
"help.text"
msgid "Tuesday"
msgstr "Tysdag"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id31535124277973\n"
"help.text"
msgid "Wednesday"
msgstr "Onsdag"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id21535124310462\n"
"help.text"
msgid "Thursday"
msgstr "Torsdag"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id921535124365435\n"
"help.text"
msgid "Friday"
msgstr "Fredag"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id211535124416253\n"
"help.text"
msgid "Saturday"
msgstr "Laurdag"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id461535124473824\n"
"help.text"
msgid "Sunday"
msgstr "Søndag"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id471535124489997\n"
"help.text"
msgid "System 2"
msgstr "System 2"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id391535124564520\n"
"help.text"
msgid "Monday (ISO 8601)"
msgstr "Måndag (ISO 8601)"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id661535124640643\n"
"help.text"
msgid "(ISO 8601, for interoperability with Gnumeric)"
msgstr "(ISO 8601, for å vera interoperabel med Gnumeric)"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id3150704\n"
"help.text"
msgid "<item type=\"input\">=WEEKNUM(DATE(1995;1;1);1)</item>  returns 1"
msgstr "<item type=\"input\">=VEKENR(DATO(1995;1;1);1)</item> returnerer 1"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id3149792\n"
"help.text"
msgid "<item type=\"input\">=WEEKNUM(DATE(1995;1;1);2)</item>  returns 52. If the week starts on Monday, Sunday belongs to the last week of the previous year."
msgstr "<item type=\"input\">=VEKENR(DATO(1;1;95);2)</item> returnerer 52. Dersom veka byrjar måndag, vil søndagen framføre høyre til det forrige året."

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id3149793\n"
"help.text"
msgid "<item type=\"input\">=WEEKNUM(DATE(1995;1;1);21)</item>  returns 52. Week 1 starts on Monday, 1995-01-02."
msgstr "<item type=\"input\">=VEKENR(DATO(1995;1;1);21)</item> returnerer 52. Veke 1 byrja på måndag 02.01.1995."

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id3149794\n"
"help.text"
msgid "<item type=\"input\">=WEEKNUM(DATE(1999;1;1);21)</item>  returns 53. Week 1 starts on Monday, 1999-01-04."
msgstr "<item type=\"input\">=VEKENR(DATO(1999;1;1);21)</item> returnerer 53. Veke 1 byrja på måndag 04.01.1999."

#: func_weeknum_ooo.xhp
