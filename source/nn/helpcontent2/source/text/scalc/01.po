msgctxt ""
"func_weeknum.xhp\n"
"par_id231535122633827\n"
"help.text"
msgid "Day of the week"
msgstr "Vekedag"

#: func_weeknum.xhp
msgctxt ""
"func_weeknum.xhp\n"
"par_id531535124180000\n"
