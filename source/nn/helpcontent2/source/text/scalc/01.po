msgid "<link href=\"text/scalc/01/05050100.xhp\" name=\"Rename\">Rename</link>"
msgstr "<link href=\"text/scalc/01/05050100.xhp\" name=\"Rename\">Endra namn</link>"

#: 05050000.xhp
msgctxt ""
