msgctxt ""
"webwizard02.xhp\n"
"tit\n"
"help.text"
msgid "Web Wizard - Documents"
msgstr "የ ዌብ አዋቂ - ሰነዶች"

#: webwizard02.xhp
msgctxt ""
"webwizard02.xhp\n"
"par_idN10547\n"
"help.text"
msgid "<link href=\"text/shared/autopi/webwizard02.xhp\">Web Wizard - Documents</link>"
