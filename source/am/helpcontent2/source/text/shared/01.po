msgctxt ""
"05210100.xhp\n"
"par_id3149810\n"
"38\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/areatabpage/MTR_FLD_X_SIZE\">Enter a width for the bitmap.</ahelp>"
msgstr "<ahelp hid=\"cui/ui/areatabpage/MTR_FLD_X_SIZE\">ለ bitmap ስፋት ያስገቡ</ahelp>"

