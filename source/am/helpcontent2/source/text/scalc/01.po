"par_id3159100\n"
"help.text"
msgid "<image id=\"img_id3149814\" src=\"sw/res/sf04.png\"><alt id=\"alt_id3149814\">Icon</alt></image>"
msgstr "<image id=\"img_id3149814\" src=\"sw/res/sf04.png\"><alt id=\"alt_id3149814\">ምልክት</alt></image>"

#: 05100000.xhp
