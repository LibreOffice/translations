msgctxt ""
"04060115.xhp\n"
"par_id050220171019072404\n"
"help.text"
msgid "=BESSELI(3.45, 4), returns 0.651416873060081"
msgstr "=ቤሴልI(3.45, 4), ይመልሳል 0.651416873060081"

