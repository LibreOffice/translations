msgctxt ""
"04060185.xhp\n"
"par_id3146077\n"
"help.text"
msgid "<item type=\"input\">=WEIBULL(2;1;1;1)</item> returns 0.86."
msgstr "<item type=\"input\">=ዌይቡል(2;1;1;1)</item> ይመልሳል 0.86."

