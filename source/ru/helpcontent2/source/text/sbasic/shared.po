msgstr "msgbox stext(i,ii)"

#: 03102100.xhp#par_id3155125.57.help.text
msgctxt "03102100.xhp#par_id3155125.57.help.text"
msgid "next ii"
msgstr "next ii"

#: 03102100.xhp#par_id3154528.58.help.text
