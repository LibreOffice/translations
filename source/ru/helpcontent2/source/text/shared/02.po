"09070100.xhp\n"
"tit\n"
"help.text"
msgid "Internet"
msgstr "Интернет"

#: 09070100.xhp
msgctxt ""
"09070100.xhp\n"
"hd_id3151100\n"
"help.text"
msgid "<link href=\"text/shared/02/09070100.xhp\" name=\"Internet\">Internet</link>"
msgstr "<link href=\"text/shared/02/09070100.xhp\" name=\"Интернет\">Интернет</link>"

#: 09070100.xhp
msgctxt ""
"09070100.xhp\n"
"par_id3154230\n"
"help.text"
