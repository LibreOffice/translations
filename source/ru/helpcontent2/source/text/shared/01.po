msgctxt ""
"01010201.xhp\n"
"par_id3159167\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/cardmediumpage/sheet\">Prints labels on individual sheets.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/cardmediumpage/sheet\">Печать этикеток на отдельных листах.</ahelp>"

