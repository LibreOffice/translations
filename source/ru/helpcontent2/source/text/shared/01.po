msgctxt ""
"01010201.xhp\n"
"par_id3151339\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/cardmediumpage/continuous\">Prints labels on continuous paper.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/cardmediumpage/continuous\">Печать этикеток на сплошной бумаге.</ahelp>"

#. 74edJ
