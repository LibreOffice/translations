"par_idN106F4\n"
"help.text"
msgid "Pica"
msgstr "Пика"

#: 00000003.xhp
msgctxt ""
"00000003.xhp\n"
"par_idN106FB\n"
"help.text"
msgid "pt"
msgstr "пт"

