msgid "<variable id=\"contextmenu\"><link href=\"text/shared/guide/contextmenu.xhp\">Using Context Menus</link></variable>"
msgstr "<variable id=\"contextmenu\"><link href=\"text/shared/guide/contextmenu.xhp\">Использование контекстных меню</link></variable>"

#. xKErR
