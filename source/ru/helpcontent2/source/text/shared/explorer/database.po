"help.text"
msgid "{d 'YYYY-MM-DD'}"
msgstr "{д 'ГГГГ-ММ-ДД'}"

#: 02010100.xhp
msgctxt ""
"02010100.xhp\n"
"par_id314975314\n"
"help.text"
msgid "'YYYY-MM-DD'"
msgstr "{Д'ГГГГ-ММ-ДД'}"

#: 02010100.xhp
