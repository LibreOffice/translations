msgid "NOT LIKE"
msgstr "NOT LIKE"

#: 02010100.xhp#par_id3161669.82.help.text
msgctxt "02010100.xhp#par_id3161669.82.help.text"
msgid "NOT LIKE"
msgstr "NOT LIKE"

#: 02010100.xhp#par_id3159141.83.help.text
