#. Fnu8g
#: table_view.xhp
msgctxt ""
"table_view.xhp\n"
"par_id3153192\n"
"help.text"
msgid "To hide column and line headers in a table:"
msgstr "Скрытие заголовков столбцов и строк в таблице:"

#. dmh8o
#: table_view.xhp
msgctxt ""
"table_view.xhp\n"
"par_id3153768\n"
"help.text"
