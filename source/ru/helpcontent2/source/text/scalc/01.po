msgctxt ""
"04060116.xhp\n"
"par_id3147260\n"
"help.text"
msgid "<item type=\"input\">=OCT2BIN(3;3)</item> returns 011."
msgstr "<item type=\"input\">=OCT2BIN(3;3)</item> возвращает значение 011."

