msgctxt ""
"04060116.xhp\n"
"par_id3154810\n"
"help.text"
msgid "IMEXP(\"ComplexNumber\")"
msgstr "IMEXP(\"КомплексноеЧисло\")"

#: 04060116.xhp
msgctxt ""
"04060116.xhp\n"
"hd_id3148581\n"
"help.text"
msgid "Example"
msgstr "Пример"

