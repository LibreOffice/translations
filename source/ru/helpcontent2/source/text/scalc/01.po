msgctxt ""
"04060104.xhp\n"
"par_id3148926\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_ISTLOG\">Tests for a logical value (TRUE or FALSE).</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_ISTLOG\">Проверка на логическое значение (ИСТИНА или ЛОЖЬ).</ahelp>"

#. ucKHh
