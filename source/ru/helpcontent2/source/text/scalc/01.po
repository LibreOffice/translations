msgctxt ""
"04060119.xhp\n"
"par_id3150224\n"
"help.text"
msgid "TBILLEQ(Settlement; Maturity; Discount)"
msgstr "РАВНОКЧЕК(Соглашение; Погашение; Скидка)"

