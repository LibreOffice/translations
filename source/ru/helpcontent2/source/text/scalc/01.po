msgstr "<ahelp hid=\"HID_FUNC_TANHYP\">Возвращает гиперболический тангенс числа.</ahelp>"

#: 04060106.xhp#hd_id3165480.458.help.text
msgctxt "04060106.xhp#hd_id3165480.458.help.text"
msgid "Syntax"
msgstr "Синтаксис"

