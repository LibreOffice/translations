msgstr "<variable id=\"exact_h1\">          <link href=\"text/scalc/01/func_exact.xhp\">СОВПАД</link>        </variable>"

#. kGvM7
#: func_exact.xhp
msgctxt ""
"func_exact.xhp\n"
"par_id3158413\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_IDENTISCH\">Compares two text strings and returns TRUE if they are identical.</ahelp> This function is case-sensitive."
msgstr "<ahelp hid=\"HID_FUNC_IDENTISCH\">Сравнивает две текстовые строки и возвращает значение ИСТИНА, если они идентичны.</ahelp> Это регистрозависимая функция."

#. Ba7M2
#: func_exact.xhp
msgctxt ""
"func_exact.xhp\n"
"par_id3148594\n"
"help.text"
msgid "EXACT(\"Text1\"; \"Text2\")"
msgstr "СОВПАД(\"Текст 1\"; \"Текст 2\")"

#. 7kKd4
#: func_exact.xhp
msgctxt ""
"func_exact.xhp\n"
"par_id3153224\n"
"help.text"
msgid "<emph>Text1</emph> refers to the first text to compare."
msgstr "<emph>Текст 1</emph>: первый сравниваемый текст."

#. 37PQt
#: func_exact.xhp
msgctxt ""
"func_exact.xhp\n"
"par_id3148637\n"
"help.text"
msgid "<emph>Text2</emph> is the second text to compare."
msgstr "<emph>Текст 2</emph>: второй сравниваемый текст."

#. DPfsN
#: func_exact.xhp
msgctxt ""
"func_exact.xhp\n"
"par_id3156263\n"
"help.text"
msgid "<item type=\"input\">=EXACT(\"microsystems\";\"Microsystems\")</item> returns FALSE."
msgstr "<item type=\"input\">=СОВПАД(\"microsystems\";\"Microsystems\")</item> возвращает ЛОЖЬ."

#. Aa6eC
#: func_exact.xhp
msgctxt ""
"func_exact.xhp\n"
"par_id701677016652715\n"
"help.text"
msgid "<link href=\"https://wiki.documentfoundation.org/Documentation/Calc_Functions/EXACT\">EXACT wiki page</link>."
msgstr "<link href=\"https://wiki.documentfoundation.org/Documentation/Calc_Functions/EXACT\">Страница wiki функции СОВПАД</link>."

#. UABhi
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"tit\n"
"help.text"
msgid "EXPAND function"
msgstr "Функция EXPAND"

#. LVpwH
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"bm_id361708283183878\n"
"help.text"
msgid "<bookmark_value>EXPAND function</bookmark_value>"
msgstr "<bookmark_value>Функция EXPAND</bookmark_value>"

#. yBGiA
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"hd_id901708806478126\n"
"help.text"
msgid "<variable id=\"h1\"><link href=\"text/scalc/01/func_expand.xhp\">EXPAND</link></variable>"
msgstr "<variable id=\"h1\"><link href=\"text/scalc/01/func_expand.xhp\">EXPAND</link></variable>"

#. 5LotC
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"par_id771708806478128\n"
"help.text"
msgid "Expand an array by adding rows or columns."
msgstr "Расширить массив путём добавления строк или столбцов."

#. mLtmc
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"par_id601738075517060\n"
"help.text"
msgid "=EXPAND(Array; Rows [; Columns [; Pad_with]])"
msgstr "=EXPAND(Массив; Строки [; Столбцы [; Заполнитель]])"

#. ppWFd
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"par_id81738076052161\n"
"help.text"
msgid "<emph>Array</emph>: the array or range to expand by the number of rows and/or columns."
msgstr "<emph>Массив</emph>: массив или диапазон, расширяемый добавлением нескольких строк или столбцов."

#. dyQRu
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"par_id191738076047602\n"
"help.text"
msgid "<emph>Rows</emph>: the final number of rows the array will expand. Default is total rows."
msgstr "<emph>Строки</emph>: конечное количество строк, на которое будет расширен массив. По умолчанию общее количество строк."

#. Kk5Vz
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"par_id1001738076043608\n"
"help.text"
msgid "<emph>Columns</emph>: (optional) The final number of columns the array will expand. Default is total columns."
msgstr "<emph>Столбцы</emph>: (необязательный параметр) Конечное количество столбцов, на которое будет расширен массив. По умолчанию общее количество столбцов."

#. xoxMh
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"par_id411737825456380\n"
"help.text"
msgid "<emph>Pad_with</emph>: (optional) a specified value to pad in new cells. By default, #N/A is used to fill the new cells."
msgstr "<emph>Заполнитель</emph>: (необязательный параметр) задаёт значение для заполнения новых ячеек. По умолчанию новые ячейки заполняются значением #Н/Д."

#. 3923w
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"par_id821738091480292\n"
"help.text"
msgid "The formula <input>{=EXPAND(A1:E3;4;7;\"@@@\")}</input> expands the array <literal>A1:E3</literal> to 7 columns and 4 rows, padding the extra cells with the string \"@@@\"."
msgstr "Формула <input>{=EXPAND(A1:E3;4;7;\"@@@\")}</input> расширяет массив <literal>A1:E3</literal> на 7 столбцов и 4 строки, заполняя дополнительные ячейки строкой \"@@@\"."

#. PhTg7
#: func_expand.xhp
msgctxt ""
"func_expand.xhp\n"
"par_id701677016751508\n"
"help.text"
msgid "<link href=\"https://wiki.documentfoundation.org/Documentation/Calc_Functions/EXPAND\">EXPAND wiki page</link>."
msgstr "<link href=\"https://wiki.documentfoundation.org/Documentation/Calc_Functions/EXPAND\">Страница wiki функции EXPAND</link>."

#. qdiU7
