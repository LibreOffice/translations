msgstr "<ahelp hid=\"HID_FUNC_POISSON\">Вычисляет распределение Пуассона.</ahelp>"

#: 04060184.xhp
msgctxt ""
"04060184.xhp\n"
"hd_id3159183\n"
"help.text"
msgid "Syntax"
msgstr "Синтаксис"

#: 04060184.xhp
msgctxt ""
