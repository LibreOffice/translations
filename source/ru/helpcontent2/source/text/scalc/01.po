msgstr "<ahelp hid=\"HID_FUNC_SINHYP\">Вычисляет гиперболический синус числа.</ahelp>"

#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"hd_id3163444\n"
"help.text"
msgid "Syntax"
msgstr "Синтаксис"

#: 04060106.xhp
