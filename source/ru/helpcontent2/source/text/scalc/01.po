msgctxt ""
"04060116.xhp\n"
"par_id3149895\n"
"help.text"
msgid "<ahelp hid=\"HID_AAI_FUNC_IMABS\">The result is the absolute value of a complex number.</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMABS\">Возвращает абсолютное значение для комплексного числа.</ahelp>"

