msgctxt ""
"04060107.xhp\n"
"par_id3155276\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_MTRANS\">Transposes the rows and columns of an array.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_MTRANS\">Служит для транспонирования строк и столбцов массива.</ahelp>"

#: 04060107.xhp
msgctxt ""
"04060107.xhp\n"
"hd_id3155294\n"
"help.text"
msgid "Syntax"
msgstr "Синтаксис"

