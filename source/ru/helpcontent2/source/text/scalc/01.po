msgctxt ""
"04060116.xhp\n"
"par_id3150963\n"
"help.text"
msgid "<item type=\"input\">=IMSUB(\"13+4j\";\"5+3j\")</item> returns 8+j."
msgstr "<item type=\"input\">=IMSUB(\"13+4j\";\"5+3j\")</item> возвращает значение 8+j."

