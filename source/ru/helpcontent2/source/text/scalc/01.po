msgctxt ""
"04060184.xhp\n"
"par_id2853733\n"
"help.text"
msgid "<item type=\"input\">=MODE.MULT(A1:A50)</item>"
msgstr "<item type=\"input\">=МОДА.НСК(A1:A50)</item>"

