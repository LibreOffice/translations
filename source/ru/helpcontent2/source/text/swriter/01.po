msgid "Vertical alignment"
msgstr "Выравнивание по вертикали"

#. NtjWj
#: 05090300.xhp
msgctxt ""
"05090300.xhp\n"
"par_id3149164\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/tabletextflowpage/vertorient\">Specify the vertical text alignment for the cells in the table.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/tabletextflowpage/vertorient\">Задайте выравнивание текста по вертикали для ячеек таблицы.</ahelp>"

#. uajQ9
