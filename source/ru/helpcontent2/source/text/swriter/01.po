"hd_id3150549\n"
"help.text"
msgid "Move Down"
msgstr "Переместить вниз"

#. RVPyR
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"par_id3156221\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/fldfuncpage/down\">Moves the selected item down in the list.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/down\">Перемещает выбранный элемент вниз в списке.</ahelp>"

#. sKtnG
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"hd_id3149215\n"
"help.text"
msgid "Name"
msgstr "Имя"

#. AC47J
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"par_id3147733\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/fldfuncpage/listname\">Enter a unique name for the <emph>Input list</emph>.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/listname\">Введите уникальное имя для <emph>списка</emph>.</ahelp>"

#. DEN3m
