msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/item\">Введите новый элемент.</ahelp>"

#. KUsBa
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"hd_id3147413\n"
"help.text"
msgid "Add"
msgstr "Добавить"

#. M3529
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"par_id3147473\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/fldfuncpage/add\">Adds the <emph>Item</emph> to the list.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/add\">Добавляет <emph>элемент</emph> в список.</ahelp>"

#. ZkhHj
