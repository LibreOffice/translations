"bm_id721692975475647\n"
"help.text"
msgid "<bookmark_value>wrap text;around objects</bookmark_value>"
msgstr "<bookmark_value>обтекание текстом;вокруг объектов</bookmark_value>"

#. zAanm
#: 05060200.xhp
msgctxt ""
