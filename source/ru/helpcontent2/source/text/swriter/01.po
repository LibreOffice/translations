msgstr "Элементы списка"

#. kkZWu
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"par_id3147618\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/fldfuncpage/listitems\">Lists the items. The topmost item is shown in the document.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/listitems\">Выводит список элементов. В документе отображается самый верхний элемент.</ahelp>"

#. 8CKY4
