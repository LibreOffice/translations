"help.text"
msgid "To apply bold formatting"
msgstr "Форматирование с использованием жирного шрифта"

#. TRQDd
#: shortcut_writing.xhp
msgctxt ""
"shortcut_writing.xhp\n"
"par_id3155861\n"
"help.text"
msgid "Select the text that you want to format."
msgstr "Выделите текст, который требуется отформатировать."

#. A2boq
#: shortcut_writing.xhp
msgctxt ""
"shortcut_writing.xhp\n"
"par_id3149836\n"
"help.text"
msgid "Press <switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+B."
msgstr "Нажмите клавишу <switchinline select=\"sys\"><caseinline select=\"MAC\">Command</caseinline><defaultinline>Ctrl</defaultinline></switchinline>+B."

#. EkKaM
#: shortcut_writing.xhp
msgctxt ""
"shortcut_writing.xhp\n"
"par_id3156112\n"
"help.text"
