"par_id3146137\n"
"help.text"
msgid "<link href=\"text/swriter/01/05150200.xhp\">AutoCorrect</link>"
msgstr "<link href=\"text/swriter/01/05150200.xhp\">Автокорекция</link>"

#. AdAHF
