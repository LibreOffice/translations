msgstr "Бутон Експортиране"

#. kHxND
#: ref_pdf_export.xhp
msgctxt ""
"ref_pdf_export.xhp\n"
"par_id3146975\n"
"help.text"
msgid "<ahelp hid=\".\">Exports the current file in PDF format.</ahelp>"
msgstr "<ahelp hid=\".\">Експортира текущия файл във формат PDF.</ahelp>"

#. g65mo
