msgstr "<switchinline select=\"sys\"><caseinline select=\"WIN\">{инсталация}\\program\\soffice.exe {параметър} </caseinline><caseinline select=\"UNIX\">{инсталация}/program/soffice {параметър} </caseinline></switchinline>"

