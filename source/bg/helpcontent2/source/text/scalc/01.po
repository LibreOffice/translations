msgctxt ""
"02110000.xhp\n"
"par_id3147471\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/dropmenu/copy\">Generates a copy when you drag-and-drop an object from the Navigator into a document.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/dropmenu/copy\">Когато плъзнете и пуснете обект от навигатора в документ, се създава копие.</ahelp>"

