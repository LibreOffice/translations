msgctxt ""
"04060110.xhp\n"
"bm_id3149171\n"
"help.text"
msgid "<bookmark_value>REPLACE function</bookmark_value>"
msgstr "<bookmark_value>REPLACE, функция</bookmark_value>"

#. D3Cy8
