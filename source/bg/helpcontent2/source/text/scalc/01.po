msgstr "От 1 (сряда) до 7 (вторник)."

#. jrbXZ
#: func_weekday.xhp
msgctxt ""
"func_weekday.xhp\n"
"par_id050220170615599110\n"
"help.text"
msgid "1 (Thursday) through 7 (Wednesday)."
msgstr "От 1 (четвъртък) до 7 (сряда)."

#. 6RzFd
#: func_weekday.xhp
msgctxt ""
"func_weekday.xhp\n"
"par_id05022017061600535\n"
"help.text"
