msgctxt ""
"03102100.xhp\n"
"par_id3151274\n"
"help.text"
msgid "DIM text(-15 to 5) as String REM 21 elements (including 0)"
msgstr "Dim text(-15 To 5) As String REM 21 елемента (включително 0)"

#. rgjVA
