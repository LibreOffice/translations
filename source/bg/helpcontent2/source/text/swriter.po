msgstr "Уголемяване на шрифта"

#: main0202.xhp
msgctxt ""
"main0202.xhp\n"
"par_id0122200903085371\n"
"help.text"
msgid "<ahelp hid=\".\">Increases the font size of the selected text.</ahelp>"
msgstr "<ahelp hid=\".\">Увеличава размера на шрифта на избрания текст.</ahelp>"

