msgctxt "SC_OPCODE_WEEK"
msgid "Number"
msgstr "Число"

#: sc/inc/scfuncs.hrc:369
msgctxt "SC_OPCODE_WEEK"
msgid "The internal number of the date."
msgstr "Вътрешното числово представяне на датата."

#: sc/inc/scfuncs.hrc:370
msgctxt "SC_OPCODE_WEEK"
