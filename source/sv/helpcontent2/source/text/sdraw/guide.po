msgstr "<variable id=\"join_objects\"><link href=\"text/sdraw/guide/join_objects.xhp\">Förbinda linjer</link></variable>"

#. FUYtU
#: join_objects.xhp
msgctxt ""
"join_objects.xhp\n"
"par_id3154512\n"
"help.text"
msgid "When you connect lines, lines are drawn between neighboring endpoints."
msgstr "När du förbinder linjer dras de mellan närliggande slutpunkter."

#. V9J7h
#: join_objects.xhp
msgctxt ""
"join_objects.xhp\n"
"hd_id3150752\n"
"help.text"
msgid "To connect lines:"
msgstr "Så här förbinder du linjer:"

#. jYDcD
#: join_objects.xhp
msgctxt ""
"join_objects.xhp\n"
"par_id3153714\n"
"help.text"
msgid "Select two or more lines."
msgstr "Markera två eller flera linjer."

#. UAfTT
#: join_objects.xhp
msgctxt ""
