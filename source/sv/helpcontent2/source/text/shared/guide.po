msgctxt ""
"round_corner.xhp\n"
"par_id3156426\n"
"help.text"
msgid "<image id=\"img_id3150774\" src=\"media/helpimg/hand01.png\" width=\"0.3335inch\" height=\"0.3335inch\"><alt id=\"alt_id3150774\">Mouse pointer as hand</alt></image>"
msgstr "<image id=\"img_id3150774\" src=\"media/helpimg/hand01.png\" width=\"0.3335inch\" height=\"0.3335inch\"><alt id=\"alt_id3150774\">Muspekaren som en hand</alt></image>"

