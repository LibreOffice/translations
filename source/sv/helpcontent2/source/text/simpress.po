msgid "<link href=\"text/shared/3dsettings_toolbar.xhp\">Extrusion On/Off</link>"
msgstr "<link href=\"text/shared/3dsettings_toolbar.xhp\">Extrusion på/av</link>"

#: main0210.xhp
msgctxt ""
"main0210.xhp\n"
"par_idN1080F\n"
