msgstr "dValue=Exp( Log(b1)+Log(b2) )"

#: 03080201.xhp#par_id3161832.15.help.text
msgid "MsgBox \"\" & dValue & chr(13) & (b1*b2) ,0,\"Multiplication by logarithm\""
msgstr "MsgBox \"\" & dValue & chr(13) & (b1*b2) ,0,\"Multiplikation med logaritm\""

