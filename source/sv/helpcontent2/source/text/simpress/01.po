msgstr "<image id=\"img_id3153607\" src=\"cmd/sc_deletecolumns.png\" width=\"0.566cm\" height=\"0.566cm\"><alt id=\"alt_id3153607\">Symbol</alt></image>"

#: 05120500m.xhp#par_id3154423.116.help.text
msgid "Delete Column"
msgstr "Radera kolumn"

#: 03070000.xhp#tit.help.text
msgid "Presentation"
msgstr "Presentation"

#: 03070000.xhp#hd_id3153144.1.help.text
msgid "<link href=\"text/simpress/01/03070000.xhp\" name=\"Presentation\">Presentation</link>"
msgstr "<link href=\"text/simpress/01/03070000.xhp\" name=\"Presentation\">Presentation</link>"

#: 03070000.xhp#par_id3146975.2.help.text
