msgid "<bookmark_value>date and time functions</bookmark_value><bookmark_value>functions; date & time</bookmark_value><bookmark_value>Function Wizard; date & time</bookmark_value>"
msgstr "<bookmark_value>Datum och tid; funktioner</bookmark_value><bookmark_value>Funktioner; datum och tid</bookmark_value><bookmark_value>Guide: Funktioner; datum & tid</bookmark_value>"

