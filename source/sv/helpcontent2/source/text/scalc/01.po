msgstr "Exempel"

#: 04060115.xhp#par_id3149686.31.help.text
msgid "<item type=\"input\">=BIN2HEX(1100100;6)</item> returns 000064."
msgstr "<item type=\"input\">=BIN.TILL.HEX(1100100;6)</item> returnerar 000064."

