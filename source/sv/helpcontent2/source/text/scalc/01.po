msgid "Example"
msgstr "Exempel"

#: 04060115.xhp#par_id3156002.86.help.text
msgid "<item type=\"input\">=HEX2BIN(64;8)</item> returns 01100100."
msgstr "<item type=\"input\">=HEX.TILL.BIN(64;8)</item> returnerar 01100100."

