msgctxt ""
"empty_cells.xhp\n"
"par_id8277230\n"
"help.text"
msgid "A1: 1 B1: <Empty> C1: =B1 (displays 0)"
msgstr "A1: 1 B1: <Empty> C1: =B1 (visar 0)"

