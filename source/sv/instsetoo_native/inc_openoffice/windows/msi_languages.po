msgstr "Åtgärden startade [tid]: [1]."

#: Error.ulf#OOO_ERROR_14.LngText.text
msgid "Action ended [Time]: [1]. Return value [2]."
msgstr "Åtgärden avslutades [tid]: [1]. Returneringsvärde [2]."

#: Error.ulf#OOO_ERROR_15.LngText.text
