"RSC_QUERY_OBJECT_TYPE\n"
"1\n"
"string.text"
msgid "The table view"
msgstr "Tabellvyn"

#: query.src
msgctxt ""
"query.src\n"
"RSC_QUERY_OBJECT_TYPE\n"
"2\n"
"string.text"
msgid "The query"
msgstr "Frågan"

