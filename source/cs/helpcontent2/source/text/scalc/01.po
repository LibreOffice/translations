#. vAGKt
#: 04060105.xhp
msgctxt ""
"04060105.xhp\n"
"par_id3083444\n"
"help.text"
msgid "<item type=\"input\">=OR(A;B)</item> returns TRUE"
msgstr "<item type=\"input\">=OR(A;B)</item> vrátí PRAVDA"

#. BcGUg
#: 04060105.xhp
msgctxt ""
"04060105.xhp\n"
"par_id3154314\n"
"help.text"
msgid "<item type=\"input\">=NOT(AND(A;B))</item> returns TRUE"
msgstr "<item type=\"input\">=NOT(AND(A;B))</item> vrátí PRAVDA"

#. 7bq9r
