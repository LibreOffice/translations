msgstr "Osnova do prezentace"

#. nK9xD
#: 01160100.xhp
msgctxt ""
"01160100.xhp\n"
"hd_id3154571\n"
"help.text"
msgid "<link href=\"text/swriter/01/01160100.xhp\">Outline to Presentation</link>"
msgstr "<link href=\"text/swriter/01/01160100.xhp\">Osnova do prezentace</link>"

#. GXZRx
#: 01160100.xhp
msgctxt ""
"01160100.xhp\n"
"par_id3155186\n"
"help.text"
msgid "<ahelp hid=\".uno:SendOutlineToStarImpress\">Sends the outline of the active document to a new presentation document.</ahelp>"
msgstr "<ahelp hid=\".uno:SendOutlineToStarImpress\">Použije osnovu aktivního dokumentu a vytvoří z ní nový dokument prezentace.</ahelp>"

#. uE622
#: 01160200.xhp
msgctxt ""
"01160200.xhp\n"
"tit\n"
"help.text"
msgid "Outline to Clipboard"
msgstr "Osnova do schránky"

