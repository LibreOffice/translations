msgid "Name"
msgstr "Název"

#. dtVMk
#: 05090100.xhp
msgctxt ""
"05090100.xhp\n"
"par_id3145244\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/formattablepage/name\">Enter an internal name for the table. You can use this name to quickly locate the table in the Navigator.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/formattablepage/name\">Zadejte interní název tabulky. Pomocí tohoto jména můžete tabulku rychle najít v Navigátoru.</ahelp>"

#. cu6QS
#: 05090100.xhp
