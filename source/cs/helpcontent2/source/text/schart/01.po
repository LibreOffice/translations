msgctxt ""
"05020000.xhp\n"
"hd_id3156441\n"
"help.text"
msgid "<link href=\"text/schart/01/05020100.xhp\" name=\"Subtitle\">Subtitle</link>"
msgstr "<link href=\"text/schart/01/05020100.xhp\" name=\"Podnadpis\">Podnadpis</link>"

#: 05020000.xhp
msgctxt ""
"05020000.xhp\n"
"hd_id3151073\n"
