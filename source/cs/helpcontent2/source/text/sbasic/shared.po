msgctxt ""
"03080101.xhp\n"
"par_id3149262\n"
"help.text"
msgid "d1 = InputBox(\"Enter the length of the side adjacent to the angle: \",\"Adjacent\")"
msgstr "d1 = InputBox(\"Zadejte délku odvěsny přilehlé úhlu: \",\"Přilehlá\")"

#. 2a3P2
