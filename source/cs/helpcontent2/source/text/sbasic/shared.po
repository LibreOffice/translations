msgid "Dim lValue As Long"
msgstr "Dim lValue As Long"

#: 03090403.xhp#par_id3145647.19.help.text
msgctxt "03090403.xhp#par_id3145647.19.help.text"
msgid "lValue = 5000"
msgstr "lValue = 5000"

