msgctxt ""
"03010305.xhp\n"
"par_id3145647\n"
"help.text"
msgid "MsgBox \"The color \" & lVar & \" consists of:\" & Chr(13) &_"
msgstr "MsgBox \"Barva \" & lVar & \" se skládá z:\" & Chr(13) &_"

