msgctxt ""
"03010305.xhp\n"
"par_id3149401\n"
"help.text"
msgid "\"green= \" & green(lVar) & Chr(13)&_"
msgstr "\"zelená= \" & green(lVar) & Chr(13)&_"

#. 9oLnG
