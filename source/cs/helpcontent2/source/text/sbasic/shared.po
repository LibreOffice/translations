msgctxt ""
"03070700.xhp\n"
"par_id3149670\n"
"help.text"
msgid "Performs the integer division on two numbers and returns the result."
msgstr "Provede celočíselné dělení dvou čísel a vrátí výsledek."

#. 7hxCB
#: 03070700.xhp
msgctxt ""
"03070700.xhp\n"
"bm_id3150669\n"
"help.text"
msgid "<bookmark_value>Operators;Integer division (\\\\)</bookmark_value>  <bookmark_value>\"\\\\\" operator (mathematical)</bookmark_value>"
msgstr "<bookmark_value>operátory;celočíselné dělení (\\\\)</bookmark_value><bookmark_value>operátor \"\\\\\" (matematický)</bookmark_value>"

#. KDgG2
