msgctxt ""
"03080101.xhp\n"
"par_id3149482\n"
"help.text"
msgid "d2 = InputBox(\"Enter the length of the side opposite the angle: \",\"Opposite\")"
msgstr "d2 = InputBox(\"Zadejte délku odvěsny protilehlé úhlu: \",\"Protilehlá\")"

