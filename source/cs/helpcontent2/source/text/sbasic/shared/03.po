msgctxt ""
"sf_calc.xhp\n"
"pyc_id451621536612334\n"
"help.text"
msgid "newrange = myDoc.CompactLeft(\"Sheet1.G1:L10\")"
msgstr "newrange = myDoc.CompactLeft(\"List1.G1:L10\")"

#. j5kGd
