msgctxt ""
"sf_menu.xhp\n"
"bas_id301643136028263\n"
"help.text"
msgid "\"Menu item: \"   & sArgs(1) & Chr(13) & _"
msgstr "\"Položka nabídky: \"   & sArgs(1) & Chr(13) & _"

#. wFTww
