"par_idN10647\n"
"help.text"
msgid "<ahelp hid=\".\">Click to access csv files.</ahelp>"
msgstr "<ahelp hid=\".\">Klepněte pro zobrazení csv souborů.</ahelp>"

#: dabawiz02text.xhp
