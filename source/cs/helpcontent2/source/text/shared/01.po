msgstr "<image id=\"img_id3150774\" src=\"res/sc06301.png\" width=\"0.222inch\" height=\"0.222inch\"><alt id=\"alt_id3150774\">Ikonka</alt></image>"

#: 05320000.xhp#par_id3155941.19.help.text
msgid "Left arrow"
msgstr "Šipka doleva"

#: 05320000.xhp#hd_id3147010.20.help.text
