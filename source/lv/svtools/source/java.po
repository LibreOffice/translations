msgid "JRE Required"
msgstr "Nepieciešams JRE"

#: javaerror.src
msgctxt ""
"javaerror.src\n"
"STR_WARNING_INVALIDJAVASETTINGS_TITLE\n"
"string.text"
msgid "Select JRE"
msgstr "Izvēlieties JRE"

#: javaerror.src
msgctxt ""
"javaerror.src\n"
"STR_ERROR_JVMCREATIONFAILED_TITLE\n"
"string.text"
msgid "JRE is Defective"
msgstr "JRE ir bojāts"
