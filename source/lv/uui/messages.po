msgctxt "STR_ENTER_SIMPLE_PASSWORD"
msgid "Enter password: "
msgstr "Ievadiet paroli: "

#: uui/inc/strings.hrc:28
msgctxt "STR_CONFIRM_SIMPLE_PASSWORD"
msgid "Confirm password: "
msgstr "Apstipriniet paroli: "

