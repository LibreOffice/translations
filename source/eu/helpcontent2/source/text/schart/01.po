"1\n"
"help.text"
msgid "<link href=\"text/schart/01/05020000.xhp\" name=\"Title\">Title</link>"
msgstr "<link href=\"text/schart/01/05020000.xhp\" name=\"Title\">Titulua</link>"

#: 05020000.xhp
msgctxt ""
"05020000.xhp\n"
"par_id3125863\n"
