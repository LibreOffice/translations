msgid "<link href=\"text/shared/01/02020000.xhp\" name=\"Redo\">Redo</link>"
msgstr "<link href=\"text/shared/01/02020000.xhp\" name=\"Redo\">Berregin</link>"

#. rLmrF
#: 02020000.xhp
msgctxt ""
"02020000.xhp\n"
"par_id3157898\n"
"help.text"
msgid "<ahelp hid=\".\">Reverses the action of the last <emph>Undo</emph> command. To select the <emph>Undo</emph> step that you want to reverse, click the arrow next to the <emph>Redo</emph> icon on the <emph>Standard</emph> bar.</ahelp>"
msgstr "<ahelp hid=\".\">Azken <emph>Desegin</emph> komandoaren ekintza desegiten du. Desegin nahi duzun <emph>Desegin</emph> urratsa hautatzeko, egin klik <emph>Estandarra</emph> barrako <emph>Berregin</emph> ikonoaren ondoan dagoen gezian.</ahelp>"

#. MERfp
#: 02030000.xhp
msgctxt ""
