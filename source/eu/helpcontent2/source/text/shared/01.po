msgstr "Ezabatu"

#. ZCgpA
#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3149669\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versionsofdialog/delete\">Deletes the selected version.</ahelp>"
msgstr "<ahelp hid=\"sfx/ui/versionsofdialog/delete\">Hautatutako bertsioa ezabatzen du.</ahelp>"

#. nMEA6
#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"hd_id3148739\n"
"help.text"
msgid "Compare"
msgstr "Konparatu"

#. sg87a
#: 01190000.xhp
msgctxt ""
"01190000.xhp\n"
"par_id3152811\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/versionsofdialog/compare\">Compare the changes that were made in each version.</ahelp> If you want, you can <link href=\"text/shared/01/02230400.xhp\" name=\"Manage Changes\"><emph>Manage Changes</emph></link>."
msgstr "<ahelp hid=\"sfx/ui/versionsofdialog/compare\">Konparatu bertsio bakoitzean egindako aldaketak.</ahelp> Nahi baduzu, <link href=\"text/shared/01/02230400.xhp\" name=\"Manage Changes\"><emph>Kudeatu aldaketak</emph></link> aukera erabili dezakezu."

#. 7yFCP
