"hd_id3147285\n"
"help.text"
msgid "<link href=\"text/shared/02/01170200.xhp\" name=\"Form Properties\">Form Properties</link>"
msgstr "<link href=\"text/shared/02/01170200.xhp\" name=\"Form Properties\">Inprimaki-propietateak</link>"

#. YnaQW
