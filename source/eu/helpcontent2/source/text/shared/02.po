msgctxt ""
"12100200.xhp\n"
"par_id3148874\n"
"help.text"
msgid "for exactly one arbitrary character"
msgstr "karaktere arbitrario bakarrarentzat"

#. bVYVp
#: 12100200.xhp
msgctxt ""
"12100200.xhp\n"
"par_id3150365\n"
"help.text"
msgid "\"?loppy\" returns \"Floppy\""
msgstr "\"?loppy\" eskatuta \"Floppy\"aurkitzen du"

#. ANadS
