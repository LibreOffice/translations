msgstr "Aldagai bat eskuz aldatu"

#: 01040900.xhp#par_idN107CF.help.text
msgid "Conditional text, Hidden text, Hidden paragraph, Variables, DDE field"
msgstr "Baldintzapeko testua, ezkutuko testua, ezkutuko paragrafoa, aldagaiak, DDE eremua"

