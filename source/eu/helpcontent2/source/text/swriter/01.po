"par_id3147473\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/fldfuncpage/add\">Adds the <emph>Item</emph> to the list.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/add\"><emph>Elementua</emph> zerrendari gehitzen dio.</ahelp>"

#. ZkhHj
