"watermark.xhp\n"
"bm_id171516897713635\n"
"help.text"
msgid "<bookmark_value>watermark;text documents</bookmark_value>            <bookmark_value>watermark;page background</bookmark_value>            <bookmark_value>page background;watermark</bookmark_value>"
msgstr "<bookmark_value>ur-marka;testu-dokumentuak</bookmark_value>            <bookmark_value>ur-marka;orriaren atzeko planoa</bookmark_value>            <bookmark_value>orriaren atzeko planoa;ur-marka</bookmark_value>"

