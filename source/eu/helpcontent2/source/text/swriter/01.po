msgid "Item"
msgstr "Elementua"

#. Quiiv
#: 04090003.xhp
msgctxt ""
"04090003.xhp\n"
"par_id3150688\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/fldfuncpage/item\">Enter a new item.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/fldfuncpage/item\">Sartu elementu berria.</ahelp>"

#. KUsBa
#: 04090003.xhp
