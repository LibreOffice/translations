msgctxt ""
"01020000.xhp\n"
"par_id4159205\n"
"93\n"
"help.text"
msgid "Selects cells from the current cell to the first cell of the current row."
msgstr ""

#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"hd_id4149897\n"
"94\n"
"help.text"
msgid "Shift+End"
msgstr ""

#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"par_id4155095\n"
"95\n"
"help.text"
msgid "Selects cells from the current cell to the last cell of the current row."
msgstr ""

