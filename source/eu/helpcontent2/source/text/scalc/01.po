"04060106.xhp\n"
"par_id3157866\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_KUERZEN\">Truncates a number by removing decimal places.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_KUERZEN\">Zenbakia trunkatzen du, dezimalak kenduz.</ahelp>"

#. ueYCC
