msgctxt ""
"04060107.xhp\n"
"par_id3163556\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_SUMMEXMY2\">Adds the squares of the variance between corresponding values in two arrays.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_SUMMEXMY2\">Bi matrizeren karratuen kenduren batuketa egiten du.</ahelp>"

