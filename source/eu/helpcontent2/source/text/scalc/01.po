msgctxt ""
"04060105.xhp\n"
"par_id3147468\n"
"help.text"
msgid "<item type=\"input\">=NOT(FALSE())</item> returns TRUE"
msgstr "<item type=\"input\">=NOT(FALSE())</item>: TRUE ematen du"

#. 5Z3qV
#: 04060105.xhp
msgctxt ""
"04060105.xhp\n"
"bm_id3150141\n"
