"sf_menu.xhp\n"
"pyc_id331636727047102\n"
"help.text"
msgid "msg += f\"Item ID: {s_args[2]}\\n\""
msgstr "msg += f\"Elementuaren IDa: {s_args[2]}\\n\""

#. FPzcf
