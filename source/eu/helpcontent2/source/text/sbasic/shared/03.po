"sf_array.xhp\n"
"pyc_id511626979992999\n"
"help.text"
msgid "arr = svc.ImportFromCSVFile(r\"C:\\Temp\\myFile.csv\", dateformat = \"YYYY/MM/DD\")"
msgstr "arr = svc.ImportFromCSVFile(r\"C:\\Temp\\nireFitxategia.csv\", dateformat = \"YYYY/MM/DD\")"

#. asqiD
