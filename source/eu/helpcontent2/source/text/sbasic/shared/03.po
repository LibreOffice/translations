msgctxt ""
"sf_array.xhp\n"
"bas_id531585561482408\n"
"help.text"
msgid "arr = SF_Array.ImportFromCSVFile(\"C:\\Temp\\myFile.csv\", DateFormat := \"YYYY/MM/DD\")"
msgstr "arr = SF_Array.ImportFromCSVFile(\"C:\\Temp\\nireFitxategia.csv\", DateFormat := \"YYYY/MM/DD\")"

#. wyEqv
