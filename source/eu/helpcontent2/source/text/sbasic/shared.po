msgstr "<variable id=\"CByte_h1\"><link href=\"text/sbasic/shared/03120105.xhp\">CByte funtzioa</link></variable>"

#. qP4eK
#: 03120105.xhp
msgctxt ""
"03120105.xhp\n"
"par_id3143267\n"
"help.text"
msgid "Converts a string or a numeric expression to the <literal>Byte</literal> type."
msgstr "Kate bat edo zenbakizko adierazpen bat <literal>Byte</literal> motara bihurtzen du."

#. 8h2Wu
#: 03120105.xhp
msgctxt ""
"03120105.xhp\n"
"par_id3147573\n"
"help.text"
msgid "Cbyte( expression As Variant) As Byte"
msgstr "Cbyte( Adierazpena As Variant) As Byte"

#. 4vJi5
#: 03120105.xhp
msgctxt ""
"03120105.xhp\n"
"par_id3145068\n"
