msgid "<link href=\"text/simpress/01/05100000.xhp\" name=\"Styles and Formatting\">Styles and Formatting</link>"
msgstr "<link href=\"text/simpress/01/05100000.xhp\" name=\"Styles and Formatting\">Estiloak eta Formatua</link>"

