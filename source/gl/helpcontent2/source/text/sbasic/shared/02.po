#: 20000000.xhp#par_id3157979.32.help.text
msgid "<ahelp hid=\".uno:ProgressBar\">Adds a progress bar to the dialog.</ahelp>"
msgstr "<ahelp hid=\".uno:ProgressBar\">Engade unha barra de progreso á caixa de diálogo.</ahelp>"

