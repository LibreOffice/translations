"03030206.xhp\n"
"hd_id3145271\n"
"help.text"
msgid "Example:"
msgstr "Exemplo:"

#: 03030206.xhp
msgctxt ""
"03030206.xhp\n"
"par_id3149378\n"
"help.text"
msgid "a1 = \"start time\""
msgstr "a1 = \"hora inicial\""

#: 03030206.xhp
msgctxt ""
"03030206.xhp\n"
"par_id3145800\n"
"help.text"
msgid "b1 = \"end time\""
msgstr "b1 = \"hora final\""

#: 03030206.xhp
msgctxt ""
"03030206.xhp\n"
"par_id3151074\n"
"help.text"
msgid "c1 = \"total time\""
msgstr "c1 = \"tempo total\""

