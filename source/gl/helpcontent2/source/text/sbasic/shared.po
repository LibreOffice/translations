msgctxt ""
"03010201.xhp\n"
"par_id3151042\n"
"help.text"
msgid "MsgBox ( sText , 64, \"Confirmation of phrase\")"
msgstr "MsgBox ( sTexto , 64, \"Confirmación de frase\")"

