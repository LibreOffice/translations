msgctxt ""
"03030206.xhp\n"
"hd_id3153969\n"
"help.text"
msgid "Return value:"
msgstr "Valor de retorno:"

#: 03030206.xhp
msgctxt ""
"03030206.xhp\n"
"par_id3156424\n"
"help.text"
msgid "Date"
msgstr "Data"

