"par_id3150715\n"
"help.text"
msgid "Put #iNumber,, \"This is the third line of text\""
msgstr "Put #iNumber,, \"Esta é a terceira liña do texto\""

#: 03020201.xhp
msgctxt ""
"03020201.xhp\n"
"par_id3155938\n"
"help.text"
msgid "Put #iNumber,,\"This is a new text\""
msgstr "Put #iNumero,,\"Isto é un texto novo\""

