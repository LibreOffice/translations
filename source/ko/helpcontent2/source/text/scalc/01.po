msgctxt ""
"04060106.xhp\n"
"par_id3147525\n"
"help.text"
msgid "<item type=\"input\">=FACT(0)</item> returns 1."
msgstr "<item type=\"input\">=FACT(0)</item>는 1을 반환합니다."

