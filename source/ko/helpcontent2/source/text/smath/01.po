msgstr "<image id=\"img_id3145418\" src=\"starmath/res/op21401.png\" width=\"8.47mm\" height=\"8.47mm\"><alt id=\"alt_id3145418\" xml-lang=\"ko-KR\">아이콘</alt></image>"

