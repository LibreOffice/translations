msgid "<image id=\"img_id3155180\" src=\"media/helpimg/starmath/op22002.svg\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id3155180\">empty set Icon</alt></image>"
msgstr ""

#. NEL98
