msgstr "Виберіть <switchinline select=\"sys\"><caseinline select=\"MAC\"><item type=\"menuitem\">%PRODUCTNAME - Параметри</item></caseinline><defaultinline><item type=\"menuitem\">Засоби - Параметри</item></defaultinline></switchinline><item type=\"menuitem\"> - %PRODUCTNAME Impress - Загальне</item>"

