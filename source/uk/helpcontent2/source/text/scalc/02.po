msgctxt ""
"06030000.xhp\n"
"par_id231592663499228\n"
"help.text"
msgid "<image src=\"cmd/sc_ok.svg\" id=\"img_id521592663499228\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id171592663499228\">Accept Icon</alt></image>"
msgstr "<image src=\"cmd/sc_ok.svg\" id=\"img_id521592663499228\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id171592663499228\">Значок Прийняти</alt></image>"

#. iHTEW
#: 06030000.xhp
msgctxt ""
