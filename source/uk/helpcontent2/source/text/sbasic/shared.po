msgctxt ""
"03090404.xhp\n"
"par_id3148552\n"
"help.text"
msgid "End, End Function, End If, End Select, End Sub"
msgstr "End, End Function, End If, End Select, End Sub"

#: 03090404.xhp
msgctxt ""
"03090404.xhp\n"
