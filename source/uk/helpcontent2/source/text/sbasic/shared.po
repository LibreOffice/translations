"help.text"
msgid "If Mid(sText,iRun,1 )<> \" \" Then Mid( sText ,iRun, 1, Chr( 1 + Asc( Mid(sText,iRun,1 )) )"
msgstr "If Mid(sText,iRun,1 )<> \" \" Then Mid( sText ,iRun, 1, Chr( 1 + Asc( Mid(sText,iRun,1 )) )"

#: 03090203.xhp
msgctxt ""
"03090203.xhp\n"
"par_id3149665\n"
"15\n"
"help.text"
msgid "iRun = iRun + 1"
msgstr "iRun = iRun + 1"

#: 03090203.xhp
msgctxt ""
"03090203.xhp\n"
"par_id3152939\n"
"16\n"
"help.text"
msgid "Wend"
msgstr "Wend"

#: 03090203.xhp
msgctxt ""
"03090203.xhp\n"
"par_id3153189\n"
"17\n"
"help.text"
msgid "MsgBox sText,0,\"Text encoded\""
msgstr "MsgBox sText,0,\"Текст закодований\""

#: 03090203.xhp
msgctxt ""
"03090203.xhp\n"
"par_id3145251\n"
