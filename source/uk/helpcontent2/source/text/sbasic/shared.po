"03020104.xhp\n"
"par_id3148455\n"
"help.text"
msgid "Print #iNumber, \"This is a new line of text\""
msgstr "Print #iNumber, \"Це новий рядок тексту\""

#: 03020104.xhp
msgctxt ""
"03020104.xhp\n"
"par_id3163805\n"
"help.text"
msgid "MsgBox \"All files will be closed\",0,\"Error\""
msgstr "MsgBox \"Всі файли будуть закриті\",0,\"Помилка\""

