msgstr "<ahelp hid=\"modules/swriter/ui/inputwinmenu/|\">Розділяє елементи списку.</ahelp>"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3155817\n"
"help.text"
msgid "Example of using a list:"
msgstr "Приклад використання списку:"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3155830\n"
"help.text"
msgid "MIN 10|20|50|<C6>|<A2:B6>|20"
msgstr "MIN 10|20|50|<C6>|<A2:B6>|20"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3147012\n"
"help.text"
msgid "Equal"
msgstr "Equal"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3147034\n"
"help.text"
