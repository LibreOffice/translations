msgid "SQRT"
msgstr "SQRT"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3153062\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/inputwinmenu/sqrt\">Calculates the square root.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/inputwinmenu/sqrt\">Обчислює квадратний корінь.</ahelp>"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3153882\n"
"help.text"
msgid "Example: SQRT 25 displays 5.00"
msgstr "Приклад. Вираз \"SQRT 25\" виводить значення 5,00"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3153909\n"
"help.text"
msgid "Power"
msgstr "Ступінь"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3147124\n"
"help.text"
msgid "POW"
msgstr "POW"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3149768\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/inputwinmenu/pow\">Calculates the power of a number.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/inputwinmenu/pow\">Обчислює степінь числа.</ahelp>"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
