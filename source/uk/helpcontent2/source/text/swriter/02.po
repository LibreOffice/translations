msgstr "Сума"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3155312\n"
"help.text"
msgid "SUM"
msgstr "SUM"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3155335\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/inputwinmenu/sum\">Calculates the sum of the selected cells.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/inputwinmenu/sum\">Обчислює суму вибраних комірок.</ahelp>"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3154411\n"
"help.text"
msgid "Example: SUM <A2:C2> displays the sum of the values in cells A2 to C2"
msgstr "Приклад: SUM <A2:C2> показує суму значень у комірках з A2 до C2"

#: 14020000.xhp
msgctxt ""
"14020000.xhp\n"
"par_id3153381\n"
"help.text"
msgid "Round"
msgstr "Округлити"

