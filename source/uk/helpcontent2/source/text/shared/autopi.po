msgctxt ""
"01090200.xhp\n"
"par_idN1055A\n"
"help.text"
msgid "Sub form based on existing relation"
msgstr "Підформа на основі наявного зв'язку"

#: 01090200.xhp
msgctxt ""
"01090200.xhp\n"
"par_idN105C5\n"
"help.text"
msgid "<ahelp hid=\".\">Click to add a subform based on an existing relation.</ahelp>"
