#: 01040500.xhp
msgctxt ""
"01040500.xhp\n"
"par_idN105EF\n"
"help.text"
msgid "Move down"
msgstr "Пересунути вниз"

#: 01040500.xhp
msgctxt ""
"01040500.xhp\n"
"par_idN105F2\n"
"help.text"
msgid "<ahelp hid=\".\">Moves the current topic row down.</ahelp>"
msgstr "<ahelp hid=\".\">Переміщення поточного рядка з темою вниз.</ahelp>"

