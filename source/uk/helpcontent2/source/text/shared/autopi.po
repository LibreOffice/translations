msgctxt ""
"01090200.xhp\n"
"par_idN105B4\n"
"help.text"
msgid "<ahelp hid=\".\">Select to add a subform.</ahelp>"
msgstr "<ahelp hid=\".\">Виберіть, щоб додати підформу.</ahelp>"

