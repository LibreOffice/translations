msgstr "<link href=\"text/shared/01/05340100.xhp\" name=\"Висота рядків\">Висота рядків</link>"

#: 05340400.xhp
msgctxt ""
"05340400.xhp\n"
"hd_id3150321\n"
"help.text"
msgid "<link href=\"text/shared/01/05340405.xhp\" name=\"Column Format\">Column Format</link>"
