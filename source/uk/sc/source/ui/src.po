msgctxt ""
"scfuncs.src\n"
"RID_SC_FUNCTION_DESCRIPTIONS2.SC_OPCODE_H_LOOKUP\n"
"6\n"
"string.text"
msgid "Index"
msgstr "Індекс"

#: scfuncs.src
msgctxt ""
"scfuncs.src\n"
"RID_SC_FUNCTION_DESCRIPTIONS2.SC_OPCODE_H_LOOKUP\n"
