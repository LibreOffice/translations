"par_id3149122\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/sheetprintpage/checkBTN_DRAWINGS\">Includes all drawing objects in the printed document.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/sheetprintpage/checkBTN_DRAWINGS\">Apima brėžinius spausdinamame dokumente.</ahelp>"

