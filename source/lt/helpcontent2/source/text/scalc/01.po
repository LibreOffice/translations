msgstr "Objektai ir paveikslai"

#. cCDGR
#: 05070500.xhp
msgctxt ""
"05070500.xhp\n"
"par_id3149581\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/sheetprintpage/checkBTN_OBJECTS\">Includes all inserted objects (if printable) and graphics with the printed document.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/sheetprintpage/checkBTN_OBJECTS\">Apima visus įterptus objektus (jei spausdinama) ir paveikslus.</ahelp>"

#. gdAEq
