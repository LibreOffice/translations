msgctxt ""
"04060185.xhp\n"
"par_id3144759\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_VARIATIONEN2\">Returns the number of permutations for a given number of objects (repetition allowed).</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_VARIATIONEN2\">Grąžina duotų objektų perstatymų skaičių (su pasikartojimais).</ahelp>"

