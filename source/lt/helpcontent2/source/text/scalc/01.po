msgctxt ""
"04060181.xhp\n"
"bm_id2848690\n"
"help.text"
msgid "<bookmark_value>CHISQ.DIST function</bookmark_value>"
msgstr "<bookmark_value>CHISQ.DIST funkcija</bookmark_value>"

#. FEGDg
