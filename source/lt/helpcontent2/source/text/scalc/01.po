msgctxt ""
"04060106.xhp\n"
"par_id3154524\n"
"help.text"
msgid "GCD(Integer1; Integer2; ...; Integer30)"
msgstr "GCD(1 sveikas; 2 sveikas; ...; 30 sveikas)"

