"help.text"
msgid "<item type=\"input\">=MINIFS(C2:C6;A2:A6;\".*\"&E2;B2:B6;\"<\"&MAX(B2:B6))</item>"
msgstr "<item type=\"input\">=MINIFS(C2:C6;A2:A6;\".*\"&E2;B2:B6;\"<\"&MAX(B2:B6))</item>"

#. cxmqK
