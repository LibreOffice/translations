msgstr "Tab"

#. if8Ns
#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"par_id3148804\n"
"help.text"
msgid "Cycles through the objects on the page in the order in which they were created."
msgstr "Einama per lapo objektus ta tvarka, kokia objektai buvo sukurti."

#. uD9Ei
#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"hd_id3145410\n"
"help.text"
