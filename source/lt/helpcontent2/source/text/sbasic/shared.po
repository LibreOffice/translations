msgctxt ""
"01170103.xhp\n"
"par_id3153305\n"
"help.text"
msgid "<ahelp hid=\"HID_EVT_FOCUSLOST\">This event takes place if a control loses the focus.</ahelp>"
msgstr "<ahelp hid=\"HID_EVT_FOCUSLOST\">Šis įvykis vyksta, jei valdiklis praranda židinį.</ahelp>"

