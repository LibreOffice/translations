#: 06100000.xhp
msgctxt ""
"06100000.xhp\n"
"hd_id3149029\n"
"help.text"
msgid "Keys 1 to 3"
msgstr "Rikiuotės 1–3"

#. vGcgi
#: 06100000.xhp
msgctxt ""
"06100000.xhp\n"
"par_id3147170\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/sortdialog/key3\">Specifies additional sorting criteria. You can also combine sort keys.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/sortdialog/key3\">Apibrėžia papildomą rikiavimo kriterijų. Taip pat galite sujungti rikiavimo raktus.</ahelp>"

#. StLHG
#: 06100000.xhp
msgctxt ""
"06100000.xhp\n"
"hd_id3147565\n"
"help.text"
msgid "Column 1 to 3"
msgstr "Stulpeliai 1–3"

