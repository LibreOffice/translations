msgctxt ""
"06090000.xhp\n"
"par_id3149027\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/converttexttable/tabs\">Converts the text to a table using tabs as column markers.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/converttexttable/tabs\">Paverčia tekstą į lentelę, naudojant skirtukus kaip stulpelio žymeklius.</ahelp>"

#. fe37j
