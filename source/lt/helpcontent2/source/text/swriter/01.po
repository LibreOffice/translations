msgctxt ""
"05090201.xhp\n"
"par_id3154196\n"
"help.text"
msgid "<link href=\"text/swriter/main0204.xhp\" name=\"Table Bar\">Table Bar</link>"
msgstr "<link href=\"text/swriter/main0204.xhp\" name=\"Lentelės mygtukų juosta\">Lentelės mygtukų juosta</link>"

