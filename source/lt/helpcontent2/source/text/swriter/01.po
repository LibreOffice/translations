#: 06100000.xhp
msgctxt ""
"06100000.xhp\n"
"hd_id3151177\n"
"help.text"
msgid "Order"
msgstr "Rikiavimas"

#. oSn3c
#: 06100000.xhp
msgctxt ""
"06100000.xhp\n"
"hd_id072020090105453\n"
"help.text"
msgid "Ascending"
msgstr "Didėjančiai"

#. SEkEJ
#: 06100000.xhp
msgctxt ""
"06100000.xhp\n"
"par_id3154270\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/sortdialog/up3\">Sorts in ascending order, (for example, 1, 2, 3 or a, b, c).</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/sortdialog/up3\">Rikiuoja didėjančia tvarka, (pavyzdžiui, 1, 2, 3 ar a, b, c).</ahelp>"

#. ivmBV
#: 06100000.xhp
msgctxt ""
"06100000.xhp\n"
"hd_id3150708\n"
"help.text"
msgid "Descending"
msgstr "Mažėjančiai"

#. zV5AE
#: 06100000.xhp
msgctxt ""
"06100000.xhp\n"
"par_id3152946\n"
"help.text"
