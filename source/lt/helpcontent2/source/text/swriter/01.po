"par_id3152963\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/columnpage/width3mf\">Enter the width of the column.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/columnpage/width3mf\">Įveskite stulpelio plotį.</ahelp>"

#. QEcCN
