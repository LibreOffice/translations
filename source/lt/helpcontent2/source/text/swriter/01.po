"04120100.xhp\n"
"par_id3154777\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/indexentry/close\">Closes the dialog.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/indexentry/close\">Užveria dialogo langą.</ahelp>"

#. 7EwbB
