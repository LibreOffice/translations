msgid "From database"
msgstr "Iš duomenų bazės"

#. FU4av
#: 12070100.xhp
msgctxt ""
"12070100.xhp\n"
"par_id3149415\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/insertdbcolumnsdialog/fromdatabase\">Accepts the database formats.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/insertdbcolumnsdialog/fromdatabase\">Pritaikomas duomenų bazės formatas.</ahelp>"

#. CXcsk
