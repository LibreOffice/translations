"par_idN10549\n"
"help.text"
msgid "<variable id=\"macrosecurity_ts\"><link href=\"text/shared/optionen/macrosecurity_ts.xhp\">Trusted Sources</link></variable>"
msgstr "<variable id=\"macrosecurity_ts\"><link href=\"text/shared/optionen/macrosecurity_ts.xhp\">Patikimi šaltiniai</link></variable>"

#. EDvrj
