#. 5toQe
#: navpane_on.xhp
msgctxt ""
"navpane_on.xhp\n"
"par_id3147571\n"
"help.text"
msgid "In the Help window, you can show or hide the navigation pane as needed."
msgstr "Žinyno lange žvalgymo polangį galima parodyti arba paslėpti."

#. xVYxF
#: navpane_on.xhp
msgctxt ""
"navpane_on.xhp\n"
"par_id3156411\n"
