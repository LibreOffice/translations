msgstr ""

#. o6aBw
#: keyboard.xhp
msgctxt ""
"keyboard.xhp\n"
"par_id3150650\n"
"help.text"
msgid "To start a slide show from the beginning, press <keycode>F5</keycode>."
msgstr ""

#. WaqYV
#: keyboard.xhp
msgctxt ""
"keyboard.xhp\n"
"par_id911616435506012\n"
