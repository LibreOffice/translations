"par_idN10544\n"
"help.text"
msgid "<variable id=\"timing\"><link href=\"text/simpress/01/effectoptionstiming.xhp\">Timing</link></variable>"
msgstr "<variable id=\"timing\"><link href=\"text/simpress/01/effectoptionstiming.xhp\">Laikas</link></variable>"

