msgctxt "SC_OPCODE_CONCAT"
msgid "Text"
msgstr "Tekst"

#: sc/inc/scfuncs.hrc:3570
msgctxt "SC_OPCODE_CONCAT"
msgid "Text for the concatenation."
msgstr "Tekst voor de samenvoeging."

#: sc/inc/scfuncs.hrc:3576
msgctxt "SC_OPCODE_CONCAT_MS"
