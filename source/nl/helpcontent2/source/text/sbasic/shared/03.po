"help.text"
msgid "newrange = oDoc.CompactLeft(\"Sheet1.G1:L10\", FilterFormula := \"=(MOD(SUM(G1:G10);2)=1)\")"
msgstr "newrange = oDoc.CompactLeft(\"Blad1.G1:L10\", FilterFormula := \"=(MOD(SUM(G1:G10);2)=1)\")"

