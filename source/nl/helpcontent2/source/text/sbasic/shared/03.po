msgctxt ""
"sf_calc.xhp\n"
"pyc_id201652272121176\n"
"help.text"
msgid "newrange = myDoc.CompactLeft(\"Sheet1.G1:L10\", wholecolumn = True)"
msgstr "newrange = myDoc.CompactLeft(\"Blad1.G1:L10\", wholecolumn = True)"

#. pBca4
