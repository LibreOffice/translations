msgctxt ""
"03050500.xhp\n"
"par_id3146916\n"
"help.text"
msgid "MsgBox \"All files will be closed\",  0,  \"Error\""
msgstr "MsgBox \"Alle bestanden worden gesloten\",  0,  \"Error\""

