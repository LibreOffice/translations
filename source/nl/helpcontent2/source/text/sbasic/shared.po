msgid "Return value:"
msgstr "Teruggegeven waarde:"

#: 03070600.xhp#par_id3153380.6.help.text
msgctxt "03070600.xhp#par_id3153380.6.help.text"
msgid "Integer"
msgstr "Integer"

#: 03070600.xhp#hd_id3154365.7.help.text
msgctxt "03070600.xhp#hd_id3154365.7.help.text"
