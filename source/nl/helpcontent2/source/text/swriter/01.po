"par_id6501968\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Edit the value.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Bewerk de waarde.</ahelp>"

