msgid "Name"
msgstr "Naam"

#. dtVMk
#: 05090100.xhp
msgctxt ""
"05090100.xhp\n"
"par_id3145244\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/formattablepage/name\">Enter an internal name for the table. You can use this name to quickly locate the table in the Navigator.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/formattablepage/name\">Voer een interne naam voor de tabel in. U kunt deze naam gebruiken om de tabel snel in de Navigator te kunnen vinden.</ahelp>"

