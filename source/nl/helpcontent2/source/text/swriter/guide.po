msgstr "<bookmark_value>regelnummers</bookmark_value>    <bookmark_value>tekst; regelnummers</bookmark_value>    <bookmark_value>alinea's;regelnummers</bookmark_value>    <bookmark_value>tekstregels; nummering</bookmark_value>    <bookmark_value>nummering; regels</bookmark_value>    <bookmark_value>nummers; regelnummering</bookmark_value>    <bookmark_value>randnummers op tekstpagina's</bookmark_value>"

