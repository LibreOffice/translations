msgctxt ""
"05000003.xhp\n"
"par_id3147243\n"
"help.text"
msgid "<ahelp hid=\"dbaccess/ui/password/newpassword\">Enter the new password.</ahelp>"
msgstr "<ahelp hid=\"dbaccess/ui/password/newpassword\">Voer het nieuwe wachtwoord in.</ahelp>"

