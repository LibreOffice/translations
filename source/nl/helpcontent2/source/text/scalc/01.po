msgctxt ""
"04060185.xhp\n"
"hd_id3154930\n"
"help.text"
msgid "TDIST"
msgstr "TVERDELING"

#: 04060185.xhp
msgctxt ""
"04060185.xhp\n"
"par_id3153372\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_TVERT\">Returns the t-distribution.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_TVERT\">Geeft als resultaat de t-verdeling.</ahelp>"

