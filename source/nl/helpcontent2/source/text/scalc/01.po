msgctxt ""
"05070500.xhp\n"
"hd_id3150887\n"
"help.text"
msgid "First page number"
msgstr "Nummer eerste pagina"

#. oAziQ
#: 05070500.xhp
msgctxt ""
"05070500.xhp\n"
"par_id3155378\n"
"help.text"
msgid "<ahelp hid=\"modules/scalc/ui/sheetprintpage/checkBTN_PAGENO\">Select this option if you want this style to restart page numbering.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/sheetprintpage/checkBTN_PAGENO\">Selecteer deze optie als u wilt dat deze stijl de paginanummering opnieuw start.</ahelp>"

#. gVwLk
