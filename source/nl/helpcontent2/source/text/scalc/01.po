msgctxt ""
"04060106.xhp\n"
"par_id3109841\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_LOG\">Returns the logarithm of a number to the specified base.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_LOG\">Geeft het logaritme van een getal tot de gespecificeerde basis.</ahelp>"

