msgstr "WEBSERVICE"

#: func_webservice.xhp
msgctxt ""
"func_webservice.xhp\n"
"par_id3149893\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_WEBSERVICE\">Get some web content from a URI.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_WEBSERVICE\">Web-inhoud van een URI verkrijgen.</ahelp>"

#: func_webservice.xhp
msgctxt ""
"func_webservice.xhp\n"
"hd_id3146944\n"
"help.text"
