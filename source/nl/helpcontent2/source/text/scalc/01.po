msgstr "Syntaxis"

#: func_webservice.xhp
msgctxt ""
"func_webservice.xhp\n"
"par_id3154844\n"
"help.text"
msgid "WEBSERVICE(URI)"
msgstr "WEBSERVICE(URL)"

#: func_webservice.xhp
msgctxt ""
"func_webservice.xhp\n"
"par_id3147469\n"
"help.text"
msgid "<emph>URI: </emph> URI text of the web service."
msgstr "<emph>URL: </emph> URL-tekst van de webservice."

#: func_webservice.xhp
msgctxt ""
"func_webservice.xhp\n"
"hd_id3150141\n"
"help.text"
