msgctxt ""
"func_floor.xhp\n"
"par_id761586287595376\n"
"help.text"
msgid "<input>=FLOOR(3.45)</input> returns 3."
msgstr "<input>=ONDERGRENS(3,45)</input> retourneert 3."

