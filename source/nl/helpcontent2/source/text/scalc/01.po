msgctxt ""
"04060182.xhp\n"
"par_id3153068\n"
"help.text"
msgid "FINV(Number; DegreesFreedom1; DegreesFreedom2)"
msgstr "FINVERSE(Getal; Vrijheidsgraden1; Vrijheidsgraden2)"

