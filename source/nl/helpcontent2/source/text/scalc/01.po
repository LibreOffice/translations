msgstr "<item type=\"literal\">UNICHAR(getal)</item>"

#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"hd_id0907200904123720\n"
"help.text"
msgid "Example"
msgstr "Voorbeeld"

#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id090720090412378\n"
"help.text"
msgid "=UNICHAR(169) returns the Copyright character <emph>©</emph>."
msgstr "=UNITEKEN(169) geeft het Copyright karakter <emph>©</emph>."

#: 04060110.xhp
msgctxt ""
"04060110.xhp\n"
"par_id050220170755399756\n"
"help.text"
