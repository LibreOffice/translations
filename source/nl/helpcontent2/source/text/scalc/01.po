"04060184.xhp\n"
"hd_id3145824\n"
"help.text"
msgid "<variable id=\"average_head\"><link href=\"text/scalc/01/04060184.xhp#average\">AVERAGE</link></variable>"
msgstr "<variable id=\"average_head\"><link href=\"text/scalc/01/04060184.xhp#average\">GEMIDDELDE</link></variable>"

