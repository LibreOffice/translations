msgctxt ""
"func_floor.xhp\n"
"par_id981586291388900\n"
"help.text"
msgid "<input>=FLOOR.PRECISE(3.45)</input> returns 3."
msgstr "<input>=ONDERGRENS.PRECIES(3,45)</input> retourneert 3."

