"help.text"
msgid "NOW"
msgstr "NU"

#: func_now.xhp
msgctxt ""
"func_now.xhp\n"
"bm_id3150521\n"
"help.text"
msgid "<bookmark_value>NOW function</bookmark_value>"
msgstr "<bookmark_value>NU-functie</bookmark_value>"

#: func_now.xhp
msgctxt ""
