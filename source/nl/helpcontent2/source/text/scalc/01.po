msgid "<variable id=\"freezecellsh1\"><link href=\"text/scalc/01/07090100.xhp\">Freeze Cells</link></variable>"
msgstr "<variable id=\"freezecellsh1\"><link href=\"text/scalc/01/07090100.xhp\">Cellen vastzetten</link></variable>"

#. sWGWB
#: 07090100.xhp
msgctxt ""
"07090100.xhp\n"
"par_id481612313262514\n"
"help.text"
msgid "Freezes the first column or the first row of the current spreadsheet."
msgstr "Zet de eerste kolom of de eerste rij van het huidige werkblad vast."

#. ozNTG
#: 12010000.xhp
msgctxt ""
"12010000.xhp\n"
"tit\n"
"help.text"
msgid "Define Database Range"
msgstr "Databasebereik definiëren"

#. AgxXS
#: 12010000.xhp
msgctxt ""
"12010000.xhp\n"
"hd_id3157909\n"
"help.text"
msgid "<link href=\"text/scalc/01/12010000.xhp\">Define Range</link>"
msgstr "<link href=\"text/scalc/01/12010000.xhp\">Bereik definiëren</link>"

#. 8zFH5
#: 12010000.xhp
msgctxt ""
"12010000.xhp\n"
"par_id3155922\n"
