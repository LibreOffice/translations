msgid "<item type=\"input\">=T.DIST.2T(1; 10)</item> returns 0.3408931323."
msgstr "<item type=\"input\">=T.DIST.RT(1; 10)</item> geeft 0.1704465662."

#: 04060185.xhp
msgctxt ""
"04060185.xhp\n"
"bm_id274930\n"
"help.text"
msgid "<bookmark_value>T.DIST.RT function</bookmark_value>        <bookmark_value>right tailed t-distribution</bookmark_value>"
msgstr "<bookmark_value>T.VERD.RZ-functie</bookmark_value>         <bookmark_value>rechtszijdige t-verdeling</bookmark_value>"

#: 04060185.xhp
msgctxt ""
"04060185.xhp\n"
"hd_id274930\n"
"help.text"
msgid "T.DIST.RT"
msgstr "T.DIST.RT"

#: 04060185.xhp
