msgctxt ""
"04060103.xhp\n"
"par_id3152811\n"
"help.text"
msgid "<item type=\"input\">=SUMIF(E2:E11;A13)</item>"
msgstr "<item type=\"input\">=SOM.ALS(E2:E11;A13)</item>"

