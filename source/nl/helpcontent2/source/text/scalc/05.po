msgctxt ""
"empty_cells.xhp\n"
"par_id8277230\n"
"help.text"
msgid "A1: 1 B1: <Empty> C1: =B1 (displays 0)"
msgstr "A1: 1 B1: <Empty> C1: =B1 (geeft 0 weer)"

#: empty_cells.xhp
msgctxt ""
"empty_cells.xhp\n"
"par_id4086428\n"
"help.text"
msgid "=B1=0 => TRUE"
msgstr "=B1=0 => WAAR"

#: empty_cells.xhp
msgctxt ""
"empty_cells.xhp\n"
"par_id9024628\n"
"help.text"
