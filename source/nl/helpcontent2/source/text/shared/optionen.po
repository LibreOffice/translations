msgctxt ""
"01060500.xhp\n"
"par_id3151041\n"
"12\n"
"help.text"
msgid "Selling Price"
msgstr "Verkoopprijs"

#: 01060500.xhp
msgctxt ""
"01060500.xhp\n"
"par_id3159149\n"
"13\n"
"help.text"
msgid "100"
msgstr "100"

