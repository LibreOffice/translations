msgctxt ""
"05210600.xhp\n"
"par_id3146847\n"
"10\n"
"help.text"
msgid "<ahelp hid=\"SVX:METRICFIELD:RID_SVXPAGE_SHADOW:MTR_FLD_DISTANCE\">Enter the distance that you want the shadow to be offset from the selected object.</ahelp>"
msgstr "<ahelp hid=\"SVX:METRICFIELD:RID_SVXPAGE_SHADOW:MTR_FLD_DISTANCE\">Voer de afstand in waarmee u de schaduw van het geselecteerde object wilt verschuiven.</ahelp>"

#: 05210600.xhp
msgctxt ""
"05210600.xhp\n"
"hd_id3150276\n"
"11\n"
"help.text"
msgid "Color"
msgstr "Kleur"

