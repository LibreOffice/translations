msgctxt ""
"05020301.xhp\n"
"par_id3154630\n"
"29\n"
"help.text"
msgid "CYAN"
msgstr "Cyaan"

#: 05020301.xhp
msgctxt ""
"05020301.xhp\n"
"par_id3148676\n"
"30\n"
"help.text"
msgid "GREEN"
msgstr "Groen"

#: 05020301.xhp
msgctxt ""
"05020301.xhp\n"
"par_id3154123\n"
"31\n"
"help.text"
msgid "BLACK"
msgstr "Zwart"

#: 05020301.xhp
msgctxt ""
"05020301.xhp\n"
"par_id3149167\n"
"32\n"
"help.text"
msgid "BLUE"
msgstr "Blauw"

#: 05020301.xhp
msgctxt ""
"05020301.xhp\n"
"par_id3158407\n"
"33\n"
"help.text"
msgid "MAGENTA"
msgstr "MAGENTA"

