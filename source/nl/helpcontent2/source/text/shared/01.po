msgctxt ""
"01070000.xhp\n"
"par_id3154068\n"
"43\n"
"help.text"
msgid "<ahelp hid=\"HID_FILESAVE_DOSAVE\">Saves the file.</ahelp>"
msgstr "<ahelp hid=\"HID_FILESAVE_DOSAVE\">Slaat het bestand op.</ahelp>"

#: 01070000.xhp
msgctxt ""
"01070000.xhp\n"
"hd_id3145744\n"
