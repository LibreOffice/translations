"rep_main.xhp\n"
"par_id9611499\n"
"help.text"
msgid "<ahelp hid=\".\" visibility=\"hidden\">Inserts a horizontal line to the current area.</ahelp>"
msgstr "<ahelp hid=\".\" visibility=\"hidden\">Haawiittimmate xuruura muli qarqarira surkanno.</ahelp>"

