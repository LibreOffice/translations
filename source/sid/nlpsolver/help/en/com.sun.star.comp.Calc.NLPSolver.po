msgstr "Horo"

#: Usage.xhp
msgctxt ""
"Usage.xhp\n"
"bm_id0603200910434044_scalc\n"
"help.text"
msgid "<bookmark_value>Solver for Nonlinear Problems;Usage</bookmark_value>"
msgstr "<bookmark_value>Linere ikkitinokki xa'muwa tiraancho;Horo</bookmark_value>"

