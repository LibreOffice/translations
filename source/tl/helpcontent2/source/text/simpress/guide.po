msgid "<variable id=\"main\"><link href=\"text/simpress/guide/main.xhp\">Instructions for Using $[officename] Impress</link></variable>"
msgstr "<variable id=\"main\"><link href=\"text/simpress/guide/main.xhp\">Mga Tagubilin sa Paggamit ng $[officename] Impress</link></variable>"

#. fgMfa
