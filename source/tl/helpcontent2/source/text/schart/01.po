msgid "Title"
msgstr "Pamagat"

#. aDGQ3
#: 04010000.xhp
msgctxt ""
"04010000.xhp\n"
"par_id3150371\n"
"help.text"
msgid "<ahelp hid=\"modules/schart/ui/inserttitledlg/maintitle\">Enter the desired title for the chart.</ahelp> This will be displayed at the top of the chart."
msgstr "<ahelp hid=\"modules/schart/ui/inserttitledlg/maintitle\">Ilagay ang gustong pamagat para sa chart.</ahelp> Ito ay ipapakita sa tuktok ng tsart."

#. PB2Ds
