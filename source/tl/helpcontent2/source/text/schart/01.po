#. tGRyh
#: 04020000.xhp
msgctxt ""
"04020000.xhp\n"
"par_id3155413\n"
"help.text"
msgid "<variable id=\"legende\"><ahelp hid=\".uno:InsertMenuLegend\">Opens the <emph>Legend </emph>dialog, which allows you to change the position of legends in the chart, and to specify whether the legend is displayed.</ahelp></variable>"
msgstr "<variable id=\"legende\"><ahelp hid=\".uno:InsertMenuLegend\">Binubuksan ang<emph> Alamat</emph> dialog, na nagpapahintulot sa iyo na baguhin ang posisyon ng mga alamat sa chart, at upang tukuyin kung ang alamat ay ipinapakita.</ahelp></variable>"

#. nyEeY
#: 04020000.xhp
msgctxt ""
"04020000.xhp\n"
"par_id3149124\n"
"help.text"
msgid "<variable id=\"sytextlegende\"><ahelp hid=\".uno:ToggleLegend\">To show or hide a legend, click <emph>Legend On/Off</emph> on the <emph>Formatting</emph> bar.</ahelp></variable>"
msgstr "<variable id=\"sytextlegende\"><ahelp hid=\".uno:ToggleLegend\">To show or hide a legend, click <emph>Legend On/Off</emph> on the <emph>Formatting</emph> bar.</ahelp></variable>"

