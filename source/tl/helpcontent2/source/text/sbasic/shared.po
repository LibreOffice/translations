msgstr "<variable id=\"Write_h1\"><link href=\"text/sbasic/shared/03020205.xhp\">Sumulat ng # Pahayag</link></variable>"

#. G4X6k
#: 03020205.xhp
msgctxt ""
"03020205.xhp\n"
"par_id3154685\n"
"help.text"
msgid "Writes data to a sequential text file with delimiting characters."
msgstr "Nagsusulat ng data sa isang sequential text file na may mga character na nagde-delimiting."

#. sDFaP
#: 03020205.xhp
msgctxt ""
"03020205.xhp\n"
