"help.text"
msgid "<image id=\"img_id5605334\" src=\"cmd/lc_addfield.svg\" width=\"1cm\" height=\"1cm\" localize=\"true\"><alt id=\"alt_id5605334\">Icon</alt></image>"
msgstr "<image id=\"img_id5605334\" src=\"cmd/lc_addfield.svg\" width=\"1cm\" height=\"1cm\" localize=\"true\"><alt id=\"alt_id5605334\">Icon</alt></image>"

