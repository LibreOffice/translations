msgctxt ""
"05010000.xhp\n"
"par_id3153716\n"
"help.text"
msgid "<variable id=\"schriftartentext\"><ahelp hid=\"modules/smath/ui/fonttypedialog/FontsDialog\">Defines the fonts that can be applied to formula elements.</ahelp></variable>"
msgstr "<variable id=\"schriftartentext\"><ahelp hid=\"modules/smath/ui/fonttypedialog/FontsDialog\">Tinutukoy ang mga font na maaaring ilapat sa mga elemento ng formula.</ahelp></variable>"

#. ug9De
#: 05010000.xhp
msgctxt ""
