"par_idN10A56\n"
"help.text"
msgid "<link href=\"text/shared/02/02160000.xhp\">Character Highlighting Color icon</link>"
msgstr "<link href=\"text/shared/02/02160000.xhp\">Icon ng Kulay ng Pag-highlight ng Character</link>"

