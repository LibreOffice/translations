msgstr "<variable id=\"bearbeitensonderzeichen\"><ahelp hid=\"modules/swriter/ui/insertfootnote/choosecharacter\">Mga pagsingit a<link href=\"text/shared/01/04100000.xhp\"> espesyal na karakter</link> bilang isang footnote o endnote anchor.</ahelp></variable>"

