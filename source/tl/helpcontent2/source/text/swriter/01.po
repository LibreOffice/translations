"hd_id3153644\n"
"help.text"
msgid "Endnote"
msgstr "Endnote"

#. KBmnh
#: 04030000.xhp
msgctxt ""
"04030000.xhp\n"
"par_id3152770\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/insertfootnote/endnote\">Inserts an endnote anchor at the current cursor position in the document, and adds an endnote at the end of the document.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/insertfootnote/endnote\">Naglalagay ng endnote anchor sa kasalukuyang posisyon ng cursor sa dokumento, at nagdaragdag ng endnote sa dulo ng dokumento.</ahelp>"

