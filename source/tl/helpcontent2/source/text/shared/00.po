msgctxt ""
"00000005.xhp\n"
"bm_id3149922\n"
"help.text"
msgid "<bookmark_value>SQL;definition</bookmark_value>"
msgstr "<bookmark_value>SQL;definition</bookmark_value>"

#. 4BE2R
