msgid "<ahelp hid=\"modules/scalc/ui/imoptdialog/charsetdropdown\">Specifies the character set for text export.</ahelp>"
msgstr "<ahelp hid=\"modules/scalc/ui/imoptdialog/charsetdropdown\">Tinutukoy ang set ng character para sa pag-export ng text.</ahelp>"

#. 58FUR
#: 00000207.xhp
msgctxt ""
