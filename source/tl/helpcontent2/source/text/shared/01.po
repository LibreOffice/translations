msgctxt ""
"image_compression.xhp\n"
"par_id111692800566619\n"
"help.text"
msgid "Choose <menuitem>Format - Image - Compress</menuitem>."
msgstr "Pumili<menuitem> Format - Larawan - I-compress</menuitem> ."

