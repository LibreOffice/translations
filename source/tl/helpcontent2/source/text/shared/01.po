msgstr "<variable id=\"allestext\"><ahelp hid=\".\">Pinipili ang buong nilalaman ng kasalukuyang file, frame, o text object.</ahelp></variable>"

#. rvjqF
#: 02090000.xhp
msgctxt ""
"02090000.xhp\n"
"par_id3155261\n"
"help.text"
msgid "To select all of the cells on a sheet, click the button at the intersection of the column and row header in the top left corner of the sheet."
msgstr "Upang piliin ang lahat ng mga cell sa isang sheet, i-click ang button sa intersection ng column at row header sa kaliwang sulok sa itaas ng sheet."

#. 452c4
