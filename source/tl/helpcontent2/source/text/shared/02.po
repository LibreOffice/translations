"par_id3149415\n"
"help.text"
msgid "<ahelp hid=\"modules/swriter/ui/insertdbcolumnsdialog/fromdatabase\">Accepts the database formats.</ahelp>"
msgstr "<ahelp hid=\"modules/swriter/ui/insertdbcolumnsdialog/fromdatabase\">Tumatanggap ng mga format ng database.</ahelp>"

#. CXcsk
