msgctxt ""
"01170004.xhp\n"
"par_id3157958\n"
"help.text"
msgid "<ahelp hid=\"SID_FM_DELETECOL\">Deletes the currently selected column.</ahelp>"
msgstr "<ahelp hid=\"SID_FM_DELETECOL\">Tinatanggal ang kasalukuyang napiling column.</ahelp>"

#. FKguu
