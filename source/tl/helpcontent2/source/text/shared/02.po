"par_id3154288\n"
"help.text"
msgid "<image id=\"img_id3153683\" src=\"cmd/lc_insertdraw.svg\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id3153683\">Icon Show Draw Functions</alt></image>"
msgstr "<image id=\"img_id3153683\" src=\"cmd/lc_insertdraw.svg\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id3153683\">Icon Show Draw Function</alt></image>"

#. DCjLk
