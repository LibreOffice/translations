msgctxt ""
"scenario.xhp\n"
"par_id3154484\n"
"help.text"
msgid "<link href=\"text/scalc/01/06050000.xhp\">Creating Scenarios</link>"
msgstr "<link href=\"text/scalc/01/06050000.xhp\">Paggawa ng mga Sitwasyon</link>"

#. AG6Vf
