msgid "Shift+End"
msgstr "Shift+End"

#. Km3hp
#: 01020000.xhp
msgctxt ""
"01020000.xhp\n"
"par_id4155095\n"
"help.text"
msgid "Selects all cells from the current cell to the last column that contains data in any row."
msgstr "Pinipili ang lahat ng mga cell mula sa kasalukuyang cell hanggang sa huling column na naglalaman ng data sa anumang row."

#. 8bBCM
#: 01020000.xhp
msgctxt ""
