"04060107.xhp\n"
"par_id3155276\n"
"help.text"
msgid "<ahelp hid=\"HID_FUNC_MTRANS\">Transposes the rows and columns of an array.</ahelp>"
msgstr "<ahelp hid=\"HID_FUNC_MTRANS\">Binabago ang mga row at column ng isang array.</ahelp>"

#. yX4Fn
