msgctxt ""
"04060105.xhp\n"
"par_id3083444\n"
"help.text"
msgid "<item type=\"input\">=OR(A;B)</item> returns TRUE"
msgstr "<item type=\"input\">=O(A;B)</item> nagbabalik ng TOTOO"

#. BcGUg
#: 04060105.xhp
msgctxt ""
"04060105.xhp\n"
"par_id3154314\n"
"help.text"
msgid "<item type=\"input\">=NOT(AND(A;B))</item> returns TRUE"
msgstr "<item type=\"input\">=HINDI(AT(A;B))</item> nagbabalik ng TOTOO"

#. 7bq9r
#: 04060105.xhp
msgctxt ""
"04060105.xhp\n"
