msgctxt ""
"func_fourier.xhp\n"
"par_id411677015788900\n"
"help.text"
msgid "<link href=\"https://wiki.documentfoundation.org/Documentation/Calc_Functions/FOURIER\">FOURIER wiki page</link>."
msgstr "<link href=\"https://wiki.documentfoundation.org/Documentation/Calc_Functions/FOURIER\">FOURIER wiki na pahina</link> ."

