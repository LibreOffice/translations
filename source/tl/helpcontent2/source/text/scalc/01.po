"04060000.xhp\n"
"par_id3155440\n"
"help.text"
msgid "<ahelp hid=\"formula/ui/functionpage/search\">Search for a part of the function name.</ahelp>"
msgstr "<ahelp hid=\"formula/ui/functionpage/search\">Maghanap ng bahagi ng pangalan ng function.</ahelp>"

#. eFtXe
