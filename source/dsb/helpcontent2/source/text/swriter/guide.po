#. cYkTc
#: hidden_text.xhp
msgctxt ""
"hidden_text.xhp\n"
"par_id3155325\n"
"help.text"
msgid "Click \"Hidden Text\" in the <emph>Type </emph>list."
msgstr "Klikniśo na „Schowany tekst“ w lisćinje <emph>Typ</emph>."

#. wTBWp
#: hidden_text.xhp
msgctxt ""
"hidden_text.xhp\n"
"par_id3154404\n"
