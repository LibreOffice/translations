#. 6mozi
#: section_insert.xhp
msgctxt ""
"section_insert.xhp\n"
"hd_id3149635\n"
"help.text"
msgid "To Insert a Section as a Link"
msgstr "Wótrězk ako zwězanje zasajźiś"

#. WDba2
#: section_insert.xhp
msgctxt ""
"section_insert.xhp\n"
"par_id3149648\n"
