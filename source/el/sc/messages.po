#: sc/inc/scfuncs.hrc:3809
msgctxt "SC_OPCODE_SUBSTITUTE"
msgid "Text"
msgstr "Κείμενο"

#: sc/inc/scfuncs.hrc:3810
msgctxt "SC_OPCODE_SUBSTITUTE"
msgid "The text in which partial words are to be replaced."
msgstr "Το κείμενο στο οποίο τμήματα λέξεων θα αντικατασταθούν."

