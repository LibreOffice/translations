msgid "Return value:"
msgstr "Τιμή επιστροφής:"

#: 03120201.xhp#par_id3149233.6.help.text
msgctxt "03120201.xhp#par_id3149233.6.help.text"
msgid "String"
msgstr "Συμβολοσειρά"

