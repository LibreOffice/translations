msgctxt ""
"03090402.xhp\n"
"par_id3156443\n"
"help.text"
msgid "ChooseMenu = Choose(Index, \"Quick Format\", \"Save Format\", \"System Format\")"
msgstr "ChooseMenu = Choose(Index, \"Quick Format\", \"Save Format\", \"System Format\")"

