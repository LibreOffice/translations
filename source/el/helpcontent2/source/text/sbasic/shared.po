msgid "d1 = InputBox$ (\"Enter the length of the side opposite the angle: \",\"opposite\")"
msgstr "d1 = InputBox$ (\"Εισαγάγετε το μήκος της απέναντι καθέτου της γωνίας: \",\"opposite\")"

