msgid "degrees=(radiant*180)/Pi"
msgstr "degrees=(radiant*180)/Pi"

#: 03080104.xhp#par_id3146975.13.help.text
msgid "radiant=(degrees*Pi)/180"
msgstr "radiant=(degrees*Pi)/180"

