msgctxt ""
"02140000.xhp\n"
"par_id3153720\n"
"help.text"
msgid "526"
msgstr "526"

#: 02140000.xhp
msgctxt ""
"02140000.xhp\n"
"par_id3154315\n"
"help.text"
msgid "Internal syntax error"
msgstr "Εσωτερικό συντακτικό σφάλμα"

#: 02140000.xhp
msgctxt ""
"02140000.xhp\n"
"par_id3083286\n"
"help.text"
msgid "Obsolete, no longer used, but could come from old documents if the result is a formula from a domain."
