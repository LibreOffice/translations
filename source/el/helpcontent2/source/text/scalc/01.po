msgctxt ""
"04060115.xhp\n"
"par_id3159341\n"
"help.text"
msgid "<item type=\"input\">=HEX2OCT(\"6a\";4)</item> returns 0152."
msgstr "Το <item type=\"input\">=HEX2OCT(\"6a\";4)</item> επιστρέφει 0152."

