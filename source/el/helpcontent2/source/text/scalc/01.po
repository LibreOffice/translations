"tit\n"
"help.text"
msgid "Delete"
msgstr "Διαγραφή"

#. g8zEj
#: 12090300.xhp
msgctxt ""
"12090300.xhp\n"
"hd_id3150276\n"
"help.text"
msgid "<link href=\"text/scalc/01/12090300.xhp\" name=\"Delete\">Delete</link>"
msgstr "<link href=\"text/scalc/01/12090300.xhp\" name=\"Διαγραφή\">Διαγραφή</link>"

#. YuSAB
#: 12090300.xhp
msgctxt ""
