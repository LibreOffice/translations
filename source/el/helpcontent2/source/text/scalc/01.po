msgctxt ""
"func_maxifs.xhp\n"
"par_id135761606425300\n"
"help.text"
msgid "<item type=\"input\">=MAXIFS(C2:C6;A2:A6;E2&\".*\";B2:B6;\"<\"&MAX(B2:B6))</item>"
msgstr "<item type=\"input\">=MAXIFS(C2:C6;A2:A6;E2&\".*\";B2:B6;\"<\"&MAX(B2:B6))</item>"

#. wHPFq
