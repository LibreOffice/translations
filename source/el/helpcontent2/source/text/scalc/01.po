"func_floor.xhp\n"
"par_id661586287604519\n"
"help.text"
msgid "<input>=FLOOR(-1.234)</input> returns -2."
msgstr "<input>=FLOOR(-1,234)</input> επιστρέφει -2."

#. 8ZGDc
