"func_floor.xhp\n"
"par_id741586287608968\n"
"help.text"
msgid "<input>=FLOOR(-45.67, -2, 0)</input> returns -46."
msgstr "<input>=FLOOR(-45,67, -2, 0)</input> επιστρέφει -46."

#. gXsTm
