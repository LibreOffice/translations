"04060106.xhp\n"
"par_id2963945\n"
"help.text"
msgid "<item type=\"input\">=FLOOR.PRECISE( -11;-2)</item> returns -12"
msgstr "Η <item type=\"input\">=FLOOR.PRECISE( -11;-2)</item> επιστρέφει -12"

