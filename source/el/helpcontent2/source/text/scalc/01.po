msgstr "Η <item type=\"input\">=FLOOR( -11;-2;0)</item> επιστρέφει -12"

#. aWzUK
#: 04060106.xhp
msgctxt ""
"04060106.xhp\n"
"par_id3163988\n"
"help.text"
msgid "<item type=\"input\">=FLOOR( -11;-2;1)</item> returns -10"
msgstr "Η <item type=\"input\">=FLOOR( -11;-2;1)</item> επιστρέφει -10"

