msgid "<image src=\"cmd/sc_formfiltered.png\" id=\"img_id3146130\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id3146130\">Icon Form Filter</alt></image>"
msgstr "<image src=\"cmd/sc_formfiltered.png\" id=\"img_id3146130\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id3146130\">Εικονίδιο φίλτρου φόρμας</alt></image>"

