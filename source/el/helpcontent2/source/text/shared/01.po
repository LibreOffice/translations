msgctxt ""
"02100001.xhp\n"
"tit\n"
"help.text"
msgid "List of Regular Expressions"
msgstr "Κατάλογος κανονικών εκφράσεων"

#. 2zsev
#: 02100001.xhp
msgctxt ""
"02100001.xhp\n"
"bm_id3146765\n"
