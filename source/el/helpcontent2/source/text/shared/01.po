msgid "Property"
msgstr "Ιδιότητα"

#. ihTK7
#: 06010601.xhp
msgctxt ""
"06010601.xhp\n"
"par_idN10590\n"
"help.text"
msgid "<ahelp hid=\".\">Defines the class of the selected term.</ahelp>"
msgstr "<ahelp hid=\".\">Καθορίζει τον τύπο του επιλεγμένου όρου.</ahelp>"

#. fAnJw
#: 06010601.xhp
msgctxt ""
"06010601.xhp\n"
"par_idN10593\n"
"help.text"
msgid "Add"
msgstr "Προσθήκη"

#. NTKDg
