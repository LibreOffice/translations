msgctxt ""
"05200200.xhp\n"
"par_id3149640\n"
"help.text"
msgid "<ahelp hid=\"cui/ui/linestyletabpage/MTR_FLD_LENGTH_2\">Enter the length of the dash.</ahelp>"
