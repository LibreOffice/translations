"hd_id729697\n"
"help.text"
msgid "Hide toolbar"
msgstr "Απόκρυψη της γραμμής εργαλείων"

#: ref_pdf_export.xhp
msgctxt ""
"ref_pdf_export.xhp\n"
"par_id769066\n"
"help.text"
