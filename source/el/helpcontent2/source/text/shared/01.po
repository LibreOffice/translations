msgctxt ""
"04150100.xhp\n"
"hd_id3153127\n"
"help.text"
msgid "Search..."
msgstr "Αναζήτηση..."

#: 04150100.xhp
msgctxt ""
"04150100.xhp\n"
"par_id3156326\n"
