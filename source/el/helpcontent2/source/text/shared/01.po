msgid "<link href=\"text/shared/01/02050000.xhp\" name=\"Copy\">Copy</link>"
msgstr "<link href=\"text/shared/01/02050000.xhp\" name=\"Αντιγραφή\">Αντιγραφή</link>"

#. nmrES
