"par_id3156042\n"
"help.text"
msgid "<ahelp hid=\"sfx/ui/descriptioninfopage/title\">Enter a title for the document.</ahelp>"
msgstr "<ahelp hid=\"sfx/ui/descriptioninfopage/title\">Εισάγετε ένα τίτλο για το έγγραφο.</ahelp>"

