"par_id3147580\n"
"help.text"
msgid "==="
msgstr "==="

#: 06040100.xhp
msgctxt ""
"06040100.xhp\n"
"par_id3145364\n"
"help.text"
msgid "1.1pt double underline"
msgstr "Διπλή υπογράμμιση 1.1pt"

