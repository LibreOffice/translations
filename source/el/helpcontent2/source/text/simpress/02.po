msgstr "<image id=\"img_id3153811\" src=\"cmd/sc_toggleobjectrotatemode.png\" width=\"1cm\" height=\"1cm\"><alt id=\"alt_id3153811\">Εικονίδιο περιστροφής</alt></image>"

