msgctxt ""
"3dsettings_toolbar.xhp\n"
"par_idN10594\n"
"help.text"
msgid "<ahelp hid=\".\">Tilts the selected object downwards by five degrees.</ahelp>"
msgstr "<ahelp hid=\".\">Δίνει κλίση προς τα κάτω 5 μοιρών στο επιλεγμένο αντικείμενο.</ahelp>"

