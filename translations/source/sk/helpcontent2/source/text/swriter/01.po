msgid "Anchor <-> Text"
msgstr "Ukotvenie <-> Text"

#: 02110000.xhp#par_id3150314.24.help.text
msgid "<ahelp hid=\"HID_NAVI_TBX12\">Jumps between the footnote text and the footnote anchor.</ahelp>"
msgstr "<ahelp hid=\"HID_NAVI_TBX12\">Prehľadáva medzi textom poznámky pod čiarou a jej ukotvením v texte.</ahelp>"

