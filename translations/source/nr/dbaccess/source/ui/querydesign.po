msgstr " - Nzinze ku - %PRODUCTNAME: Itlamo lesinghonghoyilo"

#: query.src#STR_VIEWDESIGN.string.text
msgid " - %PRODUCTNAME Base: View Design"
msgstr " - Nzinze ku - %PRODUCTNAME: Itlamo eliqalwako"

