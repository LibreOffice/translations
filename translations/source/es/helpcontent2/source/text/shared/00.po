#: 00040503.xhp#hd_id3155757.1.help.text
msgctxt "00040503.xhp#hd_id3155757.1.help.text"
msgid "Format Menu"
msgstr "Menú Formato"

#: 00040503.xhp#par_id3147294.2.help.text
msgid "Choose <emph>Format - Row - Height</emph>"
