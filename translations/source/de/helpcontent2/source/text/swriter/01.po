msgstr "<ahelp hid=\"SW:CHECKBOX:TP_TOX_SELECT:CB_FROMGRAPHICS\">Nimmt Grafiken in das Verzeichnis auf.</ahelp>"

#: 04120215.xhp#hd_id3149685.12.help.text
msgid "Text frames"
msgstr "Textrahmen"

#: 04120215.xhp#par_id3154195.13.help.text
msgid "<ahelp hid=\"SW:CHECKBOX:TP_TOX_SELECT:CB_FROMFRAMES\">Includes text frames in the index.</ahelp>"
msgstr "<ahelp hid=\"SW:CHECKBOX:TP_TOX_SELECT:CB_FROMFRAMES\">Nimmt Textrahmen in das Verzeichnis auf.</ahelp>"

#: 04120215.xhp#hd_id3155182.14.help.text
msgid "OLE objects"
msgstr "OLE-Objekten"

#: 04120215.xhp#par_id3143282.15.help.text
