msgctxt "04060116.xhp#hd_id3150465.82.help.text"
msgid "Syntax"
msgstr "Sintassi"

#: 04060116.xhp#par_id3146942.83.help.text
msgid "IMDIV(\"Numerator\"; \"Denominator\")"
msgstr "COMP.DIV(\"Numeratore\";\"Denominatore\")"

#: 04060116.xhp#par_id3150741.84.help.text
msgid "            <emph>Numerator</emph>, <emph>Denominator</emph> are complex numbers that are entered in the form \"x+yi\" or \"x+yj\"."
msgstr "<emph>Numeratore</emph>, <emph>Denominatore</emph> sono numeri complessi nella forma \"x+yi\" o \"x+yj\"."

