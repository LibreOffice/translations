msgctxt "04060116.xhp#hd_id3152980.78.help.text"
msgid "Example"
msgstr "Esempio"

#: 04060116.xhp#par_id3157901.79.help.text
msgid "            <item type=\"input\">=IMCOS(\"3+4j\") </item>returns -27.03-3.85i (rounded)."
msgstr "<item type=\"input\">=COMP.COS(\"3+4j\")</item> restituisce -27,03-3,85j (arrotondato)."

#: 04060116.xhp#bm_id3150024.help.text
msgid "<bookmark_value>IMDIV function</bookmark_value>"
msgstr "<bookmark_value>COMP.DIV, funzione</bookmark_value>"

#: 04060116.xhp#hd_id3150024.80.help.text
msgid "IMDIV"
msgstr "COMP.DIV"

#: 04060116.xhp#par_id3145825.81.help.text
msgid "<ahelp hid=\"HID_AAI_FUNC_IMDIV\">The result is the division of two complex numbers.</ahelp>"
msgstr "<ahelp hid=\"HID_AAI_FUNC_IMDIV\">Restituisce il quoziente di due numeri complessi.</ahelp>"

