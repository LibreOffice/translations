msgstr "Alterações"

#: 02230000.xhp#hd_id3152952.1.help.text
msgid "<link href=\"text/shared/01/02230000.xhp\" name=\"Changes\">Changes</link>"
msgstr "<link href=\"text/shared/01/02230000.xhp\" name=\"Changes\">Alterações</link>"

