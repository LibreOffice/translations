msgid "<link href=\"text/swriter/01/05040600.xhp\" name=\"Footnote\">Footnote</link>"
msgstr "<link href=\"text/swriter/01/05040600.xhp\" name=\"Footnote\">Oin-oharra</link>"

#: 05040600.xhp#par_id3149351.2.help.text
msgid "<ahelp hid=\"HID_FOOTNOTE_PAGE\">Specifies the layout options for footnotes, including the line that separates the footnote from the main body of document.</ahelp>"
msgstr "<ahelp hid=\"HID_FOOTNOTE_PAGE\">Oin-oharren diseinu-aukerak zehazten ditu, oin-oharra dokumentuaren gorputz nagusitik banatzen duen marra barne.</ahelp>"

