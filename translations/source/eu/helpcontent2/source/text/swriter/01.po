msgid "Paragraph"
msgstr "Paragrafoa"

#: 06080200.xhp#par_id3147526.9.help.text
msgid "<ahelp hid=\"SW:LISTBOX:TP_ENDNOTEOPTION:LB_PARA_TEMPL\">Select the paragraph style for the endnote text.</ahelp>"
msgstr "<ahelp hid=\"SW:LISTBOX:TP_ENDNOTEOPTION:LB_PARA_TEMPL\">Hautatu amaiera-oharren testuarentzako paragrafo-estiloa.</ahelp>"

