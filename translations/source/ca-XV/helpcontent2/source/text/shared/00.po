msgid "Edit Menu"
msgstr "Menú Edita"

#: 00000402.xhp#hd_id3147273.1.help.text
msgctxt "00000402.xhp#hd_id3147273.1.help.text"
msgid "Edit Menu"
msgstr "Menú Edita "

#: 00000402.xhp#par_id3085157.2.help.text
msgid "Choose <emph>Edit - Undo</emph>"
msgstr "Trieu <emph>Edita - Desfés</emph>"

