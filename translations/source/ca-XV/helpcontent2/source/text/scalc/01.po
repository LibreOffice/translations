msgid "1st, 2nd, 3rd Group"
msgstr "1r, 2n, 3r grup"

#: 12050100.xhp#hd_id3149784.1.help.text
msgid "<link href=\"text/scalc/01/12050100.xhp\" name=\"1st, 2nd, 3rd Group\">1st, 2nd, 3rd Group</link>"
