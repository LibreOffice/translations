msgstr "ツール バー"

#: 03050000.xhp#bm_id3145356.help.text
msgid "<bookmark_value>tools bar</bookmark_value>"
msgstr "<bookmark_value>ツールバー</bookmark_value>"

#: 03050000.xhp#hd_id3145356.1.help.text
msgid "<link href=\"text/shared/01/03050000.xhp\" name=\"Tools Bar\">Tools Bar</link>"
msgstr "<link href=\"text/shared/01/03050000.xhp\" name=\"ツールバー\">ツールバー</link>"

#: 03050000.xhp#par_id3150603.2.help.text
msgid "<ahelp hid=\".uno:ToolBarVisible\">Shows or hides the <emph>Tools bar</emph>.</ahelp>"
