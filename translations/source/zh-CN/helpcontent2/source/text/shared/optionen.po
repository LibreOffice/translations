msgstr "删除"

#: 01010700.xhp#par_id3148576.23.help.text
msgid "<ahelp hid=\"HID_OFA_SUBST_DELETE\">Deletes the selected font replacement.</ahelp>"
msgstr "<ahelp hid=\"HID_OFA_SUBST_DELETE\">删除选定的字体替换。</ahelp>"

