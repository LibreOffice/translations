msgstr "区域"

#: 05210000.xhp#hd_id3085157.1.help.text
msgid "<link href=\"text/shared/01/05210000.xhp\" name=\"Area\">Area</link>"
msgstr "<link href=\"text/shared/01/05210000.xhp\" name=\"区域\">区域</link>"

