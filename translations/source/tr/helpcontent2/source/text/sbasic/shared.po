msgctxt "03030206.xhp#hd_id3153969.5.help.text"
msgid "Return value:"
msgstr "Dönen değer:"

#: 03030206.xhp#par_id3156424.6.help.text
msgctxt "03030206.xhp#par_id3156424.6.help.text"
msgid "Date"
msgstr "Tarih"

