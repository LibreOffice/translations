msgid "a1 = \"start time\""
msgstr "a1 = \"başlangıç zamanı\""

#: 03030206.xhp#par_id3145800.34.help.text
msgid "b1 = \"end time\""
msgstr "b1 = \"bitiş zamanı\""

#: 03030206.xhp#par_id3151074.35.help.text
msgid "c1 = \"total time\""
msgstr "c1 = \"toplam zaman\""

