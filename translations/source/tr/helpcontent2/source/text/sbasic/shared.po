msgid "a2 = \"8:34\""
msgstr "a2 = \"8:34\""

#: 03030206.xhp#par_id3155602.38.help.text
msgid "b2 = \"18:12\""
msgstr "b2 = \"18:12\""

#: 03030206.xhp#par_id3150715.39.help.text
