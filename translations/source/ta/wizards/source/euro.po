#: euro.src#STEP_ZERO.string.text
msgid "~Cancel"
msgstr "~நீக்கு"

#: euro.src#STEP_ZERO___1.string.text
msgid "~Help"
msgstr "~உதவி"

#: euro.src#STEP_ZERO___2.string.text
msgid "<<~Back"
